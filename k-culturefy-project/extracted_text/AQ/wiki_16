7\ub0a0 \uaca9\ub82c\ud788 \ub2e4\ud22c\uac8c \ub418\ub294 \ud14c\uc624\ud544 \ub4dc \ube44\uc624\uc640 \ub9cc\ub098\uac8c \ub41c\ub2e4.\n\uc774\ud6c4 \uac8c\uc988 \ub4dc \ubc1c\uc790\ud06c\ub294 \uba54\uce20\uc758 \uc5d0\ud398\ub974\ub18d \uacf5\uc791\uc758 \ube44\uc11c\uac00 \ub41c\ub2e4. 1621\ub144\ubd80\ud130 1623\ub144\uae4c\uc9c0 \ub85c\ub9c8\uc5d0\uc11c 2\ub144\uac04 \ubc1c\ub808\ud0c0 \ucd94\uae30\uacbd\uc758 \uc9c1\uc6d0\uc73c\ub85c \uc77c\ud55c \ub4a4, \uadf8\ub294 \ud30c\ub9ac\ub85c \ub3cc\uc544\uc624\uace0, \uad81\uc815\uc758 \uc720\uba85\uc778\ub4e4\uacfc \uc9c0\uc778\ub4e4\uc5d0\uac8c \ubcf4\ub0b8 \uc11c\ud55c\uc73c\ub85c \uc720\uba85\ud574\uc9c4\ub2e4. \ub9ac\uc290\ub9ac\uc678\ub294 \ubc1c\uc790\ud06c\ub97c \uc8fc\ubaa9\ud558\uace0, \uadf8\uc5d0\uac8c \uc5f0\uac04 2,000 \ub9ac\ube0c\ub974 \uc218\uc785\uc758 \uc655\uc2e4\uace0\ubb38 \uce59\ud5c8\uc758 \uc0ac\ub8cc\ud3b8\ucc2c\uad00\uc758 \uc790\ub9ac\ub97c \uc900\ub2e4.\n1624\ub144 \uc790\uc2e0\uc758 \uc11c\ud55c\uc9d1 \uccab \uad8c\uc774 \ucd9c\uac04\ub418\uace0 \ubc1c\uc790\ud06c\uc758 \ub4dc\ub192\uc740 \uba85\ub9dd\uc774 \uc2dc\uc791\ub41c\ub2e4. \uadf8\ub294 \uc624\ud154 \ub4dc \ub791\ubd80\uc608\uc758 \ub300\uac00\uac00 \ub418\uba70, \uc7a5 \uc0e4\ud50c\ub7ad, \ub9d0\ub808\ub974\ube0c, \ubd80\uc544\ub85c\ubca0\ub974 \ub4f1\uacfc \uce5c\ud558\uac8c \uc9c0\ub0b8\ub2e4.\n\uadf8\ub7ec\ub098 \uc11c\ud55c\uc5d0\uc11c \ubcf4\uc778 \ubb34\uc885\uad50\uc801\uc778 \uae30\uc0c9\uc774\ub77c\uace0 \uc5ec\uaca8\uc9c4 \uadf8\uc758 \uad50\ub9cc\uc740 \uba38\uc9c0\uc54a\uc544 \uc608\uc218\ud68c\uc758 \ud504\ub791\uc218\uc544 \uac00\ub77c\uc2a4\uc5d0\uac8c \ube44\ub09c\uc744 \ubc1b\uac8c \ub41c\ub2e4. \ub2e4\uc74c \ud574\uc5d0 \uadf8\ub294 1627\ub144 \uc790\uc2e0\uc758 \ud338\ud50c\ub9bf&lt;Lettres de Phyllarque \u00e0 Ariste&gt;\uc5d0\uc11c \uace0\uae08\ub798 \uc791\uac00\ub4e4\uc744 \ud45c\uc808\ud55c \ud610\uc758\ub85c \ud440\uc591\ud68c \uc218\ub3c4\uc6d0\uc7a5 \uad74\ub93c\uc5d0\uac8c \ube44\ub09c \ubc1b\ub29