\n\ud14c\ub514\uc640 \ucf00\uc774\ud2b8\ub294 \ub098\ubb34 \uc704\uc5d0 \uac78\ub824 \uc788\ub358 \uc120\ubb3c \ubcf4\ub530\ub9ac\ub97c \ubc1c\uacac\ud55c\ub2e4. \ucf00\uc774\ud2b8\ub294 \uc0b0\ud0c0\uac00 \ub9d0\ud55c \ub300\ub85c \uafb8\ub7ec\ubbf8 \uc548\uc73c\ub85c \ub4e4\uc5b4\uac00 \uc5d8\ud504\ub4e4\uacfc \ub9cc\ub098\ub294\ub370, \uadf8 \uc0ac\uc774 \ud14c\ub514\ub294 \ubc94\uc8c4\uc870\uc9c1\uc5d0\uac8c \uac78\ub824 \uc120\ubb3c \ubcf4\ub530\ub9ac\uc640 \ud568\uaed8 \uadf8\ub4e4\uc758 \uc544\uc9c0\ud2b8\ub85c \ub04c\ub824\uac00\uace0 \ub9cc\ub2e4. \uadf8\ub7f0\ub370 \ubcf4\ub530\ub9ac\ub97c \ud480\uc5b4 \ubcf4\ub2c8 \uc120\ubb3c\uc0c1\uc790 \uc548\uc5d0\ub294 \uc11d\ud0c4\ub369\uc5b4\ub9ac\ubfd0\uc774\uc5c8\uace0, \ubd84\ub178\ud55c \uc870\uc9c1 \ub450\ubaa9\uc774 \ubcf4\ub530\ub9ac\ub97c \ubd88\ud0dc\uc6b0\ub824 \ud558\uc790 \ud14c\ub514\uac00 \ud544\uc0ac\uc801\uc73c\ub85c \ub9c9\ub294\ub2e4. \ud14c\ub514\uac00 \uc704\uae30\uc5d0 \ucc98\ud588\uc744 \ub54c \ubcf4\ub530\ub9ac\uc5d0\uc11c \uc5d8\ud504\ub4e4\uc774 \ub098\ud0c0\ub098 \uc870\uc9c1\uc6d0\ub4e4\uc744 \uc804\ubd80 \ucad3\uc544\ub0b4\uc5b4 \ubc84\ub9b0\ub2e4.\n\ud55c\ud3b8 \uc0b0\ud0c0\ub97c \ub05d\ub0b4 \ubbff\uc9c0 \uc54a\ub294 \uacbd\ucc30\uad00 \ub370\uc774\ube0c \ub54c\ubb38\uc5d0 \uc720\uce58\uc7a5\uc5d0 \uc218\uac10\ub41c \uc0b0\ud0c0\ub294 '\ud06c\ub9ac\uc2a4\ub9c8\uc2a4 \uc815\uc2e0'\uc774 19%\uae4c\uc9c0 \ub5a8\uc5b4\uc9c4 \uac83\uc744 \ubc1c\uacac\ud55c\ub2e4. \uc0b0\ud0c0\ub294 \uc774\ub97c \ub04c\uc5b4\uc62c\ub9ac\uae30 \uc704\ud574 \uc218\uac10\uc790\ub4e4\uc744 \ub370\ub9ac\uace0 \uc5f4\uad11\uc801\uc778 \ud06c\ub9ac\uc2a4\ub9c8\uc2a4 \ucf58\uc11c\ud2b8 \ubb34\ub300\ub97c \uc5f0\ucd9c\ud55c\ub2e4. \ub9c8\uce68\ub0b4 \ub370\uc774\ube0c\uac00 \uc758\uc2ec\uc744 \uac70\ub450\uace0 \uc720\uce58\uc7a5 \ubb38\uc744 \uc5f4\uc5c8\uc744 \ub54c, \uc5d8\ud504\uac00 \ub098\ud0c0\ub098 \uc0b0\ud0c0\uc5d0\uac8c \ub9c8\ubc95 \ubaa8\uc790\ub97c \uac74\ub124\uace0 \uc0b0\ud0c0\ub294 \uc21c\uc2dd\uac0