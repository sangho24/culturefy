744 \uc815\ubcf4 \uc740\ud3d0 \ub4f1\uc744 \uc774\uc720\ub85c \uc870\uc9c0\uace0\uc640 \uc11d\uc131\uc744 \ud0c4\ud575\ud558\ub294 \uc0c1\uc8fc\ubb38\uc744 \uacc4\uc18d \uc81c\ucd9c\ud558\uc600\ub2e4. \ub9cc\ub82525\ub144(1597) 2\uc6d4 9\uc77c \uc870\uc815 \ub17c\uc758\uc5d0\uc11c \uc870\uc120 \uc7ac\ud30c\ubcd1\uc774 \uacb0\uc815\ub418\uc5c8\ub2e4.\n\uadf8\ub7ec\ub098 \uc870\uc815 \ub17c\uc758\uac00 \uacb0\uc815\ub418\uace0 \ub098\uc11c\ub3c4 \uc5b8\uad00\ub4e4\uc740 \uacc4\uc18d\ud574\uc11c \uc870\uc9c0\uace0\uc640 \uc11d\uc131\uc758 \uc798\ubabb\uc744 \ub4e4\uc5b4 \ud0c4\ud575\uc744 \uc8fc\uc7a5\ud558\uc600\ub2e4. \uadf8\ub54c\ub9c8\ub2e4 \ub9cc\ub825\uc81c\ub294 \ubb34\uc5b8\uc73c\ub85c \ud68c\ub2f5\ud558\uc600\ub2e4. \uc774\ub54c \uc81c\ucd9c\ub41c \ud0c4\ud575 \uc0c1\uc18c\ubb38\uc5d0 \ub300\ud558\uc5ec \uc0b4\ud3b4\ubcf4\uba74 \ub2e4\uc74c\uacfc \uac19\ub2e4. \ub9cc\ub82525\ub144(1597) 2\uc6d4 8\uc77c, \uc808\uac15\ub3c4\uc5b4\uc0ac(\u6d59\u6c5f\u9053\u5fa1\u53f2) \uc8fc\uacf5\uad50(\u5468\u5b54\u6559)\uc640 \uac15\uc11c\ub3c4\uc5b4\uc0ac(\u6c5f\u897f\u9053\u5fa1\u53f2) \ud669\uae30\ud604(\u9ec3\u7d00\u8ce2)\uc740 \uc11d\uc131\uacfc \uc870\uc9c0\uace0\uac00 \ubd80\ud654\ub1cc\ub3d9\ud558\uc5ec \uad70\uc8fc\ub97c \uae30\ub9cc(\u6276\u540c\u6b3a\u7f54)\ud55c\ub2e4\uace0 \ud0c4\ud575\ud558\uc600\uace0, \ub9cc\ub825\uc81c\ub294 \ub17c\uc758\ud560 \uac83\uc744 \uc9c0\uc2dc\ud558\uc600\ub2e4. \uc870\uc9c0\uace0\ub294 \uc790\uc2e0\uc744 \ud601\uc9c1\uc2dc\ucf1c\uc11c \uc5ec\ub860\uc744 \uc7a0\uc7ac\uc6b8 \uac83\uc744 \uc694\uccad\ud558\uc600\uc73c\ub098 \ub9cc\ub825\uc81c\ub294 \ubd88\ud5c8\ud558\uc600\ub2e4. \uac19\uc740\ub2ec 14\uc77c, \ubcd1\uacfc\uc88c\uae09\uc0ac\uc911(\u5175\u79d1\u5de6\u7d66\u4e8b\u4e2d) \uc11c\uc131\ucd08(\u5f90\u6210\u695a)\ub294 \uc11d\uc131\uacfc \uc870\uc9c0\uace0\ub97c \uae09\ud788 \ud30c\uc9c1\ud560 \uac83\uc744 \uccad\ud558\uc600\uc73c\ub098 \ub9cc\ub825\uc81c\ub294 \ub300\ub2f5\ud558\uc9c0 \uc54a\uc558\ub2e4. \uac19\uc740\ub2ec 18\uc