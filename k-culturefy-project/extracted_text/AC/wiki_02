\ub41c\ub2e4.\n\uc0dd\uc2dd\uc5d0 \ubbf8\uce58\ub294 \uc601\ud5a5.\n\uc218\uc815 \ud6c4 \ucc98\uc74c 2\uc8fc \ub3d9\uc548\uc758 \ubc29\uc0ac\uc120 \uce58\ub8cc\ub294 \uce58\uba85\uc801\uc774\uc9c0\ub9cc \uae30\ud615\uc720\ubc1c\ubb3c\uc9c8\uc740 \uc5c6\ub2e4. \uc784\uc2e0 \uc911 \uace0\uc120\ub7c9\uc758 \ubc29\uc0ac\uc120\uc5d0 \ub178\ucd9c\ub418\uba74 \uae30\ud615, \uc131\uc7a5 \uc7a5\uc560, \uc9c0\uc801 \uc7a5\uc560\ub97c \uc720\ubc1c\ud558\uace0, \uc790\ub140\uc5d0\uac8c \ubc0f \uae30\ud0c0 \uc885\uc591\uc774 \ubc1c\uc0dd\ud560 \uc704\ud5d8\uc774 \uc99d\uac00\ud560 \uc218 \uc788\ub2e4. \uc774\uc804\uc5d0 \ubc29\uc0ac\uc120 \uce58\ub8cc\ub97c \ubc1b\uc740 \ub0a8\uc131\uc758 \uacbd\uc6b0, \uce58\ub8cc \ud6c4\uc5d0 \uc784\uc2e0\ud55c \uc790\ub140\uc758 \uc720\uc804\uc801 \uacb0\ud568\uc774\ub098 \uc120\ucc9c\uc131 \uae30\ud615\uc758 \uc99d\uac00\ub294 \uc5c6\ub294 \uac83\uc73c\ub85c \ubcf4\uc778\ub2e4. \uadf8\ub7ec\ub098 \ubcf4\uc870\uc0dd\uc2dd\uae30\uc220\uacfc \ubbf8\uc138\uc870\uc791\uae30\uc220\uc744 \uc0ac\uc6a9\ud558\uba74 \uc774\ub7ec\ud55c \uc704\ud5d8\uc774 \uc99d\uac00\ud560 \uc218 \uc788\ub2e4.\n\ub1cc\ud558\uc218\uccb4\uc5d0 \ubbf8\uce58\ub294 \uc601\ud5a5.\n\ub1cc\ud558\uc218\uccb4\uc800\ud558\uc99d\uc740 \uc77c\ubc18\uc801\uc73c\ub85c \uc548\uc7a5 \ubc0f \uc548\uc7a5\uc606 \uc2e0\uc0dd\ubb3c, \uc138\ud3ec\uc678 \ub1cc\uc885\uc591, \ub450\uacbd\ubd80 \uc885\uc591\uc5d0 \ub300\ud55c \ubc29\uc0ac\uc120 \uce58\ub8cc\ub97c \ud55c \ub2e4\uc74c, \uc804\uc2e0 \uc545\uc131 \uc885\uc591\uc5d0 \ub300\ud55c \uc804\uc2e0 \ubc29\uc0ac\uc120 \uc870\uc0ac \ud6c4\uc5d0 \ubc1c\uc0dd\ud55c\ub2e4. \uadf8\ub9ac\uace0 \uc18c\uc544\uc554 \uce58\ub8cc\ub97c \ubc1b\ub294 \uc5b4\ub9b0\uc774\uc758 40~50%\uc5d0\uc11c \ub0b4\ubd84\ube44 \ubd80\uc791\uc6a9\uc774 \ubc1c\uc0dd\ud55c\ub2e4. \ubc29\uc0ac\uc120 \uc720\ubc1c \ub1cc\ud558\uc218\uccb4\uc800\ud558\uc99d\uc740 \uc8fc\ub85c \uc131\uc7a5\ud638\ub974\ubaac\uacfc \uc0dd\uc2dd\uc120\ud638\ub974\ubaac\uc5d0 \uc601\ud5a5\uc744 \ub