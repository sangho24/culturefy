 \ucd9c\uc804\ud55c \uac70\uc54c \uc870\ucee4\uac00 \uc720\uc77c\ud558\uac8c \uc2b9\ub9ac\ud558\uba70 \ubca0\ud2b8\ub0a8\uc744 2\u00bd\u20131\u00bd\uc758 \uc2a4\ucf54\uc5b4\ub85c \uaebe\uc5c8\uace0, \uc544\uc81c\ub974\ubc14\uc774\uc794\uacfc \ubd88\uac00\ub9ac\uc544\ub294 \uac01\uac01 \ubabd\uace8\uacfc \uc774\uc2a4\ub77c\uc5d8\uc744 \uc0c1\ub300\ub85c \uc2b9\ub9ac\ub97c \uac70\ub450\uc5c8\ub2e4. \uc2a4\ud398\uc778\uacfc \uc544\ub974\uba54\ub2c8\uc544\uc758 \uacbd\uae30\ub294 \uc591 \ud300\uc774 2\uc2b9\uc529 \uc8fc\uace0\ubc1b\uc73c\uba70 2-2\ub85c \ubb34\uc2b9\ubd80\ub85c \ub05d\ub0ac\ub2e4.\n\ub300\ud55c\ubbfc\uad6d\uc740 \ub374\ub9c8\ud06c \ud300\uacfc \uacbd\uae30\ub97c \ud3bc\ucce4\uc73c\uba70, \uac15\uc18c\ud604\uacfc \uae40\uc0ac\ub791\uc774 \ubb34\uc2b9\ubd80\ub97c \uac70\ub480\uc73c\ub098 \ub098\uba38\uc9c0 \uc0c1\uc704 2\uac1c \ubcf4\ub4dc\uc5d0\uc11c \ud328\ubc30\ud558\uba74\uc11c 1-3\uc73c\ub85c \ud328\ubc30\ud588\ub2e4.\n11\ub77c\uc6b4\ub4dc.\n\uc778\ub3c4\uc640 \uce74\uc790\ud750\uc2a4\ud0c4\uc740 \uacf5\ub3d9 1\uc704\ub85c \ub9c8\uc9c0\ub9c9 \ub77c\uc6b4\ub4dc\uc5d0 \uc9c4\ucd9c\ud588\ub2e4. \uc778\ub3c4\ub294 \ud558\ub9ac\uce74 \ub4dc\ub85c\ub098\ubc1c\ub9ac, \ub514\ube44\ub0d0 \ub370\uc2dc\ubb34\ud06c, \ubc18\ud2f0\uce74 \uc544\uadf8\ub77c\uc648\uc758 \uc2b9\ub9ac\ub85c \uc544\uc81c\ub974\ubc14\uc774\uc794\uc5d0 3\u00bd\u2013\u00bd\ub85c \uc644\uc2b9\uc744 \uac70\ub450\uba70 \uae08\uba54\ub2ec\uc744 \ubc1b\uc558\uace0 \uce74\uc790\ud750\uc2a4\ud0c4\uc740 \ubbf8\uad6d\uacfc 1\uc2b9 1\ud328\ub85c \ubb34\uc2b9\ubd80\ub97c \uae30\ub85d\ud588\ub2e4. \uce74\uc790\ud750\uc2a4\ud0c4\uc740 \uc568\ub9ac\uc2a4 \ub9ac\uac00 \uc804\uc220\uc744 \ub193\uce58\uba70 \uc54c\ub8e8\uc544 \ub204\ub974\ub9cc\uc774 \ud3f0\uc744 \ub530\ub0b4\uba70 \uc2b9\ub9ac\ud560 \uc218 \uc788\ub294 \uae30\ud68c\ub97c \uc7a1\uc558\ub2e4. \ud558\uc9c0\ub9cc \ubbf8\uad6d\uacfc \ubb34\uc2b9\ubd80\ub97c \uae30\ub85d\ud588\ub2e4. \uce74\uc790\ud750\uc2a4\ud0c4\uc740 \uc778\ub3c4\uc5d0\uac8c \uc2b9\uc810 1\uc810\uc744 \ub4a4\uc838 \uc740\uba54\ub2ec\uc744 \ud68d\ub4dd\ud588\uc73c\uba70 \ubbf8\uad6d\uc774 \uc2a4\ud398\uc778, \uc544\ub974\uba54\ub2c8\uc544, \uc870\uc9c0\uc544\uc640\uc758 \ud0c0\uc774\ube0c\ub808\uc774\ucee4\uc5d0\uc11c \uc2b9\ub9ac\ud558\uba70 \ub3d9\uba54\ub2ec\uc744 \uc5bb\uc5c8\ub2e4.\n\ub300\ud55c\ubbfc\uad6d\uc740 \uc54c\uc81c\ub9ac\uc640 \uacbd\uae30\ub97c \ud588\uc73c\uba70, \uc870\uc5f0\ud76c\uc640 \uae40\uc0ac\ub791\uc740 \ubb34\uc2b9\ubd80\ub97c \uc774\ub918\uc73c\ub098 \uc0c1\uc704 2\uac1c \ubcf4\ub4dc\uc5d0\uc11c \ud328\ubc30\ud558\uba70 1-3\uc73c\ub85c \ud328\ubc30\ud588\ub2e4.\n\uacbd\uae30 \uacb0\uacfc.\n\uc778\ub3c4\ub294 \uc5ec\uc131 \ubd80\ubb38\uc5d0\uc11c 9\uc2b9 1\ubb34 1\ud328\ub97c \uae30\ub85d\ud558\uba70 \ucd1d \uc2b9\uc810 19\uc810\uc744 \uc5bb\uc5b4 \uae08\uba54\ub2ec\uc744 \ud68d\ub4dd\ud588\ub2e4. \uc778\ub3c4\ub294 \uc774\ubc88 \ub300\ud68c\uc5d0\uc11c \ucc98\uc74c\uc73c\ub85c \uc62c\ub9bc\ud53c\uc544\ub4dc \uc885\ud569 \uc6b0\uc2b9\uc744 \uc774\ub904\ub0c8\ub2e4. \ud3c9\uade0 \ub808\uc774\ud305\uc5d0 \ub530\ub77c \ub300\ud68c \uc774\uc804 \ud0d1 \uc2dc\ub4dc\ub97c \ubc1b\uc740 \uc778\ub3c4\ud300\uc740 7\ub77c\uc6b4\ub4dc\uc5d0\uc11c \uc870\uc9c0\uc544\ub97c, 10\ub77c\uc6b4\ub4dc\uc5d0\uc11c \uc911\uad6d\uc744 \uaebe\uc73c\uba70 \uae08\uba54\ub2ec\ub85c \ud5a5\ud558\ub294 \uae38\uc744 \uac14\uc73c\ub098 8\ub77c\uc6b4\ub4dc\uc5d0\uc11c 3\ubc88 \uc2dc\ub4dc\ub97c \ubc1b\uc740 \ud3f4\ub780\ub4dc\uc5d0\uac8c \ud328\ubc30\ud588\uace0 9\ub77c\uc6b4\ub4dc\uc5d0\uc11c\ub294 \ubbf8\uad6d\uacfc \ubb34\uc2b9\ubd80\uac00 \uc77c\uc5b4\ub0ac\ub2e4. \uce74\uc790\ud750\uc2a4\ud0c4\uc740 \uc2b9\uc810 18\uc810(8\uc2b9 2\ubb34 1\ud328)\ub85c \uc740\uba54\ub2ec\uc744 \ud68d\ub4dd\ud588\uace0, \ubbf8\uad6d\uc774 \uc2b9\uc810 17\uc810(8\uc2b9 1\ubb34 2\ud328)\uc73c\ub85c \ub3d9\uba54\ub2ec\uc744 \ucc28\uc9c0\ud588\ub2e4. \ubbf8\uad6d \ub300\ud45c\ud300\uc740 \uc2a4\ud398\uc778, \uc544\ub974\uba54\ub2c8\uc544, \uc870\uc9c0\uc544\u