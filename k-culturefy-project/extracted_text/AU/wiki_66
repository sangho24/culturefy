544 \uc0ac\ud68c\uc8fc\uc758 \uc5f0\ubc29\uacf5\ud654\uad6d.\n1945\ub144\ubd80\ud130 1991\ub144\uae4c\uc9c0 \uc720\uace0\uc2ac\ub77c\ube44\uc544 \uc0ac\ud68c\uc8fc\uc758 \uc5f0\ubc29\uacf5\ud654\uad6d\uc758 \uc778\uad6c \ud1b5\uacc4\uc5d0\ub294 \uc778\uad6c \ubc00\ub3c4, \ubbfc\uc871, \uad50\uc721 \uc218\uc900, \uad6d\ubbfc \uac74\uac15, \uacbd\uc81c\uc801 \uc9c0\uc704, \uc885\uad50\uc801 \uc18c\uc18d \ub4f1\uc774 \ud3ec\ud568\ub418\uc5c8\ub2e4. 1991\ub144 \ub9c8\uc9c0\ub9c9 \uc778\uad6c \uc870\uc0ac\uc5d0\uc11c \uc720\uace0\uc2ac\ub77c\ube44\uc544\uc758 \uc778\uad6c\ub294 23,528,230\uba85\uc73c\ub85c \uc9d1\uacc4\ub418\uc5c8\ub2e4. \uc138\ub974\ube44\uc544\uc778\uc774 \uac00\uc7a5 \ub9ce\uc558\uace0, \uadf8 \ub2e4\uc74c\uc73c\ub85c \ud06c\ub85c\uc544\ud2f0\uc544\uc778, \ubcf4\uc288\ub0d0\ud06c\uc778, \uc54c\ubc14\ub2c8\uc544\uc778, \uc2ac\ub85c\ubca0\ub2c8\uc544\uc778, \ub9c8\ucf00\ub3c4\ub2c8\uc544\uc778\uc774 \ub4a4\ub97c \uc774\uc5c8\ub2e4.\n\ubbfc\uc871.\n\uc774 \uc790\ub8cc\ub294 \uc9c0\ub09c \ub124 \ucc28\ub840\uc758 \uc720\uace0\uc2ac\ub77c\ube44\uc544 \uc778\uad6c \uc870\uc0ac(1961\ub144, 1971\ub144, 1981\ub144, 1991\ub144) \uc790\ub8cc\uc774\ub2e4. (\ud5cc\ubc95\uc5d0 \uba85\uc2dc\uc801\uc73c\ub85c \uc5b8\uae09\ub418\uc5b4 \uc788\uc5c8\uace0, \uc18c\uc218 \ubbfc\uc871\uc774\ub098 \uc774\ubbfc\uc790\ub85c \uac04\uc8fc\ub418\uc9c0 \uc54a\uc558\ub358) \uad6c\uc131 \ubbfc\uc871\uc740 \uad75\uc740 \uae00\uc528\ub85c \ud45c\uc2dc\ud558\uc600\ub2e4.\n\uc778\uad6c\ubcc4 \uacf5\ud654\uad6d.\n\uc778\uad6c \uc790\ub8cc\ub294 1991\ub144 \uc778\uad6c \uc870\uc0ac \uacb0\uacfc\ub97c \uae30\ubc18\uc73c\ub85c \ud55c\ub2e4.\n\ucd5c\ub300 \ub3c4\uc2dc.\n1991\ub144 \uc778\uad6c \uc870\uc0ac\uc5d0 \ub530\ub974\uba74, \uc720\uace0\uc2ac\ub77c\ube44\uc544\uc5d0\ub294 \uc778\uad6c\uac00 10\ub9cc \uba85\uc774 \ub118\ub294 \ub3c4\uc2dc\uac00 19\uac1c \uc788\uc5c8\ub2e4. \uc774 \ub3c4\uc2dc\uc758 \uc778\uad6c\ub294 \ub2e4\uc74c\uacfc \uac19\uc774 \ubc1c\uc804\ud558\uc600\ub2e4:\n\u