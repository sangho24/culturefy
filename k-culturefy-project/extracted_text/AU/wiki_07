\n\ud751\uc6b4\ubaa8 \ud654\uac15\uc554.\n\ud751\uc6b4\ubaa8 \ud654\uac15\uc554(biotite granite)\uc740 \ucda9\uc8fc\uc2dc \uc2dc\ub0b4 \ub3d9\ubd80\uc9c0\uc5ed(\ucda9\uc8fc\ubd84\uc9c0)\uacfc \ub300\uc18c\uc6d0\uba74, \ub178\uc740\uba74, \uc911\uc559\ud0d1\uba74, \uc591\uc131\uba74, \uc18c\ud0dc\uba74, \uc5c4\uc815\uba74 \uc77c\ub300\uc5d0 \uad11\ubc94\uc704\ud558\uac8c \ubd84\ud3ec\ud558\uba70 \uc8fc\ub85c \uc800\ubc18(\u5e95\u76e4)\uc744 \ud615\uc131\ud558\uace0 \uc788\ub2e4. \uc8fc \uad6c\uc131 \uad11\ubb3c\uc740 \ud751\uc6b4\ubaa8, \uc11d\uc601, \uc0ac\uc7a5\uc11d\uc774\ub2e4.\n\ubc31\uc6b4\ubaa8 \ud654\uac15\uc554.\n\ubc31\uc6b4\ubaa8 \ud654\uac15\uc554(Muscovite granite)\uc740 \ub3d9\ub7c9\uba74 \uc11c\uc6b4\ub9ac\ub97c \uc911\uc2ec\uc73c\ub85c \uc554\uc8fc(\u5dd6\u67f1) \ud615\ud0dc\ub85c \uc870\uc120 \ub204\uce35\uad70 \uc11d\ud68c\uc554, \uac01\uc12c\uc11d, \uc625\ucc9c \ub204\uce35\uad70 \ud669\uac15\ub9ac\uce35\uc744 \uad00\uc785\ud558\uace0 \uc788\ub2e4. \ubcf8 \uc554\uc740 \uc911\ub9bd-\uc870\ub9bd\uc9c8\uc73c\ub85c \ud751\uc6b4\ubaa8\uac00 \uc548 \ubcf4\uc778\ub2e4.\n\ud654\uac15\ubc18\uc554.\n\ud654\uac15\ubc18\uc554(Kgp)\uc740 \uc218\uc548\ubcf4\uba74 \uc218\ud68c\ub9ac\uc640 \uc0b4\ubbf8\uba74 \ubb38\uac15\ub9ac\uc758 \uacbd\uacc4\uc120\uc744 \ub530\ub77c \uc625\ucc9c \ub204\uce35\uad70 \ud669\uac15\ub9ac\uce35\uc744 \uad00\uc785\ud558\uc600\ub2e4. \uc8fc \uad6c\uc131 \uad11\ubb3c\uc740 \uac01\uc12c\uc11d, \uc11d\uc601, \uc0ac\uc7a5\uc11d\uc774\ub2e4.\n\ub2e8\uce35.\n\ucda9\uc8fc\uc2dc\uc5d0\ub294 \ub300\uaddc\ubaa8\uc758 \ub2e8\uce35\uc740 \uc5c6\uace0 \uc911, \uc18c\uaddc\ubaa8\uc758 \ub2e8\uce35\ub9cc \ubd84\ud3ec\ud55c\ub2e4.\n\ud558\ucc9c \ub2e8\uce35.\n\ud558\ucc9c(\u8377\u5ddd) \ub2e8\uce35\uc740 \ub3d9\ub7c9\uba74 \ud558\ucc9c\ub9ac\ub97c \uc911\uc2ec\uc73c\ub85c \ub0a8-\ubd81 \uc8fc\ud5a5\uc744 \uac00\uc9c0\ub294 \uacbd\uc0ac \ub2e8\uce35\uc774\ub2e4. \uc625\ucc9c \ub204\uce35\uad70\uc758 \ud669\uac15\ub9ac\uce35\uacfc \uc11c\ucc3d\ub9ac\uce35\uc744 \uc808\ub2e8\ud558\uace0 \uc788\ub2e4.\n\uc778\ub4f1\uc0b0 \ub2e8\uce35.\n\uc778\ub4f1\uc0b0 \ub2e8\uce35\uc740 \uc778\ub4f1\uc0b0 \ub3d9\uce21\uc5d0\uc11c \ub0a8-\ubd81 \uc8fc\ud5a5\uc744 \uac00\uc9c0\ub294 \ub2e8\uce35\uc774\ub2e4.\n\uc138\uace1 \ub2e8\uce35.\n\uc138\uace1 \ub2e8\uce35\uc740 \uc0b4\ubbf8\uba74 \uc2e0\ub2f9\ub9ac\uc758 \uc138\uace1 \ub9c8\uc744\uc744 \ubd81\ub3d9 40\u00b0\uc8fc\ud5a5\uc73c\ub85c \uc9c0\ub098\ub294 \uc218\uc9c1 \ub2e8\uce35\uc73c\ub85c \uc81c\ucc9c\uc2dc \ud55c\uc218\uba74 \uc11c\ucc3d\ub9ac\uc5d0\uc11c \ub300\ubbf8\uc0b0(680.8 m)\uacfc \uc801\ubcf4\uc0b0 \ubd80\uadfc\uc744 \uc9c0\ub098 \uad34\uc0b0\uad70 \uc7a5\uc5f0\uba74\uc73c\ub85c \uc774\uc5b4\uc9c4\ub2e4.\n\ud3ec\ud0c4\ub9ac \ub2e8\uce35.\n\ud3ec\ud0c4\ub9ac \ub2e8\uce35(\u6d66\u7058\u7406 \u5074\u508d\u647a\u52d5\u65b7\u5c64)\uc740 \ub3d9\ub7c9\uba74 \ud3ec\ud0c4\ub9ac\ub97c \uc9c0\ub098\ub294 \ubd81\uc11c 80\u00b0\ubc0f 85\u00b0\ubc29\ud5a5\uc73c\ub85c \ud3c9\ud589\ud558\uac8c \ubc1c\ub2ec\ud558\ub294 2\uc870\uc758 \ub2e8\uce35\uc73c\ub85c \ub300\ud5a5\uc0b0 \uaddc\uc554\uce35\uc5d0\uc11c \uc57d 250 m\uc758 \uc2ac\ub9bd(slip)\ucc28\uac00 \ud655\uc778\ub41c\ub2e4.\n\uc0ac\uae30\ub9ac \ub2e8\uce35.\n\uc0ac\uae30\ub9ac \uc2a4\ub7ec\uc2a4\ud2b8 \ub2e8\uce35\uc740 \ub3d9\ub7c9\uba74 \uc0ac\uae30\ub9ac\ub97c \uc911\uc2ec\uc73c\ub85c \ubd81\ub3d9-\ub0a8\uc11c \uc8fc\ud5a5\uc73c\ub85c \ubc1c\ub2ec\ud558\uba70 \ub2e8\uce35\uba74\uc774 \ubd81\ucabd\uc73c\ub85c 50\u00b0\uacbd\uc0ac\ud558\uace0 \ubd81\uce21\uc774 \uc0c1\uc2b9, \ub0a8\uce21\uc774 \ud558\uac15\ud55c \uc5ed\ub2e8\uce35\uc774\ub2e4.\n\uae30\ud0c0.\n\uc0b4\ubbf8\uba74 \uc7ac\uc624\uac1c\ub9ac\uc5d0\ub294 \ud56d\uc0b0\ub9ac \ub3cc\ub85c\ub9c8\uc774\ud2b8\uc9c8 \uc11d\ud68c\uc554\uacfc \ub300\ud5a5\uc0b0 \uaddc\uc554\uc744 \uc218\ud3c9 \ubcc0\uc704\uc2dc\ud0a4\ub294 \uc11c\ubd81\uc11c \uc8fc\ud5a5\uc758 \uc18c\uaddc\ubaa8 \ub2e8\uce35\uc774 \uc788\ub2e4.\n\uad11\uc0b0\uacfc \uc9c0\ud558\uc790\uc6d0.\n\ucda9\uc8fc\uc2