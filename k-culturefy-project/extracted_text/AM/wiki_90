744 \uac83\uc774\ub77c\uace0 \uc5b8\uae09\ud558\uc600\ub2e4. \uadf8\ub7ec\ub098 \ub2f9\uc2dc \ub9c8\uc774\ud06c\ub85c\uc18c\ud504\ud2b8\ub294 \uc0c8\ub85c\uc6b4 \uc7a5\uce58\ub4e4\uc774 \ud504\ub9ac\ubdf0 \ud504\ub85c\uadf8\ub7a8\uc5d0 \ucd94\uac00\ub420 \uac83\uc778\uc9c0, \uc989 \uc708\ub3c4\uc6b0 10 \ubaa8\ubc14\uc77c\uc774 \uc644\uc804\ud55c \uc0c1\ud0dc\uc5d0 \uc5b4\ub290 \uc815\ub3c4 \uc788\ub294\uc9c0\uc5d0 \ub300\ud55c \uc0c1\ud669\uc5d0 \ub300\ud55c \uc5b4\ub5a0\ud55c \uc815\ubcf4\ub3c4 \uc81c\uacf5\ud558\uc9c0 \uc54a\uc558\ub2e4. \ub300\uc2e0 \ub9c8\uc774\ud06c\ub85c\uc18c\ud504\ud2b8\ub294 \uc790\uc0ac\uc758 \ub8e8\ubbf8\uc544 950, \ub8e8\ubbf8\uc544 950 XL \ub300\ud45c \uc81c\ud488, \uc800\uac00\ud615 \ub8e8\ubbf8\uc544 550, \ub8e8\ubbf8\uc544 650\uc744 \ud3ec\ud568\ud558\uc5ec \uc708\ub3c4\uc6b0 10 \ubaa8\ubc14\uc77c\uc774 \uae30\ubcf8 \ud3ec\ud568\ub41c \uc0c8\ub85c\uc6b4 \uc7a5\uce58\ub4e4\uc744 \ud64d\ubcf4\ud558\ub294\ub370 \uc9d1\uc911\ud558\uc600\ub2e4. \uc774 \ub9b4\ub9ac\uc2a4 \uc774\ud6c4\ub85c \uc774 \uc0c8\ub85c\uc6b4 \uc708\ub3c4\uc6b0 10 \uc7a5\uce58\ub4e4\uc740 2015\ub144 12\uc6d4 4\uc77c\uc758 \ube4c\ub4dc 10586\uc744 \uae30\uc810\uc73c\ub85c \ucc38\uac00\uc790 \ud504\ub85c\uadf8\ub7a8\uc744 \ud1b5\ud574 \ubbf8\ub9ac \uc55e\uc73c\ub85c\uc758 \uc5c5\ub370\uc774\ud2b8\ub97c \ubc1b\uc744 \uc218 \uc788\uac8c \ub418\uc5c8\ub2e4. \uc708\ub3c4\uc6b0 \uae30\ubc18 LG \ub79c\uc14b \ub610\ud55c \uc774 \ubc84\uc804\uc744 \ubc1b\uc558\uc73c\ub098 \uadf8 \ub4a4\ub85c \uc5c5\ub370\uc774\ud2b8\ub418\uc9c0\ub294 \uc54a\uc558\ub2e4.\n2016\ub144 2\uc6d4 19\uc77c, \ub9c8\uc774\ud06c\ub85c\uc18c\ud504\ud2b8\ub294 \uc708\ub3c4\uc6b0 10 \ubaa8\ubc14\uc77c \ub808\ub4dc\uc2a4\ud1a4(Redstone) \ud504\ub9ac\ubdf0 \ube4c\ub4dc 14267\uc744 \uccab \ucd9c\uc2dc\ud558\uc600\ub2e4. \uc774 \ube4c\ub4dc\ub97c \uc2dc\uc791\uc73c\ub85c, \uc55e\uc73c\ub85c\uc758 \ud504\ub9ac\ubdf0 \ubc84\uc804\ub4e4\uc740 \uc774\ubbf8 \ube44 \uc778\uc0ac\uc774\ub354 \ud504\ub9ac\ubdf0\uc758 \u