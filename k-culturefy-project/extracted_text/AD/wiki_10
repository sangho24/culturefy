\ub808\ub4dc\ub294 \uc5d0\ub370\uc0ac \ubc31\uad6d\uc758 \uc12d\uc815\uc744 \uacb8\ud558\uac8c \ub418\uc5c8\ub2e4. \ubcf4\uc5d0\ubabd\uc740 1111\ub144 \uc774\ud0c8\ub9ac\uc544\ub85c \ub3cc\uc544\uac00 \uac70\uae30\uc11c \uc8fd\uc5c8\ub2e4.\n\ubcf4\uc5d0\ubabd\uc774 \uc8fd\uc790 \ud669\uc81c \uc54c\ub809\uc2dc\uc6b0\uc2a4 1\uc138\ub294 \ud0d5\ud06c\ub808\ub4dc\uc5d0\uac8c \uc548\ud2f0\uc624\ud0a4\uc544 \uacf5\uad6d\uc744 \ubc18\ud658\ud558\ub77c\uace0 \uc694\uad6c\ud588\uc9c0\ub9cc \ud0d5\ud06c\ub808\ub4dc\ub294 \ud2b8\ub9ac\ud3f4\ub9ac \ubc31\uad6d, \uc608\ub8e8\uc0b4\ub818 \uc655\uad6d \ub4f1 \uc774\uc6c3 \uc2ed\uc790\uad70 \uad6d\uac00\ub4e4\uc758 \uc9c0\uc6d0\uc744 \ubc1b\uc544 \uc774\ub97c \uac70\uc808\ud588\ub2e4. \ud0d5\ud06c\ub808\ub4dc\ub294 1112\ub144 \uc8fd\uc5c8\uace0 \uadf8 \ub4a4\ub97c \uc774\uc5b4 \ud0d5\ud06c\ub808\ub4dc\uc758 \uc870\uce74 \uc0b4\ub808\ub974\ubaa8\uc758 \ub8e8\uc9c0\uc5d0\ub85c\uc758 \uc12d\uc815\ud558\uc5d0 \ubcf4\uc5d0\ubabd 2\uc138\uac00 \uacf5\uad6d\uc744 \ubc1b\uc558\ub2e4.\n\ub8e8\uc9c0\uc5d0\ub85c\ub294 1119\ub144 \uc774\uc2ac\ub78c\uacfc\uc758 \uc804\ud22c\uc5d0\uc11c \uc8fd\uc5c8\uace0 \uacf5\uad6d\uc740 \ub2e4\uc2dc \uc608\ub8e8\uc0b4\ub818\uc758 \ubcf4\ub450\uc575 2\uc138\uc758 \uc12d\uc815\uc73c\ub85c \ub118\uc5b4\uac14\ub2e4\uac00 \ub2e4\uc2dc \ubcf4\ub450\uc575\uc758 \uc5b4\ub9b0 \ub538 \uc548\ud2f0\uc624\ud0a4\uc544\uc758 \ucf69\uc2a4\ud0d5\uc2a4\uc640 \uadf8\uc758 \ub0a8\ud3b8 \uc548\ud2f0\uc624\ud0a4\uc544\uc758 \ub808\ubabd\uc5d0\uac8c\ub85c \uc0c1\uc18d\ub418\uc5c8\ub2e4. \ub808\ubabd\uc740 \ube44\uc794\ud2f0\uc6c0 \uc81c\uad6d\uc758 \ud0ac\ub9ac\ud0a4\uc544\ub97c \uacf5\uaca9\ud588\uc73c\ub098 \ube44\uc794\ud2f0\uc6c0 \ud669\uc81c \uc694\ud55c\ub124\uc2a4 2\uc138\uc758 \ubc18\uaca9\uc744 \ubc1b\uc558\uace0 \uc694\ud55c\ub124\uc2a4\ub294 1138\ub144 \uc548\ud2f0\uc624\ud0a4\uc544\uae4c\uc9c0 \uc9c4\uaca9\ud588\ub2e4. \uadf8\ub7ec\ub098 \uc5d0\ub370\uc0ac\uc758 \uc870\uc2ac\ub7ad 2\uc138\uac00 \ubc18\ub780\uc744 \uc77