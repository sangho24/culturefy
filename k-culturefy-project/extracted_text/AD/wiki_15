\ub9f9\uad70\uc774 \ubd81\ucabd\uc744 \ud5a5\ud574 \uc774\ubbf8 \uc804\uc5f4\uc744 \uad6c\uc131\ud558\uace0 \uc788\uc5c8\ub354\ub77c\uba74 \uacf5\uaca9\uc740 \uadf8\ub4e4\uc758 \uc6b0\uc775\uc744 \ub178\ub838\uc744 \uac83\uc774\ub2e4. \ub9cc\uc57d \uc5f0\ud569\uad70\uc774 \uc5ec\uc804\ud788 \uc885\ub300\uc758 \ud615\ud0dc\ub85c \ub3d9\ucabd, \ud639\uc740 \ubd81\ub3d9\ucabd\uc744 \ud5a5\ud574 \uc9c4\uad70\ud588\ub354\ub77c\uba74, \uc5f0\ud569\uad70\uc758 \uc885\ub300\uc758 \ub05d\ubd80\ubd84\uc740 \ub098\uba38\uc9c0 \ubd80\ubd84\uc774 \uc0c8\ub85c\uc6b4 \uc704\uce58\uc5d0\uc11c \uc9c4\uc5f4\uc744 \uc9dc\uae30\ub3c4 \uc804\uc5d0 \uaca9\ud30c \ub2f9\ud588\uc744 \uac83\uc774\ub2e4. - \uc774 \uc2dc\ub300\uc5d0 \uc804\ud22c\ub300\ud615\uc744 \uc9dc\ub294 \uac83\uc740 \uc624\ub798 \uac78\ub9ac\ub294 \uc77c\uc774\uc5c8\uae30 \ub54c\ubb38\uc774\ub2e4. \uc801\uc744 \uad50\ub780\ud558\uae30 \uc704\ud574 \ud3f0 \uc790\uc774\ub4dc\ub9ac\uce20(von Seydlitz)\ub294 \uc6b4\uc6a9 \uac00\ub2a5\ud55c \uae30\ubcd1\ub300\ub97c \uc774\ub04c\uace0 \uc11c\ub458\ub7ec \uc57c\ub204\uc2a4 \ud6c4\uac94 \uc5b8\ub355\uc758 \ub4a4\ucabd \ud3f4\uc820 \ud6c4\uac94 \uc5b8\ub355\uc73c\ub85c \ub098\uc544\uac14\ub2e4. \ud3f0 \ubb90\ub7ec(von Moller)\uac00 18\ubb38\uc758 \uc911\ud3ec\ub97c \uc774\ub04c\uace0 \uc57c\ub178\uc2a4 \ud6c4\uac94\uc758 315\ubbf8\ud130 \uace0\uc9c0\uc5d0\uc11c \uc9c4\uad70\ud574\uc624\ub294 \uc5f0\ud569\uad70 \uae30\ubcd1\ub300\uc640 \ub4a4\ub530\ub974\ub294 \ubcf4\ubcd1\ub300\uc5d0 \ud3ec\uaca9\uc744 \uac00\ud588\ub2e4.\n\ubb90\ub7ec\uc758 \uc911\ud3ec\uc5d0 \ud3ec\uaca9\uc744 \ubc1b\uc73c\uba74\uc11c \ub3d9\ub9f9\uad70 \uae30\ubcd1\ub300\ub294 \ub3cc\uaca9\ud558\uc600\uace0, \ub77c\uc774\ud558\ub974\ud2b8\uc2a4\ubca0\ub974\ubca4\uc758 \ubd81\ubd80 \ubcf4\ubcd1\ub300\uc758 \uc804\uc704\uc5d0 \uc790\ub9ac\ub97c \uc7a1\uc558\uc73c\ub098, \ubb90\ub7ec\uc758 \ud3ec\uaca9\uc740 \uac70\uc138\uc9c0\uae30\ub9cc \ud558\uc600\ub2e4. \uadf8\ub7ec\ub098 \ub2f9\uc2dc\uc5d0\ub294 \ud1f4\uac0