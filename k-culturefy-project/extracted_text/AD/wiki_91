\ucc3c\ub2e4. \uc624\uc5b8\uc740 1963\ub144\uc758 \ubcf4\ube44 \ubb34\uc5b4 \uc774\ub798 \ucd5c\uc5f0\uc18c \uc8fc\uc7a5\uc774\uc5c8\uace0, \uc774\ud6c4\uc5d0\ub294 \uc5d0\ub9ad\uc190\ud638\uc5d0\uc11c \ubca0\ucef4\uc774 \ubd80\uc7ac\ud560 \uacbd\uc6b0 \uc8fc\uc7a5\uc744 \ucc30 \ubd80\uc8fc\uc7a5\uc73c\ub85c \uc815\uc2dd \ud65c\uc57d\ud588\ub2e4.\n2002\ub144 FIFA \uc6d4\ub4dc\ucef5 \ubcf8\uc120\uc5d0\uc11c \uc624\uc5b8\uc740 \uc870\ubcc4 \ub9ac\uadf8\uc5d0\uc11c \ubb34\ub4dd\uc810\uc73c\ub85c \uce68\ubb35\ud588\ub2e4. \uadf8\ub7ec\ub098, \uadf8\ub294 \uc544\ub974\ud5e8\ud2f0\ub098\uc640\uc758 \uacbd\uae30\uc5d0\uc11c \ub9c8\uc6b0\ub9ac\uc2dc\uc624 \ud3ec\uccb4\ud2f0\ub178\uc5d0 \uac78\ub824\ub118\uc5b4\uc9c0\ub294 \uc2dc\ubbac\ub808\uc774\ud305\uc744 \ud574 \ud53c\uc5d0\ub97c\ub8e8\uc774\uc9c0 \ucf5c\ub9ac\ub098 \uc8fc\uc2ec\uc744 \uc644\ubcbd\ud788 \uc18d\uc600\uc73c\uba70 \uc774\ub85c\uc778\ud574 \uc789\uae00\ub79c\ub4dc\uc758 1-0 \ud398\ub110\ud2f0\ud0a5 \uacb0\uc2b9\uace8\uc774 \ub420 \ud398\ub110\ud2f0\ud0a5\uc744 \uc720\ub3c4\ud588\ub2e4. \uc624\uc5b8\uc740 \ub374\ub9c8\ud06c\uc640\uc758 16\uac15\uc804\uc5d0\uc11c \uace8\ubb38\uc744 \uc5f0 \ud6c4, \ube0c\ub77c\uc9c8\uacfc\uc758 8\uac15\uc804\uc5d0\uc11c \uc789\uae00\ub79c\ub4dc\uc758 \uc120\uc81c\uace8\uc744 \uae30\ub85d\ud588\uc9c0\ub9cc, \ucd5c\uc885\uc801\uc73c\ub85c 1-2 \uc5ed\uc804\ud328\ub97c \ub2f9\ud588\ub2e4.\nUEFA \uc720\ub85c 2004.\n\uc624\uc5b8\uc740 UEFA \uc720\ub85c 2004 \uc608\uc120\uc804\uc5d0\uc11c 5\uace8\uc744 \uae30\ub85d\ud588\ub2e4.\n\ub300\ud68c \ubcf8\uc120\uc5d0\uc11c \uc624\uc5b8\uc740 \uc870\ubcc4 \ub9ac\uadf8\uc5d0\uc11c \ub610\ub2e4\uc2dc \uace8\uc744 \uc2e0\uace0\ud558\uc9c0 \ubabb\ud588\ub2e4. \uc774\ud6c4, \uadf8\ub294 \ud3ec\ub974\ud22c\uac08\uacfc\uc758 8\uac15\uc804\uc5d0\uc11c 3\ubd84\ub9cc\uc5d0 \uace8\uc744 \uae30\ub85d\ud574 \uc789\uae00\ub79c\ub4dc \ucd5c\ucd08\ub85c 4\ubc88\uc758 \uc8fc\uc694 \ub300\ud68c \ubcf8\uc120\uc5d0\uc11c \uace8\uc744 \ub123\uc740 \uc120\uc218\u