uad6c\uc131\ub420 \uc218\ub3c4 \uc788\ub2e4. \ud504\ub9ac\ub4dc\uba3c\uc740 \ubc95 \ubb38\ud654\uc758 \ub2e4\uc591\uc131\uc744 \uac15\uc870\ud558\uace0 \ub2e4\uc591\ud55c \ucd94\uc0c1 \uc218\uc900, \uc608\ub97c \ub4e4\uc5b4 \ubc95 \ubb38\ud654 \uc218\uc900\uc5d0\uc11c \ubc95 \ubb38\ud654\ub97c \ud0d0\uad6c\ud560 \uc218 \uc788\ub2e4\uace0 \uc9c0\uc801\ud55c\ub2e4. \ud504\ub9ac\ub4dc\uba3c\uc740 '\ub0b4\ubd80'\uc640 '\uc678\ubd80' \ubc95\ub960 \ubb38\ud654\ub97c \uad6c\ubd84\ud55c \uac83\uc73c\ub85c\ub3c4 \uc54c\ub824\uc838 \uc788\ub294\ub370, \uc804\uc790\ub294 \ubc95\uc5d0 \ub300\ud55c \uc77c\ubc18\uc801\uc778 \ud0dc\ub3c4\uc640 \uc778\uc2dd\uc744 \uac00\ub9ac\ud0a8\ub2e4. \uc0ac\ubc95\ubd80\uc640 \uac19\uc740 \ubc95\ub960 \uc2dc\uc2a4\ud15c\uc758 \uae30\ub2a5\uc778 \ubc18\uba74, \ud6c4\uc790\ub294 \ubc95\ub960 \uc2dc\uc2a4\ud15c\uc774\ub098 \ubc95\uacfc \uc9c8\uc11c \uc804\ubc18\uc5d0 \ub300\ud55c \uc2dc\ubbfc\uc758 \ud0dc\ub3c4\ub97c \ub098\ud0c0\ub0bc \uc218 \uc788\ub2e4.\n\ud398\ubbf8\ub2c8\uc998.\n\ubc95\uc740 \ud56d\uc0c1 \ud398\ubbf8\ub2c8\uc998 \ucc38\uc5ec\uc758 \uc911\uc694\ud55c \uc7a5\uc18c \uc911 \ud558\ub098\ub85c \uac04\uc8fc\ub418\uc5b4 \uc654\ub2e4. Ruth Fletcher\uac00 \uc9c0\uc801\ud588\ub4ef\uc774 \ubc95\uc5d0 \ub300\ud55c \ud398\ubbf8\ub2c8\uc2a4\ud2b8\uc758 \ucc38\uc5ec\ub294 \uc218\ub144\uc5d0 \uac78\uccd0 \ub2e4\uc591\ud55c \ud615\ud0dc\ub97c \ucde8\ud588\uc73c\uba70 \uc774\ub294 \ub610\ud55c \uc774\ub860\uacfc \uc2e4\ucc9c\uc758 \uc131\uacf5\uc801\uc778 \ud1b5\ud569\uc744 \ub098\ud0c0\ub0b8\ub2e4. \uc804\ubb38\uc801\uc778 \uc870\uc5b8 \uc11c\ube44\uc2a4\ub97c \uc81c\uacf5\ud558\uba74\uc11c \uc5ec\uc131 \ub2e8\uccb4\ub294 \ubc95\uc801\uc778 \uac1c\ub150\uacfc \ubc29\ubc95\uc744 \ube44\ud310\uc801\uc73c\ub85c \ubd84\uc11d\ud568\uc73c\ub85c\uc368 \ubc95\uc801 \ub17c\uc758\uc758 \uc870\uac74\uc5d0 \uc758\ubb38\uc744 \uc81c\uae30\ud588\ub2e4.\n\uc138\uacc4\ud654.\n\uc138\uacc4\ud654\ub294 \uc885\uc885 \uc138\uacc4 \uc0ac\ud68c \uc218\uc900\uc5d0\u