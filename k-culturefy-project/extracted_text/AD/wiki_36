 \uc624\uac00\uc0ac\uc640\ub77c \ub098\uac00\ud1a0\ud0a4()\ub97c \uaca9\ud30c\ud558\uc5ec \ub2e4\uc2dc \uc6b0\uc704\uc5d0 \uc130\uace0, \ub374\ubd84 19\ub144(1550\ub144)\uc5d0\ub294 \uc624\uac00\uc0ac\uc640\ub77c \ub098\uac00\ud1a0\ud0a4\ub97c \ubab0\uc544\ub0b4\uace0 \uc911\uc2dc\ub098\ub178\ub97c \uc81c\uc555\ud558\uc600\ub2e4.\n\uac19\uc740 \ud574 \ud558\ub8e8\ub178\ubd80\ub294 \ubb34\ub77c\uce74\ubbf8 \uc694\uc2dc\ud0a4\uc694\uc758 \uc9c0\uc131(\u652f\u57ce) \ub3c4\uc774\uc2dc\uc131()\uc744 \uacf5\uaca9\ud588\uc73c\ub098, \ub610 \ud55c \ubc88 \ubf08\uc544\ud514 \ud328\ubc30\ub97c \uacaa\uc5c8\ub2e4. [\ub3c4\uc774\uc2dc\uad6c\uc988\ub808 ()]\n\uadf8\ub7ec\ub098 \ub2e4\uc74c\ud574 \ub374\ubd84 20\ub144(1551\ub144) \uc0ac\ub098\ub2e4 \uc720\ud0a4\ub2e4\uce74()\uc758 \ubaa8\ub7b5\uc5d0 \uc758\ud574 \ub3c4\uc774\uc2dc\uc131\uc744 \ud568\ub77d\ud558\ub294 \ub370 \uc131\uacf5\ud558\uc5ec, \ubb34\ub77c\uce74\ubbf8 \uc694\uc2dc\ud0a4\uc694\ub294 \uac00\uc4f0\ub77c\uc624 \uc131()\uc5d0 \uace0\ub9bd\ub418\uc5b4 \ub300\ud56d\ud560 \ud798\uc744 \uc783\uc5b4\ubc84\ub838\ub2e4.\n\ud558\ub8e8\ub178\ubd80\ub294 \uc5d0\uce58\uace0\ub178\ucfe0\ub2c8\uc640\uc758 \uad6d\uacbd\uc9c0\ub300\uc778 \ubd81\uc2dc\ub098\ub178 \uc77c\ubd80(\uac00\uc640\ub098\uce74\uc9c0\ub9c8)\uc640 \ub0a8\uc2dc\ub098\ub178 \uc77c\ubd80\ub97c \uc81c\uc678\ud55c \uc2dc\ub098\ub178\ub178\ucfe0\ub2c8\uc758 \uac70\uc758 \uc804\uc5ed\uc744 \uc81c\uc555\ud558\uc600\ub2e4. \uc774\ub85c\uc368 \ub2e4\ucf00\ub2e4 \ud558\ub8e8\ub178\ubd80\uc640 \uc5d0\uce58\uace0\ub178\ucfe0\ub2c8\uc758 \ub098\uac00\uc624 \uac00\ucf00\ud1a0\ub77c(\uc6b0\uc5d0\uc2a4\uae30 \uac90\uc2e0)\ub294 \uc11c\ub85c \uad6d\uacbd\uc744 \ub9c8\uc8fc\ubcf4\uace0 \ub300\uce58\ud558\uac8c \ub418\uc5c8\ub2e4.\n\uac00\uc640\ub098\uce74\uc9c0\ub9c8.\n\uc2dc\ub098\ub178\ub178\ucfe0\ub2c8 \ubd81\ub3d9\ubd80, \uc2dc\ub098\ub178 \uad6d\uacbd\uc9c0\ub300 \uc8fc\ubcc0\uc758 \uc9c0\ucfe0\ub9c8\uac00\uc640 \uac15\ubcc0\uc5d0\ub294 \uc820\ucf54\uc9c0 \ud3c9\uc57c\u