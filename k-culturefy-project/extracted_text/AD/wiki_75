\uc544 \uace4\uc7a5\uc744 \ub0a8\ubc1c\ud558\uace0 \uc990\uacbc\ub2e4\uace0 \ud55c\ub2e4. \uc774\ub294 \uc911\uc559\uc5d0 \uc54c\ub824\uc9c0\uc9c0 \uc54a\uc558\uc9c0\ub9cc \uc131\ub300\uc911\uc774 \uc774\ub97c \ub4e3\uace0 \uccad\uc131\uc7a1\uae30\uc5d0 \uae30\uc220\ud558\uc600\ub2e4.\n\uadf8\ubc16\uc5d0\ub3c4 \uc655\uc870\uc2e4\ub85d\uc5d0 \uc758\ud558\uba74 \uc804\ub77c\ub3c4\uc5d0\uc11c \uadf8\uac00 \ud0d0\ud559\ud558\uac8c \uad74\uc5b4\uc11c \ud638\ubc88 \uc0ac\ub78c\ub4e4\uc774 \uadf8 \uace0\uae30\ub97c \uba39\uc73c\ub824 \ud55c\ub2e4\ub294 \ub9d0\uc774 \ub3cc\uc558\ub294\ub370, \uc774\uac83\uc774 \uc2e4\ub85d\uc5d0 \uc2e4\ub838\ub2e4. \uadf8\ub7ec\ub098 \uadf8\uac00 \uc804\ub77c\ub3c4\uc5d0\uc18c \uc5b4\ub5bb\uac8c \ud0d0\ud559\ud55c \uc9d3\uc744 \ud588\ub294\uc9c0\ub294 \uc655\uc870\uc2e4\ub85d\uc5d0 \uad6c\uccb4\uc801\uc73c\ub85c \uae30\ub85d\ub418\uc9c0 \uc54a\uc558\ub2e4.\n1757\ub144 \ud638\uc870\ucc38\uc758, \uc131\uade0\uad00\ub300\uc0ac\uc131\uc5d0 \uc784\uba85\ub410\ub2e4. \uadf8 \ud6c4 \uc2b9\uc9c0, \ud55c\uc131\ubd80\uc6b0\uc724, \ub300\uc0ac\ud5cc, 1760\ub144 \ub3c4\uc2b9\uc9c0, \ube44\ubcc0\uc0ac \ub2f9\uc0c1 \ub4f1\uc744 \uc9c0\ub0b4\uace0, 1762\ub144 \ud638\ub0a8\uc548\uc9d1\uc0ac(\u6e56\u5357\u5b89\u96c6\u4f7f)\ub85c \uc21c\ubb34\ud558\uace0 \ub3cc\uc544\uc654\ub2e4. 1764\ub144 \ud55c\uc131\ubd80\uc88c\uc724\uc774 \ub418\uc5c8\ub2e4. \uc774\ud6c4 \ud638\uc870\ucc38\ud310, \uc608\uc870\ucc38\ud310, \ub3c4\uc2b9\uc9c0 \ub4f1\uc744 \uac70\uccd0 1765\ub144 \uacbd\uae30\ub3c4\uad00\ucc30\uc0ac\ub97c \uc5ed\uc784\ud558\uace0, \uc790\ud5cc\ub300\ubd80\ub85c \uc2b9\uc9c4, \uacf5\uc870\ud310\uc11c\uc640 \uc774\uc870\ud310\uc11c, \ubcd1\uc870\ud310\uc11c \ub4f1 6\uc870\uc758 \ud310\uc11c\ub97c \ub450\ub8e8 \uc9c0\ub0c8\ub2e4.\n\uadf8\uac00 \uc5b4\ub5a4 \uc774\uc720\ub85c \uc138\uc190 \uc0b0\uacfc \uac08\ub4f1\ud588\ub294\uc9c0, \uc138\uc190\uc758 \ucd94\ub300\uc5d0 \ubc18\ub300\ud588\ub294\uc9c0\ub294 \uc54c\ub824\uc9c4 \uac83\uc774 \uc5c6\ub2e4. \uadf8\u