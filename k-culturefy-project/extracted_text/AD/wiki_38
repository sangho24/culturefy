8 \uc911\uad6d\uc744 \uc720\uc804\uc790\uc870\uc791 \ucf69\uc73c\ub85c\ubd80\ud130 \ubcf4\ud638\ud558\uae30 \uc704\ud574 \uc138\uacc4\uc801\uc778 \ucf69 \uac70\ub798 \uc5c5\uccb4\uc778 \ubc88\uc9c0(Bunge)\uc0ac\uc5d0 \ucf69\uc758 \uc911\uad6d \uc218\ucd9c\uc744 \ub9c9\uae30 \uc704\ud55c \ud3b8\uc9c0\ub97c \ubc1c\uc1a1\ud558\ub294 \u2018\uc0ac\uc774\ubc84 \ub9cc\ub9ac\uc7a5\uc131\u2019 \uc0ac\uc5c5\ub3c4 \ub4e4\uc5b4\uc788\ub2e4.\n\uc870\uc9c1.\n\uadf8\ub9b0\ud53c\uc2a4\ub294 \uc5ed\uc0ac\uc0c1 \ucd5c\ub300\uc758 \uad6d\uc81c\ud658\uacbd\ud68c\uc758\ub85c \ubd88\ub9ac\ub294 92\ub144 \ube0c\ub77c\uc9c8 \ub9ac\uc6b0\ub370\uc790\ub124\uc774\ub8e8\uc5d0\uc11c \uc5f4\ub9b0 \uc720\uc5d4\ud658\uacbd\uac1c\ubc1c\ud68c\uc758(UNCED) \ub2f9\uc2dc \uac1c\ub3c4\uad6d\uc774\ub098 \uc81c3\uc138\uacc4 \ud658\uacbd\ub2e8\uccb4\ub85c\ubd80\ud130 \uadf8\ub9b0\ud53c\uc2a4\ub294 \uc120\uc9c4\uad6d \uc911\uc2ec\uc801 \uc870\uc9c1\uc774\ub77c\ub294 \ube44\ud310\ub3c4 \ubc1b\uc558\uc9c0\ub9cc, \ud68c\uc758\ub97c \uacc4\uae30\ub85c \uc911\ub0a8\ubbf8, \ub3d9\uc720\ub7fd, \uc544\uc2dc\uc544 \ub4f1\uc73c\ub85c \uc870\uc9c1\uc744 \ub354\uc6b1 \ud655\ub300\ud588\ub2e4. \ud558\uc9c0\ub9cc \uc804 \uc138\uacc4\uc801\uc73c\ub85c 1\ucc9c\uc5ec \uba85\uc5d0 \ub2ec\ud558\ub294 \uc0c1\uadfc\ud65c\ub3d9\uac00 \ub4f1 \uac70\ub300\ud55c \uc870\uc9c1\uc744 \uc6b4\uc601\ud558\uace0, \ub2e4\uc591\ud55c \ud65c\ub3d9\uc744 \ud574\ub098\uac00\ub294\ub370 \uc7ac\uc815\uc801 \uc5b4\ub824\uc6c0\ub3c4 \uc5c6\uc9c0 \uc54a\uc558\ub2e4. \ud2b9\ud788 \ub9ce\uc740 \uc9c0\ubd80\uac00 \uad6d\uc81c\ubcf8\ubd80\uc5d0 \uc7ac\uc815\uc801\uc73c\ub85c \uc758\uc874\ud574\uc628 \uac83\ub3c4 \uc5b4\ub824\uc6c0\uc744 \ub354\ud558\ub294 \uc694\uc778\uc774\uc5c8\ub2e4. \uadf8\ub798\uc11c \ud68c\uc6d0\uc774 \uc218\uc2ed\ub9cc \uba85\uc5d0 \uc774\ub974\ub294 \ub3c5\uc77c\uc774\ub098, \ub124\ub35c\ub780\ub4dc \uadf8\ub9b0\ud53c\uc2a4 \ub4f1\uc740 \uc804\uccb4\uc758 \uc7ac\uc815\uc744 \uafb8\ub824\ub098\uac00\ub294 \ub370 \uc808\ub300\uc8