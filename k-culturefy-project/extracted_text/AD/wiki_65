\uc11c 0-1\uc73c\ub85c \ud328\ud558\uba70 \uc900\uc6b0\uc2b9\uc744 \ucc28\uc9c0\ud588\ub2e4.\n\ud558\uacc4 \uc62c\ub9bc\ud53d.\n\uc131\uc778\ub300\ud45c\ud300 \uc2dc\uc808\uc5d0\ub294 3\ubc88 \ucd9c\uc804\ud558\uc5ec 1984\ub144 \ub300\ud68c\uc5d0\uc11c 8\uac15\uc5d0 \uc9c4\ucd9c\ud588\uace0 U-23 \ub300\ud45c\ud300\uc73c\ub85c\ub294 2000\ub144 \ub300\ud68c\uc5d0\uc11c \ub3d9\uba54\ub2ec\uc744 \ubaa9\uc5d0 \uac78\uba74\uc11c \uc0ac\uc0c1 \uccab \uc62c\ub9bc\ud53d \uba54\ub2ec\uc774\ub77c\ub294 \ucf8c\uac70\ub97c \uc774\ub918\uc73c\ub098 \uc774\ud6c4\uc758 \uc62c\ub9bc\ud53d \ubcf8\uc120 \uae30\ub85d\uc740 \uc804\ubb34\ud558\ub2e4.\n\ud32c\uc544\uba54\ub9ac\uce78 \uac8c\uc784.\n\ud32c\uc544\uba54\ub9ac\uce78 \uac8c\uc784\uc5d0\ub294 5\ubc88 \ucd9c\uc804\ud558\uc5ec \uc740\uba54\ub2ec 1\uac1c(1987\ub144), \ub3d9\uba54\ub2ec 2\uac1c(1951\ub144, 1963\ub144)\ub97c \ud68d\ub4dd\ud588\uc73c\uba70 1995\ub144 \ub300\ud68c\uc5d0\uc11c\ub294 8\uac15\uc5d0 \uc9c4\ucd9c\ud588\uace0 1983\ub144 \ub300\ud68c\uc5d0\uc11c\ub294 1\uc2b9 2\ubb34\ub77c\ub294 \ub098\uc058\uc9c0 \uc54a\uc740 \uc131\uc801\uc744 \uac70\ub450\uc5c8\uc74c\uc5d0\ub3c4 \uc870\ubcc4\ub9ac\uadf8 \ud0c8\ub77d\uc758 \uace0\ubc30\ub97c \ub9c8\uc168\ub2e4.\n\uc120\uc218.\n\ud604\uc7ac \uc120\uc218 \uba85\ub2e8.\n\ub2e4\uc74c \uc120\uc218\ub4e4\uc740 2022\ub144 6\uc6d4 6\uc77c \uacfc\uc758 \uce5c\uc120 \uacbd\uae30\uc640 6\uc6d4 10\uc77c\uc5d0\uc11c 14\uc77c\uae4c\uc9c0 \uc9c4\ud589\ub418\ub294 2022\ub144 \uae30\ub9b0\ucef5\uc744 \uc704\ud574 \uc120\ubc1c\ub41c \uc120\uc218 \uba85\ub2e8\uc774\ub2e4.\n\"\ucd9c\uc7a5 \uc218\uc640 \uace8 \uc218\ub294 2022\ub144 3\uc6d4 29\uc77c \uc6b0\ub8e8\uacfc\uc774\uc804 \uc774\ud6c4 \uae30\ub85d\uc774\ub2e4\"\n\uc120\uc218 \uae30\ub85d.\n\ucd5c\ub2e4 \ucd9c\uc7a5 \uc120\uc218.\n\uc544\ub798 \uae30\ub85d\uc740 2022\ub144 2\uc6d4 1\uc77c\uc744 \uae30\uc900\uc73c\ub85c \ud55c\ub2e4. (\ud604\uc5ed \uc120\uc218\ub294 \uad75\uc740 \uae00\uc528\ub85c \ub098\ud0c0\ub0c4.)\n\ucd5c\ub2e4 \ub4dd