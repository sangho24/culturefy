\uc775\uc219\ud574\uc84c\ub2e4.\n\uadf8 \ud574 \ud6c4\ubc18\uc5d0 \ub9ac\ucc98\ub4dc\ub294 \ube59 \ud06c\ub85c\uc2a4\ube44\uc758 \uc601\ud654 &lt;\ub9ac\ub4ec \uc628 \ub354 \ub9ac\ubc84&gt;\ub97c \ubcf4\uc558\ub294\ub370 \ud06c\ub85c\uc2a4\ube44\ub294 \ucee8\uce04\ub9ac \uac00\uc218\ub85c \ub098\uc624\uace0 \uc810\ucc28 \uc778\uae30\ub97c \uc783\uc5b4\uac00\ub294 \ub0b4\uc6a9\uc774\uc5c8\ub2e4. \uc601\ud654\uc0c1\uc5d0\uc11c \uadf8\uc758 \uac00\uc7a5 \uc720\uba85\ud55c \uace1\uc774 &lt;Goodbye to Love&gt;\ub77c\uace0 \uc124\uc815\ub418\uc5b4 \uc788\uc5c8\ub294\ub370 \uc2e4\uc81c\ub85c \ub178\ub798\ub294 \ub098\uc624\uc9c0 \uc54a\uc558\ub2e4. \ub9ac\ucc98\ub4dc\ub294 \uadf8 \uace1\uc774 \uc774\ub7f4 \uac83\uc774\ub77c\uace0 \uc0c1\uc0c1\ub825\uc744 \ubc1c\ud718\ud558\uc5ec \uc791\uace1\ud588\uace0 \uae30\ubcf8\uc801\uc778 \uac00\uc0ac\ub3c4 \ubd99\uc600\ub2e4. \uc774\ud6c4 \ubca0\ud2f0\uc2a4\uc640 \ud568\uaed8 \uace1 \uc791\uc5c5\uc744 \ub9c8\ubb34\ub9ac\ud588\ub294\ub370 \ub9ac\ucc98\ub4dc\ub294 \ud37c\uc988 \uae30\ud0c0 \uc194\ub85c\ub97c \ub123\uace0 \uc2f6\uc5b4\ud588\ub2e4. \ub178\ub828\ud55c \uc138\uc158 \uc5f0\uc8fc\uc790\ub97c \ubd80\ub974\ub294 \uac83\uc5d0 \uc8fc\uc800\ud588\ub358 \uadf8\ub294 \uc804\uc5d0 \uce74\ud39c\ud130\uc2a4\uc758 \ud22c\uc5b4\uc5d0\uc11c \ud568\uaed8 \ud588\ub358 \uc778\uc2a4\ud134\ud2b8 \uc870\uc774\ub77c\ub294 \ubc34\ub4dc\uc758 \ud1a0\ub2c8 \ud3a0\ub8e8\uc18c\uc5d0\uac8c \uc5f0\uc8fc\ub97c \ubd80\ud0c1\ud588\ub2e4. \ud3a0\ub8e8\uc18c\ub294 \uc804\ud615\uc801\uc778 \ub85d \uae30\ud0c0\ub9ac\uc2a4\ud2b8\uc600\uace0 \uc545\ubcf4\ub97c \uc77d\uc744 \uc904 \ubab0\ub77c\uc11c \ub9ac\ucc98\ub4dc\ub294 \ucf54\ub4dc \ucc28\ud2b8\ub97c \uadf8\ub824\uc8fc\uc5c8\ub2e4. \ucc98\uc74c \ub2e4\uc12f \ub9c8\ub514\ub294 \uba5c\ub85c\ub514\ub97c \ub530\ub77c\uc11c \uc5f0\uc8fc\ud55c \ub2e4\uc74c \uc989\ud765\uc73c\ub85c \uac00\ub3c4\ub85d \ud588\uace0 \uadf8\ub294 \ub450 \ubc88\uc758 \ub179\uc74c \ub9cc\uc5d0 \uae30\ud0c0 \uc194\ub85c\ub97c \ub9c8\ub