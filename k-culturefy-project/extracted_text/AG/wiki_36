\ud558\uac00 \ud68c\uc804\ud55c\ub2e4\uba74 \ubcc4\ub4e4\uc5d0\uc11c\ub3c4 \uadf8\ub7f0 \ud604\uc0c1\uc774 \ub098\ud0c0\ub0a0 \uac83\u201d\uc774\ub77c\uace0 \uc8fc\uc7a5\ud588\ub2e4. \uadf8\ub294 \ub2e4\uc591\ud55c \ud56d\uc131 \uc6b4\ub3d9\uc744 \ubc14\ud0d5\uc73c\ub85c \ud0dc\uc591\uc774 \uc740\ud558 \uc911\uc2ec\uc5d0\uc11c \uc57d 30,000\uad11\ub144 \ub5a8\uc5b4\uc838 \uc788\uc73c\uba70, \uc740\ud558\ub97c \ud55c \ubc14\ud034 \ub3c4\ub294 \ub370 \uc57d 2\uc5b5 2,500\ub9cc \ub144\uc774 \uac78\ub9b0\ub2e4\ub294 \uacc4\uc0b0\uc744 \ub0b4\ub193\uc558\ub2e4. \ub610\ud55c \uc740\ud558 \uc6d0\ubc18\uc758 \uc678\uacfd\uc5d0 \uc788\ub294 \ubcc4\ub4e4\uc774 \uc911\uc2ec\ubd80\uc5d0 \uc788\ub294 \ubcc4\ub4e4\ubcf4\ub2e4 \ub290\ub9ac\uac8c \ud68c\uc804\ud55c\ub2e4\ub294 \uc0ac\uc2e4\ub3c4 \ubc1d\ud600\ub0c8\ub2e4. \uc774\ub294 \uc740\ud558\uac00 \ud558\ub098\uc758 \uace0\uccb4\ucc98\ub7fc \ud68c\uc804\ud558\ub294 \uac83\uc774 \uc544\ub2c8\ub77c \u2018\ucc28\ub4f1 \ud68c\uc804(differential rotation)\u2019\uc744 \ud55c\ub2e4\ub294 \uac83\uc744 \uc758\ubbf8\ud588\ub2e4.\n\uc624\ub974\ud2b8\uc758 \uc774 \ucd08\uae30 \ubc1c\uacac\ub4e4\uc740 \ud0dc\uc591\uc744 \uc740\ud558 \uc911\uc2ec\uc5d0 \ub454 \ub300\uce6d \uad6c\uc870\ub97c \uac00\uc815\ud588\ub358 \uadf8\uc758 \uc2a4\uc2b9 \uc57c\ucf54\ubd80\uc2a4 \uce74\ud504\ud14c\uc778\uc758 \uccb4\uacc4\ub97c \ubb34\ub108\ub728\ub838\ub2e4. \uc624\ub974\ud2b8\ub294 \ub098\uc911\uc5d0 \u201c\uce74\ud504\ud14c\uc778\uacfc \uadf8\uc758 \ub3d9\ub8cc\ub4e4\uc740 \uc740\ud558 \ud3c9\uba74\uc758 \ud761\uc218 \ud604\uc0c1\uc774 \uadf8\ub807\uac8c \uc2ec\uac01\ud558\ub2e4\ub294 \uc0ac\uc2e4\uc744 \uae68\ub2eb\uc9c0 \ubabb\ud588\ub2e4\u201d\uace0 \ud68c\uace0\ud588\ub2e4. \uadf8\uac00 \uc791\uc5c5\uc744 \uc2dc\uc791\ud560 \ub2f9\uc2dc, \ub77c\uc774\ub374 \ucc9c\ubb38\ub300\ub294 \uc704\uce58\ucc9c\ubb38\ud559\uacfc \uc790\uc624\uc120 \ub9dd\uc6d0\uacbd, \uace0\uc720 \uc6b4\ub3d9 \uc5f0\uad6c\uc5d0\ub9cc \uc9d1\uc911\ud558\uace0 \uc788\uc5c8\uace