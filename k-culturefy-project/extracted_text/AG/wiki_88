\ub2e4. \ubfd0\ub9cc \uc544\ub2c8\ub77c, \uace0\uac00\uad50 \uac1c\ud1b5\uc73c\ub85c \ube14\ub8e8\uc5b4 \ub178\uba74 \uc804\ucc28\ub294 1918\ub144 12\uc6d4 14\uc77c\uc5d0 \uc154\ubcf8\uc5d0\uc11c \ube0c\ub85c\ub4dc\ubdf0\uae4c\uc9c0 \uc5f0\uc7a5\ub418\uc5c8\ub2e4. \uadf8 \ub2f9\uc2dc, \ud1a0\ub860\ud1a0 \ucca0\ub3c4 \ud68c\uc0ac\ub294 \ud5cc\uc7a5 \ud5c8\uac00 \uc2dc\ud55c\uc774 \uac70\uc758 \ub05d\ub098\uac00\ub358 \uc9c0\ub77c \ub178\uc120 \ud655\uc7a5\uc5d0 \ud68c\uc758\uc801\uc774\uc600\uace0 \ud1a0\ub860\ud1a0 \uc2dc \ub2f9\uad6d\uc774 \uc120\ub85c \uacf5\uc0ac \ube44\uc6a9\uc744 \ub300\uc2e0 \ubd80\ub2f4\ud558\uc600\ub2e4. \uc774 \ub178\uba74 \uc804\ucc28 \ub178\uc120\uc740 \uce90\uc2ac\ud504\ub7ad\ud06c \uc5f0\uc120\uc744 \uc9c0\ub098\ub294 \uc720\uc77c\ud55c \ub178\uba74 \uc804\ucc28\uc600\uc73c\uba70, \uc774\ud6c4 1924\ub144 8\uc6d4 3\uc77c, TTC\uac00 \ud314\ub7ec\uba3c\ud2b8 \uc2a4\ud2b8\ub9ac\ud2b8\ub97c \ub530\ub77c \ub178\uba74 \uc804\ucc28 \uc120\ub85c\ub97c \uc5f0\uc7a5\ud558\uc600\uace0 \ud314\ub7ec\uba3c\ud2b8 \ub178\uba74 \uc804\ucc28\uac00 \ube14\ub8e8\uc5b4\uc640 \ud314\ub7ec\uba3c\ud2b8\uc5d0\uc11c \ud68c\ucc28\ud558\uac8c \ub418\uc5c8\ub2e4.\n\uc9c0\ud558\ucca0 \uacf5\uc0ac.\n\uc774 \uc9c0\uc5ed\uc740 1920\ub144\ub300\ubd80\ud130 \ubcc4\ub2e4\ub978 \uac1c\ubc1c\uc758 \uc9c4\ucc99\uc774 \uc774\ub8e8\uc5b4\uc9c0\uc9c0 \uc54a\uc558\ub2e4. \uc9c0\ub9ac\uc801\uc73c\ub85c \ub450 \uacc4\uace1\uc5d0 \uac78\uccd0 \uc788\uc73c\uba70 \ube44\uad50\uc801\uc73c\ub85c \uace0\ub9bd\ub418\uace0 \ubd80\uc720\ud55c \ub85c\uc988\ub370\uc77c \uc9c0\uc5ed\uacfc \uac00\uae4c\uc6cc \uc778\uad6c \ubc00\ub3c4\ub294 \ub0ae\uace0 \uc8fc\ud0dd\uc740 \uc800\ud0dd \uc218\uc900\uc774\uc600\uae30 \ub54c\ubb38\uc774\ub2e4. \ud558\uc9c0\ub9cc 1950\ub144\ub300\ubd80\ud130 \uac1c\ubc1c\uc758 \ubc14\ub78c\uc774 \ubd88\uc5b4\uc624\uae30 \uc2dc\uc791\ud588\ub2e4. \ud1a0\ub860\ud1a0 \uc2dc\ub294 \ub3c8 \ubc38\ub9ac \ud30c\ud06c\uc6e8\uc774 \uac74\uc124 \ubfd0\ub9cc \uc544\ub2c8\ub77c 