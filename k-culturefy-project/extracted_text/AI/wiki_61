9\uc0ac\ud558\uc600\ub2e4.\n\uc5b4\ub9b0 \uc2dc\uc808.\n\ub178\uc2a4\ucf00\ub294 1868\ub144 7\uc6d4 9\uc77c \ud504\ub85c\uc774\uc13c\uc758 \ube0c\ub780\ub374\ubd80\ub974\ud06c\uc548\ub370\uc5b4\ud558\ubca8\uc5d0\uc11c \ud0dc\uc5b4\ub0ac\ub2e4. \uc544\ubc84\uc9c0 \uce74\ub97c \ub178\uc2a4\ucf00(1838\ub144\uc0dd)\ub294 \ubc29\uc9c1\uacf5\uc774\uc5c8\uace0, \uc5b4\uba38\ub2c8 \uc5e0\ub9c8 \ud5e4\uc5b4\ube44\ud788(1843\ub144)\ub294 \uc721\uccb4\ub178\ub3d9\uc790\uc600\ub2e4. 1874\ub144\uc5d0\uc11c 1882\ub144 \uc0ac\uc774 \ucd08\ub4f1\ud559\uad50\uc640 \uc911\ub4f1\ud559\uad50\ub97c \uc878\uc5c5\ud588\ub2e4. 1882\ub144\uc5d0\uc11c 1886\ub144 \uc0ac\uc774 \uc2e4\uc5c5\ud559\uad50\uc5d0\uc11c \ubc14\uad6c\ub2c8 \uc81c\uc870 \uae30\uc220\uc744 \ubc30\uc6cc \uc9c1\uc778\uc774 \ub418\uc5b4\uc11c \ud504\ub791\ud06c\ud478\ub974\ud2b8, \uc554\uc2a4\ud14c\ub974\ub2f4, \ub808\uadf8\ub2c8\ucc60 \ub4f1\uc9c0\ub97c \ub5a0\ub3cc\uc544\ub2e4\ub154\ub2e4. 1884\ub144 \ub178\uc2a4\ucf00\ub294 \ub3c5\uc77c \uc0ac\ud68c\ubbfc\uc8fc\ub2f9(SPD)\uc5d0 \uc785\ub2f9\ud558\uace0 \ub178\ub3d9\uc870\ud569\uc6d0\uc774 \ub418\uc5c8\ub2e4. 1892\ub144, \ub178\uc2a4\ucf00\ub294 \uc0ac\ubbfc\ub2f9 \ube0c\ub780\ub374\ubd80\ub974\ud06c \ub3c4\ub2f9 \uc758\uc7a5\uc73c\ub85c \uc120\ucd9c\ub41c\ub2e4\n1891\ub144 \ube0c\ub780\ub374\ubd80\ub974\ud06c\uc5d0\uc11c \ub9c8\ub974\ud0c0 \ud2f0\uc5d8(Martha Thiel, 1872\ub144 ~ 1949\ub144)\uacfc \uacb0\ud63c\ud55c\ub2e4. \uc0ac\uc774\uc5d0\ub294 1\ub0a8 2\ub140\ub97c \ub450\uc5c8\ub2e4.\n1897\ub144\uc5d0\uc11c 1902\ub144 \uc0ac\uc774 \ub178\uc2a4\ucf00\ub294 \uc9c0\uc5ed \uc815\uce58\uc778\uc774\uc790 \ube0c\ub780\ub374\ube0c\ub974\ud06c-\ucfa8\ub2c8\ud788\uc2a4\ubca0\ub974\ud06c \uc9c0\uc5ed\uc758 \uc0ac\ud68c\ubbfc\uc8fc\ub2f9 \uae30\uad00\uc9c0 \ud3b8\uc9d1\uc7a5\uc73c\ub85c \uc5f4\uc131\uc801\uc73c\ub85c \uc77c\ud588\ub2e4. 1902\ub144\uc5d0\uc11c 1918\ub144 \uc0ac\uc774\uc5d0\ub294 \ucf10\ub2c8\uce20\uc5d0\uc11c \u300a\uc778\ubbfc\uc758 \uc2dc\ub300\u300b(\"Volksstimme\") \uc9c0\uc758 \uc8fc\ud544\uc774 \ub418\uc5c8\ub2e4. 1906\ub144, \ub178\uc2a4\ucf00\ub294 \uad6d\uac00\uc758\ud68c \uc758\uc6d0\uc9c1\uc5d0 \uc0ac\ubbfc\ub2f9 \ud6c4\ubcf4\ub85c \ucd9c\ub9c8\ud558\uc5ec \ub2f9\uc120\ub418\uc5c8\uace0, 1918\ub144\uae4c\uc9c0 \uc758\uc6d0\uc73c\ub85c \uc7ac\uc784\ud558\uba74\uc11c \ucf10\ub2c8\uce20 \uc9c0\uc5ed \uc720\uad8c\uc790\ub4e4\uc758 \uc774\ud574\ub97c \ub300\ubcc0\ud558\uc600\ub2e4. \uc0ac\ubbfc\ub2f9 \ub0b4\uc5d0\uc11c \uadf8\ub294 \ud574\uad70, \uc2dd\ubbfc\uc9c0 \ub4f1\uc758 \ubb38\uc81c\uc5d0 \uc2dd\uacac\uc774 \uae4a\uc740 \uad70\uc0ac\ud1b5\uc73c\ub85c \ud1b5\ud588\ub2e4. 1914\ub144, \uadf8\ub294 \u300a\uc2dd\ubbfc\uc815\uce58\uc640 \uc0ac\ud68c\ubbfc\uc8fc\uc8fc\uc758\u300b(\"Kolonialpolitik und Sozialdemokratie\")\ub97c \ucd9c\ud310\ud574 \ub3c5\uc77c\uc758 \uc2dd\ubbfc\uc8fc\uc758\ub97c \uc639\ud638\ud588\ub2e4. \uadf8\ub294 \uc0ac\ubbfc\ub2f9 \ub0b4\uc5d0\uc11c \uadfc\ubcf8\uc801 \uc774\ub860\uc801 \ub17c\uc7c1\uc5d0\ub294 \ubcc4 \ud765\ubbf8\uac00 \uc5c6\uace0, \ud604\uc7ac\uc758 \uccb4\uc81c \ub0b4\uc5d0\uc11c \uc815\uce58\uc801 \ubaa9\uc801\uc744 \uc131\ucde8\ud558\ub294 \uac83\uc744 \uc6b0\uc120\uc2dc\ud558\ub294 \uac1c\ub7c9\uc8fc\uc758\uc790\uc5d0 \uc18d\ud588\ub2e4.\n\uc81c1\ucc28 \uc138\uacc4 \ub300\uc804.\n\uc81c1\ucc28 \uc138\uacc4 \ub300\uc804 \ub2f9\uc2dc \ub178\uc2a4\ucf00\ub294 \ud504\ub9ac\ub4dc\ub9ac\ud788 \uc5d0\ubca0\ub974\ud2b8, \ud544\ub9ac\ud504 \uc0e4\uc774\ub370\ub9cc\uacfc \ud568\uaed8 \uc0ac\ubbfc\ub2f9 \ub0b4 \ubcf4\uc218\ud30c\ub97c \ud615\uc131\ud588\uc73c\uba70, \uc804\uc7c1\uc744 \ubc29\uc5b4 \uc218\ub2e8\uc73c\ub85c\uc11c \uc9c0\uc9c0\ud558\uc600\ub2e4. \ub178\uc2a4\ucf00\ub294 \uc804\uc2dc \uacf5\ucc44 \ubc1c\ud589\uc744 \uc9c0\uc9c0\ud588\uc73c\uba70, \ub54c\ub85c\ub294 \uc758\ud68c\uc5d0\uc11c \ub354\uc6b1 \uac15\uacbd\ud55c \uc815\uce58\uc801 \uc785\uc7a5\uc744 \uc8fc\uc7a5\ud558\uae30\ub3c4 \ud588\ub2e4. 1916\ub144\uc5d0\uc11c 1918\ub144\uc5d0 \uac78\uccd0 \ub178\uc2a4\ucf