\uc6d0\uc744 \ubc1b\uc544 \uc644\uc131\ub41c \ud034\uc5b4 \uc601\ud654\ub294 \uc11c\uc6b8\ud504\ub77c\uc774\ub4dc\uc601\ud654\uc81c\ub97c \ud1b5\ud574 \uacf5\uc2dd\uc801\uc73c\ub85c \uc18c\uac1c\ub41c\ub2e4.\n2014\ub144 \uc81c\uc791\uc9c0\uc6d0\uc791 &lt;\uc18c\uc6d4\uae38&gt;\uc740 \uc11c\uc6b8\ub3c5\ub9bd\uc601\ud654\uc81c, \ud31c\uc2a4\ud504\ub9c1\uc2a4 \uad6d\uc81c\ub2e8\ud3b8\uc601\ud654\uc81c \ub4f1\uc758 \uad6d\ub0b4\uc678 \uc601\ud654\uc81c\uc5d0\uc11c \uc0c1\uc601\ub418\uc5c8\uc73c\uba70, Face \u00e0 Face \uac8c\uc774&amp;\ub808\uc988\ube44\uc5b8 \uc601\ud654\uc81c\uc5d0\uc11c \uad00\uac1d\uc0c1\uc744 \uc218\uc0c1\ud558\uc600\ub2e4.\n2015\ub144 \uc81c\uc791\uc9c0\uc6d0\uc791 &lt;\ud615\uc774 \ub3cc\uc544\uc654\ub2e4&gt;\uc640 2016\ub144 \uc81c\uc791\uc9c0\uc6d0\uc791 &lt;\uc0c8\ubcbd\uc740 \uc9e7\ub2e4&gt;, &lt;\uc9c8\ud22c\ub294 \ub098\uc758 \uc190\ub2d8&gt;, &lt;\ubabb\ub09c\uc774\ub4e4&gt;, &lt;\uc5ec\uc655\uc758 \uc9d1&gt;, &lt;\ubaa8\ubaa8&gt;\ub294 2016 \uc11c\uc6b8\ud504\ub77c\uc774\ub4dc\uc601\ud654\uc81c\uc758 \ud3d0\ub9c9\uc791\uc73c\ub85c \uc0c1\uc601\ub418\uc5c8\uc73c\uba70, \uc774 \uc911 &lt;\ubaa8\ubaa8&gt;\ub294 \ud1a0\ub860\ud1a0\uad6d\uc81c\ud504\ub77c\uc774\ub4dc\uc601\ud654\uc81c(InsideOut 2017)\uc5d0 \ucd08\uccad\ub418\uc5c8\ub2e4.\n2017\ub144 \uc81c\uc791\uc9c0\uc6d0\uc791 &lt;\ud504\ub9ac\ubc84\ub4dc&gt;, &lt;\ub450 \ubc24&gt;, &lt;\uc154\ud2c0\ub7f0&gt;\uc740 2017 \uc11c\uc6b8\ud504\ub77c\uc774\ub4dc\uc601\ud654\uc81c\uc758 \ud3d0\ub9c9\uc791\uc73c\ub85c \uc0c1\uc601\ub418\uc5c8\ub2e4.\n\uad00\ub828 \ud589\uc0ac.\n\uc11c\uc6b8\ud504\ub77c\uc774\ub4dc\uc601\ud654\uc81c \uc9c0\uc5ed\uc21c\ud68c\uc0c1\uc601\ud68c.\n\uc11c\uc6b8\ud504\ub77c\uc774\ub4dc\uc601\ud654\uc81c \uc9c0\uc5ed\uc21c\ud68c\uc0c1\uc601\ud68c\ub294 \uc11c\uc6b8\uc5d0 \ube44\ud574 \uc0c1\ub300\uc801\uc73c\ub85c \ud034\uc5b4 \uc601\ud654 \ud5a5\uc720\uc758 \uae30\ud68c\uac00 \ubd80\uc871\ud55c \uc9c0\uc5ed\uc5d0 \uac70\uc8fc\ud558\ub294 \uc2dc\ubbfc\ub4e4\uc758 \ubb38\ud