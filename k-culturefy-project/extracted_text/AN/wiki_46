\uc6b0\ud06c\ub77c\uc774\ub098\uc758 \ucf54\uc790\ud06c \uc9c0\ub3c4\uc790\ub85c \ucd94\ub300\ub418\uc5c8\uc9c0\ub9cc \ub8e8\uc2a4 \ucc28\ub974\uad6d\uacfc\uc758 \uad00\uacc4\uac00 \uc545\ud654\ub418\uc790 \ub300\ubd81\ubc29 \uc804\uc7c1\uc5d0\uc11c \uc2a4\uc6e8\ub374\uc744 \uc9c0\uc9c0\ud558\uae30\ub85c \uacb0\uc2ec\ud55c\ub2e4. \uc77c\uc124\uc5d0 \ub530\ub974\uba74 \ub9c8\uc81c\ud30c\ub294 \uc220\uc5d0 \ucde8\ud55c \ub7ec\uc2dc\uc544\uc758 \ucc28\ub974\uc778 \ud45c\ud2b8\ub974 1\uc138\ub97c \ubaa8\uc695\ud55c \uc801\uc774 \uc788\uc5c8\uc73c\uba70 \ub8e8\uc2a4 \ucc28\ub974\uad6d\uc758 \ud1a0\uc9c0\ub97c \uc18c\uc720\ud558\ub294 \uc57c\uc2ec\uc744 \ud488\uace0 \uc2a4\uc6e8\ub374\uc758 \uad6d\uc655\uc774\uc5c8\ub358 \uce7c 12\uc138\ub97c \uc804\uc7c1\uc5d0 \ub04c\uc5b4\ub4e4\uc774\ub824\ub294 \uacc4\ud68d\uc744 \uc138\uc6e0\ub2e4\uace0 \ud55c\ub2e4.\n1708\ub144 10\uc6d4 \ub9c8\uc81c\ud30c\ub294 \ub300\ubd81\ubc29 \uc804\uc7c1\uc5d0\uc11c 3,000\uba85\uc758 \uce74\uc790\ud06c \ubcd1\uc0ac\ub4e4\uacfc \ud568\uaed8 \uc2a4\uc6e8\ub374\uad70\uacfc \ud569\ub958\ud588\ub2e4. 1709\ub144 6\uc6d4 \ub9c8\uc81c\ud30c\uac00 \uc18d\ud55c \uc2a4\uc6e8\ub374\uad70\uc740 \ud3f4\ud0c0\ubc14\uc5d0\uc11c \uc77c\uc5b4\ub09c \ud3f4\ud0c0\ubc14 \uc804\ud22c\uc5d0\uc11c \ub7ec\uc2dc\uc544\uad70\uc5d0 \ud328\uc804\ud558\uace0 \ub9cc\ub2e4. \ud3f4\ud0c0\ubc14 \uc804\ud22c\uc5d0\uc11c \ud328\uc804\ud55c \ub9c8\uc81c\ud30c\ub294 \uc624\uc2a4\ub9cc \uc81c\uad6d\uc758 \uc9c0\ubc30\ub97c \ubc1b\uace0 \uc788\ub358 \ubab0\ub3c4\ubc14 \ubca4\ub370\ub974\ub85c \ub3c4\uc8fc\ud588\uace0 \uadf8 \uacf3\uc5d0\uc11c \uc0ac\ub9dd\ud558\uac8c \ub41c\ub2e4.\n\ub3c5\ub9bd\ub41c \ucf54\uc790\ud06c \uad6d\uac00\ub97c \uc138\uc6b0\ub824\ub294 \ub178\ub825 \ub54c\ubb38\uc5d0 \ubcf4\ud750\ub2e8 \ud750\uba5c\ub2c8\uce20\ud0a4\uc640 \ud568\uaed8 \uc6b0\ud06c\ub77c\uc774\ub098 \uc81c2\uc758 \uc601\uc6c5\uc73c\ub85c \ucd94\uc559\ubc1b\uace0 \uc788\ub2e4. \uc870\uc9c0 \ubc14\uc774\ub7f0, \ube45\ud1a0\ub974 \uc704\uace0, \