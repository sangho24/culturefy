\ub97c \uc720\ub7fd\uc73c\ub85c \uc81c\ud55c\ud558\uc5ec \uc544\ube44\uc2dc\ub2c8\uc544\ub294 \uc790\uc720\ub86d\uac8c \ub2e4\ub8f0 \uc218 \uc788\ub3c4\ub85d \ub17c\uc758\ud588\ub2e4\ub294 \uc815\ud669\uc774 \uc788\ub2e4. \uadf8\ub7ec\ub098 \uc774\ud0c8\ub9ac\uc544\ub294 \uc544\ube44\uc2dc\ub2c8\uc544\uc5d0\uc11c \ub9c8\uc74c\uaecf \ud589\ub3d9\uc5d0 \ub098\uc11c\ub3c4 \ub41c\ub2e4\uace0 \ud310\ub2e8\ud55c \ubc18\uba74, \uc601\uad6d\uc740 \uc544\ube44\uc2dc\ub2c8\uc544\uac00 \uc81c\ud55c\uad6c\uc5ed\uc784\uc744 \ubd84\uba85\ud788 \ud588\ub2e4\ub294 \uc785\uc7a5\uc744 \uacac\uc9c0\ud558\uc600\uae30\uc5d0, \uc601\uad6d\uacfc \uc774\ud0c8\ub9ac\uc544\ub294 \uc81c\ub300\ub85c \uc18c\ud1b5\ud558\uc9c0 \ubabb\ud55c \uac83\uc73c\ub85c \ubcf4\uc778\ub2e4. \uc774 \ubb38\uc81c\ub294 \uac00\uc744\uc5d0 \uc808\uc815\uc5d0 \ub2ec\ud588\ub2e4.\n\ud574\uccb4.\n\uc2a4\ud2b8\ub808\uc0ac \uc804\uc120\uc774 \ud574\uccb4\uc704\uae30\uc5d0 \ub193\uc778 \uac83\uc740 \uc544\ube44\uc2dc\ub2c8\uc544 \uc704\uae30\uac00 \ubc1c\uc0dd\ud558\uba74\uc11c\ubd80\ud130\uc600\ub2e4. \uc544\ube44\uc2dc\ub2c8\uc544\ub294 1895\ub144 \uc81c1\ucc28 \uc774\ud0c8\ub9ac\uc544-\uc5d0\ud2f0\uc624\ud53c\uc544 \uc804\uc7c1\uc5d0\uc11c \uc774\ud0c8\ub9ac\uc544\uc758 \uce68\uacf5\uc744 \ubc1b\uc558\uc73c\ub098, \uc774\ud0c8\ub9ac\uc544\ub294 \uad74\uc695\uc801\uc778 \ud328\ubc30\ub97c \ub2f9\ud558\uace0 \ucca0\uc218\ud574\uc57c \ud588\ub2e4. \uadf8\ub7ec\ub098 \uc774\ud0c8\ub9ac\uc544\ub294 \uc5d0\ud2f0\uc624\ud53c\uc544\ub97c \uc9c0\ubc30\ud558\ub824\ub294 \uc57c\uc2ec\uc744 \uc808\ub300 \uc78a\uc9c0 \uc54a\uc558\uace0, \ubb34\uc194\ub9ac\ub2c8\ub294 \uc2dd\ubbfc\uc9c0 \ud655\uc7a5\uc758 \uae30\ud68c\ub97c \uc5ff\ubcf4\uc558\ub2e4. \uc774 \ub54c\ubb38\uc5d0 \ud68c\uc758 \ub2f9\uc2dc\uc5d0\ub3c4 \ubc34\uc2dc\ud0c0\ud2b8 \uacf5\uc791\uc740 \ub3c5\uc77c\uc744 \ubd09\uc1c4\ud55c\ub2e4\ub294 \uba85\ubaa9\uc73c\ub85c \uc601\uad6d\uacfc \ud504\ub791\uc2a4\ub85c \ud558\uc5ec\uae08 \uc774\ud0c8\ub9ac\uc544\ub97c \uc81c\ub