\n\uc2dc\ub9ac\uc988.\n1928\ub144 \uc774\uc804\uc5d0 \ubc1c\ud589\ub41c \uc9c0\ud3d0\ub294 \uc774\uc5c8\ub2e4. 1929\ub144 \uc7ac\ubb34\ubd80 \uc138\ucd9c \ubc95\uc548\uc5d0 \ub530\ub77c 1928\ub144 \uc774\ud6c4 \ubc1c\ud589\ub41c \uc9c0\ud3d0\ub294 \uac00 \ub418\uc5c8\ub294\ub370, \uc774\ub294 \uc7ac\ubb34\ubd80\uac00 \uc774\uc804 \ud06c\uae30\uc5d0\uc11c 8\uc7a5\uc744 \uc0dd\uc0b0\ud558\ub358 \ud55c \uc7a5\uc758 \uc6a9\uc9c0\uc5d0\uc11c 12\uc7a5\uc758 \uc9c0\ud3d0\ub97c \uc0dd\uc0b0\ud560 \uc218 \uc788\ub3c4\ub85d \ud588\ub2e4.\n\uc774\ub7ec\ud55c \ub300\ud615 \uc9c0\ud3d0\uc758 \ud604\ub300\uc801 \uce21\uc815\uce58\ub294 \ud3c9\uade0 \uc774\ub2e4. \uc18c\ud615 \uc9c0\ud3d0(\uc774\uc804 \ub300\ud615 \uc9c0\ud3d0\uc5d0 \ube44\ud574 \ud06c\uae30\uac00 \uc791\uc544\uc11c \uc774\ub807\uac8c \ubd88\ub9bc)\ub294 \ud3c9\uade0 \uc774\uba70, \ud604\ub300 \ubbf8\uad6d \ud654\ud3d0\uc758 \ud06c\uae30\uc774\ub2e4. \uac01 \uce21\uc815\uce58\ub294 \uc5ec\ubc31\uacfc \uc808\ub2e8\uc744 \uace0\ub824\ud558\uc5ec \u00b1 \uc774\ub2e4. (\ucc38\uace0: \ud06c\uae30 \ucc28\uc774\ub294 \uc778\uce58\uc758 \uc815\uc758\uc5d0 \ub300\ud55c \uc5ed\uc0ac\uc801 \ubcc0\ud654\ub97c \ud3ec\ud568\ud560 \uc218\ub3c4 \uc788\ub2e4.)\n\ub300\ud615 \uc9c0\ud3d0.\n1914\ub144 \uc2dc\ub9ac\uc988 FRN\uc740 \ub450 \uac00\uc9c0 \ub300\ud615 \uc9c0\ud3d0 \uc911 \uccab \ubc88\uc9f8\uc600\ub2e4. \uc561\uba74\uc740 5\ub2ec\ub7ec, 10\ub2ec\ub7ec, 20\ub2ec\ub7ec, 50\ub2ec\ub7ec, 100\ub2ec\ub7ec\uc600\uc73c\uba70, \ucc98\uc74c\uc5d0\ub294 \ube68\uac04\uc0c9 \uc778\uc7a5\uc73c\ub85c \uc778\uc1c4\ub418\uc5c8\uace0 \ub098\uc911\uc5d0\ub294 \ud30c\ub780\uc0c9 \uc778\uc7a5\uc73c\ub85c \uacc4\uc18d\ub418\uc5c8\ub2e4. 1918\ub144 \uc2dc\ub9ac\uc988 \uc9c0\ud3d0\ub294 500\ub2ec\ub7ec, 1,000\ub2ec\ub7ec, 5,000\ub2ec\ub7ec, 10,000\ub2ec\ub7ec \uc561\uba74\uc73c\ub85c \ubc1c\ud589\ub418\uc5c8\ub2e4. \ud6c4\uc790\uc758 \ub450 \uc561\uba74\uc740 \uae30\uad00 \uc18c\uc7a5\ud488\uc73c\ub85c\ub9cc \uc874\uc7ac\ud55c\ub2e4. \ub2e4\uc74