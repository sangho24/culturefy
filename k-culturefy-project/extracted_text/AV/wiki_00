\ud68c\uc5f0\uc124\uc5d0\uc11c \ubd80\uc815\uc120\uac70\ub97c \uc774\uc720\ub85c \uc120\uac70\ub97c \ubb34\ud6a8\ud654\ud574\uc57c \ud55c\ub2e4\ub294 \ub73b\uc744 \ucc9c\uba85\ud558\uc600\ub2e4. \uc774\uc5d0 \uac80\uc740 \uc154\uce20\ub2e8\uc740 6\uc6d4 10\uc77c \ub9c8\ud14c\uc624\ud2f0 \ub300\ud45c\ub97c \uc554\uc0b4\ud558\ub294 \uac83\uc73c\ub85c \ub300\uc751\ud588\uace0, \uc774\uac83\uc774 \ud070 \ud30c\ubb38\uc744 \uc77c\uc73c\ud0a4\uba70 \ubb34\uc194\ub9ac\ub2c8 \uc815\uad8c\uc5d0 \uc77c\uc2dc\uc801\uc778 \uc704\uae30\ub97c \ubd88\ub7ec\uc77c\uc73c\ucf30\ub2e4. 1924\ub144 12\uc6d4 31\uc77c \uac80\uc740 \uc154\uce20\ub2e8\uc758 \ub300\ud45c\ub4e4\uc740 \ubb34\uc194\ub9ac\ub2c8\ub97c \ub9cc\ub098, \ubc18\ub300\ud30c\ub97c \ubd84\uc1c4\ud558\uc9c0 \uc54a\uc73c\uba74 \uc790\uccb4 \ud589\ub3d9\uc5d0 \ub098\uc11c\uaca0\ub2e4\ub294 \ucd5c\ud6c4\ud1b5\ucca9\uc744 \uc804\ub2ec\ud588\ub2e4.\n\ubcf8\uc778\uc758 \uc9d1\uad8c\uc5d0 \uc77c\uc870\ud55c \ubb34\uc7a5\uc138\ub825\uc758 \ubc18\ub780\uc744 \ub450\ub824\uc6cc\ud55c \ubb34\uc194\ub9ac\ub2c8\ub294 \uac80\uc740 \uc154\uce20\ub2e8\uc758 \ud1b5\ucca9\uc744 \ubc1b\uc544\ub4e4\uc774\uace0 \ubbfc\uc8fc\uc8fc\uc758\uc758 \uc678\uc591\uc744 \ubc84\ub9ac\uae30\ub85c \uacb0\uc815\ud588\ub2e4. 1925\ub144 1\uc6d4 3\uc77c, \ubb34\uc194\ub9ac\ub2c8\ub294 \ub300\uc758\uc6d0\uc5d0\uc11c\uc758 \uaca9\ub82c\ud55c \uc5f0\uc124\uc744 \ud1b5\ud574 4\uc778 \uc9c0\ub3c4\uc790\uc758 \ud3ed\ub825\uc5d0 \ub300\ud574 \ucc45\uc784\uc744 \uc9c0\uaca0\ub2e4\uace0 \uc778\uc815\ud558\uba74\uc11c\ub3c4 \ub9c8\ud14c\uc624\ud2f0 \ub300\ud45c\uc758 \uc554\uc0b4\uc740 \uc77c\uc808 \uc5b8\uae09\ud558\uc9c0 \uc54a\uc558\ub2e4. \uc774\ub0a0 \uc5f0\uc124 \uc774\ud6c4\ub85c \ud30c\uc2dc\uc2a4\ud2b8 \uc138\ub825\uc774 \uc758\ud68c\ub97c \uc7a5\uc545\ud55c \uc0c1\ud0dc\uc5d0\uc11c \uc5ec\ub7ec \ubc95\uc548\uc744 \ud1b5\uacfc\uc2dc\ucf1c, \uc774\ud0c8\ub9ac\uc544\uc5d0\uc11c \uc9c0\ucf1c\uc838 \uc654\ub358 \ubbfc\uc8fc\uc801 \uc790\uc720\ub97c \uc81c\ud55c\ud55