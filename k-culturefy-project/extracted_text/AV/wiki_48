ud0a4\ucf00\ub85c\uac00 \uac70\uc808\ud558\uc790, \ud0a4\ucf00\ub85c\ub294 \ub3c4\uc2dc\ub97c \ub5a0\ub098 \ub9dd\uba85\ud588\ub2e4. \ud074\ub85c\ub514\uc6b0\uc2a4\ub294 \uc989\uc2dc \ub809\uc2a4 \ud074\ub85c\ub514\uc544 \ub370 \uc5d1\uc2e4\ub9ac\uc624 \ud0a4\ucf00\ub85c\ub2c8\uc2a4(Lex Clodia de exsilio Ciceronis)\ub97c \ud1b5\uacfc\uc2dc\ucf1c \uc6c5\ubcc0\uac00\ub97c \ucd94\ubc29\ud558\uace0, \ud314\ub77c\ud2f0\ub178 \uc5b8\ub355\uc5d0 \uc788\ub294 \uadf8\uc758 \uc9d1\uc744 \uc5ec\uc2e0 \ub9ac\ubca0\ub974\ud0c0\uc2a4\ub97c \uc704\ud55c \uc2e0\ub2f9\uc73c\ub85c \ub9cc\ub4e4\uc5c8\uc73c\uba70, \uc6d0\ub85c\uc6d0\uc774\ub098 \uc2dc\ubbfc\ub4e4\uc774 \uadf8 \uc6c5\ubcc0\uac00\ub97c \uc18c\ud658\ud558\ub294 \uac83\uc744 \uae08\uc9c0\ud588\ub2e4.\n\ud3fc\ud398\uc774\uc6b0\uc2a4\uc5d0 \ub300\ud55c \ubc18\ub300.\n\ud074\ub85c\ub514\uc6b0\uc2a4\uc758 \ub124 \uac00\uc9c0 \ubc95\uc548\uc774 \uc131\uacf5\ud558\uba74\uc11c \uadf8\ub294 \ub9c9\ub300\ud55c \uc815\uce58\uc801 \uc9c0\uc9c0\ub97c \uc5bb\uc5c8\ub2e4. \uc774\ub7ec\ud55c \uc9c0\uc9c0\ub294 \ud2b9\ud788 \ubc14\ud2f0\ub2c8\uc6b0\uc2a4 \uc7ac\ud310\uc5d0\uc11c \ud3ed\ub3c4\uc758 \ud798\uc744 \uc6b0\uc5f0\ud788 \ubc1c\uacac\ud55c \uac83\uacfc \uacb0\ud569\ud558\uc5ec \uadf8\uac00 \ub3c5\ub9bd\uc801\uc778 \uc815\uce58 \uc8fc\uccb4\ub85c \ud65c\ub3d9\ud558\ub294 \uac83\uc744 \uac00\ub2a5\ud558\uac8c \ud588\ub2e4. \ud3fc\ud398\uc774\uc6b0\uc2a4\uc5d0 \ub300\ud56d\ud558\uc5ec \ud074\ub85c\ub514\uc6b0\uc2a4\ub294 \uc7a5\uad70\uc744 \uc758\uc2ec\ud558\ub294 \uc6d0\ub85c\uc6d0 \uc758\uc6d0\ub4e4\uc758 \uc9c0\uc9c0\ub97c \ud655\ubcf4\ud558\uae30 \uc704\ud574 \uc6c0\uc9c1\uc600\ub2e4. \ud3fc\ud398\uc774\uc6b0\uc2a4\uc758 \ub3d9\ubc29 \uc815\ucc29\uc9c0\uc5d0 \ubaa9\ud45c\ub97c \uc124\uc815\ud55c \ud074\ub85c\ub514\uc6b0\uc2a4\ub294 \ud3fc\ud398\uc774\uc6b0\uc2a4\uac00 \ud398\uc2dc\ub204\uc2a4\uc758 \ub300\uc0ac\uc81c\ub85c \uc784\uba85\ud588\ub358 \uac08\ub77c\ud2f0\uc544\uc758 \ud14c\ud2b8\ub77c\ub974\ud06c \ub370\uc774\uc624\ud