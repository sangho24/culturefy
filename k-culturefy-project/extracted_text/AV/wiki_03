\uba54\ub9ac\uce74.\n\ubbf8\uad6d.\n\uc591\uad6d\uc740 1957\ub144 3\uc6d4 6\uc77c\uc5d0 \uc678\uad50 \uad00\uacc4\ub97c \uc218\ub9bd\ud588\ub2e4.\n\ubbf8\uad6d\uc740 \uac00\ub098\uc758 \uc8fc\uc694 \uad50\uc5ed\uad6d \uc911 \ud558\ub098\uc774\ub2e4. \uac00\ub098 \ub300\ud1b5\ub839\uc2e4\uc740 \uac00\ub098 \uc8fc\uc7ac \ubbf8\uad6d \ub300\uc0ac\uad00\uacfc \ud611\ub825\ud558\uc5ec \ubbf8\uad6d \uc0c1\uacf5\ud68c\uc758\uc18c\ub97c \uc124\ub9bd\ud558\uc600\uc73c\uba70, \uc774\ub97c \ud1b5\ud574 \ubbfc\uac04 \ubd80\ubb38\uc5d0\uc11c \uc591\uad6d \uac04 \uacbd\uc81c \uad00\uacc4\ub97c \ub354\uc6b1 \uac15\ud654\ud558\uace0\uc790 \ud558\uc600\ub2e4. \uac00\ub098\uc5d0\uc11c \ud65c\ub3d9 \uc911\uc778 \uc8fc\uc694 \ubbf8\uad6d \uae30\uc5c5\uc73c\ub85c\ub294 ACS, CMS \uc5d0\ub108\uc9c0, \ucf54\uce74\ucf5c\ub77c, S.C. \uc874\uc2a8, \ub7a0\uc2a4\ud134 \ud4e8\ub9ac\ub098, \uc2a4\ud0c0\ud0a4\uc2a4\ud2b8, A.H. \ub85c\ube48\uc2a4, \uc2a4\ud138\ub9c1, \ud654\uc774\uc790, IBM, 3M, \ubaa8\ud1a0\ub85c\ub77c, \uc2a4\ud29c\uc5b4\ud2b8 &amp; \uc2a4\ud2f0\ube10\uc2a8, \ud504\ub77c\uc774\uc2a4\uc6cc\ud130\ud558\uc6b0\uc2a4\ucfe0\ud37c\uc2a4, NCR (\ub0b4\uc154\ub110 \uce90\uc2dc \ub808\uc9c0\uc2a4\ud130) \ub4f1\uc774 \uc788\ub2e4. \ucd5c\uadfc \uc5ec\ub7ec \ubbf8\uad6d \uae30\uc5c5\uc774 \uac00\ub098\uc5d0 \ub300\ud55c \ud22c\uc790\uc5d0 \ub098\uc130\uac70\ub098 \ud22c\uc790 \uac00\ub2a5\uc131\uc744 \uac80\ud1a0 \uc911\uc774\uba70, \uc8fc\uc694 \ubd84\uc57c\ub294 \uae08\uad11 \uac1c\ubc1c, \ubaa9\uc7ac \uc81c\ud488, \uc11d\uc720 \uc0b0\uc5c5 \ub4f1\uc774\ub2e4. \ubbf8\uad6d\uc758 \ub300\ud615 \uad11\uc0b0 \uae30\uc5c5 \ub274\ubaac\ud2b8\ub294 2004\ub144 \uac00\ub098\uc758 \uad11\uc0b0\uc5c5\uc5d0 \uc9c4\ucd9c\ud558\uc600\uc73c\uba70, \ucd5c\ub300 10\uc5b5 \ub2ec\ub7ec \uaddc\ubaa8\uc758 \ud22c\uc790\ub97c \uacc4\ud68d\ud558\uace0 \uc788\ub2e4. 1997\ub144 \ub9d0\uc5d0\ub294 \ub204\uc5d0\ubcf4 \ud398\ud2b8\ub864\ub9ac\uc5c4\uc774 \uac00\ub098\uc758 \ud574\uc591 \uad11\ubb3c \uad8c\uc5ed \uc911