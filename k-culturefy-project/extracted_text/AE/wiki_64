\uc11c \ud569\uc758\ud588\ub2e4. \uadf8\ub7ec\ub098 \uc2ed\uc790\uad70\uc744 \ubb3c\ub9ac\uce58\uace0 \ub098\uc790 \ub2e4\uc2dc\uae08 \ub0b4\ubd84\uc774 \ubd88\uac70\uc84c\ub2e4. \ub2f9\uc2dc \uccb4\ud788\ub97c \uc77c\uc2dc\uc801\uc73c\ub85c \ub2e4\uc2a4\ub9ac\ub358 \ub9ac\ud22c\uc544\ub2c8\uc544 \uacf5\uc790 \uc9c0\uae30\ub9cc\ud0c0\uc2a4 \uce74\ub9ac\ubd80\ud0c0\uc774\ud2f0\uc2a4\ub294 \ubc00\ub808\ud2f0\ub125(Milet\u00ednek) \uc601\uc8fc \ubcf4\ub809(Bo\u0159ek)\uc744 \ud750\ub77c\ub371\ud06c\ub784\ub85c\ubca0 \uc2dc\uc758 \uc9c0\uc0ac\ub85c \uc784\uba85\ud588\ub2e4. \ubcf4\ub809\uc740 \uc591\ud615\ud30c \uadc0\uc871\uc774\uc5c8\ub2e4. \uc9c0\uae30\uc2a4\ub9cc\ud0c0\uc2a4\uac00 \uccb4\ud788\ub97c \ub5a0\ub098\uc790, \ud750\ub77c\ub371\ud06c\ub784\ub85c\ubca0 \uc2dc\ubbfc\ub4e4\uc740 \ubcf4\ub809\uc744 \uc9c0\ub3c4\uc790\ub85c \uc778\uc815\ud558\uc9c0 \uc54a\uace0 \uc9c0\uc288\uce74\uc5d0\uac8c \ub3c4\uc6c0\uc744 \uccad\ud588\ub2e4. \uc9c0\uc288\uce74\ub294 \uc774 \uc694\uccad\uc5d0 \uc751\ud558\uc5ec, 1423\ub144 8\uc6d4 4\uc77c \uc2a4\ud2b8\ub77c\ud638\ube0c \ub18d\uc7a5\uc5d0\uc11c \ubcf4\ub809\uc758 \uc591\ud615\ud30c \uad70\ub300\ub97c \ubc15\uc0b4\ub0b4\uace0 \uadf8\ub97c \ucad3\uc544\ub0c8\ub2e4.\n\uc774\uc81c \uc9c0\uc288\uce74\ub294 \uc9c0\uae30\uc2a4\ubb38\ud2b8 \uc655\uc758 \ubcf8\uc9c4\uc778 \ud5dd\uac00\ub9ac\ub97c \uce68\uacf5\ud558\uace0\uc790 \ud588\ub2e4. \ube44\ub85d \ud5dd\uac00\ub9ac\uc778\ub4e4\uc758 \uc218\uc801 \uc6b0\uc704\ub85c \uc778\ud574 \uc774 \ud5dd\uac00\ub9ac \uc6d0\uc815\uc740 \uadf8\ub2e4\uc9c0 \uc131\uacf5\uc801\uc774\uc9c0 \ubabb\ud588\uc73c\ub098, \uc774 \ub54c \uc9c0\uc288\uce74\uac00 \ubcf4\uc5ec\uc900 \ud1f4\uac01 \uc804\uc220\uc740 \uc9c0\uc288\uce74\uc758 \uac00\uc7a5 \uc704\ub300\ud55c \uad70\uc0ac\uc801 \uc5c5\uc801 \uc911 \ud558\ub098\uc600\ub2e4. 1424\ub144, \ub2e4\uc2dc \ud6c4\uc2a4\ud30c \uc0ac\uc774\uc5d0 \ub0b4\ubd84\uc774 \uc77c\uc5b4\ub0ac\uace0, \uc9c0\uc288\uce74\ub294 1\uc6d4 6\uc77c \uc2a4\uce7c\ub9ac\uccb4\uc