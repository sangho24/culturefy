34\ub294 \uc0ac\uc591\ud588\uc73c\ub098 \uc18d\uc73c\ub85c \uae30\ubed0\ud588\uace0, \uc720\ubb34\ub97c \uc0ac\ub791\ud558\ub294 \ud6a8\ubb38\ud669\ud6c4\ub3c4 \uadf8\ub7ec\ud588\ub2e4.\n\uc774\ud574 \uc624\ucd08\uce60\uad6d\uc758 \ub09c\uc774 \uc77c\uc5b4\ub098\uc790, \uc591\ub098\ub77c\ub294 \ub9e8 \uba3c\uc800 \uc624\ub098\ub77c\uc640 \ucd08\ub098\ub77c\uc758 \uacf5\uaca9\uc744 \ubc1b\uc544 \uadf9\ubcbd(\u68d8\u58c1)\uc5d0\uc11c \uc9c0\uace0 \uc218\ub9cc \uba85\uc774 \uc0b4\ud574\ub2f9\ud588\ub2e4. \uc591\ud6a8\uc655\uc740 \ub450\ub824\uc6cc\ud558\uba70 \ub2e4\uc2dc \uc694\uaca9\ud588\uc73c\ub098 \ud328\ud588\ub2e4. \uc0ac\uc138\uac00 \uae09\ud574\uc9c0\uc790 \uc218\uc591\uc131\uc744 \uc9c0\ud0a4\uba74\uc11c \uc870\uc815\uc758 \ud1a0\ubc8c\uad70\uc744 \uc774\ub04c\uace0 \uc788\ub294 \uc8fc\uc544\ubd80\uc5d0\uac8c \ub3c4\uc6c0\uc744 \uccad\ud588\uc73c\ub098 \uc8fc\uc544\ubd80\ub294 \uc751\ud558\uc9c0 \uc54a\uace0 \ub2e4\ub9cc \uc624\ub098\ub77c\uc640 \ucd08\ub098\ub77c\uc758 \uc591\ub3c4\ub97c \ub04a\uc5c8\ub2e4. \uc591\ud6a8\uc655\uc740 \ud55c\uc548\uad6d\uacfc \uc7a5\uc6b0\ub97c \uc7a5\uc218\ub85c \uc4f0\uace0 \ub05d\uae4c\uc9c0 \uc800\ud56d\ud558\uc5ec \uc624\ub098\ub77c\uc640 \ucd08\ub098\ub77c\ub97c \uc774\uacbc\ub2e4. \uc624\ub098\ub77c\uc640 \ucd08\ub098\ub77c\ub294 \uc11c\ucabd\uc73c\ub85c \ub098\uc544\uac00\uace0 \uc2f6\uc5c8\uc73c\ub098 \uc218\uc131\uc911\uc778 \uc591\ub098\ub77c\uac00 \uaebc\ub824\uc838 \uadf8\ub7ec\ud558\uc9c0 \ubabb\ud588\uace0 \ub9c8\uce68\ub0b4 \uc8fc\uc544\ubd80\uc5d0\uac8c \uc2f8\uc6c0\uc744 \uac78\uc5c8\ub2e4\uac00 \ub300\ud328\ud588\ub2e4. \uc591\ub098\ub77c\uac00 \uc624\ub098\ub77c\uc640 \ucd08\ub098\ub77c \ubc18\uad70\uc744 \uc8fd\uc774\uace0 \uc0ac\ub85c\uc7a1\uc740 \uac83\uc774 \uc870\uc815\uc778 \ud55c\ub098\ub77c\uc640 \ube44\uc2b7\ud588\ub2e4. \uadf8\ub7ec\ub098 \ucc98\uc74c \uc9c0\uc6d0 \uc694\uccad\uc744 \ub4e4\uc5b4\uc8fc\uc9c0 \uc54a\uc740 \uc8fc\uc544\ubd80\uc640\uc758 \uc0ac\uc774\uc5d0 \ud2c8\uc774 \uc0dd\uacbc\ub2