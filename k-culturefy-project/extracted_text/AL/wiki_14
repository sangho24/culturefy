\uac00 \ubc1c\uc0dd\ud588\ub2e4.\n\ubd84\ud654\uac00 \ub05d\ub0a0 \ubb34\ub835 \ub610\ub294 \uadf8 \uc9c1\ud6c4 \uac70\ub300\ud574\uc77c\uc774 \ubc1c\uc0dd\ud588\ub2e4. \uc77c\ubcf8 \uc11c\ubd80 \ud574\uc548\uc744 \ub530\ub77c \ub113\uc740 \uc9c0\uc5ed\uc5d0 \uc4f0\ub098\ubbf8 \ud1f4\uc801\ubb3c\uc774 \ud615\uc131\ub418\uc5c8\ub294\ub370, \ud2b9\ud788 \ubd84\ud654\uc5d0 \ub354 \uac00\uae4c\uc6b4 \uc9c0\uc5ed\uacfc\ub294 \ub2ec\ub9ac \uc4f0\ub098\ubbf8 \uc774\ud6c4 \ud0a4\uce74\uc774-\uc544\uce74\ud638\uc57c \ud14c\ud504\ub77c\uac00 \ud1f4\uc801\ub41c 300 km \ub5a8\uc5b4\uc9c4 \uacf3\uae4c\uc9c0 \ubd84\uba85\ud558\uac8c \ub098\ud0c0\ub0ac\ub2e4. \uc4f0\ub098\ubbf8\uc758 \uc6d0\uc778\uc740 \uc798 \uc774\ud574\ub418\uc9c0 \uc54a\uc9c0\ub9cc, \uc2dc\uc810\uc744 \uace0\ub824\ud560 \ub54c \uce7c\ub370\ub77c \uac00\uc7a5\uc790\ub9ac\uc758 \ubd95\uad34 \uac00\ub2a5\uc131\uc774 \uc81c\uae30\ub418\uc5c8\ub2e4.\n\ud1f4\uc801\ubb3c.\n\ubd84\ud654\uc758 \ucd1d \ubd80\ud53c\ub294 \ud604\uc7ac \u00a0DRE\ub85c \ucd94\uc815\ub418\uba70, \uc774\ub294 \uc774\uc804 \ucd94\uc815\uce58\uc758 \uc57d \ub450 \ubc30\uc5d0 \ub2ec\ud55c\ub2e4. \uc774\ub54c \ud5e4\ub4dc\ub77c\uc778\uc758 \ubd80\ud53c \ucd94\uc815\uce58 \ub294 \uacbd\uc11d\uacfc \uac19\uc740 \uac00\ubcbc\uc6b4 \ud1f4\uc801\ubb3c\uacfc \uad00\ub828\uc774 \uc788\uc73c\uba70, DRE \uacc4\uc0b0\uc5d0 \uc0ac\uc6a9\ub418\ub294 \uc77c\ubc18\uc801\uc778 \uc6a9\uc554\uacfc\ub294 \ub2e4\ub974\ub2e4. \ubd84\ud654\ub294 \ubd80\ubd84\uc801\uc73c\ub85c \uc218\uc911\uc5d0\uc11c \ubc1c\uc0dd\ud588\uc73c\uba70, \uadf8 \ud2b9\uc131\uc744 \uc644\uc804\ud788 \uc774\ud574\ud558\uae30 \uc704\ud574\uc11c\ub294 \ud654\uc0b0\uc1c4\uc124\uc131 \ubc00\ub3c4\ub958\uc758 \uc77c\ubd80\uac00 \uc218\uc911 \ubc00\ub3c4\ub958\ub85c \ubcc0\ud615\ub418\uc5b4 \ucd1d \uc758 \ud574\uc800 \ud1f4\uc801\ubb3c \ubd80\ud53c\uc5d0 \uae30\uc5ec\ud588\ub2e4\ub294 \uc0ac\uc2e4\uc744 \uae68\ub2ec\uc544\uc57c \ud588\ub2e4. \uce7c\ub370\ub77c \ubc14\ub2e5 \uc790\uccb4\ub294 \ud57