80\uc640 \ud611\ub825\ud588\ub2e4. 1954\ub144 4\uc6d4, \uac04\ub514 \uae30\ub150 \uc544\uce74\ub370\ubbf8\ub294 \ub098\uc774\ub85c\ube44 \uc655\ub9bd \uacf5\uacfc\ub300\ud559\uad50\uc5d0 \ud1b5\ud569\ub418\uc5c8\uace0 1956\ub144 4\uc6d4\uc5d0 \uccab \ud559\uc0dd \ubaa8\uc9d1\uc5d0 \ubb38\uc744 \uc5f4\uc5c8\ub2e4.\n\ud559\uc0dd\ub4e4\uc774 \ub300\ud559\uc5d0 \ub3c4\ucc29\ud55c \uc9c1\ud6c4, \ucf00\ub0d0\uc758 \uace0\ub4f1 \uad50\uc721 \ud328\ud134\uc740 \uc815\ubc00 \uc870\uc0ac\ub97c \ubc1b\uc558\ub2e4. 1958\ub144 \ub7f0\ub358 \ub300\ud559\uad50\uc758 \ubd80\ucd1d\uc7a5\uc774 \uc758\uc7a5\uc744 \ub9e1\uc740 \uc2e4\ubb34\uc9c4\uc758 \ucd94\ucc9c\uc744 \ud1b5\ud574 \ub098\uc774\ub85c\ube44 \uc655\ub9bd \uacf5\uacfc\ub300\ud559\uc758 \uc874 \ub85d\uc6b0\ub4dc \uacbd\uc774 \ubcc0\ud654\ub418\uc5c8\ub2e4. 1961\ub144 6\uc6d4 25\uc77c, \uc774 \ub300\ud559\uc740 \"\ub85c\uc5f4 \uce7c\ub9ac\uc9c0 \ub098\uc774\ub85c\ube44\"\ub77c\ub294 \uc774\ub984\uc73c\ub85c \ub3d9\uc544\ud504\ub9ac\uce74\uc5d0\uc11c \ub450 \ubc88\uc9f8 \ub300\ud559\uc774 \ub418\uc5c8\ub2e4.\n1964\ub144 5\uc6d4 20\uc77c\uc5d0 \ub098\uc774\ub85c\ube44 \uc655\ub9bd \ub300\ud559\uad50\ub294 \"\ub098\uc774\ub85c\ube44 \uc720\ub2c8\ubc84\uc2dc\ud2f0 \uce7c\ub9ac\uc9c0\"\ub85c \uac1c\uba85\ub418\uc5c8\ub2e4. \"\uc720\ub2c8\ubc84\uc2dc\ud2f0 \uce7c\ub9ac\uc9c0\"\uc758 \uc9c0\uc704\uc5d0 \ub3c4\ub2ec\ud55c \ud6c4, \uae30\uad00\uc740 \ud559\uc0dd\ub4e4\uc5d0\uac8c \ub7f0\ub358 \ub300\ud559\uad50\uac00 \uc218\uc5ec\ud558\ub294 \ud559\uc0ac \ud559\uc704\ub97c \uc900\ube44\ud558\ub294 \ub3d9\uc2dc\uc5d0 \ub300\ud559 \uc878\uc5c5\uc7a5 \ud504\ub85c\uadf8\ub7a8\uc744 \uacc4\uc18d \uc81c\uacf5\ud588\ub2e4. \uc720\ub2c8\ubc84\uc2dc\ud2f0 \uce7c\ub9ac\uc9c0 \ub098\uc774\ub85c\ube44\ub294 1966\ub144 \uad6d\ub0b4 \uacfc\ud559\ubd80\ub97c \uc81c\uc678\ud558\uace0 \ub3d9\uc544\ud504\ub9ac\uce74 \ub300\ud559\uad50\uc758 \ud559\uc704\ub9cc\uc744 \uc704\ud55c \ud559\uc0dd\ub4e4\uc744 \uc900\ube44\ud558\uae30 \uc2dc\uc791\ud560 \ub