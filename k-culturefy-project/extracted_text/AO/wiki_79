 \uc559\ud22c\uc548 \ub4e4\ub77c\ub098\ub974\ub4dc\uac00 \ucc98\uc74c \uad6c\uc785\ud588\ub2e4. \uc5d0\ub4dc\ubabd \ub4dc \uacf5\ucfe0\ub974\ub294 1889\ub144 \ud55c \uace8\ub3d9\ud488 \uc0c1\uc810\uc5d0\uc11c \uc774 \uadf8\ub9bc\uc744 \ubc1c\uacac\ud588\ub294\ub370, \ub208 \ub36e\uc778 \ud48d\uacbd \uc18d \uc131\uc774\ub098 \uad50\ud68c\ub97c \uadf8\ub9b0 \ub098\ubb34 \ud310\ub12c \ub4a4\uc5d0 \uc228\uaca8\uc838 \uc788\uc5c8\ub2e4. \ucfe0\ub974\ubca0 \uce74\ud0c8\ub85c\uadf8 \ub808\uc870\ub124 2\uad8c\uc744 \ucd9c\ud310\ud558\uace0 \ucfe0\ub974\ubca0 \ubbf8\uc220\uad00\uc744 \uc124\ub9bd\ud55c \ub85c\ubca0\ub974 \ud398\ub974\ub2c8\uc5d0\uc5d0 \ub530\ub974\uba74, \ud5dd\uac00\ub9ac \uc218\uc9d1\uac00 \ud5c8\ud2b8\ubc84\ub2c8 \ud398\ub80c\uce20 \ub0a8\uc791\uc774 1910\ub144 \ubca0\ub974\ub0c9\uc8c8 \uac24\ub7ec\ub9ac\uc5d0\uc11c \uc774 \uadf8\ub9bc\uc744 \uad6c\uc785\ud558\uc5ec \ubd80\ub2e4\ud398\uc2a4\ud2b8\ub85c \uac00\uc838\uac14\ub2e4. \uc81c2\ucc28 \uc138\uacc4 \ub300\uc804 \ub9d0, \uc18c\ub828\uad70\uc774 \uc774 \uadf8\ub9bc\uc744 \uc57d\ud0c8\ud558\uc600\uc9c0\ub9cc \ub098\uc911\uc5d0 \ud5c8\ud2b8\ubc84\ub2c8\uac00 \ubab8\uac12\uc744 \uc9c0\ubd88\ud558\uace0 \ub418\ucc3e\uc558\ub2e4. \ud5c8\ud2b8\ubc84\ub2c8\ub294 1947\ub144 \uacf5\uc0b0\uc8fc\uc758 \uc815\uad8c \uc218\ub9bd \uc9c1\uc804\uc758 \ud5dd\uac00\ub9ac\ub97c \ub5a0\ub0ac\ub2e4. \uadf8\ub294 \ub2e8 \ud55c \uc810\uc758 \uc608\uc220 \uc791\ud488\ub9cc\uc744 \uac00\uc9c0\uace0 \ub5a0\ub0a0 \uc218 \uc788\uc5c8\uace0, \u300a\uc138\uc0c1\uc758 \uae30\uc6d0\u300b\uc744 \uc120\ud0dd\ud558\uc5ec \ud30c\ub9ac\ub85c \uac00\uc838\uac14\ub2e4.\n1955\ub144 \u300a\uc138\uc0c1\uc758 \uae30\uc6d0\u300b\uc740 \uacbd\ub9e4\uc5d0\uc11c 150\ub9cc \ud504\ub791(\ub2f9\uc2dc \uc57d 4,285 \ub2ec\ub7ec \uac00\uce58)\uc5d0 \ud314\ub838\ub2e4. \uc0c8 \uc18c\uc720\uc8fc\ub294 \uc815\uc2e0\ubd84\uc11d\ud559\uc790 \uc790\ud06c \ub77c\uce89\uc774\uc5c8\ub2e4. \uadf8\uc640 \uadf8\uc758 \uc544\ub0b4\uc778 \ubc30\uc6b0 \uc2e4\ube44\uc5