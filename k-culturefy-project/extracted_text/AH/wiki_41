 \ud560 \uc218 \uc788\ub2e4. \ucc9c\uc0ac\ubd80\uc778\uc740 \uace8\ub4e0 \ub9ac\ud2b8\ub9ac\ubc84\ub85c \ubb18\uc0ac\ub41c\ub2e4.\n \ubc88\uac1c\uc640 \ud138\ubcf4\uc758 \uc544\ub454\ud55c \uc801\uc218. \uc6d0\uc791 \uc18c\uc124\uacfc \ub2ec\ub9ac \ubc94\uc8c4\uacc4\uc758 \ub098\ud3f4\ub808\uc639\uc774\ub77c \ubd88\ub9ac\uae30 \uc704\ud574 \ub098\uc11c\ub294 \ubc94\uc8c4 \uacc4\ud68d\uac00. \ubcf8\uc791\uc5d0 \ub098\uc624\ub294 \ub300\ubd80\ubd84\uc758 \ub3c4\ub09c \uc0ac\uac74\uc744 \uc9c0\ud718\ud558\uc9c0\ub9cc \ub098\uc911\uc5d0\ub294 \ubc88\uac1c\uc5d0 \uc758\ud574 \uc694\uac01\ub41c\ub2e4 \uc7a5\ubb3c \ucde8\ub4dd\uc744 \ud574 \uc790\uc2e0\uc758 \ub450\ub1cc\ub97c \uc544\ub07c\uc9c0 \uc54a\uc73c\uba70 \uac00\ub054\uc740 \ubc94\uc8c4 \uc0ac\uc8fc, \uc778\uc9c8 \ub0a9\uce58, \uc7a5\ubb3c \uad50\ud658, \ub4f1\uc73c\ub85c \uc5f0\uba85\ud558\uace0 \uc788\ub2e4. \uac70\uc774\ud0c0 \uad50\uc218\ub294 \ubcf4\ub77c \ub291\ub300\ub85c \ubb18\uc0ac\ub41c\ub2e4.\n\n\n\uc8fc\uc694 \uc778\ubb3c.\n\uaebd\uc1e0, \ub545\ub538\uc774\n \uac70\uc774\ud0c0 \uad50\uc218\uc758 \uc544\ub454\ud55c \ubd80\ud558. \ud574\uc801\uc9c8\uc744 \uadf8\ub9cc\ub454 \ud6c4 \uac70\uc774\ud0c0 \uad50\uc218\uc5d0\uac8c \ubc1c\ud0c1\ub418\uc5b4 \ubc94\uc8c4\ub97c \uc800\uc9c0\ub974\ub294 \uacf5\ubc94\uc790. \uac70\uc774\ud0c0 \uad50\uc218\uac00 \ubc94\uc8c4\ub97c \uc2e4\ud589\ud560 \ub54c\uc5d0\ub294 \uc2e4\uc9c8\uc801\uc73c\ub85c \ubc94\uc8c4\ub97c \uc800\uc9c0\ub9ac\ub294 \ud589\ub3d9\uc694\uc6d0\uc774\ub2e4. \uad50\uc218\uc758 \ubc1c\uba85\ud488\uc744 \uc774\uc6a9\ud558\uc9c0\ub9cc \ubaa8\uc790\ub780 \uc9c0\uc131\uc73c\ub85c \uc778\ud574 \ub9dd\uac00\ud2b8\ub9ac\uac70\ub098 \ubc88\uac1c\uc5d0 \uacc4\ub77d\uc774\ub098 \uacbd\ubd80\uc758 \ucc38\uacac\uc73c\ub85c \uc778\ud574 \uc2e4\ud328. \uad50\uc218\uc758 \ubc94\uc8c4 \uacc4\ud68d\uc744 \uc2e0\uc6a9\ud558\uc9c0 \uc54a\ub294\ub2e4.\n \ubc88\uac1c\uc758 \uc120\uc758\uc758 \uacbd\uc7c1\uc790\uc774\uba74\uc11c \uacbd\ucc30\ub85c\uc11c\uc758 \ucc45\uc7