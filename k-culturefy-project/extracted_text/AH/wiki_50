\ub2e4 \uc720\uc5f0\ud558\uac8c \ubc14\uafb8\ub824\ub294 \uac83\uc774\ub2e4. \uce58\ub8cc\uc0ac\ub294 \ud658\uc790 \uac1c\uc778\uc758 \ub290\ub08c\ub4e4\uc744 \ub2e4\ub8e8\uace0 \uc2e4\uc81c\ub85c \ubc1c\uc0dd\ud55c \uc0c1\ud669\uc774\ub4e0 \uc2e4\uc81c\ub85c \uc788\uc744\ubc95\ud55c \uc0c1\ud669\uc774\ub4e0 \ubaa8\ub450 \uac80\ud1a0\ud558\uba70, \ud658\uc790 \uac1c\uc778\uc758 \ub290\ub08c\uacfc \uc0c1\ud669\uc5d0 \uc811\uadfc\ud558\ub294 \ubc29\uc2dd\ub3c4 \uac80\ud1a0\ud55c\ub2e4.\n\ubcc0\uc99d\ubc95\uc801 \ud589\ub3d9\uce58\ub8cc.\n\ubcc0\uc99d\ubc95\uc801 \ud589\ub3d9\uce58\ub8cc\ub294 \uc778\uc9c0\ud589\ub3d9\uce58\ub8cc(cognitive behavioral treatment, CBT)\uc640 \uc720\uc0ac\ud558\uba70, \uc778\uc9c0\ud589\ub3d9\uce58\ub8cc\uc5d0 \uba85\uc0c1(mediation)\uc744 \ucd94\uac00\ud558\uc600\ub2e4. \uc774\ub97c \ud1b5\ud574 \uce58\ub8cc\ub294 \ud658\uc790\ub4e4\uc774 \uc99d\uc0c1\uc744 \uad00\ub9ac\ud560 \uc218 \uc788\ub294 \uae30\uc220\uc744 \uc2b5\ub4dd\ud558\uac8c \ud55c\ub2e4. \uae30\uc220\uc5d0\ub294 \uc815\uc11c \uc870\uc808(emotion regulation), \ub9c8\uc74c\ucc59\uae40(mindfulness), \uc2a4\ud2b8\ub808\uc2a4 \uc778\ub0b4(stress hardiness) \ub4f1\uc744 \ud3ec\ud568\ud55c\ub2e4.\n\uc778\uc9c0\ud589\ub3d9\uce58\ub8cc.\n\uc778\uc9c0\ud589\ub3d9\uce58\ub8cc(Cognitive behavioral therapy, CBT) \uc5ed\uc2dc \uacbd\uacc4\uc120 \uc131\uaca9\uc7a5\uc560 \uce58\ub8cc\uc5d0 \uc0ac\uc6a9\ub41c\ub2e4. \uc774 \uce58\ub8cc\ub294 \uc7a5\uc560\ub85c\ubd80\ud130 \ubb38\uc81c\ub97c \ubc1c\uacac\ud558\uc5ec \uac1c\uc778\uc758 \ud589\ub3d9\uacfc \uc2e0\ub150\uc744 \ubc14\uafb8\ub294 \uac83\uc744 \uc911\uc2ec\uc73c\ub85c \ud55c\ub2e4. \uc778\uc9c0\ud589\ub3d9\uce58\ub8cc\ub294 \uc790\uc0b4\uc0ac\uace0(suicidal thoughts)\uc640 \uc790\ud574\ud589\ub3d9\uc740 \ubb3c\ub860 \ubd88\uc548\uacfc \uae30\ubd84 \uc99d\uc0c1(mood symptom)\uc744 \uc904\uc5ec\uc8fc\ub294 \uac83\uc73c\ub85c \uc54c\ub824\uc838 \uc788\ub2e4.\n\uc815\uc2e0\ud654\uae30\ubc18\uce58\ub8cc.\n\uc815\uc2e0\ud654\uae30\ubc18\