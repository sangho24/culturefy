436\u0430\u0435\u0442,\n\u0417\u0432\u0435\u0437\u0434\u043d\u0430\u044f, \u0441\u0432\u044f\u0442\u0430\u044f \u0410\u0431\u0445\u0430\u0437\u0438\u044f!\n\u0420\u0430\u0434\u0430, \u0420\u0435\u0439\u0434\u0430, \u0420\u0430\u0440\u0438\u0440\u0430!\n\u0420\u0430\u0434\u0430, \u0420\u0435\u0440\u0430\u043c\u0430, \u0420\u0435\u0440\u0430\u0448\u0430!\n\u041c\u0430\u0442\u044c \u0433\u0435\u0440\u043e\u044f,\n\u0433\u0434\u0435 \u0440\u043e\u0436\u0434\u0430\u0435\u0442\u0441\u044f \u043d\u0430\u0440\u0442 - \u043a\u0430\u043a \u0441\u0432\u044f\u0442\u043e!\n\u0410\u0431\u0445\u0430\u0437\u0438\u044f, \u0434\u0430 \u0431\u043b\u0430\u0433\u043e\u0441\u043b\u043e\u0432\u0438\u0442 \u0442\u0435\u0431\u044f \u043c\u0438\u0440\n\u043d\u0430 \u0442\u044b\u0441\u044f\u0447\u0438 \u043b\u0435\u0442 \u0432\u043f\u0435\u0440\u0451\u0434.\n\u0414\u0435\u0442\u0438 \u043e\u0431\u044a\u0435\u0434\u0438\u043d\u044f\u044e\u0442\u0441\u044f \u043a\u0430\u043a \u043e\u0434\u0438\u043d \u043d\u0430\u0440\u043e\u0434,\n\u0411\u0440\u0430\u0442\u044c\u044f \u043f\u043b\u0435\u0447\u043e\u043c \u043a \u043f\u043b\u0435\u0447\u0443.\n\u0418\u0434\u0438\u0442\u0435, \u0430\u0431\u0445\u0430\u0437\u044b!\n\u0411\u0440\u0430\u0442\u044c\u044f, \u0438\u0434\u0438\u0442\u0435!\n\u0412 \u0417\u0430\u043a\u0430\u0432\u043a\u0430\u0437\u044c\u0435,\n\u043c\u044b \u0432\u0441\u0435\u0433\u0434\u0430 \u0437\u0434\u0435\u0441\u044c,\n\u0411\u043e\u0433 \u0441\u043c\u043e\u0442\u0440\u0438\u0442 \u0437\u0434\u0435\u0441\u044c,\n\u0434\u043b\u044f \u043b\u0443\u0447\u0448\u0435\u0433\u043e \u0431\u0443\u0434\u0443\u0449\u0435\u0433\u043e!\n\u0418\u0434\u0438\u0442\u0435, \u0430\u0431\u0445\u0430\u0437\u044b!\n\u0421\u043e\u043b\u043d\u0446\u0435 \u0432\u0441\u0442\u0430\u0435\u0442!\n\u041a\u0430\u043a\u043e\u0435 \u0441\u0432\u0435\u0442\u043b\u043e\u0435 \u0431\u0443\u0434\u0443\u0449\u0435\u0435!\n\u041b\u044e\u0431\u0438\u0442\u044c \u043d\u0430\u0432\u0441\u0435\u0433\u0434\u0430,\n\u044d\u04