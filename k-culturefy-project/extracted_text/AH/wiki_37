744 \ubd80\uc5ec\ud558\ub294 \ub2e4\ub978 \uba54\ucee4\ub2c8\uc998\uc774 \ud544\uc694\ud558\ub2e4. \uc774\ub97c \ud558\uae30 \uc704\ud574\uc11c\ub294 \ub300\uac1c \ud655\uc7a5 \ud14c\ud06c\ub2c8\uceec\ub7ec(, ETC)\ub97c \ub3c4\uc785\ud55c\ub2e4. \ud655\uc7a5 \ud14c\ud06c\ub2c8\uceec\ub7ec\uc5d0\uc11c\ub294 \ud14c\ud06c\ub2c8\uceec\ub7ec\uc758 \uac8c\uc774\uc9c0 \uad70 formula_8\uc744 \ud655\uc7a5\ud558\uc5ec formula_9\ub85c\uc11c \ud14c\ud06c\ub2c8\ud398\ub974\ubbf8\uc628\uacfc \uc77c\ubc18 \ud398\ub974\ubbf8\uc628(\ucffc\ud06c, \ub819\ud1a4)\uc744 \uc11c\ub85c \uc0c1\ud638\uc791\uc6a9\ud558\uac8c \ud55c\ub2e4. (\ub300\uac1c \ud14c\ud06c\ub2c8\uceec\ub7ec\uc5d0 \uc138\ub300\ub97c \ud3ec\ud568\uc2dc\ucf1c formula_10\ub85c \uc7a1\ub294\ub2e4.) \uc774\ub807\uac8c \ub418\uba74 \ud655\uc7a5 \ud14c\ud06c\ub2c8\uceec\ub7ec\uac00 \uae68\uc9c0\ub294 \ub0ae\uc740 \uc5d0\ub108\uc9c0\uc5d0\uc11c\ub294 \ud398\ub974\ubbf8\uc628-\ud14c\ud06c\ub2c8\ud398\ub974\ubbf8\uc628-\ud398\ub974\ubbf8\uc628-\ud14c\ud06c\ub2c8\ud398\ub974\ubbf8\uc628 \uc720\ud6a8 \uc0c1\ud638\uc791\uc6a9\uc774 \uc0dd\uae34\ub2e4. \uc774 \uc0c1\ud638\uc791\uc6a9\uc5d0 \uc758\ud558\uc5ec \ud14c\ud06c\ub2c8\uceec\ub7ec \uc751\uc9d1\uc744 \ud1b5\ud574 \ud398\ub974\ubbf8\uc628\uc740 \uc9c8\ub7c9\uc744 \uac00\uc9c0\uac8c \ub41c\ub2e4.\n\ud398\ub974\ubbf8\uc628 \uc9c8\ub7c9\uc744 formula_11, \ud655\uc7a5 \ud14c\ud06c\ub2c8\uceec\ub7ec\uc758 \uacb0\ud569 \uc0c1\uc218\ub97c formula_12, \ud655\uc7a5 \ud14c\ud06c\ub2c8\uceec\ub7ec \uc5d0\ub108\uc9c0 \ub208\uae08\uc744 formula_13, \ud14c\ud06c\ub2c8\uceec\ub7ec \uc751\uc9d1\uc744 formula_14\uc73c\ub85c \uc4f0\uba74,\nformula_15\n\uc774 \ub41c\ub2e4. \uc989 \ud655\uc7a5 \ud14c\ud06c\ub2c8\uceec\ub7ec \uc5d0\ub108\uc9c0 \ub208\uae08\uc774 \ub354 \ub192\uc744\uc218\ub85d \uc0dd\uc131\ub418\ub294 \ud398\ub974\ubbf8\uc628 \uc9c8\ub7c9\uc740 \ub354 \uc791\ub2e4. \ub530\ub77c\uc11c \uaf2d\ub300\uae30 \ucffc\ud06c\uc640 \uac19\uc740 \ubb34\uac70\uc6b4 \ucffc\ud06c\uc758 \uc9c8\ub