m und Drang; \u75be\u98a8\uacfc \u6012\u6fe4) \uc6b4\ub3d9\uc5d0 \uc601\ud5a5\uc744 \ubc1b\uc740 \uac83\uc73c\ub85c \ubd84\ub958\ub418\uae30\ub3c4 \ud55c\ub2e4. \ubaa8\ucc28\ub974\ud2b8\uc758 \ub2e8\uc870 \uad50\ud5a5\uace1\uc740 \uad50\ud5a5\uace1 40\ubc88\uacfc \uc774 \uace1\ubfd0\uc774\uba70, \uad50\ud5a5\uace1 40\ubc88 \uc5ed\uc2dc\ub3c4 \uc774 \uc6b4\ub3d9\uc5d0 \uc601\ud5a5\uc744 \ubc1b\uc740 \uac83\uc73c\ub85c \ubcfc \uc218\ub3c4 \uc788\ub2e4. \ub2e8\uc870\uc758 \uc0ac\uc6a9\ub3c4 \uadf8\ub807\uc9c0\ub9cc, \uc774 \uace1\uc740 \ud2b9\uc774\ud55c \ud3b8\uc131(\ud2b9\ud788 \ud604\uc545\uae30)\uc73c\ub85c \ud2b9\ubcc4\ud558\ub2e4.\n&lt;score vorbis=\"1\"&gt;\n\uc81c1\uc545\uc7a5.\n\\relative c {\n \\tempo \"Allegro con brio\" 4 = 170\n \\key g \\minor\n g8 g4 g g g8 |\n d8 d4 d d d8 |\n es8 es4 es es es8 |\n fis,8 fis4 fis fis fis8 |\n&lt;/score&gt;\n&lt;score vorbis=\"1\"&gt;\n\uc81c2\uc545\uc7a5.\n\\relative c {\n \\version \"2.18.2\"\n \\tempo \"Andante\"\n \\key ees \\major\n \\time 2/4\n \\tempo 4 = 50\n \\partial 4 \\partial 8 bes,8\\p^\\markup { \\italic {con sordini} } (aes g) r8\n c8 (bes aes) r8\n aes (g f) r8 bes (aes g) r8\n ees'-! ees (f,) r8 bes-!\n aes (g) r8 c-!\n c (bes) r8\n d, (ees4) r8\n&lt;/score&gt;\n&lt;score&gt;\n\uc81c3\uc545\uc7a5.\n\\relative c {\n \\version \"2.18.2\"\n \\tempo \"Menuetto\"\n \\key bes \\major\n \\time 3/4\n g,4\\f d g\n bes2 c8 (a)\n g4 fis g\n a8 (fis) d4 r4\n c'4\\p c c\n b4.\\fp (c16 d c4)\n ees ees ees\n d4.\\fp (ees16 f ees4)\n g4\\f g g\n fis (c') bes-!\n \\grace bes16 (a4) g fis\n g2 r4\\bar \":|.\"\n&lt;/score&gt;\n&lt;score&gt;\n \\relative c\" {\n \\version \"2.18.2\"\n \\key g \\major\n \\tempo \"Trio\"\n \\time 3/4\n d2 \\p b'8 (g)\n g (fis) fis4 fis\n fis (c') b16 (a g fis)\n g8 (b) d,4 r4\n g (fis e)\n a4.\\fp (g8) e4\n \\grace a16 (g4) fis e\n d2 r4 \\bar \":|.\"\n \\relative c\" {\n \\key g \\major\n \\time 3/4\n b2\\p r4\n r4 c c\n c (fis,) g16 (a b c)\n b8 (d) b4 r4\n e4 (d cis)\n fis4.\\fp (e8) d4\n