\ubcc4 \uc778\uad6c\uc758 \uc131, \uc5f0\ub839 \ubd84\ud3ec\uac00 \uc81c\uc2dc\ub418\uc5b4 \uc788\ub2e4. \ud55c\ud3b8 \ubd84\ub2e8\uc774 \ub418\uba74\uc11c \uc774\ub0a8 \ud669\ud574\uc9c0\uc5ed\uc740 \uacbd\uae30\uc9c0\uc5ed\uc73c\ub85c, \uc774\ubd81\uc758 \uacbd\uae30\uc9c0\uc5ed\uc740 \ud669\ud574\ub3c4\ub85c, \uac15\uc6d0\ub3c4\ub294 \ub0a8\ubd81\uc73c\ub85c \uac08\ub77c\uc84c\ub294\ub370 \uc774\ub7f0 \ud589\uc815 \uacbd\uacc4 \ubcc0\uacbd\uc744 \uace0\ub824\ud558\uc5ec \ub0a8\ubd81\uc758 \uc778\uad6c\uad6c\uc870\ub97c \uc0b0\ucd9c\ud560 \uc218 \uc788\ub2e4.\n\u00a0 \ubd81\ud55c\uc758 \ucd1d\uc870\uc0ac(\uc13c\uc11c\uc2a4)\ub294 1993\ub144 \ucd5c\ucd08\ub85c \uc2e4\uc2dc\ub418\uc5c8\ub2e4\uace0 \uc54c\ub824\uc838 \uc788\ub2e4. \uc77c\uc81c\uc2dc\uae30 \ub54c \ucd1d 5\ud68c \uc778\uad6c\ucd1d\uc870\uc0ac(\uad6d\uc138\uc870\uc0ac)\uac00 \uc774\ub8e8\uc5b4\uc84c\uace0 \ub0a8\ud55c\uc5d0\uc11c\ub294 1949\ub144 \uc784\uc2dc\ucd1d\uc870\uc0ac\uac00 \uc774\ub8e8\uc5b4\uc84c\uc9c0\ub9cc \uc804\uc7c1\uc73c\ub85c \uac70\uc758 \uc18c\uc2e4\ub418\uace0 1966\ub144 \ucd1d\uc870\uc0ac\ub97c \uc81c\uc678\ud558\uba74 1955\ub144\ubd80\ud130 \ub9e4 5\ub144\ub9c8\ub2e4 \ucd1d\uc870\uc0ac\uac00 \uc774\ub8e8\uc5b4\uc84c\ub2e4. \ubd81\ud55c\uc5d0\uc11c \uc778\uad6c\ucd1d\uc870\uc0ac\uac00 \ucd5c\uadfc\uae4c\uc9c0 \uc774\ub8e8\uc5b4\uc9c0\uc9c0 \uc54a\uc558\ub358 \uac83\uc740 \uacbd\uc81c\uc801\uc778 \uc774\uc720\ub3c4 \uc788\uaca0\uc9c0\ub9cc \uc815\uc8fc\uc778\uad6c\uccb4\uacc4\uc640 \uc8fc\ubbfc\ub4f1\ub85d\uc870\uc0ac\uc5d0 \uae30\ucd08\ud558\uc5ec \uc778\uad6c\ub97c \uaf64 \uc815\ud655\ud558\uac8c \uc9d1\uacc4\ud560 \uc218 \uc788\ub2e4\ub294 \ud655\uc2e0\uc774 \uc788\uc5c8\uae30 \ub54c\ubb38\uc774\ub77c\uace0 \ucd94\uce21\ub41c\ub2e4. \ubd81\ud55c\uc774 \uc678\ubd80\uc5d0 \uc778\uad6c\uc790\ub8cc\ub97c \uacf5\uac1c\ud55c \uac83\uc740 1980\ub144\ub300 \uc911\ud6c4\ubc18\uc774\uc9c0\ub9cc \uadf8 \uc774\uc804\uc5d0\ub3c4 \uc8fc\ubbfc\ub4f1\ub85d\uc81c\ub3c4\ub97c \ud1b5\ud574 \uc778\u