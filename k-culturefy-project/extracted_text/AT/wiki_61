3\uc744 \uc554\uc2dc\ud55c\ub2e4.\n1961\ub144, \uc9c0\ub9ac\ud559\uc790\uc774\uc790 \uc5ed\uc0ac\ud559\uc790 \ud3f4 \ud718\ud2c0\ub9ac(Paul Wheatley)\ub294 \ub2e4\uc591\ud55c \uc0ac\ub8cc\ub4e4\uc744 \uac80\ud1a0\ud558\uc5ec \ub791\uce74\uc218\uce74\ub294 \uc624\ub298\ub0a0 \ud30c\ud0c0\ub2c8 \ub3c4\uc2ec \uc778\uadfc\uc5d0 \uc788\uc5c8\uc744 \uac83\uc73c\ub85c \ubcf4\uc558\ub2e4. \ud504\ub791\uc2a4 \uace0\uace0\ud559\uc790\uc774\uc790 \uc5ed\uc0ac\ud559\uc790 \ubbf8\uc140 \uc790\ud06c\uc5d0\ub974\uad6c\uc54c\uc26c(Michel Jacq-Hergoualc'h)\ub294 \uc774\uc804\uc5d0 \uc57c\ub791 \uc778\uadfc\uc5d0 \uc788\uc5c8\ub358 \ud30c\ud0c0\ub2c8\uac15 \uc5b4\uadc0\ub97c \ub791\uce74\uc218\uce74\ub85c \ube44\uc815\ud588\ub2e4. \ub610\ud55c \uadf8\ub294 \ud30c\ud0c0\ub2c8, \uc0ac\uc774\ubd80\ub9ac, \uc584\ub77c(Yala) \uc0ac\uc774\uc758 \ubaa8\ub4e0 \uc9c0\uc5ed\uc774 \ub791\uce74\uc218\uce74 \uc77c\ubd80\uc600\ub2e4\uace0 \uc8fc\uc7a5\ud558\uc600\ub2e4. \ud604\ub300 \uace0\uace0\ud559\uc801 \ud0d0\ud5d8\uc744 \ud1b5\ud574 \ud30c\ud0c0\ub2c8 \ubd81\ucabd 15km \uc57c\ub791 \uc778\uadfc\uc5d0 \uc788\ub294 \ud55c \ub9c8\uc744\uc5d0\uc11c \u300e\uc591\uc11c\u300f\uc5d0 \ubb18\uc0ac\ub41c \ub3c4\uc2dc\ub85c \ucd94\uc815\ub418\ub294 \uc720\uc801\ub4e4\uc744 \ubc1c\uacac\ud558\uc600\ub2e4. \ub3c4\uc2dc\ub294 \ud574\uc548\uc5d0\uc11c 16km \uc815\ub3c4 \ub5a8\uc5b4\uc838 \uc788\uc73c\uba70 \uc218\ub85c\ub4e4\uc744 \uacbd\uc720\ud558\uc5ec \ubc14\ub2e4\ub85c \ud1b5\ud558\ub294 \uac15\ub4e4\uacfc \uc5f0\uacb0\ub418\uc5b4 \uc788\uc5c8\ub2e4. \uc218\ub85c\uc5d0 \ud1a0\uc0ac\uac00 \uc313\uc784\uc73c\ub85c\uc368 \uc1e0\ud1f4\ub97c \ub9de\uc774\ud55c \uac83\uc73c\ub85c \ubcf4\uc778\ub2e4.\n\uace0\uace0\ud559 \ubc1c\uad74.\n\ud3f4 \ud718\ud2c0\ub9ac\uac00 \ub791\uce74\uc218\uce74 \ud6c4\ubcf4\uc9c0\ub97c \uc81c\uc548\ud568\uc5d0 \ub530\ub77c, 1960\ub144\ub300 \uba87\uba87 \uace0\uace0\ud559 \ubc1c\uad74\uc774 \uc218\ud589\ub418\uc5b4 \ub791\uce74\uc218\uce74 \uc704\uce58\ub97c \ube44\uc815\ud558\ua