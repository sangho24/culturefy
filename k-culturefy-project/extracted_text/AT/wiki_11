uc744 \ub9de\uc544 \uc0ac\ub9dd\ud588\ub2e4\uace0 \uae30\uc220\ub418\uc5b4 \uc788\ub2e4. \ub610\ud55c \ubbf8 \uc81c24\ubcf4\ubcd1\uc0ac\ub2e8 \ubcf4\uace0\uc11c\uc640 \ubcd1\ucc38\ubd80\ub300 \ubcf4\uace0\uc11c \uc5ed\uc2dc 7\uc6d4 20\uc77c 16\uc2dc 30\ubd84\uacbd\uc5d0 \uae30\uad00\ucc28\uac00 \ub300\uc804\uc5ed\uc73c\ub85c \ucd9c\ubc1c\ud558\uc600\ub2e4\uace0 \uae30\ub85d\ud558\uace0 \uc788\ub294\ub370 \uc815\ub9ac\ud558\uc790\uba74 \ubbf8\uad6d \uce21 \uc790\ub8cc\uc5d0\uc11c\ub294 \uc77c\uad00\ub418\uac8c \uae40\uc7ac\ud604 \uae30\uad00\uc0ac\uc758 \uc21c\uc9c1\uc77c(\uc791\uc804\uc77c)\uacfc \uae30\uad00\ucc28 \ubc1c\ucc28 \uc2dc\uac01\uc744 7\uc6d4 20\uc77c 16\uc2dc 20\ubd84\uc5d0\uc11c 30\ubd84 \uc0ac\uc774\ub85c \uae30\ub85d\ud558\uace0 \uc788\ub2e4.\n\uae40\uc7ac\ud604 \uae30\uad00\uc0ac\uc758 \uc21c\uc9c1\uc77c(\uc791\uc804\uc77c)\uc774 7\uc6d4 19\uc77c\ub85c \uc798\ubabb \uc54c\ub824\uc9c0\uac8c\ub41c \uc9c1\uc811\uc801\uc778 \uc774\uc720\ub294 1962\ub144 12\uc6d4 5\uc77c \ub300\uc804\uad11\uc5ed\uc2dc \ud310\uc554\ub3d9 \uc18c\uc7ac \ub300\uc804\uc5ed\u2013\uc138\ucc9c\uc5ed \uad6c\uac04\uc5d0 \uac74\ub9bd\ub418\uc5c8\ub358 \uae40\uc7ac\ud604 \uae30\uad00\uc0ac \uc21c\uc9c1\ube44\uc5d0 \uc21c\uc9c1\uc77c\uc774 7\uc6d4 19\uc77c\ub85c \uc798\ubabb \uae30\uc7ac\ub418\uc5c8\uae30 \ub54c\ubb38\uc774\ub2e4.\n\uc989 1962\ub144 12\uc6d4 10\uc77c \uc21c\uc9c1\ube44 \uc81c\ub9c9\uc2dc \uad00\ub828 \uae30\uc0ac\uc5d0\uc11c\ub3c4 7\uc6d4 20\uc77c\uc5d0 \uae40\uc7ac\ud604 \ucca0\ub3c4 \uae30\uad00\uc0ac\uac00 \uc21c\uc9c1\ud588\ub2e4\uace0 \uae30\uc220\ub418\uc5b4 \uc788\uc9c0\ub9cc \uc815\uc791 \uc21c\uc9c1\ube44\uc5d0 7\uc6d4 19\uc77c\ub85c \uc21c\uc9c1\uc77c\uc774 \uc798\ubabb \uae30\uc7ac\ub418\uc5c8\uace0 \uadf8 \ud6c4 \ucca0\ub3c4\uccad \uc2dc\uc808\ubd80\ud130 \uc774 \uc21c\uc9c1\ube44\uc5d0 \uae30\uc7ac\ub41c \uc21c\uc9c1\uc77c\uc5d0 \ub9de\ucd94\uc5b4 \uae40\uc7ac\ud604 \uae30\uad00\uc0ac \ud604\uc591\uc0ac\uc5c5\uc744 \uc9c4\ud589 \ud558\uba74\uc11c \uc644\