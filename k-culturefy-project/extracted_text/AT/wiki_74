44 \uac00\uc9c0\uace0 \uc788\ub2e4. \ubc84\ubc0d\uc5c4\uc758 \"\uac78\ub808\uc5d0 \uc624\ub978 \ud3f4\ub9ac\", \ubaac\uba38\uc2a4\uc154\uc758 \"\uac15\ud55c \ub9d0, \uc57d\ud55c \ub2f9\ub098\uadc0\", \uc2a4\ucf54\ud2c0\ub79c\ub4dc \uc11c\ubd80\uc758 \"\ud6c8\ud06c, \ucee4\ub514, \ud6c8\ud06c\", \ubbf8\ub4e4\uc988\ube0c\ub7ec\uc758 \"\ubaac\ud0c0-\ud0a4\ud2f0\", \ub178\ud305\uc5c4\uc154\uc758 \"\ud5c8\uc2a4\ud0a4 \ud4e8\uc2a4\ud0a4 \ud551\uac70 \uc624\uc5b4 \uc378\", \ub7f0\ub358 \ub3d9\ubd80\uc758 \"\ud558\uc774 \uc9c0\ubbf8 \ub098\uc774\ucee4\", \uc653\ud3ec\ub4dc\uc758 \"\uc810\ud504 \ub354 \ub108\ucee4\", \ub7f0\ub358 \ubd81\ubd80\uc758 \"\uc6d4-\uc5d0-\uc5d0\uc774\ucee4\" \ub610\ub294 \"\uc6cc\ub2c8 \uc5d0\ucf54\", \uce74\ub514\ud504\uc758 \"\uc2a4\ud0c0\uac70 \ub8e8\ub2c8\", \ucf58\uc6d4 \uc138\uc778\ud2b8 \uc544\uc774\ube0c\uc2a4\uc758 \"\ud3fc\ud398\ub9ac\ub178\", \ub7ad\ucee4\uc154\uc758 \"\ud2b8\ub7ec\uc2a4\ud2b8\"\uac00 \uc5ec\uae30\uc5d0 \ud3ec\ud568\ub41c\ub2e4. \uc774 \uac8c\uc784\uc740 \ub54c\ub54c\ub85c \uc601\uad6d \uad70\ub300\uc758 \ud558\uc0ac\uad00\uc774\ub098 \uc7a5\uad50\ub4e4\uc5d0\uac8c\uc11c\ub3c4 \uc9c4\ud589\ub41c\ub2e4.\n\uba55\uc2dc\ucf54\uc640 \uc5d8\ud30c\uc18c\uc5d0\uc11c\ub294 \uc5d8\uce5c\uccb4 \ub370 \uc544\uacfc(\ubb3c\ubc8c\ub808)\ub77c\uace0 \uc54c\ub824\uc838 \uc788\ub2e4.\n\uce60\ub808\uc5d0\uc11c \uc774 \uac8c\uc784\uc740 \uce74\ubc1c\ub9ac\ud1a0 \ub370 \ube0c\ub860\uc2a4(\uc791\uc740 \ud669\ub3d9 \ub9d0)\ub77c\uace0 \ubd88\ub9b0\ub2e4\n\ube44\uc2b7\ud55c \uac8c\uc784\uc778 \ub9d0\ub69d\ubc15\uae30\uac00 \ud55c\uad6d\uc758 \uace0\ub4f1\ud559\uc0dd\uae4c\uc9c0\uc758 \uc544\uc774\ub4e4\uc5d0\uac8c\uc11c \ud589\ud574\uc9c4\ub2e4. \ub9d0\ub69d\ubc15\uae30\uc5d0\ub294 \ub450 \ud300\uc774 \uc788\ub2e4. \uccab \ubc88\uc9f8\ub294 \ud55c \uc0ac\ub78c\uc774 \ubcbd\uc5d0 \uae30\ub300\uace0 \uc77c\uc5b4\uc11c\ub294 \ub098\uba38\uc9c0 \ud300\ub4e4\uc740 \uc55e\uc5d0 \uc788\ub294 \uc0ac\ub78c\uc758 \ub2e4\ub9ac \uc0ac\uc774\uc5d0 \uba38\ub