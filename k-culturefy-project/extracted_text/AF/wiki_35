\uc804\uc791\uc758 \uadf8\uc758 \ubaa8\uc2b5\uc740 \uc804\ud600 \uc5c6\ub294 \ubbf8\ub0a8\uc774 \ub418\uc5b4 \uc788\ub2e4(\uc774\ubc88 \uc791\ud488\uc740 \uc804\uc791\ubcf4\ub2e4 \uacfc\uac70\uc758 \uc774\uc57c\uae30).\n 26\uc138. \uc544\ub124\uac00\uc640 \uc804\ud22c\uc5d0\uc11c \uc9c4 \ud6c4, \uc5ec\ub7ec \ub098\ub77c\ub97c \ubc29\ub791\ud558\uba70 \uc804\uc220\uc744 \ubab8\uc5d0 \uc775\ud78c\ub2e4. \uc804\uc791\uc5d0\uc11c\ub3c4 \ubcf4\uc2a4\ub85c\uc11c \ub4f1\uc7a5\ud588\uace0, \uadf8\ub54c \ud0c0\uace0 \uc788\ub358 \uc804\ud568\uc758 \uc5f4\ud654\ud310\uc5d0 \ud0c4\ub2e4.\n 56\uc138. \ub178\ubd80\ud0c0\ub2e4\uc758 \uc5ec\ub825\ud544\ub450. \ub2e4\ucf00\ub2e4 \uba78\ub9dd \ud6c4 \uac00\uc774\ub178\uad6c\ub2c8\ub97c \ud1b5\uce58\ud558\uc9c0\ub9cc \uc787\ud0a4\uc758 \ub2e4\ubc1c\uacfc \ub178\ubd80\uce74\uc4f0 \uc0dd\uc874\uc758 \uc18c\ubb38\uc744 \ub4e3\uace0, \uadf8 \uadfc\uc6d0\uc744 \ud30c\ud5e4\uce58\uae30 \uc704\ud574 \ucd9c\uc9c4.\n 25\uc138. \ub178\ubd80\ub098\uac00\uc758 \uc544\ub4e4. \uc0dd\ubaa8\uc758 \uc9c0\uc704\ub85c \ud615(\ud615\uc2dd\uc0c1\uc73c\ub85c\ub294 \ub3d9\uc0dd) \ub178\ubd80\ud0c0\uce74\uc640\ub294 \ubc18\ub300\ub85c \uc6b0\uc6d4\uac10\uc744 \ub290\ub07c\uace0 \uc788\ub2e4.\n 58\uc138. \uace0\uac00 \ub2cc\uc790\uc758 \ud53c\ub97c \uc787\ub294 \ubb34\uc0ac. \uad8c\ubaa8\uc220\uc218\ubcf4\ub2e4\ub294 \uc815\uc815\ub2f9\ub2f9\ud55c \uc2f8\uc6c0\uc744 \uc990\uae30\uba70, \uad00\ub3d9\uad00\ub839\uc744 \ub9e1\uace0 \uc788\ub2e4.\n 37\uc138. \ud788\ub370\uc694\uc2dc\uc758 \uc2ec\ubcf5\uc73c\ub85c, \uc9c0\ubaa8 \ubc0f \uad70\ub7b5\uc5d0 \ub2a5\ud55c \ucc38\ubaa8. \uc624\ub79c \uac10\uc625\uc0dd\ud65c\ub85c \ubc1c\uc774 \ubd80\uc790\uc720.\n 16\uc138. \uac04\ubca0\uc5d0\uc758 \uc801\ub0a8\uc73c\ub85c \uc774\uc81c \ub9c9 \ucd9c\uc804\ud55c \uc2e0\ucc38. \uc804\uc791\uc5d0\uc11c\ub3c4 \uc911\uac04\ubcf4\uc2a4\ub85c\uc11c \ub4f1\uc7a5.\n 57\uc138. \ud788\ub370\uc694\uc2dc\uc758 \ucc38\ubaa8 \uc911 1\uba85. \uac89\ubcf4\uae30\uc640\ub294 \uc815\ubc18\ub300\ub85c \uc9c0\uc6a9\uacb8\ube44\uc758 \uc2e4\ubb34\ud615 \ubb34\uc7a5.\n 21\uc138. \ud788\ub370\uc694\uc2dc\ub098 \uac04\ubca0\uc5d0\uc5d0 \uc2ec\ucde8\ud574, \ucc3d\ubcd1\uc73c\ub85c\uc11c \uadf8\ub4e4\uc744 \ub3d5\uae30\ub97c \ubc14\ub780\ub2e4.\n 22\uc138. \ud3c9\uade0 \uc774\uc0c1\uc758 \uad34\ub825\uc758 \uc18c\uc720\uc790. \uae30\uc694\ub9c8\uc0ac\uc640\ub294 \ud615\uc81c\uc640 \uac19\uc740 \uc0ac\uc774\uc774\uc790 \ub77c\uc774\ubc8c.\n 24\uc138. \uad34\uc0c1\ud55c \ubaa8\uc2b5\uacfc\ub294 \uc815\ubc18\ub300\ub85c, \ubcf4\ud1b5\uc73c\ub85c \ub9d0\ud560 \uc218 \uc788\ub294 \ud788\ub370\uc694\uc2dc\uc758 \uc2dc\uc885.\n 46\uc138.\u300c\uc6d0\uc22d\uc774\u300d\ub77c\ub294 \ubcc4\uba85\uc744 \uac00\uc9c4 \ucd9c\uc138\uac00\ub3c4\ub97c \ub2ec\ub824\ub098\uac04 \ub0a8\uc790. \uc0ac\uc2e4\uacfc \ube44\uc2b7\ud558\uac8c \ubb18\uc0ac\ub41c, \uc2e4\uc9c8\uc801\uc778 \ub178\ubd80\ub098\uac00\uc758 No.2.\n 47\uc138. \ub178\ubd80\ub098\uac00\uc640\ub294 \uc816 \ud615\uc81c(\uac19\uc740 \uc0ac\ub78c\uc758 \uc816\uc744 \uba39\uace0 \uc790\ub780 \ud615\uc81c)\ub85c, \uace0\ucc38 \uac00\ub85c. \ub9d0\uc218\ub294 \uc801\uc9c0\ub9cc \ubc1c\uc5b8\uad8c\uc740 \ud06c\ub2e4.\n 23\uc138. \uc4f0\ub124\uc624\ud0a4\uc758 \ucc28\ub0a8\uc73c\ub85c \uac00\ub3c5\uc744 \uc787\uae30 \uc804\uae4c\uc9c0\ub294 \uc5ec\ub7ec \ub098\ub77c\ub97c \ub3cc\uc544\ub2e4\ub154\ub2e4. \ub2e4\uce74\ud1a0\ub77c\uc640\ub294\u300c\ud574\uc801\u300d\u30fb\u300c\uc0b0\uc801\u300d\uc73c\ub85c \ubd80\ub974\ub294 \uc0ac\uc774.\n 41\uc138. \ud788\ub370\uc694\uc2dc\uc758 \uc560\uad50\uc5d0 \ubc18\ud574 \uc758\ud615\uc81c\uac00 \ub41c\ub2e4. \ud788\ub370\uc694\uc2dc\ubcf4\ub2e4\ub294 \uc5f0\ud558\uc774\uc9c0\ub9cc \uc624\ub2e4\uac00 \uace0\ucc38\uc758 \uac00\ub85c\uc774\uae30 \ub54c\ubb38\uc5d0 \ud0dc\ub3c4\ub294 \uac70\ub9cc.\n 18\uc138. \ub098\uac00\uc694\uc2dc\uc758 \ub3d9\uc0dd\uc73c\ub85c \ub178\ubd80\ub098\uac00\uc758 \ud544\ub450 \uc2dc\ub3d9\uc778 \ubbf8\ub0a8\uc790.\n 49\uc138. \uc77c\uc871\uc758 \uc874\u