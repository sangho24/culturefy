\ud569\ub2c8\ub2e4. \uc2e4\ub9ac\uc678\uad50\ub294 \uc774\ub7ec\ud55c \ud658\uacbd\uc5d0\uc11c \uc790\uad6d\uc758 \uacbd\uc81c\uc801, \uc548\ubcf4\uc801 \uc774\uc775\uc744 \ucd5c\ub300\ud55c \ud655\ubcf4\ud558\ub294 \ub370 \uae30\uc5ec\ud569\ub2c8\ub2e4.\n\uc720\uc5f0\ud55c \ub300\uc751: \uace0\uc815\ub41c \uc774\ub150\uc774\ub098 \uac00\uce58\uc5d0 \uc5bd\ub9e4\uc774\uc9c0 \uc54a\uae30 \ub54c\ubb38\uc5d0, \ubcc0\ud654\ud558\ub294 \uad6d\uc81c \uc815\uc138\uc5d0 \uc720\uc5f0\ud558\uac8c \ub300\uc751\ud558\uace0 \ub2e4\uc591\ud55c \ud30c\ud2b8\ub108\uc640 \ud611\ub825\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\uc704\uae30 \uad00\ub9ac: \uc790\uad6d\uc758 \uc548\ubcf4\uc640 \uacbd\uc81c\uc5d0 \uc704\ud611\uc774 \ub418\ub294 \uc694\uc18c\ub97c \uc904\uc774\uace0, \uc7a0\uc7ac\uc801\uc778 \uac08\ub4f1\uc744 \uc608\ubc29\ud558\uac70\ub098 \uc9c0\ud61c\ub86d\uac8c \uc870\uc815\ud558\ub294 \ub370 \ud6a8\uacfc\uc801\uc785\ub2c8\ub2e4.\n\uade0\ud615 \uc678\uad50: \uac15\ub300\uad6d \uc0ac\uc774\uc5d0\uc11c \uc5b4\ub290 \ud55c\ucabd\uc5d0 \uce58\uc6b0\uce58\uc9c0 \uc54a\uace0, \ud544\uc694\uc5d0 \ub530\ub77c \ub2e4\uc591\ud55c \uad6d\uac00\uc640 \ud611\ub825\ud558\uc5ec \uc678\uad50\uc801 \uc785\uc9c0\ub97c \uac15\ud654\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \n\uc548\uc2dc\uc131 \uc804\ud22c\uc640 \uc2e4\ub9ac\uc678\uad50\uc758 \uad00\uacc4\n\uc548\uc2dc\uc131 \uc804\ud22c\ub294 \uace0\uad6c\ub824\uc758 \ub6f0\uc5b4\ub09c \uad70\uc0ac\uc801 \uc5ed\ub7c9\uc744 \ubcf4\uc5ec\uc8fc\ub294 \uc0ac\ub840\uc774\uc9c0\ub9cc, \ub3d9\uc2dc\uc5d0 \ub2f9\uc2dc \uace0\uad6c\ub824 \uc678\uad50\uc758 \ud55c\uacc4\uc640 \uc2e4\ub9ac\uc678\uad50\uc758 \ud544\uc694\uc131\uc744 \uac04\uc811\uc801\uc73c\ub85c \ubcf4\uc5ec\uc8fc\uae30\ub3c4 \ud569\ub2c8\ub2e4.\n\uad70\uc0ac\uc801 \uc2b9\ub9ac\uc640 \uc678\uad50\uc801 \uace0\ub9bd: \uc548\uc2dc\uc131 \uc804\ud22c\uc5d0\uc11c \uace0\uad6c\ub824\ub294 \uad70\uc0ac\uc801\uc73c\ub85c \ud070 \uc2b9\ub9ac\ub97c \uac70\ub450\uc5c8\uc9c0\ub9cc, \ub2f9\uc2dc \uc2e0\ub77c\ub294 \ubc31\uc81c\uc640 \uace0\uad6c\ub824\uc758 \uc555\ubc15\uc744 \ub9c9\uae30 \uc704\ud574 \ub2f9\ub098\ub77c\uc5d0 \uc9c0\uc18d\uc801\uc73c\ub85c \ub3c4\uc6c0\uc744 \uc694\uccad\ud558\uba70 \uc678\uad50\uc801 \uc5f0\ub300\ub97c \uac15\ud654\ud558\uace0 \uc788\uc5c8\uc2b5\ub2c8\ub2e4. \uace0\uad6c\ub824\ub294 \uc2e0\ub77c\uc758 \uad70\uc0ac \uc6d0\uc870 \uc694\uccad\uc744 \uac70\ubd80\ud558\ub294 \ub4f1 \ud55c\ubc18\ub3c4 \ub0b4 \ub2e4\ub978 \uc138\ub825\uacfc\uc758 \uad00\uacc4\uc5d0\uc11c \uc2e4\ub9ac\uc801 \uc720\uc5f0\uc131\uc744 \ubcf4\uc774\uc9c0 \ubabb\ud588\uace0, \uc774\ub294 \uacb0\uad6d \ub098\ub2f9\uc5f0\ud569\uc758 \ubc30\uacbd\uc774 \ub418\uc5b4 \uace0\uad6c\ub824\ub97c \uc678\uad50\uc801\uc73c\ub85c \uace0\ub9bd\uc2dc\ud0a4\ub294 \uacb0\uacfc\ub97c \ucd08\ub798\ud588\uc2b5\ub2c8\ub2e4.\n\uac15\ub300\uad6d\uacfc\uc758 \uad00\uacc4 \uc124\uc815: \uace0\uad6c\ub824\ub294 \uc218\ub098\ub77c\uc758 \uce68\ub7b5\uc5d0 \ub9de\uc11c \uc2f8\uc6b0\uace0, \ub2f9\ub098\ub77c\uc758 \ub300\uaddc\ubaa8 \uce68\uacf5\uc744 \uc131\uacf5\uc801\uc73c\ub85c \ubc29\uc5b4\ud558\ub294 \ub4f1 \uac15\ub300\uad6d\uacfc\uc758 \ub300\uacb0\uc5d0\uc11c \uacb0\ucf54 \uad74\ubcf5\ud558\uc9c0 \uc54a\ub294 \uac15\uacbd\ud55c \uba74\ubaa8\ub97c \ubcf4\uc600\uc2b5\ub2c8\ub2e4. \uc774\ub294 \uc790\uad6d\uc758 \uc548\ubcf4\ub97c \uc9c0\ud0a4\uae30 \uc704\ud55c \uc911\uc694\ud55c \uc2e4\ub9ac\uc801 \uc120\ud0dd\uc774\uc5c8\uc9c0\ub9cc, \ub3d9\uc2dc\uc5d0 \uc678\uad50\uc801 \uc720\uc5f0\uc131 \ubd80\uc871\uc73c\ub85c \uc778\ud574 \uc8fc\ubcc0 \uc815\uc138\ub97c \uace0\uad6c\ub824\uc5d0 \uc720\ub9ac\ud558\uac8c \ud65c\uc6a9\ud558\ub294 \ub370\ub294 \ud55c\uacc4\uac00 \uc788\uc5c8\uc2b5\ub2c8\ub2e4.\n\uad6d\ub0b4 \uc815\uce58\uc758 \uc601\ud5a5: \uc5f0\uac1c\uc18c\ubb38\uc758 \uc815\ubcc0\uc73c\ub85c \uc655\uad8c\uc774 \uc57d\ud654\ub418\uace0 \uadc0\uc871 \uc138\ub825 \uac04\uc758 \uac08\ub4f1\uc774 \uc2ec\ud654\ub41c \uace0\uad6c\ub824 \ub0b4\ubd80 \uc0c1\ud669\ub3c4 \uc678\u