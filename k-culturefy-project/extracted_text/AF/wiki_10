\ud55c\ub2e4. \uacb0\uacfc\uc801\uc73c\ub85c, \uadf8\ub294 \uc790\ubc14\uc6cc\ud06c\uc758 \ud798\uc744 \uc57d\ud558\uac8c \ud558\ub294 \uac83\uc5d0 \uc77c\uc870 \ud588\ub2e4.\n \ud558\ub8e8\uce74\uc758 \uc5b4\uba38\ub2c8. \ub808\uc804\ub354\ub97c \uc88b\uc544\ud558\ub294 \ub538\uc744 \uc704\ud574 \ub808\uc804\ub354\uc758 \ud615\ud0dc\ub97c \ud55c \ucf00\uc774\ud06c\ub97c \ub9cc\ub4e4\uac70\ub098 \ub808\uc804\ub354\uc758 \uc544\ud50c\ub9ac\ucf00\ub97c \uaff0\ub9e4\uac70\ub098 \uc790\uae30 \uc804\uc5d0 \ub808\uc804\ub354\uc758 \uc774\uc57c\uae30\ub97c \ub4e4\ub824\uc8fc\ub294 \ub4f1 \uc0c1\ub0e5\ud55c \uc5ec\uc131\uc774\uc5c8\uc9c0\ub9cc, \uc6b0\uc5f0\ud788 \uc9c4\uc9dc \ub808\uc804\ub354(\ub780\uc2e0)\ub97c \ubcf4\uace0 \uacf5\ud3ec\uc2ec\uc744 \uc548\uc544, \uac70\uae30\uc5d0 \uc8fc\ubaa9\ud55c \uc790\ubc14\uc6cc\ud06c\ub97c \ubd80\ud65c \uc2dc\ud0a4\ub824\uace0 \ud558\ub294 \ub0a8\ud3b8\uc5d0 \uc758\ud574 \ub538\uc744 \uc9c0\ud0a8\ub2e4\uace0 \ud558\ub294 \uc77c\uc744 \uad6c\uc2e4\uc5d0 \uc790\ubc14\uc6cc\ud06c\uc758 \uc601\ud63c\uc758 \uc778\ud615\uc5d0 \uae30\uc0dd \ub2f9\ud574 \uadf8 \ubab8\uc744 \ub358\uc838 \uacf5\ud3ec\uc758 \ub9c8\uc74c\uacfc \ubab8\uc758 \uc591\ubd84\uc744 \uc7c8\ubc14\uc6cc\ud06c\uc758 \ubd80\ud65c\uc744 \uc704\ud55c \uc591\uc2dd\uc73c\ub85c \ub418\uace0 \uc788\uc5c8\ub2e4. \uc7c8\ubc14\uc6cc\ud06c\uc758 \uccb4\ub0b4(\uc2ec\uc7a5)\uc5d0 \ubc1b\uc544\ub4e4\uc5ec\uc9c0\uace0 \uc0dd\uba85\ub3c4 \uc0bc\ucf1c\uc838\uc11c \uc788\uc5c8\uc9c0\ub9cc, \uc0e4\ub860, \ub780\uc2e0, \uc2dc\ub860, \uc6a9\uc774\uc758 \ubd84\ud22c\uc5d0 \uc758\ud574 \uad6c\uc870\ub41c\ub2e4. (\ub2e8 \ub780\uc2e0\uc740 \uc7c8\ubc14\uc6cc\ud06c\uc640 \ud568\uaed8 \uc6b0\uc8fc \uc18d\uc73c\ub85c \ub0a0\uc544\uac04\ub2e4. \ud6c4\uc5d0 \ub780\uc2e0\uc740 \ubbf8\ub2c8\ub780\uc2e0\uc774 \ub418\uc5b4\uc788\ub2e4. &lt;\uc6d0\ud310 50\ud654 \ucc38\uc870&gt;)\n \ubd88\uc758 \uc11c\uc790\ub85c \ubd88\ub9ac\uace0 \uc788\ub358 DWC\uc758 \uc0ac\uc6d0. \uadf8\ub7ec\ub098 \ub514\ub178\uac0