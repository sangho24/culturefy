4. \uc5c7\uc911\ubaa8\ub9ac\ub294 \uc717\uc0ac\ub78c\uc774 \uc0ac\uc5f0\uc744 \uc544\ub8b0\ub294 \ub300\ubaa9\uc774\ub098 \ud310\uc18c\ub9ac\uc758 \ub9e8 \ub05d \ub300\ubaa9\uc5d0 \uc4f0\uc778\ub2e4. \uc720\uba85\ud55c \ub300\ubaa9\uc740 \u3008\ucd98\ud5a5\uac00\u3009\uc758 \"\ud68c\ub3d9\uc131\ucc38\ud310 \uc601\uac10\uaed8\uc11c\"\uc640, \u3008\uc218\uad81\uac00\u3009\uc758 \"\uc774\ub0b4 \uadfc\ubcf8\uc744 \ub4e4\uc5b4\ub77c\"\uc774\ub2e4."}
