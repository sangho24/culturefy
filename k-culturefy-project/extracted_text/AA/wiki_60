\ud55c \uc5f0\uc124\uc744 \uac70\ubd80\ud558\uace0 \ub300\uc2e0 \ucca0\ud559\uc801 \uac15\uc758\ub97c \ud588\ub2e4. \uc774 \uae30\uac04 \ub3d9\uc548 \uc288\ub8b0\ub529\uac70\ub294 \ud30c\ub3d9-\uc785\uc790 \uc774\uc911\uc131\uc5d0 \ub300\ud55c \uc8fc\ub958 \uc591\uc790\uc5ed\ud559\uc758 \uc815\uc758\ub85c\ubd80\ud130 \ub3cc\uc544\uc130\uace0, \ud30c\ub3d9 \uc544\uc774\ub514\uc5b4 \ub2e8\ub3c5(wave idea alone)\uc744 \ucd09\uc9c4\ud558\uc5ec \ub9ce\uc740 \ub17c\ub780\uc744 \uc77c\uc73c\ucf30\ub2e4.\n\uacb0\ud575\uacfc \uc8fd\uc74c.\n\uc288\ub8b0\ub529\uac70\ub294 \uacb0\ud575\uc744 \uc553\uc558\uace0 1920\ub144\ub300\uc5d0 \uc5ec\ub7ec \ucc28\ub840 \uc544\ub85c\uc790\uc758 \uc694\uc591\uc18c\uc5d0 \uba38\ubb3c\ub800\ub2e4. \uadf8\uac00 \ud30c\ub3d9 \ubc29\uc815\uc2dd\uc744 \uacf5\uc2dd\ud654\ud55c \uacf3\uc774 \ubc14\ub85c \uadf8\uacf3\uc774\uc5c8\ub2e4. 1961\ub144 1\uc6d4 4\uc77c 73\uc138\uc758 \ub098\uc774\ub85c \ube48\uc5d0\uc11c \uacb0\ud575\uc73c\ub85c \uc0ac\ub9dd\ud558\uc600\ub2e4. \uadf8\ub294 \uc548\ub2c8\ub97c \ubbf8\ub9dd\uc778\uc73c\ub85c \ub0a8\uaca8\ub450\uace0 \uc624\uc2a4\ud2b8\ub9ac\uc544 \uc54c\ud504\ubc14\ud750(Alpbach)\uc5d0 \uc788\ub294 \uac00\ud1a8\ub9ad \uacf5\ub3d9\ubb18\uc9c0\uc5d0 \ubb3b\ud614\ub2e4. \ube44\ub85d \uadf8\ub294 \uac00\ud1a8\ub9ad \uc2e0\uc790\ub294 \uc544\ub2c8\uc5c8\uc9c0\ub9cc, \ubb18\uc9c0\ub97c \ucc45\uc784\uc9c0\uace0 \uc788\ub294 \uc0ac\uc81c\ub294 \uc288\ub8b0\ub529\uac70\uac00 \uad50\ud669\uccad \uacfc\ud559\uc6d0\uc758 \ud68c\uc6d0\uc774\ub77c\ub294 \uac83\uc744 \uc54c\uac8c \ub41c \ud6c4 \ub9e4\uc7a5\uc744 \ud5c8\ub77d\ud588\ub2e4.\n\uac1c\uc778\uc801 \uc0b6.\n1920\ub144 4\uc6d4 6\uc77c, \uc288\ub8b0\ub529\uac70\ub294 \uc548\ub124\ub9c8\ub9ac (\uc544\ub2c8) \ubca0\ub974\ud154Annemarie (Anny) Bertel\uacfc \uacb0\ud63c\ud588\ub2e4.\n1938\ub144 \uc544\uc77c\ub79c\ub4dc\ub85c \uc774\ubbfc\uc744 \uac14\uc744 \ub54c, \uadf8\ub294 \uc790\uc2e0\uacfc \uc544\ub0b4, \uadf8\ub9ac\uace0 \ub610 \ub2e4\ub978 \uc5ec\uc131 \ud790\ub3