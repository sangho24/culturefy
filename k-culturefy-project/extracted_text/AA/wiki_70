548 \uc2dd\uad6c\ub4e4\uc774 \uac70\ubb38\uace0\ub098 \ud241\uc18c \ub4f1 \uc804\ud1b5 \uc545\uae30\ub97c \uc5f0\uc8fc\ud588\ub294\ub370 \ud64d\uc601\ud6c4\ub294 \uadf8 \uc911 \uc559\uae08\uc744 \uc5f0\uc8fc\ud588\ub2e4\uace0 \ud55c\ub2e4. \uc544\ubc84\uc9c0 \ud64d\uc900\uc740 \uad6d\uc545\uc5d0 \uc870\uc608\uac00 \ub9ce\uc558\ub358 \uad00\uacc4\ub85c \uc544\ub4e4\uc5d0\uac8c \uae08\ud30c(\u9326\u5761), \ub09c\ud30c(\u862d\u5761) \ub4f1\uc758 \ubcc4\ud638\ub97c \uc9c0\uc5b4 \uc8fc\uc5c8\ub2e4. \ud615 \uae08\ud30c \ud64d\uc11d\ud6c4\ub294 \uad6c\ud55c \ub9d0\uc758 \uc591\uc758\uc0ac\uc774\uae30\ub3c4 \ud588\ub2e4. \ud64d\uc601\ud6c4\uc758 \ub9c9\ub0b4\ub3d9\uc0dd \ubbfc\ud6c4\ub294 \ud574\ubc29 \ud6c4 30\uc5ec\ub144\uac04 \uacbd\ub0a8 \ud569\ucc9c\uad70 \uc0bc\uac00\uba74\uc5d0\uc11c '\ubc31\uc911\uc57d\uad6d'\uc744 \uacbd\uc601\ud558\uba70 \uc778\uc220\uc744 \ubca0\ud480\uc5b4 \uc57d\uc0ac\ub85c\uc11c \ud3c9\uc0dd \uc120\uc5c5\uc744 \uc313\uc544 \uc9c0\uc5ed\ubbfc\ub4e4\uc758 \ud070 \uc874\uacbd\uc744 \ubc1b\uc558\ub2e4. \uc57d\uc0ac \ud64d\ubbfc\ud6c4\ub294 \ud615\ud3b8\uc774 \uc5b4\ub824\uc6b4 \ud658\uc790\ub4e4\uc740 \uc57d\uac12\uc744 \ubc1b\uc9c0 \uc54a\uc558\uace0, \uacf5\uc9dc\ub85c \uc57d\uc744 \uc9c0\uc5b4\uc8fc\uba70 '\uc6ec\ub9cc\ud558\uba74 \uc57d \uba39\uc9c0 \ub9c8\uc154\uc694'\ub85c \ucc98\ubc29\ud558\uc5ec \uc720\uba85\ud558\uc600\ub2e4. \ub610\ud55c, \ud64d\ub09c\ud30c\ub294 1933\ub144 \ud615 \uae08\ud30c\uc640 \ud568\uaed8 \ub09c\ud30c\ud2b8\ub9ac\uc624\ub97c \uacb0\uc131\ud574 \ud65c\ub3d9\ud588\ub2e4. \ud2b8\ub9ac\uc624\uc911 \uc77c\ucc0d \uc694\uc808\ud55c \uc870\uce74 \ud64d\uc131\uc720(1908 ~ 1936)\ub294 \ud615 \ud64d\uc11d\ud6c4\uc758 \uc14b\uc9f8 \uc544\ub4e4\uc774\uae30\ub3c4 \ud588\ub2e4.\n\uc11c\uc591 \uc74c\uc545\uc5d0 \uc785\ubb38\ud558\ub2e4.\n1899\ub144 \uc628 \uac00\uc871\uc774 \uace0\ud5a5\uc744 \ub5a0\ub098 \uc11c\uc6b8\ub85c \uc774\uc0ac\ud55c \ud6c4 \uadf8\ub294 \uad50\ud68c\ub97c \ub2e4\ub2c8\uac8c \ub418\uba74\uc11c \uc11c\uc59