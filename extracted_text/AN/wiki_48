\ub7ec\ub098 \uc7a5\uae30 \uc5f0\uad6c\ub4e4\uc5d0\uc11c\ub294 \uc131\uc778 \ub300\uc0c1\uc73c\ub85c \uc774\ub904\uc9c0\uc9c0 \uc54a\uc558\uc73c\uba70, \ud608\uc555 \uc99d\uac00\uc5d0 \ub300\ud55c \uc6b0\ub824\ub294 \uc218\ub9bd\ub418\uc9c0 \uc54a\uc558\ub2e4. \uba54\ud2f8\ud398\ub2c8\ub370\uc774\ud2b8\ub294 \uc2dc\ub0c5\uc2a4 \uac04\uadf9 \ub0b4 \ub3c4\ud30c\ubbfc\uacfc \ub178\ub974\uc5d0\ud53c\ub124\ud504\ub9b0 \ub18d\ub3c4\ub97c \uc99d\uac00\uc2dc\ucf1c, \uc2e0\uacbd\uc804\ub2ec\uc744 \ucd09\uc9c4\ud55c\ub2e4. \ub3c4\ud30c\ubbfc\uacfc \ub178\ub974\uc5d0\ud53c\ub124\ud504\ub9b0 \uc7ac\ud761\uc218 \uc218\uc1a1\uccb4\ub97c \ucc28\ub2e8\ud558\uc5ec, \uc774\ub7ec\ud55c \uc2e0\uacbd\uc804\ub2ec\ubb3c\uc9c8\ub4e4\uc774 \uc2dc\ub0c5\uc2a4\uc5d0\uc11c \uc0ac\ub77c\uc9c0\ub294 \uac83\uc744 \ub2a6\ucd98\ub2e4. \ub371\uc2a4\ud2b8\ub85c\uc554\ud398\ud0c0\ubbfc\uc740 \ub2e8\uae30\uc791\uc6a9 \uc554\ud398\ud0c0\ubbfc\uc774\ub2e4.\n\uc601\uad6d\uc5d0\uc11c\ub294 \uc784\uc0c1 \uac00\uc774\ub4dc\ub77c\uc778\uc73c\ub85c\uc11c \uc2e0\uacbd\uc790\uadf9\uc81c\ub97c \uc77c\uc120 \uce58\ub8cc\uc81c\ub85c \uc0ac\uc6a9\ud560 \uac83\uc744 \uad8c\uace0\ud558\uace0 \uc788\ub2e4. \ub3d9\uc2dc\uc5d0 \uc601\uad6d\uc5d0\uc11c\ub294 \uc57d\ubb3c \ub0a8\uc6a9 \uc7a5\uc560\ub098 \uae30\ud0c0 \ubd80\uc791\uc6a9\uc73c\ub85c \uc778\ud558\uc5ec \uc790\uadf9\uc81c \uce58\ub8cc\uac00 \ubd88\uac00\ud55c \uc774\ub4e4\uc5d0\uac8c \uc544\ud1a0\ubaa9\uc138\ud2f4\uc774 \uc77c\uc120 \uce58\ub8cc\uc81c\ub85c \uad8c\uace0\ub418\uace0 \uc788\ub2e4. \uce90\ub098\ub2e4\uc5d0\uc11c\ub294 \uba54\ud2f8\ud398\ub2c8\ub370\uc774\ud2b8\ub098 \ub9ac\uc2a4\ub371\uc2a4\uc554\ud398\ud0c0\ubbfc\uc774 \uc77c\uc120 \uce58\ub8cc\uc81c\ub85c \uad8c\uace0\ub418\uace0 \uc788\ub2e4. \uce90\ub098\ub2e4\uc5d0\uc11c 2\ucc28 \uce58\ub8cc\uc81c\ub294 \uc544\ud1a0\ubaa9\uc138\ud2f4\uc774\ub098 \uba54\ud2f8\ud398\ub2c8\ub370\uc774\ud2b8\uac00 \uc0ac\uc6a9\ub418\uace0 \uc788\ub2e4.\n\ube44\uc790\uadf9\uc81c \uc57d\ubb3c.\n\ube44\uc790\uadf9\u