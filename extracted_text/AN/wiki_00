\uc678\uad6d\uc73c\ub85c \ub098\uac00\uc11c \uc784\uc0c1\ud65c\ub3d9\uc744 \ud574\uc57c \ud558\ub294 \uc218\uace0\ub97c \ub35c\uac8c \ub418\uc5c8\ub2e4.\n1995\ub144 5\uc6d4 15\uc77c \uac00\uc871 \uc815\uc2e0 \uce58\ub8cc\uc5d0 \uacf5\ud5cc\ud55c \uacf5\ub85c\ub85c \ub300\ud1b5\ub839 \ud45c\ucc3d\uc744 \uc218\uc5ec\ubc1b\uc558\ub2e4.\n\uae30\ud0c0 \ud65c\ub3d9.\n\uadf8\ub294 \ub300\ud55c\uc758\uc0ac\ud559\ud68c\uc640 \uac00\uc871\uce58\ub8cc\ud559\ud68c \ub4f1\uc5d0\uc11c\ub3c4 \ud65c\ub3d9\ud558\uc600\uace0, 1995 10\uc6d4 14\uc77c \uac74\uac15\uac00\uc871 \uc2e4\ucc9c\ubcf8\ubd80 \ucc3d\ub9bd\uc5d0\ub3c4 \ucc38\uc5ec\ud558\uc600\ub2e4.\n\ud55c\uac15\ub85c\ud130\ub9ac\ud074\ub7fd\uc758 \ud68c\uc6d0\uc73c\ub85c\ub3c4 \ud65c\ub3d9\ud558\uc600\uc73c\uba70, \ud55c\uac15\ub85c\ud130\ub9ac\ud074\ub7fd\uc758 \ud68c\uc7a5\uc744 \uc5ed\uc784\ud558\uae30\ub3c4 \ud588\ub2e4. 1981\ub144 7\uc6d4 10\uc77c\uc5d0\ub294 \ud0c1\uad6c\uc120\uc218\ub85c \uc5ec\uc790\ud0c1\uad6c \uad6d\uac00\ub300\ud45c\ub85c \uc120\ubc1c\ub41c \uc774\ub9ac\uc5ec\uc790\uace0\ub4f1\ud559\uad50 \uc591\uc601\uc790 \uc120\uc218\uc5d0\uac8c \uc7a5\ud559\uae08 10\ub9cc\uc6d0\uc744 \uc218\uc5ec\ud558\uae30\ub3c4 \ud588\ub2e4. \uadf8\ub294 \uc778\uc0b0\uc7a5\ud559\ud68c(\u4ec1\u5c71\u596c\u5b78\u6703)\ub97c \uc124\ub9bd, 1982\ub144 2\uc6d4 18\uc77c \ud0c1\uad6c\uc120\uc218 \uc591\uc601\uc790 \ub4f1\uc5d0\uac8c \uc7a5\ud559\uae08 60\ub9cc\uc6d0\uc744 \uc804\ub2ec, \uc774\ub4e4 \ud559\uc0dd\uc774 \uc878\uc5c5\ud560 \ub54c\uae4c\uc9c0 \ud559\ube44\ub97c \uc9c0\ubd88\ud558\uc600\ub2e4. \uadf8\ub294 \uccb4\uc721 \uc120\uc218\uc640 \uace0\ud559\uc0dd\ub4e4\uc758 \uc7a5\ud559\uae08 \uc9c0\uae09 \ubc0f \ud559\ube44\ub97c \ub300\uc2e0 \uc9c0\uae09\ud558\uae30\ub3c4 \ud588\ub2e4.\n\uc0dd\uc560 \ud6c4\ubc18.\n1985\ub144 \ub1cc\uc885\uc591\uc758 \uc77c\uc885\uc778 \uc218\ub9c9\uc885\uc5d0 \uac78\ub838\uc73c\ub098 \uc218\uc220\ub85c \uc644\ucf8c\ud588\uace0, 91\ub144 \ub1cc\uc878\uc911\uc73c\ub85c \ubc18\uc2e0\ubd88\uc218\ua