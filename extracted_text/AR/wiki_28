 \ud56d\uc9c4\uade0\uc81c\uac00 \uc57d\ud6a8\uac00 \uc788\ub294\uc9c0\ub294 \ubd88\ubd84\uba85\ud558\ub2e4. \uc9c0\ub8e8\uc131 \ud53c\ubd80\uc5fc\uc5d0 \ud6a8\uacfc\uac00 \uc788\ub2e4\uace0 \uc5f0\uad6c\ub97c \ud1b5\ud574 \ubc1d\ud600\uc9c4 \uc57d\uc740 \ucf00\ud1a0\ucf54\ub098\uc878, \ud50c\ub8e8\ucf54\ub098\uc878, \ubbf8\ucf54\ub098\uc878, \ubc14\uc774\ud3ec\ub098\uc878(bifonazole), \uc138\ub974\ud0c0\ucf54\ub098\uc878(sertaconazole), \ud074\ub85c\ud2b8\ub9ac\ub9c8\uc878, \ud50c\ub8e8\ud2b8\ub9ac\ub9c8\uc878(flutrimazole), \uc2dc\ud074\ub85c\ud53c\ub85d\uc2a4, \ud14c\ub974\ube44\ub098\ud540, \ubd80\ud14c\ub098\ud540(butenafine), \uc140\ub808\ub284\ud669, \ub9ac\ud2ac\uae00\ub8e8\ucf58\uc0b0\uc5fc\uc774\ub098 \ub9ac\ud2ac\ud638\ubc15\uc0b0\uc5fc\uacfc \uac19\uc740 \ub9ac\ud2ac\uc5fc \uc885\ub958\uac00 \uc788\ub2e4. \uad6d\ubd80\uc131 \ud074\ub9bc\ubc14\uc878(climbazole)\uc740 \uce58\ub8cc\uc5d0 \ud6a8\uacfc\uac00 \uc801\ub2e4. \uad6c\uac15 \ud56d\uc9c4\uade0\uc81c \uccb4\uacc4\uce58\ub8cc\ub85c\ub294 \uc774\ud2b8\ub77c\ucf54\ub098\uc878, \ud50c\ub8e8\ucf54\ub098\uc878, \ucf00\ud1a0\ucf54\ub098\uc878\uc774 \uc788\uc73c\uba70, \ud14c\ub974\ube44\ub098\ud540\uc774 \ud6a8\uacfc\uc801\uc774\ub2e4.\n\ud56d\uc5fc\uc99d\uc81c.\n\uad6d\ubd80\uc131 \ucf54\ub974\ud2f0\ucf54\uc2a4\ud14c\ub85c\uc774\ub4dc\ub294 \ub2e8\uae30 \uce58\ub8cc\uc5d0 \ud6a8\uacfc\uac00 \uc788\uc73c\uba70 \uc544\uc878(azole)\uacfc \uac19\uc740 \ud56d\uc9c4\uade0\uc81c \uce58\ub8cc\uc640 \ud6a8\uacfc\uac00 \uac19\uac70\ub098 \uadf8 \uc774\uc0c1\uc778 \uac83\uc73c\ub85c \ubc1d\ud600\uc84c\ub2e4. \ub9ac\ud2ac\uc5fc \uce58\ub8cc \uc678\uc5d0\ub3c4, \ud0c0\ud06c\ub85c\ub9ac\ubb34\uc2a4(tacrolimus)\ub098 \ud53c\uba54\ud06c\ub85c\ub9ac\ubb34\uc2a4(pimecrolimus) \uac19\uc740 \uce7c\uc2dc\ub274\ub9b0 \uc5b5\uc81c\uc81c(calcineurin inhibitor)\uc758 \ud6a8\uacfc\ub3c4 \ubc1d\ud600\uc84c\ub2e4.\n\ud504\ub808\ub4dc\ub2c8\uc190(prednisone)\uacfc \uac19\uc740 \uad6c\uac15\uba74\uc5ed\uc5b5\uc81c\uce58\ub8cc\ub294 \ubd