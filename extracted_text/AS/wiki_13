\uc5f0\uc8fc\uc5d0 \ub2a5\ud558\ub2e4. \uc2e4\uc81c\ub85c, [[\ub77c\ub9ac\uac00 1982-83|1982-83 \uc2dc\uc98c]] \ub9ac\uadf8 \uc6b0\uc2b9\uc744 \ud655\uc815\uc9d3\uace0 \uc74c\ubc18\uc744 \ucd9c\uc2dc\ud588\ub294\ub370, \ubc14\uc2a4\ud06c \uc9c0\uc5ed\uc5d0\uc11c \uae30\ub300 \uc774\uc0c1\uc758 \uc218\uc775\uc744 \ub0c8\ub2e4.\n\uc544\ub974\uace0\ud14c\uc758 \ub610\ub2e4\ub978 \ucde8\ubbf8\ub85c\ub294 [[\uace8\ud504]]\uac00 \uc788\ub294\ub370, \ub2f9\uc2dc \ucd95\uad6c \uc120\uc218\ub4e4 \uc0ac\uc774 \uace8\ud504\ub97c \ud558\ub294 \uc120\uc218\ub294 \uadf8\ub2e4\uc9c0 \ub9ce\uc9c0 \uc54a\uc558\ub2e4. \uc720\ub144 \uc2dc\uc808, \uadf8\ub294 \uace0\ud5a5\uc5d0\uc11c \uace8\ud504 \ucf54\uc2a4 [[\uce90\ub514]]\ub97c \ub9e1\uae30\ub3c4 \ud588\ub2e4.\n\uc678\ubd80 \ub9c1\ud06c.\n[[\ubd84\ub958:1956\ub144 \ucd9c\uc0dd]]\n[[\ubd84\ub958:\uc0b4\uc544\uc788\ub294 \uc0ac\ub78c]]\n[[\ubd84\ub958:\uc0ac\ub77c\uc6b0\uce20 \ucd9c\uc2e0]]\n[[\ubd84\ub958:\uc2a4\ud398\uc778\uc758 \ub0a8\uc790 \ucd95\uad6c \uc120\uc218]]\n[[\ubd84\ub958:\ubc14\uc2a4\ud06c\uc8fc \ucd9c\uc2e0 \ucd95\uad6c \uc120\uc218]]\n[[\ubd84\ub958:\ub0a8\uc790 \ucd95\uad6c \uc719\uc5b4]]\n[[\ubd84\ub958:\ub77c\ub9ac\uac00\uc758 \ucd95\uad6c \uc120\uc218]]\n[[\ubd84\ub958:\uc138\uad70\ub2e4 \ub514\ube44\uc2dc\uc628 B\uc758 \ucd95\uad6c \uc120\uc218]]\n[[\ubd84\ub958:\ud14c\ub974\uc138\ub77c \ub514\ube44\uc2dc\uc628\uc758 \ucd95\uad6c \uc120\uc218]]\n[[\ubd84\ub958:\ube4c\ubc14\uc624 \uc544\ud2c0\ub808\ud2f1\uc758 \ucd95\uad6c \uc120\uc218]]\n[[\ubd84\ub958:\uc544\ud2c0\ub808\ud2f1 \ube4c\ubc14\uc624\uc758 \ucd95\uad6c \uc120\uc218]]\n[[\ubd84\ub958:\uc2a4\ud398\uc778 \uc544\ub9c8\ucd94\uc5b4 \ucd95\uad6c \uad6d\uac00\ub300\ud45c\ud300 \uc120\uc218]]\n[[\ubd84\ub958:\uc2a4\ud398\uc778 B \ucd95\uad6c \uad6d\uac00\ub300\ud45c\ud300 \uc120\uc218]]\n[[\ubd84\ub958:\uc2a4\ud398\uc778 \ub0a8\uc790 \ucd95\uad6c \uad6d\uac00\ub300\ud45c\ud300 \uc120\uc218]]\n[[\ubd84\ub958:\ubc14\uc2a4\ud06c \ub0a8\uc790 \ucd95\uad6c \uad6d