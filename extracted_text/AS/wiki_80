\uba38\ub2c8\ub294 \ubcf4\uc2a4\ud134 \ucd9c\uc2e0\uc778 \ub9e4\ub9ac \uc5d8\ub7f0 \ub9e5\ucf54\ub9e5(\uacb0\ud63c \uc804 \uc131\uc528 \uc624\ube0c\ub77c\uc774\uc5b8, 1861-1913)\uc774\ub2e4. \uc874 \ub9e5\ucf54\ub9e5\uc740 \uc5b4\ub9b0 \uc2dc\uc808 12\uba85\uc758 \uc790\uc2dd \uc911 \ud558\ub098\uc600\ub294\ub370 \ud615\uc81c\uc911 \uba87 \uba85\uc740 \uc5b4\ub9b0 \uc2dc\uc808 \ud639\uc740 \uccad\uc18c\ub144\uae30\uc5d0 \uc77c\ucc0d \uc8fd\uc5c8\ub2e4\uace0 \ub9d0\ud588\ub2e4. \uc0ac\uc2e4 \ub9e4\ub9ac \uc5d8\ub7f0 \ub9e5\ucf54\ub9e5\uc740 8\uba85\uc758 \uc544\uc774\ub97c \uc784\uc2e0\ud588\uace0 \uc774 \uc911 6\uba85\uc740 \uc778\uad6c \uc870\uc0ac \uae30\ub85d\uc774\ub098 \uae30\ud0c0 \ud589\uc815\uae30\ub85d\uc5d0 \ub0a8\uc744 \ub9cc\ud07c \ucda9\ubd84\ud788 \uc624\ub798 \uc0b4\uc558\ub2e4. \uc874 \ub9e5\ucf54\ub9e5\uc758 \ud615, \ub204\ub098\uc778 \ud328\ud2b8\ub9ad(1911\ub144 \uc0ac\ub9dd), \uce90\uc11c\ub9b0(1906\ub144 \uc0ac\ub9dd), \uc81c\uc784\uc2a4(1906\ub144 \uc0ac\ub9dd)\ub294 \uac01\uac01 24\uc138, 19\uc138, 17\uc138\uc758 \ub098\uc774\ub85c \uc0ac\ub9dd\ud588\ub2e4. \ud615\uc81c\uc778 \uc5d0\ub4dc\uc6cc\ub4dc('\ub179\ucf54')\ub294 1963\ub144 67\uc138\uc758 \ub098\uc774\ub85c \ubcf4\uc2a4\ud134\uc5d0\uc11c \uc0ac\ub9dd\ud588\ub2e4. \ub9e5\ucf54\ub9e5\uc758 \ud615 \ub3c4\ub110\ub4dc\ub294 1966\ub144 65\uc138\uc758 \ub098\uc774\ub85c \ud14d\uc0ac\uc2a4\uc8fc\uc5d0\uc11c \uc0ac\ub9dd\ud588\ub2e4. \uc874 \ub9e5\ucf54\ub9e5\uc5d0\uac8c\ub294 \uc544\ubc84\uc9c0\uc758 \uccab \uc544\ub0b4\uc5d0\uc11c \ub0b3\uc740 \ud574\ub9ac\ub77c\ub294 \uc774\ubcf5\ud615\uc81c\uac00 \uc788\uc5c8\ub294\ub370 \ud574\ub9ac\ub294 1902\ub144 18\uc138\uc758 \ub098\uc774\ub85c \ud504\ub9b0\uc2a4\uc5d0\ub4dc\uc6cc\ub4dc\uc12c\uc5d0\uc11c \uc0ac\ub9dd\ud588\ub2e4.\n\ub9e5\ucf54\ub9e5\uc740 \uc790\uc2e0\uc758 \uc544\ubc84\uc9c0\uac00 13\uc138\uc600\ub358 \uc2dc\uc808 \uc8fd\uc5c8\ub2e4\uace0 \ub9d0\ud588\ub294\ub370, \ub2e4\ub978 \uc790\ub8cc\ub4e4\uc5d0 \ub530\ub974\uba74 \uc544\ubc84