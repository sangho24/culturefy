\uc740 2019\ub144 \uc0ac\uc774\ubc84\ubc94\uc8c4 \uc791\uc804\uc744 \ud1b5\ud574 20\uc5b5 \ub2ec\ub7ec\ub97c \ubaa8\uae08\ud588\ub2e4\ub294 \ubcf4\uace0\uc11c\ub97c \ubc1c\ud45c\ud588\ub2e4.\n\uad6d\uc81c\uc801\uc778 \ubc18\uc751.\n\uc720\uc5d4 \uc548\uc804\ubcf4\uc7a5\uc774\uc0ac\ud68c\ub294 \uc720\uc5d4 \uc548\uc804 \ubcf4\uc7a5 \uc774\uc0ac\ud68c \uacb0\uc758 \uc81c825\ud638(1993\ub144 5\uc6d4), \uc720\uc5d4 \uc548\uc804 \ubcf4\uc7a5 \uc774\uc0ac\ud68c \uacb0\uc758 \uc81c1695\ud638(2006\ub144 7\uc6d4), \uc720\uc5d4 \uc548\uc804 \ubcf4\uc7a5 \uc774\uc0ac\ud68c \uacb0\uc758 \uc81c1874\ud638(2009\ub144 6\uc6d4), \uc720\uc5d4 \uc548\uc804 \ubcf4\uc7a5 \uc774\uc0ac\ud68c \uacb0\uc758 \uc81c2094\ud638(2013\ub144 3\uc6d4), \ucd5c\uadfc\uc5d0\ub294 \uc720\uc5d4 \uc548\uc804 \ubcf4\uc7a5 \uc774\uc0ac\ud68c \uacb0\uc758 \uc81c2371\ud638(2017\ub144 8\uc6d4) \ub4f1 \uc5ec\ub7ec \ucc28\ub840 \ub97c \ud1b5\uacfc\uc2dc\ucf30\ub2e4. \uad6d\uc81c\uc0ac\ud68c\uac00 \uc548\ubcf4\ub9ac 1874\ud638\ub97c \uc804\uba74 \uc2dc\ud589\ud560 \uacbd\uc6b0 \uc870\uc120\uc740 15\uc5b5~37\uc5b5 \ub2ec\ub7ec\uc758 \uc190\uc2e4\uc744 \uc785\uc744 \uac83\uc73c\ub85c \ucd94\uc0b0\ub41c\ub2e4.\n\ubbf8\uad6d\uc740 1950\ub144\ubd80\ud130 6.25 \uc804\uc7c1\uc744 \uc77c\uc73c\ud0a8 \uc870\uc120\uc5d0 \ub300\ud574 \uae08\uc218\uc870\uce58\ub97c \uc720\uc9c0\ud558\uace0 \uc788\uc73c\uba70, \uae08\uc218\uc870\uce58\uc640 \uad00\ub828\ub41c \uc81c\uc7ac\uc870\uce58\ub294 \uc870\uc120\uc758 \uac01\uc885 \ubd88\ubc95\ud589\uc704\uc640 \uc9c0\uc18d\uc801\uc778 \uce68\ub7b5\ud589\uc704\uc5d0 \ub300\uc751\ud558\uae30 \uc704\ud55c \uac83\uc73c\ub85c \ud655\ub300\ub418\uace0 \uc788\ub2e4.\n2005\ub144, \ubbf8\uad6d \uc560\uad6d\uc790\ubc95 \uc81c311\uc870\uc5d0 \ub530\ub77c, \uc870\uc120\uc758 \ud604\uae08 \uc911 2,500\ub9cc \ub2ec\ub7ec\uac00 \ub9c8\uce74\uc624\uc5d0 \uc704\uce58\ud55c \ubc29\ucf54\ub378\ud0c0\uc544\uc2dc\uc544\uc5d0 \ub3d9\uacb0\ub418\uc5c8\uace0, \uc774\ub294 \uc870\uc120\uc774 \ubd88\ubc95 \ud6