ar temp = list[i];\n list[i] = list[j];\n list[j] = temp;\n var sorted = false;\n while (!sorted) {\n sorted = true;\n for (var i = 1; i &lt; list.length - 1; i += 2) {\n if (list[i] &gt; list[i + 1]) {\n swap(list, i, i + 1);\n sorted = false;\n for (var i = 0; i &lt; list.length - 1; i += 2) {\n if (list[i] &gt; list[i + 1]) {\n swap(list, i, i + 1);\n sorted = false;"}
