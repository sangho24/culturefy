asks\ngulp.task ( 'taskName', function () {\n//do something\nfunction fn1 () {\n// do something\nfunction fn2 () {\n// Do something else\n// Task with array of function names\ngulp.task ( 'taskName', ['fn1','fn2']);\n\uae30\ubcf8 \ud0dc\uc2a4\ud06c.\n// Gulp default task\ngulp.task ( 'default', [ \"]);"}
