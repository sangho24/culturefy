\ud574\ub098 \ud3ec\ub3c4\ub2f9\uc2e0\ud569\uc131\uc744 \ub9c9\uc744 \uc218 \uc788\uc73c\uba70, \ud3ec\ub3c4\ub2f9\uc758 \uc9c0\ubc29\uacfc \uadfc\uc721 \uc138\ud3ec\ub85c\uc758 \uc6b4\ubc18\uc744 \ud65c\uc131\ud654\ud560 \uc218 \uc788\uace0, \ud3ec\ub3c4\ub2f9\uc758 \uae00\ub9ac\ucf54\uac90 \ud615\ud0dc\ub85c\uc758 \uc800\uc7a5\uc744 \ud65c\uc131\ud654\ud560 \uc218 \uc788\ub2e4.\n\uc778\uc290\ub9b0\uc740 \uc8fc\ub85c \uc2dd\uc0ac \ud6c4\uc758 \ud608\ub2f9\uce58 \uc0c1\uc2b9\uc5d0 \ubc18\uc751\ud558\uc5ec \ubca0\ud0c0 \uc138\ud3ec\uc5d0 \uc758\ud574 \ud608\uc561 \uc18d\uc73c\ub85c \ubc29\ucd9c\ub418\uace0, \uc774\uc790 \ub0b4\uc758 \ub791\uac8c\ub974\ud55c\uc2a4\uc12c\uc5d0\uc11c \ucc3e\uc544\ubcfc \uc218 \uc788\ub2e4. \uc778\uc290\ub9b0\uc740 \uc57d 2/3\uc758 \uccb4\uc138\ud3ec\uc5d0\uc11c \uc5f0\ub8cc\ub85c\uc11c\uc758 \uc0ac\uc6a9, \ub2e4\ub978 \ubd84\uc790\ub85c\uc758 \uc804\ud658, \ub610\ub294 \uc800\uc7a5\uc744 \uc704\ud574 \ud608\uc561\uc73c\ub85c\ubd80\ud130 \ud3ec\ub3c4\ub2f9\uc744 \ud761\uc218\ud558\ub294 \ub370\uc5d0 \uc4f0\uc778\ub2e4. \uc800\ud608\ub2f9\uc740 \ubca0\ud0c0 \uc138\ud3ec\ub85c\ubd80\ud130\uc758 \uc778\uc290\ub9b0 \ubc29\ucd9c\uc744 \uac10\uc18c\uc2dc\ud0a4\uace0, \uae00\ub9ac\ucf54\uac90\uc774 \ud3ec\ub3c4\ub2f9\uc73c\ub85c \ubd84\ud574\ub418\uac8c \ud55c\ub2e4. \uc774 \uacfc\uc815\uc740 \ud638\ub974\ubaac\uc778 \uae00\ub8e8\uce74\uace4\uc5d0 \uc758\ud574 \uc8fc\ub85c \uc870\uc808\ub418\ub294\ub370, \uae00\ub8e8\uce74\uace4\uc740 \uc778\uc290\ub9b0\uacfc \ubc18\ub300\uc758 \uc791\uc6a9\uc744 \ud55c\ub2e4.\n\uc0ac\uc6a9 \uac00\ub2a5\ud55c \uc778\uc290\ub9b0\uc758 \uc591\uc774 \ucda9\ubd84\ud558\uc9c0 \uc54a\uac70\ub098 \uc138\ud3ec\ub4e4\uc774 \uc778\uc290\ub9b0\uc5d0 \uc798 \ubc18\uc751\ud558\uc9c0 \uc54a\uac70\ub098, \ub610\ub294 \uc778\uc290\ub9b0 \uc790\uccb4\uac00 \uacb0\ud568\uc774 \uc788\ub2e4\uba74, \ud3ec\ub3c4\ub2f9\uc740 \ud544\uc694\ub85c \ud558\ub294 \uccb4\uc138\ud3ec\uc5d0 \uc81c\ub300\ub85c \ud761\uc218\ub418\uc9c0 \uc54a\ua