94 1958\ub144 \ubc1c\uacac \ub418\uc5c8\ub294\ub370 \uc774\ubbf8 \ud30c\uad34\ub41c 2\u20273\ud638\ubb34\ub364\uc744 \uc218\uc2b5\ud558\uace0 1\ud638\ubb34\ub364\uc744 \ubc1c\uad74\ud558\uc600\ub2e4.\n\uc694\ub155\uc131(\u907c\u5bdc\u7701) \ubd81\ud45c\uc2dc(\u5317\u7968\u5e02)\uc758 \ub77c\ub9c8\ub3d9\uace0\ubd84\uad70\uc740 \ub300\ubd80\ubd84 \ubaa8\uc6a9\uc120\ube44 \ubb38\ud654\uc758 \ubc94\uc8fc\uc5d0 \uc18d\ud558\uba70 \ubd80\uc5ec\uc758 \ubb34\ub364\uc591\uc2dd\uacfc \uc720\uc0ac\uc131\uc774 \ud655\uc778\ub418\uc5b4 \ubaa8\uc6a9\uc120\ube44\uc758 \uce68\ub7b5\uc73c\ub85c \ubd99\uc7a1\ud600\uc628 \ubd80\uc5ec\uc778\uc774 \uc815\ucc29\ud55c \uc9c0\uc5ed\uc73c\ub85c\ub9cc \uc774\ud574\ub418\uc5b4 \uc654\uc73c\ub098, \uccad\ub3d9\uae30\uc2dc\ub300 \ubb34\ub364\uc774 12\uae30\uac00 \uc788\uc73c\uba70 \ube44\ud30c\ud615\ub3d9\uac80\uc758 \ucd9c\ud1a0\ubb18\uac00 \ub2e4\uc218 \ubc1c\uacac\ub418\uc5c8\ub2e4.\n\uc694\ub3d9\uc9c0\uc5ed\uc778 \uc694\ub155\uc131(\u907c\u5bdc\u7701) \uc2ec\uc591\uc2dc(\u700b\u967d\u5e02)\uc758 \uc815\uac00\uc790\uc640 \uc720\uc801 \uae30\uc6d0\uc804 6~5\uc138\uae30\uc5d0 \uc2ec\uc591\uc9c0\uc5ed\uc774 \uc0c8\ub85c\uc6b4 \uc911\uc2ec\uc9c0\ub85c \ubd80\uac01\ub418\uc5c8\uc74c\uc744 \ubcf4\uc5ec\uc8fc\ub294 \uc911\uc694\ud55c \uc720\uc801\uc774\ub2e4. \ud2b9\ud788 6512\ud638(\uccad\ub3d9\ub2e8\uac80\ub300\ubb18) \ub300\ud615 \ubaa9\uad00\ubb18\uc5d0\uc11c\ub294 \ube44\ud30c\ud615\ub3d9\uac80, \ub2e4\ub274\uacbd \ub4f1 400\uc5ec \uc810\uc5d0 \ub2ec\ud558\ub294 \uccad\ub3d9\uae30\uac00 \ub300\ub7c9\uc73c\ub85c \ubd80\uc7a5\ub418\uc5b4\uc788\ub294\ub370, \uc774\ub294 \uace0\uc870\uc120 \uad00\ub828 \uc720\uc801 \uac00\uc6b4\ub370 \uac00\uc7a5 \ub9ce\uc740 \uc218\ub7c9\uc5d0 \ud574\ub2f9\ud55c\ub2e4. \ud2b9\ud788 \uc2ec\uc591 \uc2e0\ubbfc\uc2dc(\u65b0\u2ea0\u5e02) \ubd81\uc678\uc720\uc801(\u5317\u5d34\u907a\u8e5f)\uc774 \uc8fc\ubaa9\ud560\ub9cc \ud55c\ub370, \ucd9c\ud1a0\ub41c \ube44\ud30c\ud615\ub3d9\uac80\uc740 \ub3d9\uc77c \ubb38\ud654\uce35