\uc704 4\uac1c\ud300\uc774 \"\uce84\ud398\uc624\ub098\ud22c \ub4dc \ud3ec\ub974\ud22c\uac08\"\ub85c \uace7\ubc14\ub85c \uac15\ub4f1\ub418\uba70 17\uc704\ud300\uacfc 18\uc704\ud300\uc740 \"\uce84\ud398\uc624\ub098\ud22c \ub4dc \ud3ec\ub974\ud22c\uac08\" \ubd81\ubd80\uc640 \ub0a8\ubd80 \uc9c0\uc5ed\uc758 \uc900\uc6b0\uc2b9\ud300\uacfc \ud50c\ub808\uc774\uc624\ud504\ub97c \uac00\uc838 \uc2b9\uaca9 \ud639\uc740 \uac15\ub4f1\uc744 \uacb0\uc815\ud558\uac8c \ub41c\ub2e4.\n2017-18 \uc2dc\uc98c.\n20\uac1c\ud300\uc774 \ucd1d 38\uacbd\uae30\uc529 \uce58\ub974\uba70 \ucd5c\uc0c1\uc704 2\uac1c\ud300\uc740 \"\ud504\ub9ac\uba54\uc774\ub77c\ub9ac\uac00\"\ub85c \uc2b9\uaca9\ud558\uba70 2018-19 \uc2dc\uc98c \ucc38\uac00\ud300\uc744 18\uac1c\ud300\uc73c\ub85c \ub9de\ucd94\uae30 \uc704\ud574 \uae30\uc874\uc758 3\uac1c\ud300\uc5d0\uc11c \ucd5c\ud558\uc704 4\uac1c\ud300\uc774 \"\uce84\ud398\uc624\ub098\ud22c \ub4dc \ud3ec\ub974\ud22c\uac08\"\ub85c \ubc14\ub85c \uac15\ub4f1\ub41c\ub2e4.\n2018-19 \uc2dc\uc98c.\n18\uac1c\ud300\uc774 \ucd1d 34\uacbd\uae30\uc529 \uce58\ub974\uba70 \ucd5c\uc0c1\uc704 2\uac1c\ud300\uc740 \"\ud504\ub9ac\uba54\uc774\ub77c\ub9ac\uac00\"\ub85c \uc2b9\uaca9\ud558\uba70 \uae30\uc874\ucc98\ub7fc 3\uac1c\ud300\uc774 \"\uce84\ud398\uc624\ub098\ud22c \ub4dc \ud3ec\ub974\ud22c\uac08\"\ub85c \uac15\ub4f1\ub41c\ub2e4.\n2019-20 \uc2dc\uc98c.\n\ud3ec\ub974\ud22c\uac08\uc758 \ucf54\ub85c\ub09819 \ubc94\uc720\ud589\uc73c\ub85c \uc778\ud574 2020\ub144 3\uc6d4 12\uc77c\uc5d0 LPFP\ub294 \ub9ac\uadf8\ub97c \ubb34\uae30\ud55c \uc911\uc9c0\ud588\uc73c\uba70 \ub2f9\uc2dc \ub9c8\uc9c0\ub9c9 \uc21c\uc704 \uae30\uc900\uc73c\ub85c \uac00\uc7a5 \ucd5c\uc0c1\uc704 2\uac1c\ud300\uc774 \uc2b9\uaca9\ud558\uace0 \ucd5c\ud558\uc704 2\uac1c\ud300\uc740 \uac15\ub4f1\ub418\uae30\ub85c \ud588\uc9c0\ub9cc \ud504\ub9ac\uba54\uc774\ub77c\ub9ac\uac00\uc758 \ube44\ud1a0\ub9ac\uc544 FC\uc640 CD \uc544\ubca0\uc2a4\uac00 \uc7ac\uc815 \ubb38\uc81c \ub4f1\uc73c\ub85c \ub9ac\uadf8 \ucc38\uac00 \uc790\uaca9\uc7