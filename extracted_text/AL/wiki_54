\uc7a5\uc2dd\ud55c\ub2e4\ub294 \uc990\uae30\ub294 \ubc29\ubc95\ub3c4 \ub098\uc624\uace0 \uc788\uc5b4 \uc77c\ubc18\uc801\uc778 \uba38\ub9ac \ubaa8\uc591\uc758 \ud558\ub098\uac00 \ub418\uc5b4 \uc788\ub2e4. \ubc88\uc744 \uc2f8\ub294 \uc637\uac10 \uc7a5\uc2dd\uc744 \ubc88\uce90\ud504, \uadfc\uc6d0\uc758 \uc8fc\uc704\uc5d0 \ud718\uac10\ub294 \uc7a5\uc2dd\uc744 \uc288\uc288(chouchou) \ub4f1\uc774\ub77c\uace0 \ubd88\ub7ec, \uc5b4\ub290\ucabd\uc774\ub098 \uac04\ud3b8\ud558\uac8c \uc7a5\ucc29\ud560 \uc218 \uc788\ub3c4\ub85d \uace0\ubb34\ub97c \ub123\uc740 \ud0c0\uc785\uc758 \uae30\uc131\ud488\uc774 \ud310\ub9e4\ub418\uace0 \uc788\ub2e4.\n\ucc28\uc774\ub098 \ub4dc\ub808\uc2a4\uc640 \ubc88.\n\uc911\uad6d\uc5d0\uc11c\ub294 \uccad\uc758 \uc2dc\ub300\uc758 \ub9cc\uc8fc\uc871\uc758 \uc5ec\uc131\uc758 \uba38\ub9ac \ubaa8\uc591\uc5d0, \uc591\ud30c\ub450\ub77c\ub294 \uba38\ub9ac\uce74\ub77d\uc744 \ub450 \uac1c\ub85c \ub098\ub204\uc5b4 \uacbd\ub2e8\uc7a5\uc5d0 \uac10\uc740 \ubc88\uc758 \uc77c\uc885\uc774 \uacc4\uc2b9\uac00\ub808\ub77c\uace0 \uc788\uc5c8\ub2e4. \ub9cc\uc8fc\uc871\uc758 \uc5ec\uc131\uc758 \uc815\uc7a5\uc740 \uce58\ud30c\uc624\ub77c\ub294 \ud604\ub300\uc758 \ucc28\uc774\ub098 \ub4dc\ub808\uc2a4\uc758 \uc804\uc2e0\uc774 \ub41c \ubbfc\uc871\ubcf5\uc774\uba70, \u300c\ucc28\uc774\ub098 \ub4dc\ub808\uc2a4\ub77c\uba74 \uc591\ud30c\ub450\uc758 \uc5ec\uc131\u300d\uc774\ub77c\ub294 \uac1c\ub150\uc740 \uc77c\uc885\uc758 \uc5f0\ud310\uc774\ub77c\uace0\ub3c4 \ub9d0\ud560 \uc218 \uc788\uc5b4 \uce5c\uc219\ud55c \uacf3\uc774\ub2e4.\n\ub9cc\ud654\u00b7\uc560\ub2c8\uba54\uc774\uc158\u00b7\uac8c\uc784 \ub4f1, \uc77c\ubcf8\ub0b4\uc758 \uac01\uc885 \uc11c\ube0c \uceec\ucc98 \uc791\ud488 \ub4f1\uc5d0 \uc788\uc5b4\ub3c4, \uc911\uad6d\uc778 \ud639\uc740 \uc911\ud654\uacc4\uc758 \ub8e8\ud2b8\ub97c \uac00\uc9c0\ub294 \uce90\ub9ad\ud130\uc758 \uc0c1\uc9d5\uc73c\ub85c\uc11c \uc5ec\uc131 \uce90\ub9ad\ud130\uc758 \uba38\ub9ac \ubaa8\uc591\uc5d0 \ucc28\uc774\ub098 \ub4dc\ub808\uc2a4\uc640 \uc544\uc6b