 \uc2dc\uc98c \ub3d9\uc548\uc5d0 \uc77c\uc5b4\ub09c \uadf8\uc758 2\uac1c\uc758 \ucd5c\uace0 \ub2e8\uba74\uc758 \ucc98\ub9ac\ub4e4\uc740 \uadf8\uac00 \uc67c\uc190\uc7a1\uc774 \ud22c\uc218\uc774\uc790 \uc804 \ubcf4\ub108\uc2a4 \uc790\ub791\uac70\ub9ac \ud504\ub7ad\ud06c \ubc14\uc6b0\ub9cc\uc744 \uc2dc\uce74\uace0 \ud654\uc774\ud2b8\uc0ad\uc2a4\ub85c, \uadf8\ub9ac\uace0 \ubca0\ud14c\ub791 \ucd9c\ubc1c \ud22c\uc218 \uc0c8\ubbf8 \ud654\uc774\ud2b8\ub97c \uc778\ub514\uc5b8\uc2a4\ub85c \ubcf4\ub0b4\ub294 \uac83\uc744 \ubcf4\uc558\ub2e4. \uadf8\ub7ec\ub098 \ubc14\uc6b0\ub9cc\uc740 1960\ub144\uc758 \ud654\uc774\ud2b8\uc0ad\uc2a4\uc640 \uc5bb\uc740 \ub4dd\uc810 \ud3c9\uade0\uc5d0\uc11c \uc544\uba54\ub9ac\uce78 \ub9ac\uadf8\ub97c \uc774\ub04c\uc5c8\uace0, \ud654\uc774\ud2b8\ub294 \uc778\ub514\uc5b8\uc2a4\uc5d0\uac8c \ubcf4\uace0\ud558\ub294 \uac83\ubcf4\ub2e4 \ub73b\ubc16\uc5d0 \uc740\ud1f4\ud558\uc5ec \uc790\uc2e0\uc758 \uc774\uc801\uc744 \ucde8\uc18c\ud558\uc600\ub2e4. \ud574\ub9ac\uc2a4\ub294 \ub610\ud55c \uc790\uc2e0\uc774 \uc694\ud0a4\uc758 \ud611\ub825\uc790 \ud551\ud0a4 \ud788\uae34\uc2a4\ub97c \uac10\ub3c5\uc73c\ub85c\uc11c \ud574\uace0\ud558\uc600\uace0, 1959\ub144 7\uc6d4 3\uc77c \uc18c\uc720\uc790\ub97c \uc0c1\ub2f4\ud558\uc9c0 \uc54a\uc73c\uba74\uc11c \uc138\ub108\ud130\uc2a4\uc758 \ucf54\uce58 \ube4c\ub9ac \uc800\uc9c0\uc2a4\uc640 \uadf8\ub97c \ub300\uccb4\ud560 \ub54c \uc694\ud0a4\uc640 \ucda9\ub3cc\ud558\uc600\ub2e4. \uc800\uc9c0\uc2a4\ub294 1960\ub144 6\uc6d4 \uc790\uc2e0\uc758 \ud574\uace0\uc640 \ud788\uae34\uc2a4\uc5d0 \uc758\ud558\uc5ec \ub300\uccb4\uac00 \uc788\uae30 \uc804\uc5d0 \ub808\ub4dc\uc0ad\uc2a4\uc758 \uc9c0\ub3c4\uc790\ub85c\uc11c \ub2ec\ub825\uc758 \ud574\ubcf4\ub2e4 \uc801\uac8c \uc9c0\uc18d\ub418\uc5c8\ub2e4. \ud574\ub9ac\uc2a4\uc758 \ud574\uace0\ub294 4\ub2ec \ud6c4\uc5d0 \uc644\uc804\ud788 \uc774\uc5b4\uc9c0\uc9c0 \uc54a\uc558\ub2e4.\n\ud574\ub9ac\uc2a4\ub294 \ud654\uc774\ud2b8\uc0ad\uc2a4\ub97c \uc704\ud55c \uc2a4\uce74\uc6b0\ud2b8 (1961\ub1