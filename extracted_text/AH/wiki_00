\uc758 \uc591 \uc99d\uc190\uc774\uba70, \ud48d\uacc4\uad70(\u8c4a\u6eaa\u541b)\uc758 \uc591\uc190\uc790\uc774\ub2e4. \uc544\ubc84\uc9c0\ub294 \uc644\ud3c9\uad70 \uc774\uc2b9\uc751(\u5b8c\u5e73\u541b \u674e\u6607\u61c9)\uc73c\ub85c \ubcf8\ub798\ub294 \uc120\uc870\uc758 \uc544\ud649\uc9f8 \uc11c\uc790 \uacbd\ucc3d\uad70\uc758 \ud6c4\uc190\uc774\uc5c8\uc9c0\ub9cc, \uc544\ubc84\uc9c0 \uc774\uc2b9\uc751\uc774 \ud48d\uacc4\uad70 \ub2f9\uc758 \uc591\uc790\ub85c \uc785\uc591\ub418\uc5c8\ub2e4. \n1878\ub144(\uace0\uc885 15) \uace0\uc885\uc758 \ud2b9\uba85\uc73c\ub85c \uad70\uad00\uc5d0 \uc784\uba85\ub418\uc5c8\ub2e4. 1878\ub144 \ube48\uc804\ub3c4\uac10\uc5d0 \ub0ad\uccad\uc73c\ub85c \ucc38\uc5ec\ud55c \uacf5\ub85c\ub85c \uadf8\ud574 9\uc6d4 23\uc77c \uc544\ub9c8 1\ud544\uc744 \ud558\uc0ac\ubc1b\uc558\ub2e4.\n1879\ub144(\uace0\uc885 16) \uc74c\uc11c\ub85c \ucd9c\uc0ac\ud558\uc5ec \ubd80\uc0ac\uc6a9(\u526f\u53f8\u52c7)\uc774 \ub418\uace0, 1880\ub144 \ud63c\uc804\uc758 \uc885\ucc99 \uc9d1\uc0ac(\u5b97\u621a\u57f7\u4e8b)\ub85c \ucc38\uc5ec\ud558\uc600\ub2e4. 1882\ub144(\uace0\uc885 19) \uc0ac\uc6a9(\u53f8\u52c7)\uc744 \uc9c0\ub0b4\uace0, \uadf8\ud574 6\uc6d4 11\uc77c \uc885\ucc99\uc9d1\uc0ac\ub85c \ucc28\ud558\ub418\uc5c8\ub2e4. 1888\ub144(\uace0\uc885 25) 5\uc6d4 \uadf8 \ud574\uc5d0 \uace0\uc885\uc774 \uce5c\ud788 \ub0b8 \uc751\uc81c(\u61c9\u88fd)\uc5d0 \ud569\uaca9\ud558\uc5ec \uacfc\uac70 \uc751\uc2dc\uc790\uaca9\uc774 \uc8fc\uc5b4\uc84c\ub2e4. 1888\ub144 \uc2dd\ub144\uc2dc(\u5f0f\u5e74\u8a66) \uc9c4\uc0ac(\u9032\u58eb)\uc5d0 \uae09\uc81c\ud558\uc5ec \uadf8\ud574 11\uc6d4 16\uc77c \uc601\ub989\ucc38\ubd09(\u6c38\u9675\u53c3\u5949), 1889\ub144 8\uc6d4 29\uc77c \uc120\uacf5\uac10\uac10\uc5ed, 1890\ub144 1\uc6d4 5\uc77c \uae08\ubd80\ub3c4\uc0ac\ub97c \uc9c0\ub0c8\ub2e4. 1890\ub144 3\uc6d4 7\uc77c \uc758\uae08\ubd80\ub85c\ubd80\ud130 \uc2e0\ubcd1\uc744 \uc0c1\uc8fc\ud558\uc600\uc73c\ub098 \uc655\uc774 \uba74\uc9c1\uc2dc\ud0a4\uc9c0 \uc54a\uc558\ub2e4. 1890\ub144(\uace0\uc885 27) \uac10\uc5ed(\u76e3\u5f79)\uc744 \uac70\uccd0 1891\ub144 1\uc6d4 29\uc77c \uc81c\uc6a9\uac10\uc8fc\ubd80, 4\uc6d4 29\uc77c \uc218\ub989 \ub2a5\ub839(\u7d8f\u9675\u9675\u4ee4)\uc744 \uac70\uccd0 \uadf8\ud574 8\uc6d4 15\uc77c \uacbd\uae30\ub3c4 \uc9c4\uc704\ud604\ub839(\u632f\u5a01\u7e23\u4ee4)\uc73c\ub85c \ubd80\uc784\ud558\uc600\ub2e4.\n1893\ub144 1\uc6d4 29\uc77c \uac15\uc6d0\ub3c4 \uac04\uc131\uad70\uc218, 1894\ub144 1\uc6d4 28\uc77c \uacbd\uae30\ub3c4 \uc0ad\ub155\uad70\uc218\ub85c \ubd80\uc784\ud588\ub2e4\uac00, \uadf8\ud574 9\uc6d4 14\uc77c \uc218\uc6d0\uad70 \ud310\uad00\uc774 \ub418\uace0, 9\uc6d4 22\uc77c \ubd80\uc784\ud558\uc600\ub2e4. 1895\ub144(\uace0\uc885 32) \uc2e0\ubcd1\uc744 \uc774\uc720\ub85c \uc815\uc7a5\uc744 \uc62c\ub824 \uc218\uc6d0\uad70 \ud310\uad00\uc9c1\uc5d0\uc11c \uba74\uc9c1\uc744 \uccad\ud558\uc600\uc73c\uba70, 1895\ub144 5\uc6d4 2\uc77c \uba74\uc9c1\ub418\uc5c8\ub2e4. 8\uc6d4 10\uc77c \uace0\uc885\uc740 \uadf8\ub97c \uc218\uc6d0\uad70\uc218\ub85c \uc2b9\uc9c4\uc2dc\ucf30\ub2e4. 1895\ub144(\uace0\uc885 32\ub144) 10\uc6d4 15\uc77c \uc885\ucc99 \uc9d1\uc0ac(\u5b97\u621a\u57f7\u4e8b)\ub85c \uc784\uba85\ub418\uace0, \uc774\ud574 11\uc6d4 1\uc77c \uc8153\ud488\uc73c\ub85c \uac00\uc790(\u52a0\u8cc7)\ub418\uc5c8\ub2e4.\n1896\ub144(\uace0\uc885 33, \uac74\uc591 1) 3\uc6d4 7\uc77c \ub18d\uc0c1\uacf5\ubd80\ucc38\uc11c\uad00(\u8fb2\u5546\u5de5\u90e8\u53c3\u66f8\u5b98)\uc774 \ub418\uace0, 4\uc6d4 19\uc77c \ub18d\uc0c1\uacf5\ubd80\ucc38\uc11c\uad00 \uc11c \uc8fc\uc784\uad00 6\ub4f1(\u8fb2\u5546\u5de5\u90e8\u53c3\u66f8\u5b98\u654d\u594f\u4efb\u5b98\u516d\u7b49), \uadf8\ud574 9\uc6d4 26\uc77c 4\ud488\uc5d0\uc11c \uc8153\ud488\uc73c\ub85c \uc2b9\uc9c4\ud558\uc600\ub2e4. \uadf8\ud574 9\uc6d4 27\uc77c \ube48\uc804\uc5d0 \ucc38\uc5ec\ud55c \uc885\ud5cc\uad00 \uc774\ud558\ub97c \uc2dc\uc0c1\ud560 \ub54c \ubc18\uc219\ub9c8 1\ud544\uc744 \ud558\uc0ac\ubc1b\uc558\ub2e4. 1896\ub144 12\uc6d4 6\uc77c \ub18d\uc0c1\uacf5\ubd80\ucc38\uc11c\uad00\uc73c\ub85c \uc7ac\u