\uc878\uc5c5\ud558\uc600\ub2e4. 1956\ub144 \uadf8\ub294 \ube0c\ub8e8\ud074\ub9b0 \ubc95\ud559 \ub300\ud559\uc6d0\uc73c\ub85c\ubd80\ud130 \ubc95\ud559 \ud559\uc704\ub97c \ubc1b\uc558\ub2e4.\n\uc815\uce58 \uacbd\ub825.\n1956\ub144\ubd80\ud130 1975\ub144\uae4c\uc9c0 \uc0ac\ubc95 \uc5c5\ubb34\ub97c \uc720\uc9c0\ud55c \ub3d9\uc548 \ub529\ud0a8\uc2a4\ub294 \ud560\ub818\uc5d0\uc11c \ubbfc\uc8fc\ub2f9 \uc870\uc9c1\uc744 \ud1b5\ud558\uc5ec \uc62c\ub77c\uac00 J. \ub808\uc774\ubaac\ub4dc \uc874\uc2a4\uc758 \ube44\ud638 \uc544\ub798 \uce74\ubc84 \ubbfc\uc8fc\ub2f9 \ud074\ub7fd\uc5d0\uc11c \uc2dc\uc791\ud558\uc600\ub2e4. \uadf8\ub294 \ub370\ub2c8 \ud328\ub7f4, \ud37c\uc2dc \uc11c\ud2bc, \ubca0\uc774\uc990 \ud328\ud130\uc2a8\uacfc \ucc30\uc2a4 \ub7ad\uac94\uc744 \ud3ec\ud568\ud55c \uc544\ud504\ub9ac\uce74\uacc4 \ubbf8\uad6d\uc778 \uc815\uce58\uc778\ub4e4\uc758 \uc601\ud5a5\uc801\uc778 \ub2e8\uccb4\uc758 \uc77c\ubd80\uac00 \ub418\uc5c8\uace0 \ud6c4\uc790\uc758 \uc138\uba85\uc740 \ub529\ud0a8\uc2a4\uc640 \ud568\uaed8 \"4\uc778\ubc29\"\uc73c\ub85c \uc54c\ub824\uc84c\ub2e4. \ud22c\uc790\uac00\ub85c\uc11c \ub529\ud0a8\uc2a4\ub294 1971\ub144 \ud37c\uc2dc \uc11c\ud2bc\uc774 \uc774\ub108 \uc2dc\ud2f0 \ubc29\uc1a1 \ud68c\uc0ac\ub97c \ucc3d\ub9bd\ud558\ub294 \uac83\uc5d0 \ub3c4\uc6c0\uc744 \uc900 50\uba85\uc758 \uc544\ud504\ub9ac\uce74\uacc4 \ubbf8\uad6d\uc778 \ud22c\uc790\uac00\ub4e4 \uc911\uc758 \ud558\ub098\uc600\ub2e4.\n1966\ub144 \ub529\ud0a8\uc2a4\ub294 \ub274\uc695\uc8fc \uc758\ud68c\uc758 \uc758\uc6d0\uc73c\ub85c \uc7a0\uc2dc \uc9c0\ub0c8\ub2e4. \uadf8\ub294 \ub274\uc695 \uc2dc\uc7a5 \uc5d0\uc774\ube0c\ub7ec\ud584 \ube54\uc5d0 \uc758\ud558\uc5ec \ubd80\uc2dc\uc7a5\uc73c\ub85c\uc11c \ud6c4\ubcf4\ub85c \uc62c\ub790\uc73c\ub098 \ucd5c\ud6c4\uc801\uc73c\ub85c \uc784\uba85\ub418\uc9c0 \uc54a\uc558\ub2e4. \uadf8\ud6c4\uc5d0 \ub529\ud0a8\uc2a4\ub294 \ub274\uc695 \uc120\uac70\uc704\uc6d0\ud68c\uc758 \ud68c\uc7a5 (1972\ub144-1973\ub144)\uacfc \ub274\uc695\uc758 \uc11c\uae30 (1975\ub144-1985