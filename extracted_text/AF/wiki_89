\ub140\uc758 \uc99d\uc870\ud560\uba38\ub2c8, \ud560\uba38\ub2c8, \uadf8\ub9ac\uace0 \uc5b4\uba38\ub2c8 \ubaa8\ub450 \ud55c \uc9c0\ubd95 \uc544\ub798\uc5d0\uc11c \uc0b4\uc558\ub2e4. \uadf8\ub140\uc758 \uc99d\uc870\ud560\uba38\ub2c8\ub294 \uae08\uc8fc \uc6b4\ub3d9\uc744, \uadf8\ub140\uc758 \ud560\uba38\ub2c8\ub294 \uc778\uc885 \ud3c9\ub4f1\uacfc \uc5ec\uc131 \ucc38\uc815\uad8c \ubb38\uc81c\ub97c \uc5f0\uad6c\ud588\ub2e4.\n\uc568\ub9ac\uc2a4\uc758 \uc5b4\uba38\ub2c8\ub294 \ub9c8\ud06c \ud2b8\uc6e8\uc778\uc758 \uc870\uce74\ub538\uc774\uc5c8\uace0, \uadf8\ub140\uc758 \uc544\ubc84\uc9c0\ub294 \ud2b8\uc6e8\uc778\uc758 \uc0ac\uc5c5 \ub9e4\ub2c8\uc800\uc600\uace0, \uadf8 \ud6c4 \ucc30\uc2a4 L\uc774 \uc4f4 \ub9ce\uc740 \ucc45\uc744 \ucd9c\ud310\ud588\ub2e4. \uc6f9\uc2a4\ud130\uc640 \ud68c\uc0ac\ub294 1884\ub144\uc5d0 \uc124\ub9bd\ub418\uc5c8\ub2e4. \ucc98\uc74c\uc5d0 \uc0ac\uc5c5\uc740 \uc131\uacf5\uc801\uc774\uc5c8\uace0 \uc568\ub9ac\uc2a4\uac00 5\uc0b4\uc774\uc5c8\uc744 \ub54c \uac00\uc871\uc740 \ub274\uc695\uc758 \ud070 \uac08\uc0c9 \ub3cc\ub85c \uc774\uc0ac\ud588\uace0 \ub871\uc544\uc77c\ub79c\ub4dc\uc5d0 \uc5ec\ub984 \uc9d1\uc774 \uc788\uc5c8\ub2e4. \uadf8\ub7ec\ub098 \ucd9c\ud310\uc0ac\ub294 \uc5b4\ub824\uc6c0\uc5d0 \ubd80\ub52a\ud614\uace0 \ub9c8\ud06c \ud2b8\uc6e8\uc778\uacfc\uc758 \uad00\uacc4\ub294 \uc810\uc810 \uc545\ud654\ub418\uc5c8\ub2e4. 1888\ub144, \uadf8\ub140\uc758 \uc544\ubc84\uc9c0\ub294 \uace0\uc7a5\uc774 \ub098\uc11c \ud734\uac00\ub97c \ub0c8\uace0, \uac00\uc871\ub4e4\uc740 \ub2e4\uc2dc \ud504\ub808\ub3c4\ub2c8\uc544\ub85c \uc774\uc0ac\ub97c \uac14\ub2e4. \uadf8\ub294 \uadf8 \ud6c4 1891\ub144 \uc57d\ubb3c \uacfc\ub2e4 \ubcf5\uc6a9\uc73c\ub85c \uc790\uc0b4\ud588\ub2e4.\n\uc568\ub9ac\uc2a4\ub294 \ud504\ub808\ub3c4\ub2c8\uc544 \uc0ac\ubc94\ud559\uad50\ub97c \ub2e4\ub154\uace0 1894\ub144\uc5d0 \uc911\uad6d \ud68c\ud654\ub85c \uc878\uc5c5\ud588\ub2e4. 1894\ub144\ubd80\ud130 1896\ub144\uae4c\uc9c0, \uadf8\ub140\ub294 \ucf54\ud2b8 \uac00 269\ubc88\uc9c0\uc5d0 \