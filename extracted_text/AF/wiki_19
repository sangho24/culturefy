\uce60\uba74\uc870\u00b7\uac70\uc704\u00b7\uc624\ub9ac \ub4f1\uc744 \uac00\ub9ac\ud0a4\ub294 \ub2e8\uc5b4\ub77c\uace0 \uc815\uc758\ud560 \uc218 \uc788\ub2e4. \ub2e4\ub9cc \ud55c\uad6d \ubc95\ub839\uc0c1\uc73c\ub85c\ub294 \uc9c1\uc811\uc801\uc73c\ub85c \uac00\uae08\ub958\ub97c \uc815\uc758\ud558\uace0 \uc788\uc9c4 \uc54a\uc73c\ub098, \uac00\ucd95 \ubc0f \ucd95\uc0b0\ubb3c \uc774\ub825\uad00\ub9ac\uc5d0 \uad00\ud55c \ubc95\ub960 \uc81c8\uc870\uc7582\uc5d0 \ub530\ub77c \uac00\uae08\ub958\ub97c \"\ub2ed\uacfc \uc624\ub9ac\"\ub85c \uc881\uac8c \ubcf4\uace0 \uc788\ub2e4.\n\ube0c\ub9ac\ud0dc\ub2c8\ucee4 \ubc31\uacfc\uc0ac\uc804\uc5d0\uc11c\ub294 \uc77c\ubc18\uc801\uc778 \uac00\uae08\ub958\uc758 \ubaa9\ub85d\uc5d0 \ucd94\uac00\ub85c \ud638\ub85c\uc0c8\u00b7\uc9d1\ube44\ub458\uae30\ub97c \uae30\uc785\ud558\uace0 \uc788\ub2e4. \uc544\uc6b8\ub7ec R. D. \ud06c\ub85c\ud37c\ub4dc\uac00 \uc9d1\ud544\ud55c \u300a\uac00\uae08\ub958 \ubc88\uc2dd\uacfc \uc720\uc804\uc801 \ud2b9\uc9d5\u300b\uc774\ub77c\ub294 \uc800\uc11c\uc5d0\uc11c\ub294 \ube44\ub458\uae30\uac00 \uc0dd\ub7b5\ub41c \ub300\uc2e0 \uafe9\u00b7\uba54\ucd94\ub77c\uae30\ub97c \ucd94\uac00 \uae30\uc785\ud558\uace0 \uc788\ub294\ub370, \uc774 \uc911 \uba54\ucd94\ub77c\uae30\ub294 \uc0ac\uc721\ub41c \uc0c1\ud0dc\uc5d0\uc11c \ubc88\uc2dd\ud558\uace0 \uc57c\uc0dd\uc73c\ub85c \ubc29\uc0ac\ub418\ub294 \uacbd\uc6b0\uac00 \ub9ce\ub2e4. \ub610\ud55c \uc601\uad6d\uc758 \uc870\ub958\ud559\uc790 \uc5d0\ub4dc\uba3c\ub4dc \ub515\uc2a8\uc740 \uc790\uc2e0\uc758 \uc800\uc11c \u300a\uad00\uc0c1\uc6a9 \ubc0f \uac00\uc815\uc6a9 \uac00\uae08\ub958: \uadf8 \uc5ed\uc0ac\uc640 \uad00\ub9ac\u300b\uc5d0\uc11c \uacf5\uc791\u00b7\ud638\ub85c\uc0c8\u00b7\ud639\uace0\ub2c8\u00b7\uce60\uba74\uc870\u00b7\uac70\uc704 \uc5ec\ub7ec \uc885\ub958\u00b7\uba38\uc2a4\ucf54\ube44\uc624\ub9ac\ub97c \ud3ec\ud568\ud55c \uc624\ub9ac \uc885\ub958\u00b7\ubc34\ud140\ub2ed\uc744 \ube44\ub86f\ud55c \ub2ed \uc804 \uc885\ub958\ub97c \ubaa9\ub85d\uc5d0 \ud3ec\ud568\uc2dc\ucf30\u