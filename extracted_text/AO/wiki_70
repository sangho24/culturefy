3c\ub85c \uc5f0\uacb0\ub418\ub294 2\uac1c\uc758 \ud1b5\ub85c\uac00 \uc788\ub2e4. \uc2b9\uac1d\uc774 \uae30\ucc28\ub97c \ub193\uce5c \ud6c4\uc5d0\ub294 \ucd5c\ub300 8\ubd84 \uc774\uc0c1 \uae30\ub2e4\ub824\uc57c \ud55c\ub2e4.\n\uc6d0\ub798 \uac01 \uc2b9\uac15\uc7a5 \uac04\uc5d0 \ubcf4\ud589\uc790 \ud130\ub110\uc774 \uc5f0\uacb0\ub418\uc5c8\uc73c\ub098, \uc9c0\uae08\uc740 \uc7ac\uac1c\ub97c \uc911\ub2e8\ud588\ub2e4.\n\uc778\uc811 \uc5ed.\n\ub85c\uc6b0 \uc5ed\uc740 \ub3d9\ucca0\uc120\uc758 \uc885\ucc29\uc5ed\uc73c\ub85c, \uc2b9\uac1d\uc740 \ud6d9\ud568 \ubc29\uba74 \uc5f4\ucc28\ub97c \ud0c0\uace0 \uac00\uc6b0\ub8fd\uacfc \uc2e0\uc81c\ub97c \ubc29\ubb38\ud560 \uc218 \uc788\ub2e4.\n\ub3d9\uc2dc\uc5d0, \ub85c\uc6b0 \uc5ed\uc740 \ub904\ud6c4 \uad50\ub97c \uac70\uccd0 \uad11\uc120 \ucca0\ub85c \uc120\uc804 \uc5ed\uacfc \uc5f0\uacb0\ub418\uc9c0\ub9cc, \ud604\uc7ac \ub450 \uc5ed \uac04\uc758 \uc5f4\ucc28\ub294 \ub300\ub959\uac04\uc758 \uc9c1\uacb0 \uc5ec\uac1d\uc5f4\ucc28\uc778 \uc131\uc81c\uc9c1\ud1b5\ucc28(\u57ce\u969b\u76f4\u901a\u8eca)\ub85c\ub9cc \uc874\uc7ac\ud55c\ub2e4.\n\uc5ed\uc0ac.\n1949\ub144 10\uc6d4 1\uc77c \uc774\uc804\uc5d0\ub294 \uad6d\uacbd\uc774 \uc790\uc720\ub85c \uc6e0\uae30 \ub54c\ubb38\uc5d0, \uad6c\uad11\ucca0\ub85c\ub294 \ub85c\uc6b0 \uc5ed \uc5c6\uc774 \ubb34\uc815\ucc28 \ud1b5\uacfc\ud558\uc5ec \uc6b4\ud589\uc744 \uc2dc\uc791\ud588\ub2e4. 1949\ub144 10\uc6d4 14\uc77c \uc911\uad6d \uacf5\uc0b0\ub2f9\uc5d0 \uc758\ud55c \uad11\uc800\uc6b0\uc2dc\uc640 \uc758 \ud1b5\uc81c\uc5d0 \ub300\uc751\ud558\uc5ec \uad6c\uad11\ucca0\ub85c\uad6d\uc740 \uac19\uc740 \ub0a0\uc5d0 \uad6d\uacbd \uac04 \uc6b4\ud589\uc744 \uc885\ub8cc\ud558\uace0, \uc885\ucc29\uc5ed\uc73c\ub85c \ub85c\uc6b0 \uc5ed\uc744 \uc124\uce58\ud55c\ub2e4\uace0 \ubc1c\ud45c\ud588\ub2e4. \uc2b9\uac1d\uc740 \ub85c\uc6b0 \uc5ed\uc5d0\uc11c \ud558\ucc28\ud55c \ud6c4 \ub904\ud6c4 \uad50 \uc9c0\ub098 \uc911\uad6d \ubcf8\ud1a0\ub85c \ub4e4\uc5b4\uc11c\uc57c\ud55c\ub2e4. \ub85c\uc6b0 \uc5ed\uc758 \uc2dc\uc124\u