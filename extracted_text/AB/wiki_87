uac00 29\uace8\ub85c \uacf5\ub3d9 2\uc704\uc5d0 \uc62c\ub77c \uc788\ub2e4. \uadf8\uc758 \ub4dd\uc810 \uae30\ub85d\uc740 \ud55c \ucabd \ubc1c\ub85c \ud3b8\uc911\ub41c \uac83\uc73c\ub85c \uc54c\ub824\uc838 \uc788\uc73c\ub098, \ud06c\ub85c\uc544\ud2f0\uc544 \ub300\ud45c\ub85c \uae30\ub85d\ud55c 45\uace8 \uc911 8\uace8\ub9cc \uc67c\ubc1c\ub85c (\uba38\ub9ac\ub85c\ub294 4\uace8, \ub098\uba38\uc9c0\ub294 \uc624\ub978\ubc1c\ub85c) \uae30\ub85d\ud588\uace0, \uc720\uace0\uc2ac\ub77c\ube44\uc544 \ub300\ud45c\ub85c \uae30\ub85d\ud55c \ub531 \ud55c \uace8\uc740 UEFA \uc720\ub85c 1992 \uc608\uc120\uc5d0\uc11c \ud398\ub85c \uc81c\ub3c4\ub97c \uc0c1\ub300\ub85c \uc67c\ubc1c\uc744 \uc774\uc6a9\ud574 \ucc28\ub123\uc5c8\ub2e4.\n\uadf8\uac00 UEFA \uc720\ub85c 1996\uc5d0\uc11c \uae30\ub85d\ud55c \uc608\uc120 12\uace8 \uae30\ub85d\uc740 2007\ub144\uc5d0 \ubd81\uc544\uc77c\ub79c\ub4dc\uc758 \ub370\uc774\ube44\ub4dc \ud790\ub9ac\uac00 UEFA \uc720\ub85c 2008 \uc608\uc120\uc5d0\uc11c 13\uace8\uc744 \uae30\ub85d\ud558\uae30 \uc804\uae4c\uc9c0 \uc874\uc7ac\ud588\ub2e4.\n\uc740\ud1f4 \ud6c4 \ud65c\ub3d9.\n\uc288\ucf00\ub974\ub294 \ud604\uc5ed \uc740\ud1f4\ub97c \uc55e\ub450\uace0 \uc790\uadf8\ub808\ube0c\uc640 \uba87\uba87 \ub3c4\uc2dc\uc5d0 \ub2e4\ubcf4\ub974 \uc288\ucf00\ub974 \ucd95\uad6c \uad50\uc2e4\uc744 \uc5f4\uc5c8\ub2e4. \ud604\uc7ac\uae4c\uc9c0 \uadf8\ub294 \uc720\uc18c\ub144 \ucd95\uad6c \uad50\uc2e4\uc5d0 \uc2dc\uac04\uacfc \ub178\ub825\uc744 \uc3df\uc544\ubd80\uc73c\uba70, \uadf8\ub97c \ub530\ub77c \uafc8\uc744 \uc887\ub294 \ub2e4\uc218\uc758 \uc720\ub9dd\uc8fc\ub97c \uc721\uc131\ud55c\ub2e4.\n\ub17c\ub780.\n1996\ub144, \uadf8\ub294 \ub450 \uba85\uc758 \uc545\uba85\ub192\uc740 \ubc94\uc8c4\uc790\uc640 \ud611\ub825\ud574 \ud06c\ub85c\uc544\ud2f0\uc544 \ud30c\uc1fc \uc9c0\ub3c4\uc790 \uc548\ud14c \ud30c\ubca8\ub9ac\uce58\uc758 \ubb34\ub364 \uc55e\uc5d0\uc11c \ucc0d\uc740 \uc0ac\uc9c4\uc774 \uc720\ud3ec\ub418\uc5c8\ub2e4.\n2011\ub144 \uadf8\ub294 \uacf5\ud56d\uc5d0 \ub2e4\ub978 \uc2b