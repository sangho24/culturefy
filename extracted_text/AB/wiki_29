\ub144 30\uacbd\uae30 12\uc2b9(\ubaa8\ub450 \uc120\ubc1c)(\uc190\ubbfc\ud55c \uc1a1\uc9c4\uc6b0\uc640 \uacf5\ub3d9 6\uc704) 9\ud328 185.2\uc774\ub2dd 94\uc790\ucc45\uc810 ERA 4.56 2001\ub144 29\uacbd\uae30 12\uc2b9(\ubaa8\ub450 \uc120\ubc1c)(\uc804\uc900\ud638(\ud604\ub300)\uc640 \uacf5\ub3d9 7\uc704) 10\ud328 185.1\uc774\ub2dd 89\uc790\ucc45\uc810 ERA 4.32) \ud0d3\uc778\uc9c0 \uc624\ub978\ucabd \uc5b4\uae68 \ud68c\uc804\uadfc \uc774\uc0c1\uc73c\ub85c \uc778\ud574 24\uacbd\uae30 8\uc2b9(\ubaa8\ub450 \uc120\ubc1c) 7\ud328 13\uc774\ub2dd 66\uc790\ucc45\uc810 ERA 4.47\uc5d0 \uadf8\uce58\uc790 8\uc6d4 2\uc77c \uad11\uc8fc SK\uc804\ubd80\ud130 \uc120\ubc1c\ub85c \ubcf4\uc9c1\ubcc0\uacbd\ud588\uace0 9\uc6d4 28\uc77c \uad11\uc8fc LG\uc804\uc5d0\uc11c 12\ud68c\ucd08 \uad6c\uc6d0\ub4f1\ud310 \ud6c4 \uacf5 1\uac1c\ub9cc \ub358\uc9c4 \ub4a4 \ud589\uc6b4\uc758 \uc2b9\ub9ac(3-3 \ub3d9\uc810\uc774\ub358 \uc5f0\uc7a5 12\ud68c\ub9d0 \ud64d\uc138\uc644 \ub05d\ub0b4\uae30 \uc2a4\ud034\uc988)\ub97c \uc548\uae30\ub3c4 \ud588\ub2e4. \uc774\ub4ec\ud574\uc778 2003\ub144 10\uc2b9(\ubaa8\ub450 \uc120\ubc1c) 13\ud328, \ud3c9\uade0 \uc790\ucc45 3.82\ub85c \ubd80\uc9c4\ud588\uc9c0\ub9cc, 2004\ub144\uc5d0 222\u2154 \uc774\ub2dd \ub3d9\uc548 17\uc2b9(\ubaa8\ub450 \uc120\ubc1c) 8\ud328 \ud3c9\uade0 \uc790\ucc45 2.87\uc758 \ube7c\uc5b4\ub09c \uc131\uc801\uc744 \uc62c\ub838\ub2e4. \uc774 \uc2dc\uc808 \uadf8\ub294 '\uc774\uc624\uc218'\ub77c\ub294 \ud55c\uad6d\uc2dd \uc774\ub984\uc744 \ubcc4\uba85\uc73c\ub85c \uc5bb\uae30\ub3c4 \ud588\ub2e4. 2004\ub144\ubd80\ud130 2007\ub144\uae4c\uc9c0 200\uc774\ub2dd \uc774\uc0c1\uc744 \ud22c\uad6c\ud558\uba70 \uc774\ub2dd \uc774\ud130\uc758 \uc5ed\ud560\uc744 \ud558\uc600\ub2e4.\n\uadf8\ub7ec\ub098 \ud55c \uc2dc\uc98c\uc5d0 \ub108\ubb34 \ub9ce\uc740 \uc774\ub2dd\uc744 \uc18c\ud654\ud55c \ud6c4\uc720\uc99d \ub54c\ubb38\uc774\uc5c8\ub294\uc9c0, 2005\ub144\uc5d0\ub294 \ubd80\uc9c4\uc5d0 \ube60\uc84c\ub2e4. \ub2f9\uc2dc \ucd5c\uc545\uc774\uc5c8\ub358 KIA \ud0c0\uc774\