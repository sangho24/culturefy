\ubc29 \uae30\ub2a5\uc774 \uc788\ub2e4. \uac8c\ub2e4\uac00 \uac01\uc885 \uc138\uade0\uc131 \uc9c8\ubcd1 \uc608\ubc29 \ubc0f \uce58\ub8cc, \ubd88\uc784 \uce58\ub8cc, \uc131\uae30\ub2a5 \uac15\ud654, \uc544\ud1a0\ud53c, \uc5ec\ub4dc\ub984 \uce58\ub8cc, \uc5d0\uc774\uc988 \uc608\ubc29\uacfc \uce58\ub8cc\uc5d0 \ud6a8\uacfc\uac00 \uc788\ub2e4\ub294 \uc2e4\ud5d8\uacb0\uacfc\uac00 \uc5f0\uc774\uc5b4 \ubc1c\ud45c\ub418\uc5b4\uc654\ub2e4.\n\ud604\uc7ac \uc120\uc9c4\uad6d\uc5d0\uc11c\ub294 \uc140\ub808\ub284 \uc5f4\ud48d\uc774 \ubd88\uc5b4 \uc140\ub808\ub284\uc758 \uc0c8\ub85c\uc6b4 \ud6a8\ub2a5\uc744 \ubc1d\ud788\ub294 \uc5f0\uad6c\uac00 \uacc4\uc18d\ud574\uc11c \ucd94\uc9c4\ub418\uace0 \uc5ec\ub7ec \ub9e4\uc7a5\uc5d0\uc11c FDA\uc2b9\uc778\uc744 \ubc1b\uc740 \uc140\ub808\ub284 \ubcf4\uc870\uc81c\uac00 \ud314\ub9ac\uba70 \uc804\ubb38\uac00\ub4e4\uc740 \uc774 \ud604\uc0c1\uc774 \uc624\ub7ab\ub3d9\uc548 \uc720\uc9c0\ub420 \uac83\uc774\ub77c \uc804\ub9dd\ud55c\ub2e4.\n\uc140\ub808\ub284\uc740 \ub2e8\ubc31\uc9c8 \ubcc0\ud615\uc744 \ucd5c\uc18c\ud654\ud574 \uc885\uc591\uc758 \uc131\uc7a5\uc744 \uc5b5\uc81c\ud558\uc9c0\ub9cc \uc885\uc591\uc744 \uc608\ubc29\ud558\ub294 \ub370\ub294 \ud6a8\uacfc\uac00 \uc5c6\ub2e4.\n\ub3d9\uc704 \uc6d0\uc18c.\n\uc790\uc5f0\uc5d0\ub294 \uc140\ub808\ub284 \ub3d9\uc704 \uc6d0\uc18c\uac00 \ubaa8\ub450 6\uac00\uc9c0\uac00 \uc874\uc7ac\ud558\ub098, \uc774 \uc911 \uc548\uc815\ud55c \uac83\uc740 5\uac00\uc9c0(74Se,76Se,77Se,78Se,80Se)\uc774\ub2e4. \uc774 \uc911 77Se,78Se,80Se\ub294 \ubc18\uac10\uae30\uac00 32\ub9cc 7000\ub144\uc778 79Se\uc640 \ud568\uaed8 \ud575\ubd84\uc5f4\uc758 \ubd80\uc0b0\ubb3c\ub85c \uc0dd\uc131\ub418\uae30\ub3c4 \ud55c\ub2e4. \uc790\uc5f0\uc5d0 \uc874\uc7ac\ud558\ub294 \ub2e4\ub978 \ubc29\uc0ac\uc131 \ub3d9\uc704 \uc6d0\uc18c\uc778 82Se\ub294 \ubc18\uac10\uae30 1020\ub144\uc744 \uac70\uccd0 82Kr\ub85c \ubd95\uad34\ud558\ub294\ub370 \ubc18\uac10\uae30\uac00 \uc6cc\ub099 \uae38\ub2e4 \ubcf4\ub2c8 \uc5b4\ub5a4 \uacbd\uc6b0 \uc548\uc815\ud