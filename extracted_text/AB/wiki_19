\uc73c\ub85c \uc784\uba85\ub418\uc5c8\ub2e4. 1936\ub144 8\uc6d4 1\uc77c\uc5d0 \ub300\uc704\ub85c \uc9c4\uae09\ud558\uc600\uace0, \uac19\uc740 \ud574 12\uc6d4 17\uc77c\uc5d0 \uc77c\ubcf8 \uc721\uad70\ub300\ud559\uad50 \uc81c51\uc0dd\uc73c\ub85c \uc785\ud559\ud558\uc600\uc73c\uba70, 12\uc6d4 28\uc77c\uc5d0\ub294 \uc721\uad70\ub300\ud559\uad50 \uc785\ud559\uacfc \uad00\ub828\ud558\uc5ec \uc721\uad70\uc0ac\uad00\ud559\uad50 \ub9c8\uc220(\u99ac\u8853) \uad50\uad00\uc5d0\uc11c \uadfc\uc704 \uae30\ubcd1 \uc5f0\ub300 \uc18c\uc18d\uc73c\ub85c \uc804\ubcf4\ub418\uc5c8\ub2e4. 1938\ub144 12\uc6d4 8\uc77c\uc5d0 \uc721\uad70\ub300\ud559\uad50\ub97c \uc878\uc5c5\ud558\uc600\uace0, 12\uc6d4 10\uc77c\uc5d0 \uadfc\uc704 \uae30\ubcd1 \uc5f0\ub300 \uc911\ub300\uc7a5\uc73c\ub85c \uc784\uba85\ub418\uc5c8\ub2e4. \uc77c\uc81c \uac15\uc810\uae30\uc5d0 \uc77c\ubcf8\uc721\uad70\ub300\ud559\uc744 \uc878\uc5c5\ud55c \uc870\uc120\uc778\uc740 \uc774\uac74\uc744 \ud3ec\ud568\ud558\uc5ec \uc601\uce5c\uc655 \uc774\uc740, \uc774\uc6b0, \ud64d\uc0ac\uc775 \ubfd0\uc774\ub2e4. 1939\ub144 9\uc6d4 13\uc77c\uc5d0 \uc721\uad70\ub300\ud559\uad50 \uc5f0\uad6c\ubd80 \uc8fc\uc0ac \uacb8 \ubcd1\ud559 \uad50\uad00\uc73c\ub85c \uc804\ubcf4\ub418\uc5c8\ub2e4. 1940\ub144 8\uc6d4 1\uc77c\uc5d0 \uc18c\uc88c\ub85c \uc9c4\uae09\ud558\uc600\uc73c\uba70, 1943\ub144 3\uc6d4 1\uc77c\uc5d0 \uc911\uc88c\ub85c \uc9c4\uae09\ud558\uc600\ub2e4. 1944\ub144 5\uc6d4 15\uc77c\uc5d0 \uc721\uad70\ub300\ud559\uad50 \uc5f0\uad6c\ubd80 \uc8fc\uc0ac\ub85c \uc804\ubcf4\ub418\uc5c8\ub2e4.\n\uadc0\ud654\uc640 \ub9d0\ub144.\n1947\ub144\uc5d0 \uc2e0\uc801\uac15\ud558\ub85c \ud3c9\ubbfc\uc774 \ub418\uc790 \uc774\uac74\uc740 \u2018\ubaa8\ubaa8\uc57c\ub9c8 \uac90\uc774\uce58\u2019\ub85c \uac1c\uba85\ud558\uc600\uace0, \ubd80\uc778 \uc774 \uc138\uc774\ucf54(\u8aa0\u5b50)\ub294 \ubcf8\ub798 \uc774\ub984\uc778 \uc694\uc2dc\ucf54(\u4f73\u5b50), \uc7a5\ub0a8 \ucda9\uc740 \ub2e4\ub2e4\ud788\uc0ac(\u5fe0\u4e45), \ucc28\ub0a8 \uae30(\u6c82)\ub294 \ud0a8\u