\uc77c\uc5d0\ub294 3\ucc9c \uba85 \uc815\ub3c4, \uacb0\uacfc\uc801\uc73c\ub85c\ub294 \uc57d 1\ub9cc \uba85\uc758 \uc0ac\ub78c\ub4e4\uc744 \uc218\uc6a9\ud558\uae30\uc5d0 \uc774\ub978\ub2e4. \uc77c\ubcf8\uad70\uc774 \ud559\uad50\uc5d0 \ub4e4\uc5b4\uc640 \uc911\uad6d\uc778\ub4e4\uc744 \ub04c\uace0 \uac00\ub824\uace0 \ud560 \ub54c\ub9c8\ub2e4 \uc774\ub97c \ub9c9\uc544\ub0b4\uae30 \uc704\ud574 \ud798\uc37c\ub2e4. \ub354 \ub9ce\uc740 \ud76c\uc0dd\uc790\ub4e4\uc744 \uc0b4\ub9ac\uc9c0 \ubabb\ud588\ub2e4\ub294 \uc790\ucc45\uac10 \ud639\uc740 \ucc38\uc0c1\uc744 \ubaa9\uaca9\ud55c \ud6c4\uc720\uc99d\uc73c\ub85c \uace0\ud5a5\uc778 \ubbf8\uad6d\uc73c\ub85c \ub3cc\uc544\uac04 \uc9c0 1\ub144 \ub9cc\uc778 1940\ub144 \uc790\uc0b4\ud558\uae30\uc5d0 \uc774\ub978\ub2e4.\n\ub09c\uc9d5 \ub300\ud559\ubcd1\uc6d0\uc5d0\uc11c \uc77c\ud558\ub358 \uc758\uc0ac \ub85c\ubc84\ud2b8 O. \uc70c\uc2a8(Robert O. Wilson)\uc740 \ub09c\uc9d5 \ub300\ud559\uc0b4\uc774 \ubc8c\uc5b4\uc9c0\uace0 \ub05d\uae4c\uc9c0 \ub0a8\uc544 \ub09c\uc9d5\uc758 \uc720\uc77c\ud55c \uc678\uacfc\uc758\uc0ac\uac00 \ub418\uc5c8\ub2e4. \ud559\uc0b4\ub85c \ubd80\uc0c1\ub2f9\ud55c \uc0ac\ub78c\ub4e4\uc744 \uce58\ub8cc\ud588\uc73c\uba70 \uac08 \uacf3 \uc5c6\ub294 \ud658\uc790\ub4e4\uc744 \ubaa8\ub450 \uc218\uc6a9\ud558\uc5ec \ub300\ud559\ubcd1\uc6d0\uc740 \ub610 \ub2e4\ub978 \ud53c\ub09c\ucc98\uac00 \ub418\uc5c8\ub2e4.\n\ud30c\ub098\uc774 \ud638 \uc0ac\uac74.\n\ud30c\ub098\uc774 \ud638 \uc0ac\uac74\uc740 1937\ub144 12\uc6d4, \ub09c\uc9d5 \ub300\ud559\uc0b4\uc774 \ud55c\ucc3d \ubc8c\uc5b4\uc9c0\ub358 \uc2dc\uae30\uc5d0 \ub09c\uc9d5 \uc778\uadfc\uc758 \uc7a5\uac15(\uc591\ucbd4\uac15) \uc720\uc5ed\uc5d0\uc11c \uc77c\ubcf8 \ud574\uad70(\ud574\uad70 \uc18c\uc18d \uc804\ud22c\uae30)\uc5d0 \uc758\ud558\uc5ec \ub2f9\uc2dc \ub09c\uc9d5\uc778\uadfc \uc591\ucbd4\uac15 \uc720\uc5ed\uc744 \uc21c\ucc30\uc911\uc774\ub358 \ubbf8\uad6d \uc544\uc2dc\uc544\ud568\ub300 \uc18c\uc18d\uc758 \uacbd\ube44\uc815\uc778 \ud30c\ub098\uc774 \ud638\uac00 \uce68\uba