uc640 \uc5d0\ub974\ub418\uc2dc \uc0ac\uc774\uc758 \uad00\uacc4\ub294 \uc545\ud654\ub418\uace0 \ub9d0\uc558\ub2e4.\n\uc99d\uba85.\n\ud574\uc11d\uc801 \uc99d\uba85\uc758 \uac1c\ub7b5.\n\uc218\ub860\uc801 \ud568\uc218\uc778 \uc18c\uc218 \uacc4\ub7c9 \ud568\uc218\uc758 \uc810\uadfc\uc801 \uc131\uc7a5\uc740 \ub9ac\ub9cc \uc81c\ud0c0 \ud568\uc218\ub97c \ud1b5\ud574 \ubcf5\uc18c\ud574\uc11d\ud559\uc801\uc778 \uba85\uc81c\ub85c \uce58\ud658\ud560 \uc218 \uc788\ub2e4. \uc6b0\uc120, \ub2e4\uc74c\uacfc \uac19\uc740 \ub3d9\uce58 \uad00\uacc4\ub294 \ucd08\ub4f1\uc801\uc73c\ub85c \ubcf4\uc77c \uc218 \uc788\ub2e4.\nformula_17\n\uc5ec\uae30\uc11c\nformula_18\n\ub294 \uc81c2\uc885 \uccb4\ube44\uc1fc\ud504 \ud568\uc218\uc774\uba70, formula_19\ub294 \ud3f0 \ub9dd\uace8\ud2b8 \ud568\uc218\uc774\ub2e4. \ubc18\uba74, \ub9ac\ub9cc \uc81c\ud0c0 \ud568\uc218\uc758 \ub85c\uadf8 \ub3c4\ud568\uc218\ub294 \ub2e4\uc74c\uacfc \uac19\uc774 \uc4f8 \uc218 \uc788\ub2e4.\nformula_20\n\ub450 \ud569\uc744 \uc11c\ub85c \uc5f0\uad00\uc9d3\uae30 \uc704\ud574, \ub2e4\uc74c\uacfc \uac19\uc740 \ubcf5\uc18c\ud574\uc11d\ud559\uc801 \ubcf4\uc870\uc815\ub9ac\ub97c \uc0ac\uc6a9\ud55c\ub2e4.\nformula_21\n\ub530\ub77c\uc11c, \uc81c\ud0c0 \ud568\uc218\uc640 \uccb4\ube44\uc1fc\ud504 \ud568\uc218\ub97c \ub2e4\uc74c\uacfc \uac19\uc774 \uc5f0\uad00\uc9c0\uc744 \uc218 \uc788\ub2e4.\nformula_22\n\uc774\uc81c, \uc6b0\ubcc0\uc774 formula_23 \uadf9\ud55c\uc5d0\uc11c 1/2\ub85c \uc218\ub834\ud568\uc744 \uacbd\ub85c\uc801\ubd84\ubc95\uc73c\ub85c \uc99d\uba85\ud560 \uc218 \uc788\ub2e4.\n\ucd08\ub4f1\uc801 \uc99d\uba85\uc758 \uac1c\ub7b5.\n\ub2e4\uce20\uc790\uc640 \uc9c0\uce74\uc624(Tikao Tatuzawa)\uc640 \uac00\ub124\uc2dc\ub85c \uc774\uc138\ud0a4(Iseki Kanesiroo)\uc758 \ub4f1\uc2dd\uc744 \uc774\uc6a9\ud558\uac70\ub098 \ub610\ub294 \uc544\ud2c0\ub808 \uc140\ubca0\ub974\uadf8\uc758 \ub2e4\uc74c \ub4f1\uc2dd\nformula_24\n\uc744 \uc774\uc6a9\ud558\uc5ec \uc99d\uba85\uc758 \ud575\uc2ec\uc801\uc774\ub77c \ud560 \uc218 \uc788\ub294, \uc