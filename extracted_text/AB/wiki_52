\ub825\uc774 \uade0\ud615\uc744 \uc774\ub8e8\uc5b4 \ubcc4\uc744 \uc548\uc815\uc801\uc73c\ub85c \uc720\uc9c0\ud55c\ub2e4.\n\uae08\uc18d\uc5d0\uc11c\ub294 \uc591\uc774\uc628\ub4e4\uc774 \ubd80\ubd84\uc801\uc73c\ub85c \uc774\uc628\ud654\ub418\uc5b4 \uc788\uc73c\uba70, \uc6d0\uc790 \uc0ac\uc774\uc758 \uac04\uaca9\uc740 \uc77c\ubc18\uc801\uc778 \uc6d0\uc790 \uac04 \uac70\ub9ac \uc218\uc900\uc774\ub2e4. \uc774 \uacbd\uc6b0 \uc911\ub825\uc758 \uc601\ud5a5\uc740 \ubb34\uc2dc\ud560 \uc218 \uc788\uc744 \uc815\ub3c4\ub85c \uc791\uc73c\uba70, \uc624\ud788\ub824 \uc591\uc774\uc628 \uc911\uc2ec\uc740 \uc74c\uc804\ud558\ub97c \ub760\ub294 \uc804\uc790 \uae30\uccb4\uc5d0 \uc758\ud574 \ub04c\uc5b4\ub2f9\uaca8\uc9c4\ub2e4. \uc774 \ud798\uc740 \uc804\uc790 \ucd95\ud1f4 \uc555\ub825\uc5d0 \uc758\ud574 \uade0\ud615\uc744 \uc774\ub8ec\ub2e4.\n\ud68c\uc804\ud558\uc9c0 \uc54a\ub294 \ubc31\uc0c9\uc65c\uc131\uc5d0\uc11c \ucd95\ud1f4\uc555\uc774 \ubc84\ud2f8 \uc218 \uc788\ub294 \ucd5c\ub300\uc758 \uc9c8\ub7c9\uc740 \ud0dc\uc591\uc758 \uc57d 1.44\ubc30 \uc815\ub3c4\uc774\uba70, \uc774\ub97c \ucc2c\ub4dc\ub77c\uc138\uce74\ub974 \ud55c\uacc4\ub77c\uace0 \ubd80\ub978\ub2e4. \ucc2c\ub4dc\ub77c\uc138\uce74\ub974 \ud55c\uacc4\ub97c \ub118\uc5b4\uc120 \ucc9c\uccb4\ub294 \ud3ed\ubc1c\uc744 \uc77c\uc73c\ud0a4\uac8c \ub41c\ub2e4. \uc774\ub97c Ia\ud615 \ucd08\uc2e0\uc131\uc774\ub77c \ud55c\ub2e4.\n\uc804\uc790 \ucd95\ud1f4\uc555\uc740 \uc911\uc131\uc790\ucd95\ud1f4\uc555\uc758 m/m\ubc30 \uc791\ub2e4.\n\ubd88\ud655\uc815\uc131 \uc6d0\ub9ac.\n\uc804\uc790 \ucd95\ud1f4\uc555\uc758 \uac1c\ub150\uc740 \ud558\uc774\uc820\ubca0\ub974\ud06c\uc758 \ubd88\ud655\uc815\uc131 \uc6d0\ub9ac\ub85c \uc124\uba85\ud560 \uc218 \uc788\ub2e4. \uc774 \uc6d0\ub9ac\uc5d0 \ub530\ub974\uba74, \uc5b4\ub5a4 \uc785\uc790\uc758 \uc704\uce58 \u0394x\uc640 \uc6b4\ub3d9\ub7c9 \u0394p\uc758 \ubd88\ud655\uc815\uc131\uc740 \ub2e4\uc74c\uacfc \uac19\uc740 \uad00\uacc4\ub97c \uac00\uc9c4\ub2e4.\nformula_1\n\uc5ec\uae30\uc11c \u210f\ub294 \ub5