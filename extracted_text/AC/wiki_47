4 \ub5a0\ub09c\ub2e4. \uac8c\ub974\ub2e4(\uac94\ub2e4)\ub294 \ub9c8\uc8fc\uce58\ub294 \ubaa8\ub4e0 \uc0ac\ub78c\ub4e4\uacfc \ubaa8\ub4e0 \ub3d9\ubb3c\ub4e4\uc5d0\uac8c \uce74\uc774\uc758 \ud589\ubc29\uc744 \ubb3c\uc5b4\ubcf4\uc558\ub2e4.\n\uac15.\n\ud55c\ubc88\ub3c4 \uc2e0\uc9c0 \uc54a\uc740 \uac8c\ub974\ub2e4(\uac94\ub2e4)\uc758 \ube68\uac04 \uad6c\ub450\ub97c \uc120\ubb3c\ub85c \ubc1b\uc740 \uac15\uc740 \uce74\uc774\uac00 \ubb3c\uc5d0 \ube60\uc838 \uc8fd\uc9c0 \uc54a\uc558\ub2e4\uace0 \uc54c\ub824\uc900\ub2e4. \uac8c\ub974\ub2e4(\uac94\ub2e4)\ub294 \uce74\uc774\uac00 \ubb3c\uc5d0 \ube60\uc838 \uc8fd\uc9c0 \uc54a\uc740 \uac83\uc744 \uc54c\uac8c \ub418\uc5c8\uc9c0\ub9cc, \uc65c \uce74\uc774\uac00 \uc0ac\ub77c\uc838 \ubc84\ub838\ub294\uc9c0 \uc54c \uc218 \uc5c6\uc5c8\ub2e4.\n\uc78a\ub2e4.\n\uac8c\ub974\ub2e4(\uac94\ub2e4)\ub97c \ub9cc\ub09c \ub178\ud30c\ub294 \uac8c\ub974\ub2e4(\uac94\ub2e4)\uc640 \ud568\uaed8 \uc0b4\uace0 \uc2f6\uc5c8\uae30 \ub54c\ubb38\uc5d0 \ub9c8\ubc95\uc744 \ubd80\ub824 \uac00\uc871\ub4e4\uacfc \uce74\uc774\ub97c \uc78a\uac8c \ub9cc\ub4e0\ub2e4. \ub610\ud55c \uc7a5\ubbf8\ub97c \ubcf4\uba74 \uac8c\ub974\ub2e4(\uac94\ub2e4)\uac00 \uc9d1\uacfc \uce74\uc774\ub97c \ub5a0\uc62c\ub9b0\ub2e4\uace0 \uc0dd\uac01\ud588\uae30 \ub54c\ubb38\uc5d0 \ub9c8\ubc95\uc744 \ubd80\ub824 \uc7a5\ubbf8 \ub364\ubd88\uc744 \ub545 \uc18d\uc73c\ub85c \uc0ac\ub77c\uc9c0\uac8c \ub9cc\ub4e0\ub2e4. \uadf8\ub7ec\ub098 \uc7a5\ubbf8 \ub364\ubd88\uc740 \uac8c\ub974\ub2e4(\uac94\ub2e4)\uac00 \ub530\ub73b\ud55c \ub208\ubb3c\uc744 \ud758\ub9ac\uace0 \ub9d0\uc744 \uac74\ub124\ub294 \ubc14\ub78c\uc5d0 \uae68\uc5b4\ub0ac\uace0, \ub545 \uc18d\uc5d0 \uc788\ub294 \ub3d9\uc548 \uce74\uc774\uc758 \uc2dc\uccb4\ub97c \ubcf4\uc9c0 \ubabb\ud588\ub2e4\uace0 \ub9d0\ud574\uc900\ub2e4.\n\uae4c\ub9c8\uadc0.\n\uac8c\ub974\ub2e4(\uac94\ub2e4)\ub294 \uc601\uc6d0\ud55c \uc5ec\ub984\uc744 \uac00\uc9c4 \uc544\ub984\ub2e4\uc6b4 \uc815\uc6d0\uc5d0\uc11c \ub3c4\ub9dd\ucce4\uace0, \uce74\uc774\uac00 \uacf5\uc8fc