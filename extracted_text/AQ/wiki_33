\ud648\ud398\uc774\uc9c0\ub97c \uacf5\uc870\uc218\uc0ac\ud558\uc5ec \uc544\ub3d9 \uc74c\ub780\ubb3c \uc18c\uc9c0\uc790\ub97c \uac80\uac70\ud588\ub2e4. \uac80\uac70 \ub300\uc0c1\uad6d \ubc0f \uac80\uac70\uc790 \uc218\ub294 32\uac1c\uad6d 310\uba85(\ub300\ud55c\ubbfc\uad6d \uacbd\ucc30\uccad \ubc1c\ud45c), \ud639\uc740 38\uac1c\uad6d 337\uba85 (\ubbf8\uad6d \ubc95\ubb34\ubd80 \ubc1c\ud45c, \uc601\uad6d, \uc544\uc77c\ub79c\ub4dc, \ubbf8\uad6d, \ud55c\uad6d, \ub3c5\uc77c, \uc2a4\ud398\uc778 , \uc0ac\uc6b0\ub514\uc544\ub77c\ube44\uc544, \uc544\ub78d \uc5d0\ubbf8\ub9ac\ud2b8, \uccb4\ucf54\uacf5\ud654\uad6d, \uce90\ub098\ub2e4 \ub4f1)\uc774\ub2e4. \uc774 \uc911 \ud55c\uad6d\uc778\uc740 223\uba85\uc73c\ub85c 72%\ub97c \ucc28\uc9c0\ud588\ub2e4.\n\ubbf8\uad6d.\n\ud14d\uc0ac\uc2a4 \uc8fc\uc758 \ub9ac\ucc98\ub4dc \uadf8\ub798\ucf54\ud504\uc2a4\ud0a4\ub294 1\ud68c \ub2e4\uc6b4\ub85c\ub4dc\uc640 1\ud68c \uc811\uc18d \uc2dc\uccad\uc73c\ub85c 70\uac1c\uc6d4\uacfc \ubcf4\ud638\uad00\ucc30 10\ub144\ud615\uc774 \uc120\uace0\ub410\ub2e4. \ub9c8\uc774\ud074 \uc554\uc2a4\ud2b8\ub871\uacfc \uc790\uc774\ub85c \ud50c\ub85c\ub808\uc2a4 \ub4f1\uc740 \uc544\ub3d9 \ud3ec\ub974\ub178\ubb3c\uc744 \uc785\uc218\ud558\uace0 \uc18c\uc9c0\ud55c \ud610\uc758\ub85c 5\ub144 \uc9d5\uc5ed\ud615\uc774 \uc120\uace0\ub410\ub2e4. 45\uc138 \ubbf8\uad6d\uc778\uc740 2018\ub144 10\uc6d4, \ub3c8\uc138\ud0c1\uacfc \ud568\uaed8 \ube44\ud2b8\ucf54\uc778\uc73c\ub85c 377\ub2ec\ub7ec\ub97c \ub0b4\uace0 \uc774 \uc0ac\uc774\ud2b8\uc5d0\uc11c \uc544\ub3d9 \uc74c\ub780\ubb3c \ub4f1 2,686\uac1c\uc758 \uc601\uc0c1\uc744 \ub0b4\ub824\ubc1b\uc740 \ud610\uc758\ub85c 15\ub144\ud615\uc744 \uc120\uace0\ubc1b\uc558\ub2e4.\n\ubbf8\uad6d \ubc95\ubb34\ubd80\ub294 \ubc94\uc8c4\uc778\uc778\ub3c4\uc870\uc57d\uc744 \uadfc\uac70\ub85c \ud55c\uad6d \uc815\ubd80\uc5d0 \uc190\uc815\uc6b0\uc758 \uc1a1\ud658\uc744 \uc694\uccad\ud588\ub2e4. \ubbf8\uad6d \ubc95 \uc0c1 \uc544\ub3d9\uc131\ucc29\ucde8 \uc601\uc0c1\uc744 \uac00\uc9c0\uace0 \uc788\ub294