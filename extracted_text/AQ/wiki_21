8\uc640 \uac19\uc740 \uc544\ud504\ub9ac\uce74\uacc4 \uc774\ubbfc\uc790 \uac00\uc815\uc758 \uccad\uc18c\ub144\ub4e4\uacfc \ud568\uaed8 \ucd95\uad6c\ub97c \uc2dc\uc791\ud588\ub2e4. \uadf8\ub294 \ub2e4\uc591\ud55c \ud3ec\uc9c0\uc158\ub4e4\uc744 \ub6f4 \ud6c4 \uc790\uc2e0\uc758 \ud3ec\uc9c0\uc158\uc744 \uc13c\ud130\ubc31\uc73c\ub85c \uace0\uc815\ud588\uc73c\uba70 2013\ub144, \uc5d0\ube0c\ub9ac\ub97c \ub5a0\ub098 \ubc1c\ub791\uc2dc\uc5d4\uc758 \uc720\uc18c\ub144 \ud300\uc5d0 \uc785\ub2e8\ud588\ub2e4. \uadf8\ub294 \uc138\ub974\ud788\uc624 \ub77c\ubaa8\uc2a4\uc758 \uc218\ube44 \uc2e4\ub825\uc744 \ubcf4\uba70 \uc790\uc2e0\uc758 \uc218\ube44 \uc2a4\ud0c0\uc77c\uc744 \uac1c\ubc1c\ud588\uc73c\uba70 \ub3d9\ub8cc\ub4e4\uc744 \ud3ec\ud568\ud55c \uc8fc\ubcc0 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \uc5b4\ub9b0 \uc2dc\uc808\ubd80\ud130 \uc131\uc778\uc758 \uc2e0\uccb4 \ub2a5\ub825\uc744 \uac00\uc9c0\uace0 \uc788\ub2e4\ub294 \ud3c9\uac00\ub97c \ub4e4\uc5c8\ub2e4.\n\uad6c\ub2e8 \uacbd\ub825.\n\ub808\ub4dc\ubd88 \uc798\uce20\ubd80\ub974\ud06c.\n\ub9e8\uccb4\uc2a4\ud130 \uc720\ub098\uc774\ud2f0\ub4dc\ub97c \ube44\ub86f\ud55c \uc5ec\ub7ec \uc8fc\uc694 \uc720\ub7fd \uad6c\ub2e8\ub4e4\uc758 \uad00\uc2ec\uc744 \ubc1b\uc740 \uc6b0\ud30c\uba54\uce74\ub178\ub294 2015\ub144 7\uc6d4 220\ub9cc \uc720\ub85c\uc758 \uc774\uc801\ub8cc\uc5d0 \ub808\ub4dc\ubd88 \uc798\uce20\ubd80\ub974\ud06c\uc5d0 \ud569\ub958\ud588\ub2e4. \uadf8\ub294 2015\ub144 7\uc6d4 29\uc77c \ub9d0\ubafc FF\uc640\uc758 UEFA \ucc54\ud53c\uc5b8\uc2a4\ub9ac\uadf8 \uacbd\uae30\uc5d0\uc11c \uad50\uccb4 \uc120\uc218\ub85c \ud65c\uc57d\ud588\ub2e4. \uadf8\ub294 \uc774\ud2c0 \ud6c4 \uc7a5\ud06c\ud2b8 \ud448\ud150\uacfc\uc758 \ub9ac\uadf8 \uacbd\uae30\uc5d0\uc11c \ub808\ub4dc\ubd88\uc758 \ub9ac\uc800\ube0c \ud300\uc778 FC \ub9ac\ud37c\ub9c1\uc18c\uc18d\uc73c\ub85c \ud504\ub85c \ub370\ubdd4\uc804\uc744 \uce58\ub800\ub2e4. 2016\ub144 3\uc6d4 19\uc77c, \uadf8\ub294 \ub808\ub4dc\ubd88 \uc798\uce20\ubd80\ub974\ud06c \uc18c\uc18d\uc73c\ub85c SV \ub9c8\ud