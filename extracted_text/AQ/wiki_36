\uc774 \uac78\ub9ac\ub098 1\ub2ec\uc5d0\uc11c 27\ub144\uae4c\uc9c0 \ub2e4\uc591\ud55c \uc7a0\ubcf5\uae30\ub97c \uac00\uc9c4\ub2e4. \ubb34\uc99d\uc0c1 \uc7a0\ubcf5\uae30\uac00 \uc9c0\ub09c \ud6c4 \uc2e0\uacbd\uc131 \uc5f4\ud654\uac00 \uc9c4\ud589\ub418\uba70, \ud589\ub3d9 \ubcc0\ud654\ub098 \uc9c0\uc801\ub2a5\ub825 \ud558\ub77d, \uadfc\uc721\uac04\ub300\uacbd\ub828, \uc2e4\uba85, \uc6b4\ub3d9\uc2e4\uc870 \ub4f1\uc744 \uac70\uce5c \ud6c4 \ub300\ubd80\ubd84 \uc0ac\ub9dd\uc5d0 \uc774\ub978\ub2e4.\n\uc9c4\ud589.\n\uc9c4\ud589\ub2e8\uacc4\ub294 4\ub2e8\uacc4\ub85c \ub098\ub25c\ub2e4.\n\ubcd1\ub9ac\ud559.\n\uc2e0\uacbd \uc138\ud3ec\uc640 \uc2e0\uacbd \uc544\uad50 \uc138\ud3ec\uc5d0 \ub274\ud074\ub808\uc624\ucea1\uc2dc\ub4dc\uac00 \ub2e4\ub7c9\uc73c\ub85c \uc0dd\uc131\ub41c\ub2e4. \ubc18\uba74 \ubc14\uc774\ub7ec\uc2a4 \uc678\ud53c\ub97c \uc774\ub8e8\ub294 \uc720\uc804\uc790\ub294 \uc81c\ud55c\uc801\uc73c\ub85c \ubc1c\ud604\ub418\uace0, \uadf8 \uacb0\uacfc \uc8fc\ub85c \ubcd1\ub9ac\uc131\uc744 \uac00\uc9c0\ub294 \ubc14\uc774\ub7ec\uc2a4 \uae30\uc9c8\ub2e8\ubc31\uc9c8\uc740 \uc0dd\uc131\ub418\uc9c0 \uc54a\uc544 \uba74\uc5ed\uacc4\ub85c\ubd80\ud130 \uc228\uc744 \uc218 \uc788\uac8c\ub41c\ub2e4. \uc774\ub85c\uc368 \uba74\uc5ed\uacc4\uc5d0 \uc758\ud574 \uccad\uc18c\ub418\uc9c0 \uc54a\uace0 \uacc4\uc18d \uc2e0\uacbd\uc138\ud3ec\uc5d0 \ub274\ud074\ub808\uc624\ucea1\uc2dc\ub4dc\uac00 \ucd95\uc801\ub41c \uacb0\uacfc \uc544\uae09\uc131 \uacbd\ud654\uc131 \ubc94\ub1cc\uc5fc\uc774 \ubc1c\uc0dd\ud558\uac8c \ub41c\ub2e4.\n\uc9c4\ub2e8.\n\uba38\ud06c \ub9e4\ub274\uc5bc\uc5d0 \ub530\ub974\uba74 \uc9c4\ub2e8\uacfc\uc815\uc740 \ub2e4\uc74c\uacfc \uac19\ub2e4.\uc6b0\uc120 \uce58\ub9e4\uc640 \uc2e0\uacbd\uadfc\uc721 \uacfc\ubbfc\uc131\uc744 \ubcf4\uc774\ub294 \uc80a\uc740 \ud658\uc790\uc5d0\uac8c\uc11c \uc544\uae09\uc131 \uacbd\ub1cc\uc131 \ubc94\ub1cc\uc5fc\uc744 \uc758\uc2ec\ud574\ubcfc \uc218 \uc788\ub2e4. \uc774\ud6c4 EEG, CT, MRI, \ub1cc\ucc99\uc218\uc561 \uac80\uc0ac, \ud64d\uc5ed \ud608\