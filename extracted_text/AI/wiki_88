744 \ud1b5\ud574 \uccb4\uc9c8\uc744 \ud655\uc815\ud558\uace0 \uce58\uc720\ud558\ub294 \uc694\ubc95\uc744 \uc2dc\ud589\ud558\uae30\ub3c4 \ud55c\ub2e4.\n\uc774 \uc678\uc5d0\ub3c4 \uccb4\uc9c8\ud310\ubcc4\ub9cc\uc744 \uc804\ubb38\uc801\uc73c\ub85c \ub3c4\ubaa8\ud558\ub294 8\uccb4\uc9c8 '\uc5bc\uad74\uacbd\ud608 \uc0ac\uc9c4\ud310\ubcc4\ubc95'\uc774 \uac1c\ubc1c\ub418\uc5b4 \uc778\uccb4 \uacbd\ub7b5\uc758 \uae30\uc138\ub97c \uc624\uc218\ud608 \ub610\ub294 \uc190\uac00\ub77d\uc758 \ud574\ub2f9 \uacbd\ud608\uc744 \uc0ac\ub78c\uc758 \uc5bc\uad74\uc5d0\uc11c \ucc3e\uc544\ub0b4\uace0 \uc5bc\uad74\uc0ac\uc9c4\uc744 \ud1b5\ud574 \uc790\uc11d \uadf9\uc131\uc73c\ub85c \uc7a5\ubd80\uac15\uc57d\uc758 \uacbd\ud608\uac12\uc744 \uce21\uc815\ud558\uc5ec \uc5ec\ub35f\uac00\uc9c0 \uccb4\uc9c8\uc744 \ud310\ubcc4\ud558\ub294 \uc5f0\uad6c\uc640 \uc784\uc0c1\uc774 \uc9c4\ud589\ub418\uace0 \uc788\ub2e4.\n\uc790\uc11d\uacfc \uacbd\ub77d\ud608\uc758 \ubc18\uc751\uc758 \uadfc\uac70\ub85c\uc368 \ubab8\uc758 \ud2b9\uc815 \ubd80\uc704\uc5d0 \uc0dd\uc131\ub418\ub294 \uac83\uc73c\ub85c \ucd94\uc815\ub418\ub294 \uc0dd\uccb4\uc790\uc131\ub098\ub178\uc785\uc790(BMNPs)\uc758 \uc790\uc131\uc774 \uc790\uc11d\uc758 \uadf9\uc131\uacfc \ubc18\uc751\ud558\ub294 \uac83\uc73c\ub85c \uc8fc\ubaa9\ud558\uace0 \uc788\ub2e4.\n\uc720\uc804\uc790 \uac10\ubcc4\ubc95 \uac1c\ubc1c\uc758 \ud544\uc694\uc131\n\uc720\uc804\uc790 \ubd84\uc11d \uae30\uc220\uc758 \ubc1c\ub2ec\ub85c 30\uc5b5\uac1c\uc758 \uac1c\uc778 \uc720\uc804\uc790 \uc804\uc7a5\uac80\uc0ac \uac00\uaca9\ub3c4 \uc800\ub834\ud574\uc9c0\uba74\uc11c \ub9ce\uc740 \uc0ac\ub78c\ub4e4\uc774 \uac80\uc0ac \uacb0\uacfc\ub97c \ucde8\ub4dd\ud558\uace0 \uc788\ub2e4. \uccb4\uc9c8\ud310\ubcc4\uc758 \uac1d\uad00\uc801 \uac80\uc99d \uc218\ub2e8\uc73c\ub85c\uc11c \uacf5\uc778\ub41c \ud310\ubcc4 \uac80\uc99d \uc7a5\uce58\ub97c \ub9c8\ub828\ud558\uae30 \uc704\ud574 \uc720\uc804\uc790 \uac80\uc0ac\ubc95\uc744 \uc2dc\ub3c4\ud574 \ubcfc \uc218 \uc788\ub2e4. \ud310\ubcc4\uc758 \uacb0\