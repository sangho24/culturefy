\ub110\ub9ac\uc2a4\ud2b8\ub85c\uc11c \uc790\uc2e0\uc758 \uacbd\ub825\uc744 \uc2dc\uc791\ud558\uc5ec \ub370\uc77c\ub9ac \uc775\uc2a4\ud504\ub808\uc2a4, \ub370\uc77c\ub9ac \uc2a4\ucf00\uce58\uc640 \uc774\uc5b4\uc11c \ub370\uc77c\ub9ac \uba54\uc77c\uc744 \uc704\ud558\uc5ec \uc77c\ud558\uc600\ub2e4. 1937\ub144 ~ 38\ub144 \uae30\uac04\uc5d0 \uc911\uc77c \uc804\uc7c1\uc774 \uc77c\uc5b4\ub09c \ub3d9\uc548 \uadf8\ub294 \uc885\uad70 \uae30\uc790\uc600\ub2e4.\n1927\ub144 7\uc6d4 \uadf8\ub294 \uc81c3\ub300 \ud0ac\ub77c\ub2cc \ub0a8\uc791\uc774 \ub418\ub294 \ub370 \uc790\uc2e0\uc758 \uc0bc\ucd0c\uc758 \ub4a4\ub97c \uc774\uc5b4 \uc0c1\uc6d0\uc5d0 \uadc0\uc871\uc81c\uc774\uba74\uc11c \uc6e8\uc2a4\ud2b8\ubbfc\uc2a4\ud130\uad81\uc5d0 \uc0c1\uc6d0\uc5d0\uc11c \uc138\uc2b5 \uc758\uc11d\uc774 \uc8fc\uc5b4\uc84c\ub2e4.\n\uac00\uc871.\n\ud0ac\ub77c\ub2cc \uacbd\uc740 1945\ub144 \uace8\uc6e8\uc774\uc8fc \uc624\uc6b0\ud130\ub7ec\ub4dc\uc758 (\uba54\ub9ac) \uc274\ub77c \uce90\uc2a4\uce74\ud2b8 \ub358\ub86d (MBE, 1919 ~ 2007)\uc5d0\uac8c \uacb0\ud63c\ud558\uc600\ub2e4. \uadf8\ub140\ub294 1872\ub144 \ub354\ube14\ub9b0 \ubcfc\uc2a4\ube0c\ub9ac\uc9c0 \uc774\uc6c3\uc5d0 \ub79c\uc2a4\ub2e4\uc6b4 \ub85c\ub4dc \ub7ed\ube44\uc7a5\uc744 \uac74\uc124\ud55c \ud5e8\ub9ac \ub358\ub86d\uc758 \uc190\ub140\uc600\ub2e4. \uadf8\ub140\uc758 \ubd80\uce5c\uc740 \uc624\uc6b0\ud130\ub7ec\ub4dc\uc758 \uc544\uc77c\ub79c\ub4dc \uc131\uacf5\ud68c\uc758 \uc758\uc804\uc0ac\uc81c \ub354\uae00\ub7ec\uc2a4 \ub77c\uc774\uc5bc \ucc48\ub4e4\ub7ec \ub358\ub86d\uc774\uc5c8\ub2e4. \ud0ac\ub77c\ub2cc \ub0a8\uc791 \ubd80\ubd80\ub294 3\uba85\uc758 \uc544\ub4e4 - \"\ub808\ub4dc\"\ub85c\ub3c4 \uc54c\ub824\uc9c4 \uc870\uc9c0 \ub808\ub4dc\uba3c\ub4dc, \"\ub9c8\uc6b0\uc2a4\"\ub85c \uc54c\ub824\uc9c4 \ub9c8\uc774\ud074\uacfc \uc874, \uadf8\ub9ac\uace0 \ub538 \ubaa8\ub2c8\uce74 \ub370\ubcf4\ub77c\ub97c \ub450\uc5c8\ub2e4.\n\uad70\uc0ac \uacbd\ub825.\n1938\ub144 11\uc6d4 \uc80a\uc740 \ud0ac\ub77c\ub2cc \uacbd\uc740 \ud6d7\ub0a0\uc758 \ub3