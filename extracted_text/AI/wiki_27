\uc758\ud5cc\uc774 \uadf8\uc758 \ub3d9\uc0dd\uc73c\ub85c \ub4f1\uc7ac\ub410\uc9c0\ub9cc, \uc544\ubc84\uc9c0 \uc774\uac10\uc758 \ubb18\ube44\ubb38\uc5d0\ub294 \uc774\uc758\ud5cc\uc774 \uadf8\uc758 \ud615\uc73c\ub85c \ub098\ud0c0\ub09c\ub2e4.\n\uc655\uc2e4\uacfc\uc758 \uad00\uacc4.\n\uadf8\uc758 \uc544\ub4e4 \ub610\ub294 \uc11c\uc790 \uc774\uc6d0\ube48(\u674e\u5143\u8cd3)\uc740 \uc120\uc131\uad70 \uc774\ubb34\uc0dd\uc758 5\ub300\uc190 \uc774\ucc9c \uc774\ud64d\uc8fc\uc758 \ub0a8\ud3b8\uc73c\ub85c, \uc120\uc6d0\ub85d \uc815\uc885\ub300\uc655\uc790\uc190\ub85d \uc120\uc131\uad70\ud30c \ud3b8\uc5d0 \ub4f1\uc7ac\ub418\uc5b4 \uc788\ub2e4. \uc774\uc6d0\ube48\uacfc \uc774\uacbd\ud0dc\ub294 \uc120\uc6d0\ub85d\uc758 \uc131\uc885\ub300\uc655\uc790\uc190\ub85d \uc720\ubd80\ub85d\uc5d0\ub3c4 \uc2e4\ub824\uc788\ub294\ub370, \uc774\uc6d0\ube48\uc758 \ucc98 \ud639\uc740 \ucca9 \uc774\uc560\uc784\uc774 \uc774\ud64d\uc8fc\uc758 \ucca9 \uc2e0\uc21c\ubcf5(\u7533\u798f\u9806)\uc758 \ub538\uc774\ub2e4. \uc2e0\uc21c\ubcf5\uc740 \ud61c\uc219\uc639\uc8fc \uc774\uc218\ub780(\u60e0\u6dd1\u7fc1\u4e3b \u674e\u79c0\u862d)\uacfc \uace0\uc6d0\uc704 \uc2e0\ud56d(\u7533\u6c86)\uc758 \uc591\uc790 \uc2e0\uc218\uacbd(\u7533\u79c0\u6d87)\uc758 \uc544\ub4e4 \uc2e0\ub840(\u7533\u6b1a)\uc758 \uc11c\ub140\uc774\ub2e4. \uc815\uc885\ub300\uc655\uc790\uc190\ub85d\uc5d0\ub294 \uc774\uc560\uc784\uc758 \uc5b4\uba38\ub2c8 \uc774\ub984\uc740 \uc5c6\uace0 \uc591\ub140\uc758 \ub538\ub85c \ub4f1\uc7ac\ub418\uc5c8\uc73c\ub098, \uc131\uc885\ub300\uc655\uc720\ubd80\ub85d \ud61c\uc219\uc639\uc8fc\ud3b8\uc5d0\ub294 \uc774\uc560\uc784\uc758 \uc0dd\ubaa8 \uc2e0\uc21c\ubcf5\uc774 \uc2e0\ub840\uc758 \ub178\ube44 \ucd9c\uc2e0 \ucca9 \ud5a5\uc6b4(\u9999\u96f2)\uc758 \ub538\ub85c \ub098\ud0c0\ub09c\ub2e4. \uc774\uc6d0\ube48\uc758 \ub538 4\uba85\uc740 \uc131\uc885\uc758 \uc6786\ub300\uc190, \ud61c\uc219\uc639\uc8fc\uc758 \uc6785\ub300\uc190\uc774 \ub418\ubbc0\ub85c \uc720\ubd80\ub85d\uc5d0 \uc774\ub984\uc774 \ub4f1\uc7ac\ub418\uc5b