\ub098\uae30 \ub098\uc624\uc2a4\uc5d0 \ub4f1\uc774 \uc788\ub2e4. \ub610, \uac19\uc740 \ub144\uc5d0 \uc77c\uc5b4\ub0ac\ub358 \ub374\uc1fc \ub300\uc9c0\uc9c4\uc73c\ub85c \uc778\ud574 \uc678\ub3d9\ub538\uc778 \uc694\ub124\ud788\uba54(\u4e0e\u7962)\ub97c \uc783\uc5c8\ub2e4. \uc774 \uc988\uc74c\uc5d0 \uc8855\uc704 \uc4f0\uc2dc\ub9c8\ub178\uce74\ubbf8(\u5bfe\u99ac\u5b88)\uc5d0 \uc784\uba85\ub41c\ub2e4.\n\ub374\uc1fc 18\ub144(1590\ub144)\uc758 \uc624\ub2e4\uc640\ub77c \uc815\ubc8c\uc5d0\ub3c4 \ucc38\uc804\ud558\uc5ec, \uc57c\ub9c8\ub098\uce74\uc131 \uacf5\uaca9\uc5d0 \ucc38\uac00\ud558\uc600\ub2e4. \uace7 \ub3c4\ud1a0\ubbf8\uad6d \uac00\uac8c\uac00\uc640(\u639b\u5ddd)\uc5d0 5\ub9cc 1000\uc11d\uc758 \uc18c\ub839\uc744 \ubc1b\uc558\ub2e4. \uac00\uac8c\uac00\uc640\uc5d0\uc11c\ub294 \uc131\uc758 \uac1c\uc218\uc640 \uc131\uc544\ub798 \ub9c8\uc744\uc744 \ub9cc\ub4dc\ub294 \uac83\uc744 \uc2e4\uc2dc\ud574, \ub9e4\ubc88 \ud64d\uc218\uc758 \ud53c\ud574\uac00 \ub9ce\uc558\ub358 \uc624\uc774\uac00\uc640(\u5927\u4e95\u5ddd)\uc758 \uc81c\ubc29\uc758 \uac74\uc124\uc774\ub098 \uc720\ub85c\uc758 \ubcc0\uacbd\uc744 \uac15 \ub9de\uc740\ud3b8\uc73c\ub85c \uba85\ub839\ud574 \uc2a8\ud478\uc131\uc8fc\u00b7\ub098\uce74\ubb34\ub77c \uac00\uc988\uc6b0\uc9c0\ub3c4 \ud568\uaed8 \ud589\ud558\uace0 \uc788\ub2e4.\n\uc784\uc9c4\uc65c\ub780 \ub54c\uc5d0\ub294 \ud788\ub370\uc4f0\uad6c\uc758 \uc219\ub85c\uaca9\uc774\uc5c8\ub358 \uc81c \ub2e4\uc774\ubb18\uc640 \ud568\uaed8 \ucd9c\ubcd1\uc744 \uba74\ud588\uc9c0\ub9cc, \uad70\uc120\uc758 \uc81c\uc870\ub098 \ud6c4\uc2dc\ubbf8\uc758 \ubcf4\uccad \ub4f1\uc744 \ub2f4\ub2f9\ud574 \uc778\ubd80\ub97c \ubc14\ucce4\ub2e4. \ubd84\ub85c\ucfe0 4\ub144(1595\ub144)\uc5d0\ub294 \ud788\ub370\uc4f0\uad6c\uac00 \ubaa8\ubc18 \ud610\uc758\ub85c \ucc98\ud615\ub418\uc5b4, \uac00\uc4f0\ud1a0\uc694\uc640 \ud568\uaed8 \ud788\ub370\uc4f0\uad6c\ub97c \uc12c\uae30\ub358 \uc911\uc2e0\uc778 \uc640\ud0c0\ub77c\uc138 \uc2dc\uac8c\uc544\ud0a4\uac00 \ud788\ub370\uc4f0\uad6c\ub97c \ubcc0\ud638\