\uc778\ud560 \uc218 \uc788\ub2e4.\n\ub3d9\ub300\ubb38\uad6c \uac11.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ub3d9\ub300\ubb38\uad6c \uc744.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc911\ub791\uad6c \uac11.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc911\ub791\uad6c \uc744.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc131\ubd81\uad6c \uac11.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc131\ubd81\uad6c \uc744.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uac15\ubd81\uad6c \uac11.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \