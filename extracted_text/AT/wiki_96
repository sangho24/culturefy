n\uc18d\ucd08\uc2dc\u00b7\uc778\uc81c\uad70\u00b7\uace0\uc131\uad70\u00b7\uc591\uc591\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ud64d\ucc9c\uad70\u00b7\ud6a1\uc131\uad70\u00b7\uc601\uc6d4\uad70\u00b7\ud3c9\ucc3d\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ucda9\uccad\ubd81\ub3c4.\n\uccad\uc8fc\uc2dc \uc0c1\ub2f9\uad6c.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uccad\uc8fc\uc2dc \uc11c\uc6d0\uad6c.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uccad\uc8fc\uc2dc \ud765\ub355\uad6c.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uccad\uc8fc\uc2dc \uccad\uc6d0\uad6c.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ucda9\uc8fc\uc2dc.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc81c\ucc9c\uc2dc\u00b7\ub2e8\uc591\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ubcf4\uc740\uad70\u00b7\uc625\ucc9c\uad70\u00b7\uc601\ub3d9\uad70\u00b7\uad34\uc0b0\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc99d\ud3c9\uad70\u00b7\uc9c4\ucc9c\uad70\u00b7\uc74c\uc131\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ucda9\uccad\ub0a8\ub3c4.\n\ucc9c\uc548\uc2dc \uac11.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ucc9c\uc548\uc2dc \uc744.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ucc9c\uc548\uc2dc \ubcd1.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uacf5\uc8fc\uc2dc\u00b7\ubd80\uc5ec\uad70\u00b7\uccad\uc591\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ubcf4\ub839\uc2dc\u00b7\uc11c\ucc9c\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc544\uc0b0\uc2dc \uac11.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc544\uc0b0\uc2dc \uc744.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\uc11c\uc0b0\uc2dc\u00b7\ud0dc\uc548\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ub17c\uc0b0\uc2dc\u00b7\uacc4\ub8e1\uc2dc\u00b7\uae08\uc0b0\uad70.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\ud398\uc774\uc9c0\ub97c \ud1b5\ud574 \ud655\uc778\ud560 \uc218 \uc788\ub2e4.\n\ub2f9\uc9c4\uc2dc.\n\uc870\uc0ac \ubc29\ubc95\uc744 \ube44\ub86f\ud55c \uc790\uc138\ud55c \ub0b4\uc6a9\uc740 \uc911\uc559\uc120\uac70\uc5ec\ub860\uc870\uc0ac\uc2ec\uc758\uc704\uc6d0\ud68c \ud648\