\ub9ac\uac00\ub85c \ub3cc\uc544\uac14\ub2e4. 3\ubc88\uc9f8\uc774\uc790 \ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc2b9\uaca9\uc744 \uc774\ub8e9\ud55c \uad6c\ub2e8\uc740 \uc9c0\ub85c\ub098\ub85c, \ud14c\ub124\ub9ac\ud398\uc640\uc758 \ud50c\ub808\uc774\uc624\ud504 \uacb0\uc120\uc5d0\uc11c 3-1\ub85c \uc774\uae30\uba70 3\ub144 \ub9cc\uc5d0 1\ubd80 \ub9ac\uadf8 \ubb34\ub300\ub85c \uadc0\ud658\ud588\ub2e4.\n\ucc98\uc74c\uc73c\ub85c \ub77c \ub9ac\uac00\uc5d0\uc11c \uac15\ub4f1\uc744 \ud655\uc815\uc9c0\uc740 \uad6c\ub2e8\uc740 \ub808\ubc18\ud14c\ub85c, 2022\ub144 5\uc6d4 12\uc77c\uc5d0 \ub808\uc54c \ub9c8\ub4dc\ub9ac\ub4dc\uc5d0 0-6\uc73c\ub85c \ud328\ud558\uba74\uc11c \uac15\ub4f1\uc774 \uacb0\uc815\ub418\uc5b4 5\ub144 \ub9cc\uc5d0 1\ubd80 \ub9ac\uadf8\uc758 \ud589\ubcf4\ub97c \uc815\ub9ac\ud588\ub2e4. \ub4a4\uc774\uc5b4 \uc54c\ub77c\ubca0\uc2a4\uac00 2022\ub144 5\uc6d4 15\uc77c\uc5d0 \ub808\ubc18\ud14c\uc5d0 1-3\uc73c\ub85c \ud328\ud558\uba74\uc11c 6\ub144 \ub9cc\uc5d0 \uac15\ub4f1\uc744 \ud655\uc815\uc9c0\uc5c8\ub2e4. \ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc138\uad70\ub2e4 \ub514\ube44\uc2dc\uc628 \uac15\ub4f1\uc744 \ud655\uc815\uc9c0\uc740 3\ubc88\uc9f8 \uad6c\ub2e8\uc740 \uadf8\ub77c\ub098\ub2e4\ub85c \uc5d0\uc2a4\ud30c\ub1f0\uacfc 2022\ub144 5\uc6d4 22\uc77c \ucd5c\uc885\uc804\uc5d0\uc11c \ube44\uae30\ub294 \uc640\uc911\uc5d0 \uce74\ub514\uc2a4\uc640 \ub9c8\uc694\ub974\uce74\uac00 \ub098\ub780\ud788 \uc2b9\ub9ac\ub97c \uc2e0\uace0\ud558\uba70 \uc21c\uc704\uc5d0\uc11c \ubc00\ub838\ub2e4. \uadf8\ub77c\ub098\ub2e4\ub294 1\ubd80 \ub9ac\uadf8\uc5d0\uc11c 3\ub144 \ub9cc\uc5d0 \ubc00\ub824\ub0ac\ub2e4.\n2022\ub144 4\uc6d4 16\uc77c, \uc54c\ucf54\ub974\ucf58\uc740 \uce74\ub974\ud0c0\ud5e4\ub098\uc5d0\uac8c \ud328\ud558\uba74\uc11c 12\ub144 \ub9cc\uc5d0 \uc138\uad70\ub2e4 \ub514\ube44\uc2dc\uc628\uc5d0\uc11c \uac15\ub4f1\ub418\uc5b4 \uac00\uc7a5 \uba3c\uc800 \uac15\ub4f1\uc744 \ud655\uc815\uc9c0\uc5c8\ub2e4. \ub4a4\uc774\uc5b4 \ud478\uc5d4\ub77c\ube0c\ub77c\ub2e4\uac00 2022\ub144 5\uc6d4 7\uc7