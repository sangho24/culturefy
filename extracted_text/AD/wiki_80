\ub3c5 \uc2f8\uc6c0\uc740 \uc878\uc9c0\uc5d0 \uc804\uad6d\uc758 \uc288\uace0 \ub2e4\uc774\ubb18\ub97c \uac00\uc4f0\ubaa8\ud1a0\ud30c\uc640 \uc18c\uc820\ud30c\ub85c \uac08\ub77c\ub193\uc558\ub2e4.(\ud558\uc9c0\ub9cc \uc694\uc2dc\ud788\uc0ac\uac00 \ud0dc\uc5b4\ub098\uae30 \uc804\ubd80\ud130 \uc774\ubbf8 \uc18c\uc820\uc740 \uc5ec\ub7ec \ub2e4\uc774\ubb18\ub4e4\uacfc \ud30c\ubc8c\uc744 \ud615\uc131\ud558\uace0 \uc788\uc5c8\uace0, \uc694\uc2dc\ud788\uc0ac\uac00 \uc544\ub2cc \uc694\uc2dc\ubbf8\uc640 \uacb0\ud569\ub418\uace0 \uc788\uc5c8\ub358 \uac83\uc73c\ub85c \ubcf4\uc778\ub2e4)\n\ub2e4\uc774\ubb18 \uc9d1\uc548\uc758 \ub0b4\ubd84.\n\uc774 \ubb34\ub835 \uc720\ub825 \ub2e4\uc774\ubb18 \uac00\ubb38\uc5d0\uc11c\ub294 \uac00\ubb38\uc744 \ud1b5\uc194\ud558\ub294 \uac00\ub3c5 \uc790\ub9ac\ub97c \ub450\uace0 \uc2ec\uac01\ud55c \ub0b4\ubd84\uc774 \ubc1c\uc0dd\ud558\uace0 \uc788\uc5c8\ub2e4\n\ub300\ud45c\uc801\uc778 \uc0ac\ub840\uac00 \uc0b0\uac04\ub808\uc774(\u4e09\u7ba1\u9818)\uc758 \ud558\ub098\ub85c \uc190\uaf3d\ud788\ub358 \uc720\ub825 \ub2e4\uc774\ubb18 \uac00\ubb38 \ud558\ud0c0\ucf00\uc57c\ub9c8 \uac00\ubb38\uc758 \ub0b4\ubd84\uc73c\ub85c \ub2f9\uc2dc \uac00\ubb38\uc758 \uac00\ub3c5 \ud558\ud0c0\ucf00\uc57c\ub9c8 \ubaa8\uce58\ucfe0\ub2c8\ub294 \uc6d0\ub798 \ub3d9\uc0dd \ubaa8\uce58\ud1a0\uc694(\u6301\u5bcc)\uc5d0\uac8c \uac00\ub3c5\uc744 \ubb3c\ub824\uc8fc\uae30 \uc704\ud574 \ubaa8\uce58\ud1a0\uc694\ub97c \uc790\uc2e0\uc758 \uc591\uc790\ub85c \ub9de\uc544\ub4e4\uc600\ub294\ub370, \uc5d0\uc774\ucfc4(\u6c38\u4eab) 9\ub144(1437\ub144)\uc5d0 \uce5c\uc544\ub4e4 \uc694\uc2dc\ub098\ub9ac(\u7fa9\u5c31)\uac00 \ud0dc\uc5b4\ub098\uc790 \ubd84\uc548 5\ub144(1448\ub144)\uc5d0 \ubaa8\uce58\ud1a0\uc694\ub97c \ud3d0\uc801\uc2dc\ucf1c \ubc84\ub9ac\uace0 \uce5c\uc544\ub4e4 \uc694\uc2dc\ub098\ub9ac\ub97c \ud6c4\uacc4\uc790\ub85c \uad50\uccb4\ud55c \ub4a4 2\ub144\ub4a4\uc5d0\ub294 \uc694\uc2dc\ub098\ub9ac\ub97c \ud558\ud0c0\ucf00\uc57c\ub9c8 \uac00\ubb38\uc758 \uac00\ub3c5\uc744 \uc2b9\uacc4\