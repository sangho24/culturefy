\ub358 \uc911 \uad50\ud1a0\ub85c \uc0c1\uacbd\ud558\uc5ec \uc870\uc815\uc5d0 \ud5cc\uae08, \uc885\uc624\uc704\ud558\uc5d0 \uc704\uacc4\ub418\uace0 \ube48\uace0\ub178\uce74\ubbf8\uc5d0 \uc784\uad00\ub418\uc5c8\ub2e4. \ub354\uc6b1\uc774 \ubb34\ub85c\ub9c8\uce58 \ub9c9\ubd80\uc758 \ub9c9\uc815\uc5d0\ub3c4 \uc0bc\uac00 \uc81c13\ub300 \uc1fc\uad70 \uc544\uc2dc\uce74\uac00 \uc694\uc2dc\ud14c\ub8e8(\u8db3\u5229\u7fa9\u8f1d)\ub97c \uc54c\ud604\ud588\ub2e4. \ub374\ubd84 10\ub144(1541\ub144) \ubbf8\uce74\uc640\ub178\uce74\ubbf8\uc5d0 \uc784\uad00\ub418\uc5c8\ub2e4.\n\uc774\ub807\uac8c \ud574\uc11c \ub178\ubd80\ud788\ub370\ub294 \uc8fc\uac00 \uc624\uc640\ub9ac \uc288\uace0\ub2e4\uc774 \uae30\uc694\uc2a4 \uc624\ub2e4 \uac00\ubb38\uc5d0 \uc2e0\uc885 \uad00\uacc4\ub85c \ub530\ub974\uba74\uc11c\ub3c4, \uc8fc\uac00\uc758 \uc8fc\uad70\uc774\ub77c\uace0 \ud560 \uc218 \uc788\ub294 \uc624\uc640\ub9ac \uc288\uace0 \uc2dc\ubc14 \uac00\ubb38(\u5c3e\u5f35\u5b88\u8b77 \u65af\u6ce2\u6c0f)\uc744 \ub2a5\uac00\ud558\uac8c \ub41c\ub2e4. \ub3d9\uc0dd\uc778 \uc624\ub2e4 \ub178\ubd80\uc57c\uc2a4(\u7e54\u7530\u4fe1\u5eb7)\uc640 \uc624\ub2e4 \ub178\ubd80\ubbf8\uc4f0(\u7e54\u7530\u4fe1\u5149) \ub4f1 \uce5c\uc9c0\uc640 \uac00\uc2e0\uc744 \uc624\uc640\ub9ac \uc801\uc7ac\uc801\uc18c\uc5d0 \ubc30\uce58\ud558\uc5ec \uad6d\ub0b4\uc758 \ud0c0\uc138\ub825\uc744 \uc555\ub3c4\ud558\ub294 \uc9c0\uc704\uc5d0 \uc624\ub978\ub2e4. \uadf8\ub7ec\ub098 \ub178\ubd80\ud788\ub370\ub294 \ub9cc\ub144\uae4c\uc9c0 \uc288\uace0\ub2e4\uc774 \uac00\uc2e0\uc774\ub780 \uc9c0\uc704\uc5d0 \uc548\uc8fc\ud574 \uc624\uc640\ub9ac \uad6d \uc804\uc5ed\uc744 \uc9c0\ubc30\ud558\uc9c4 \uc54a\uc558\ub2e4.\n\ub300\uc678\uc801\uc73c\ub85c \uc788\uc5b4 \ub374\ubd84 4\ub144(1535\ub144) \ub9c8\uc4f0\ub2e4\uc774\ub77c \uae30\uc694\uc57c\uc2a4(\u677e\u5e73\u6e05\u5eb7)\uac00 \uae30\uc694\uc2a4\uc131\uc73c\ub85c\ubd80\ud130 \ubd88\uacfc 5\ud0ac\ub85c\ubbf8\ud130 \uc9c0\uc810\uc778 \ubaa8\ub9ac\uc57c\ub9c8\uc5d0\uc11c \uc554\uc0b4\ub2f9\ud558\uc790, \