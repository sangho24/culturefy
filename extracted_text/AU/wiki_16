\uc218\uc5d0 \uc758\ud574 \uc9c1\uc811\uc801\uc73c\ub85c \uc804\ub798\ub418\uc5c8\ub2e4.\n\uae08\uacf5\uc608\ud488\uc744 \ube44\ub86f\ud55c \uae08\uc18d\uacf5\uc608\ud488\uc758 \ub300\ud45c\uc791\uc73c\ub85c\ub294 \uc544\uac00\uba64\ub17c \ub9c8\uc2a4\ud06c, \uacf5\uc131\uc804 \uc740\uc794, \ud669\uc18c\uba38\ub9ac \ub9ac\ud1a4, \ub124\uc2a4\ud1a0\ub974\uc758 \uae08\uc794 \ub4f1\uc774 \uc788\uc73c\uba70 \uc774\ub4e4 \ubaa8\ub450 \ubbf8\ucf00\ub124 A\u00b7B\uace0\ubd84\uad70\uc5d0\uc11c \ucd9c\ud1a0\ub418\uc5c8\ub2e4. \ud55c\ud3b8 \uc544\ud14c\ub124\uc5d0\uc11c \ubc1c\uacac\ub418\uc5c8\uc73c\ub098 \ubcf8\ub798\ub294 \ud06c\ub808\ud0c0\uc5d0\uc11c \uc81c\uc791\ub41c \uac83\uc73c\ub85c \ucd94\uc815\ub418\ub294 \ud14c\uc138\uc6b0\uc2a4\uc758 \ubc18\uc9c0\ub3c4 \uc870\uadf8\ub9cc \ud45c\uba74\uc5d0 \uc218\ub9ce\uc740 \uc778\ubb3c\ud654\ub97c \ubb18\uc0ac\ud55c \uc815\uad50\ud55c \uc791\ud488\uc73c\ub85c \uc720\uba85\ud558\uba70, \uc774\ub4e4\ub3c4 \ubbf8\ucf00\ub124\uc758 A, B\uace0\ubd84\uad70\uc5d0\uc11c \ub2e4\uc218\uac00 \ucd9c\ud1a0\ub418\uc5c8\ub2e4. \uc774\ub7ec\ud55c \uacf5\uc608\ud488\ub4e4\uc740 \uadf8\ub9ac\uc2a4 \ubcf8\ud1a0\uc640 \ud06c\ub808\ud0c0\uc758 \uc791\ud488\uc744 \uc5b4\ub5bb\uac8c \uad6c\ubcc4\ud560 \uac83\uc778\uac00, \uadf8\ub9ac\uace0 \uad6c\ubcc4\ud560 \uc218 \uc788\ub2e4\uba74 \uadf8 \uc758\ubbf8\ub294 \uc5b4\ub5bb\uac8c \ub418\ub294\uac00\uc5d0 \uad00\ud55c \uc758\ubb38\uc744 \ub0a0\uce74\ub86d\uac8c \uc81c\uc2dc\ud558\uace0 \uc788\ub2e4\ub294 \ud3c9\uac00\ub2e4.\n\ubbf8\ucf00\ub124 \uad81\uc804\uc740 \uc5b8\ub355 \uaf2d\ub300\uae30\ub97c \uc9c0\ubc18\uc73c\ub85c \uc0bc\uc544 \ud07c\uc9c1\ud55c \ub3cc\ub2f4\uc744 \ubc29\uc5b4\ubcbd\uc73c\ub85c \ub458\ub7ec \uc870\uc131\ub41c \uac83\uc774 \uc77c\ubc18\uc801\uc774\ub2e4. \uc0ac\uc790\uc758 \ubb38\uc740 \ubbf8\ucf00\ub124 \uac74\ucd95\uc758 \uc7a5\uc2dd\uc774 \uc5b4\ub5a0\ud588\ub294\uac00\ub97c \uc0b4\ud3b4\ubcfc \uc218 \uc788\ub294 \uba87 \uc548\ub418\ub294 \uc99d\uac70 \uc911 \ud558\ub098\uc