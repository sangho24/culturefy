\n\ubaa8\uc74c.\n\uc9e7\uc740 \ubaa8\uc74c\uc758 \uc608: k'it, \"mirror\" \ubc0f \"rabbit\"\uc758 , \"foot\" \ubc0f \"cook\"\uc758 , \"dress\" \ubc0f \"merry\"\uc758 , \"strut\" \ubc0f \"curry\"\uc758 , \"trap\" \ubc0f \"marry\"\uc758 , \"lot\" \ubc0f o'range\"\uc758 , ago \ubc0f sofa\"'\uc758 .\n\uc7a5\ubaa8\uc74c\uc758 \uc608: fleece\uc758 , goose\uc758 , bear\uc758 , nurse \ubc0f furry\uc758 , north, force \ubc0f thought\uc758 , father \ubc0f start\uc758 .\n\uc7a5\ubaa8\uc74c\uc778 \uc911\uc804\uc124 \ubaa8\uc74c \ub294 \ub2e4\ub978 \uacf3\uc5d0\uc11c \uc804\ud1b5\uc801\uc778 \uae30\ud638\uc778 \ub85c \uc804\uc0ac\ub41c\ub2e4. \ud604\ub300 RP\uc5d0\uc11c \uc9c0\ubc30\uc801\uc778 \uc2e4\ud604\uc740 \ub2e8\ubaa8\uc74c\ud654\uc774\ub2e4.\n\"\uc7a5\ubaa8\uc74c\"\uacfc \"\ub2e8\ubaa8\uc74c\".\nRP \ubaa8\uc74c \uccb4\uacc4\uc5d0 \ub300\ud55c \ub9ce\uc740 \uc804\ud1b5\uc801\uc778 \uc124\uba85\uc740 \ube44\uc774\uc911\ubaa8\uc74c \ubaa8\uc74c\uc744 \"\uc7a5\ubaa8\uc74c\"\uacfc \"\ub2e8\ubaa8\uc74c\"\uc73c\ub85c \ubd84\ub958\ud55c\ub2e4. \uc774\ub294 RP\uc5d0 \ubaa8\uc74c \uae38\uc774\ub9cc\uc774 \uc720\uc77c\ud55c \ucc28\uc774\uc778 \ucd5c\uc18c \ub300\ub9bd\uc30d\uc774 \uc788\ub2e4\ub294 \uc758\ubbf8\ub85c \ubc1b\uc544\ub4e4\uc5ec\uc838\uc11c\ub294 \uc548 \ub41c\ub2e4. \"\uc7a5\ubaa8\uc74c\"\uacfc \"\ub2e8\ubaa8\uc74c\"\uc740 \uc5ec\ub7ec \uc74c\uc131\uc801 \ud2b9\uc9d5\uc744 \uc704\ud55c \ud3b8\ub9ac\ud55c \ud3ec\uad04 \uc6a9\uc5b4\uc774\ub2e4. \uc704\uc5d0 \ud45c\uc2dc\ub41c \uc7a5\ub2e8 \uc9dd\ub4e4\uc740 \ub610\ud55c \ubaa8\uc74c \uc74c\uc9c8\uc758 \ucc28\uc774\ub97c \ud3ec\ud568\ud55c\ub2e4.\nRP\uc5d0\uc11c \"\uc7a5\ubaa8\uc74c\" \uace0\ubaa8\uc74c\uc73c\ub85c \ubd88\ub9ac\ub294 \uc640 \ub294 \uc57d\uac04 \uc774\uc911\ubaa8\uc74c\ud654\ub418\uc5b4 \uc788\uc73c\uba70, \uc74c\uc131\ud559 \ubb38\ud5cc\uc5d0\uc11c\ub294 \uc885\uc885 \uc774\uc911\ubaa8\uc74c \uc640 \ub85c \uc138\ubc00\ud558\uac8c \uc804\uc0ac\ub41c\ub2e4. \uc774\uc911\ubaa8\uc74c \uc758 \uc2dc\uc791\uc8