. \ud2f0\uac00\ub4e0 c\ub294 \uc0dd\uba85\uccb4 \uac70\uc8fc\uac00\ub2a5 \uc601\uc5ed\uc5d0\uc11c \uacf5\uc804\ud55c\ub2e4. \ud589\uc131\uc758 \ud3c9\ud615 \uc628\ub3c4\ub294 -47\u00a0\u00b0C\uc774\uc9c0\ub9cc \ub450\uaebc\uc6b4 \ub300\uae30\ub97c \uac00\uc9c0\uace0 \uc788\ub2e4\uba74 \ud45c\uba74\uc774 \ud6e8\uc52c \ub354 \ub530\ub73b\ud574\uc9c8 \uc218 \uc788\ub2e4. \uc9c0\uad6c\uc758 \ud3c9\ud615 \uc628\ub3c4\ub294 -18\u00a0\u00b0C\uc774\uc9c0\ub9cc \uc9c0\uad6c\uc758 \ub300\uae30\ub294 \uadf8\ubcf4\ub2e4 \ud6e8\uc52c \ub192\uc740 \uc628\ub3c4\ub97c \uc720\uc9c0\ud55c\ub2e4. \uac70\uc8fc \uac00\ub2a5\uc131\uc5d0 \ub300\ud558\uc5ec \uae0d\uc815\uc801\uc778 \uc694\uc18c \uc911\uc5d0 \ud558\ub098\ub294 \ubcc4\uc774\ub2e4. \ub300\ubd80\ubd84\uc758 \uc801\uc0c9\uc65c\uc131\uc740 \uac15\ud55c \ud50c\ub808\uc5b4\ub97c \ubc29\ucd9c\ud558\uc5ec \ub300\uae30\ub97c \ubc97\uaca8\ub0b4\uace0 \uac70\uc8fc \uac00\ub2a5\uc131\uc744 \uc81c\uac70\ud560 \uc218 \uc788\ub2e4. \uc88b\uc740 \uc608\ub85c\ub294 \ucf00\ud50c\ub7ec-438b\uac00 \uc788\ub294\ub370 \uc774\ub294 \ubaa8\ud56d\uc131\uc774 \ud65c\ub3d9\uc801\uc778 \ubcc4\uc774\uae30 \ub54c\ubb38\uc5d0 \uc0ac\ub78c\uc774 \uc0b4 \uc218 \uc5c6\uc744 \uac00\ub2a5\uc131\uc774 \ub192\ub2e4. \ub610 \ub2e4\ub978 \uc608\ub85c\ub294 \ud0dc\uc591\uc5d0 \uac00\uc7a5 \uac00\uae4c\uc6b4 \ubcc4\uc778 \uc13c\ud0c0\uc6b0\ub8e8\uc2a4\uc790\ub9ac \ud504\ub85d\uc2dc\ub9c8\uac00 \uc788\ub2e4. \ud558\uc9c0\ub9cc \ubaa8\ud56d\uc131\uc778 \ud2f0\uac00\ub4e0\uc758 \ubcc4\uc740 \uc774\ub4e4 \ud56d\uc131\uacfc \ub2ec\ub9ac \ud65c\ub3d9\uc801\uc774\uc9c0 \uc54a\uace0 \uc870\uc6a9\ud558\uc5ec \ud589\uc131\uc5d0 \uc0dd\uba85\uccb4\uac00 \uac70\uc8fc\ud560 \uac00\ub2a5\uc131\uc774 \ub192\ub2e4. \uc678\uacc4 \ud589\uc131\uc774 \uc7a0\uc7ac\uc801\uc73c\ub85c \uac70\uc8fc\ud560 \uac00\ub2a5\uc131\uc774 \uc788\ub294 \ub2e4\ub978 \uc870\uc6a9\ud55c \uc801\uc0c9\uc65c\uc131\uc73c\ub85c\ub294 \ub85c\uc2a4 128\uacfc \ub8e8\uc774\ud150\uc758 \ubcc4\uc774 \uc788\ub2e4. \