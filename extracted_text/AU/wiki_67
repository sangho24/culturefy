c800\ud558\uc9c0 \uc54a\uc558\ub2e4. 1926\ub144\uc5d0\ub294 \uc791\uac00\uc640 \ube44\uc11c\uc758 \ud611\ub825\uc801\uc778 \uc5c5\ubb34 \uad00\uacc4\ub97c \ub9fa\uc5c8\ub2e4. \uc694\uc2dc\uc57c\ub294 1957\ub144\uc5d0 \ubaac\ub9c8\ub97c \uc790\uc2e0\uc758 \ub538\ub85c \uc785\uc591\ud588\ub294\ub370 \uc774\ub294 \ub2f9\uc2dc \ub808\uc988\ube44\uc5b8\ub4e4\uc774 \uc11c\ub85c \uc7ac\uc0b0\uc744 \uacf5\uc720\ud558\uace0 \uc758\ud559\uc801 \uacb0\uc815\uc744 \ub0b4\ub9b4 \uc218 \uc788\ub294 \uc720\uc77c\ud55c \ud569\ubc95\uc801\uc778 \ubc29\ubc95\uc774\uc5c8\ub2e4. \ub450 \uc0ac\ub78c\uc740 \ud568\uaed8 \uc18c\ub828, \ub9cc\uc8fc\ub85c \uc5ec\ud589\uc744 \ub5a0\ub0ac\ub2e4\uac00 \ud504\ub791\uc2a4 \ud30c\ub9ac\uc5d0\uc11c 1\ub144\uac04 \uba38\ubb3c\ub800\uace0 1927\ub144\ubd80\ud130 1928\ub144\uae4c\uc9c0 \ubbf8\uad6d\uc744 \uac70\uccd0 \uc77c\ubcf8\uc73c\ub85c \ub3cc\uc544\uc654\ub2e4. 1930\ub144\ub300 \ud6c4\ubc18\uc5d0\ub294 \ub124\ub35c\ub780\ub4dc\ub839 \ub3d9\uc778\ub3c4\uc640 \ud504\ub791\uc2a4\ub839 \uc778\ub3c4\ucc28\uc774\ub098\ub3c4 \ubc29\ubb38\ud588\ub2e4.\n\ubb38\ud559 \uacbd\ub825.\n\uc694\uc2dc\uc57c\uc758 \ucd08\uae30 \uc791\ud488 \uac00\uc6b4\ub370 \ud558\ub098\uc778 \u300a\uaf43 \uc774\uc57c\uae30\u300b(, 1916\ub144 ~ 1924\ub144)\ub294 \uc5f0\uc560 \uc6b0\uc815\uc5d0 \uad00\ud55c 52\ud3b8\uc758 \uc774\uc57c\uae30\ub85c \uc77c\ubcf8\uc758 \uc5ec\ud559\uc0dd\ub4e4 \uc0ac\uc774\uc5d0\uc11c \uc778\uae30\ub97c \ub04c\uc5c8\ub2e4. \u300a\uaf43 \uc774\uc57c\uae30\u300b\uc5d0 \ub4f1\uc7a5\ud558\ub294 \uc5f0\uc560\ub294 \ub300\ubd80\ubd84 \uc9dd\uc0ac\ub791, \uba40\ub9ac\uc11c \ubc14\ub77c\ubcf4\uae30, \ub610\ub294 \ubd88\ud589\ud55c \uacb0\ub9d0\uc5d0 \uad00\ud55c \uc774\uc57c\uae30\uc774\ub2e4. \uc774\ub7ec\ud55c \uc774\uc57c\uae30\ub294 \uc885\uc885 \uc5ec\uc131\uacfc \uc5ec\uc131\uc758 \uc695\ub9dd\uc744 \ubabd\ud658\uc801\uc778 \ubb38\uccb4\ub85c \ubb18\uc0ac\ud55c \uac83\uc774 \ud2b9\uc9d5\uc774\ub2e4.\n\u300a\ub2e4\ub77d\ubc29\uc758 \ub450 \ucc98\ub140\u3