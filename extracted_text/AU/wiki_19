ub3d9\uc548 8\uc2b9 11\ud328, \ud3c9\uade0\uc790\ucc45\uc810 5.34, 182\ud53c\uc548\ud0c0\ub97c \uae30\ub85d\ud588\ub2e4.\n1940\ub144 \uc2dc\uc98c\uc5d0\ub294 \ub9ac\ud2c0\ub85d\uacfc \uc774\uc2a4\ud134 \ub9ac\uadf8\uc758 \uc2a4\ud06c\ub79c\ud134 \ub808\ub4dc\uc0ad\uc2a4\uc5d0\uc11c \ub6f0\uc5c8\ub2e4. \ub9ac\ud2c0\ub85d\uc5d0\uc11c\ub294 21\uacbd\uae30(14\uc120\ubc1c)\uc5d0 \ucd9c\uc804\ud574 5\uc2b9 9\ud328, 4.58\uc758 \ud3c9\uade0\uc790\ucc45\uc810\uc744 \ub098\ud0c0\ub0c8\uace0, \uc2a4\ud06c\ub79c\ud134\uc5d0\uc11c\ub294 13\uacbd\uae30(10\uc120\ubc1c)\uc5d0 \ucd9c\uc804\ud574 4\uc2b9 6\ud328, \ube44\uad50\uc801 \ub0ae\uc740 2.72\uc758 \ud3c9\uade0\uc790\ucc45\uc810\uc744 \uae30\ub85d\ud588\ub2e4.\n\uc774\ud6c4 \ub450 \uc2dc\uc98c \ub354 \uc2a4\ud06c\ub79c\ud134\uc5d0\uc11c \ub6f0\uc5c8\ub2e4. 1941\ub144\uc5d0\ub294 29\uacbd\uae30\uc5d0 \ucd9c\uc804\ud574 193\uc774\ub2dd\uc744 \ub358\uc9c0\uba70 15\uc2b9 9\ud328, \ud3c9\uade0\uc790\ucc45\uc810 2.70, 181\ud53c\uc548\ud0c0\ub97c \uae30\ub85d\ud588\uc73c\uba70, 1942\ub144\uc5d0\ub294 26\uacbd\uae30(21\uc120\ubc1c)\uc5d0 \ucd9c\uc804\ud574 166\uc774\ub2dd\uc744 \ub358\uc9c0\uba70 10\uc2b9 10\ud328, \ud3c9\uade0\uc790\ucc45\uc810 2.93, 171\ud53c\uc548\ud0c0\ub97c \uae30\ub85d\ud588\ub2e4. 1943\ub144\uc5d0\ub294 \ud074\ub798\uc2a4 B \uc778\ud130\uc2a4\ud14c\uc774\ud2b8 \ub9ac\uadf8\uc758 \ub7ad\ucee4\uc2a4\ud130 \ub808\ub4dc\ub85c\uc9c0\uc2a4\uc5d0\uc11c\ub3c4 \uc7a0\uc2dc \ub6f0\uba70 \ub2e4\uc12f \uacbd\uae30\uc5d0\uc11c 39\uc774\ub2dd \ub3d9\uc548 3\uc2b9 2\ud328\ub97c \uae30\ub85d\ud588\ub2e4.\n\uad70\ubcf5\ubb34\uc640 \ud604\uc5ed \ub9c8\uc9c0\ub9c9 \uc2dc\uc98c (1944~1946).\n\ud06c\ub77c\uc6b0\uc2a4\ub294 1944\ub144\uacfc 1945\ub144 2\ub144\uac04 \uc81c2\ucc28 \uc138\uacc4 \ub300\uc804\uc5d0 \ucc38\uc804\ud55c \ubbf8\uad6d \uc721\uad70\uc5d0 \uad70\ubcf5\ubb34\ub97c \ud558\uba74\uc11c \uc120\uc218\ub85c \ub6f8 \uc218 \uc5c6\uc5c8\ub2e4. \uadf8\ub7ec\ub2e4 1946\ub144 \ud074\ub798\uc2a4 D \uc774\uc2a4\ud134 \uc1fc\u