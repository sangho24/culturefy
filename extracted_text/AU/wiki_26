uc774\uc8fc\uc790\ub85c \uaddc\uc815\ud558\uc5ec \uc774\ubbfc\uc744 \ud5c8\uc6a9\ud558\uc600\ub2e4. \uce90\ub098\ub2e4\ub85c \uc774\uc8fc\ud55c \ubbf8\uad6d \ub0b4 \uc9d5\ubcd1 \uac70\ubd80\uc790\ub4e4\uc740 \ucd5c\uc18c 30,000\uba85\uc5d0\uc11c \ucd5c\ub300 100,000\uba85\uc5d0 \uc774\ub97c \uac83\uc73c\ub85c \uc608\uc0c1\ub41c\ub2e4. \uc2a4\uc6e8\ub374\uc758 \uacbd\uc6b0 \uacf5\uac1c\uc801\uc73c\ub85c \ubca0\ud2b8\ub0a8 \uc804\uc7c1\uc5d0 \ub300\ud574 \ube44\ud310\uc801\uc778 \ubaa9\uc18c\ub9ac\ub97c \ub0c8\uae30 \ub54c\ubb38\uc5d0 \ubca0\ud2b8\ub0a8 \uc804\uc7c1 \uae30\uac04 \ubbf8\uad6d\uacfc \uc2a4\uc6e8\ub374\uc758 \uad00\uacc4\ub294 \uc545\ud654\ub418\uc5c8\uace0, \ubca0\ud2b8\ub0a8\uc5d0 \ud3c9\ud654\ub97c! \uc2dc\ubbfc\uc5f0\ud569\uacfc \uac19\uc740 \uc77c\ubd80 \ub2e8\uccb4\ub4e4\uc740 \ubca0\ud2b8\ub0a8 \uc804\uc7c1\uc5d0\uc11c \ubc1c\uc0dd\ud55c \ub099\uc624\uc790\ub4e4\uacfc \uc804\uc7c1 \uac70\ubd80\uc790\ub4e4\uc744 \uc2a4\uc6e8\ub374\uc73c\ub85c \ubcf4\ub0b4\ub294 \uac83\uc744 \uace0\uc548\ud588\ub2e4. \uc2a4\uc6e8\ub374\uc5d0 \uc774\uc8fc\ud55c \ubca0\ud2b8\ub0a8 \uc804\uc7c1 \uac70\ubd80\uc790\ub4e4\uc758 \uc218\ub294 \uc57d 1,000\uba85\uc73c\ub85c \uc774 \uc911 3\ubd84\uc758 1\ub9cc\uc774 \uc9d5\ubcd1 \uac70\ubd80\uc790\ub4e4\uc774\uc5c8\uace0, \uc18c\uc218\uc758 \uac70\ubd80\uc790\ub4e4\uc774 \uc2a4\uc6e8\ub374\uc5d0 \ub0a8\uc544 \uc2a4\uc6e8\ub374 \uc0ac\ud68c\uc5d0 \uc815\ucc29\ud588\ub2e4.\n\ub178\uc870.\n\ubbf8\uad6d\uc774 \ubca0\ud2b8\ub0a8 \uc804\uc7c1\uc5d0 \ucc38\uc804\ud560 \ub2f9\uc2dc, AFL-CIO\uc758 \ub178\uc870 \uc9c0\ub3c4\ubd80\ub294 \uacf5\uac1c\uc801\uc73c\ub85c \uc804\uc7c1\uc744 \uc9c0\uc9c0\ud588\ub2e4. \uc774\ub294 1947\ub144 \ud0dc\ud504\ud2b8-\ud558\ud2c0\ub9ac\ubc95\uc774 \uacf5\uc0b0\uc8fc\uc758\uc790\uc640 \uac19\uc740 \uae09\uc9c4\uc8fc\uc758\uc790\ub4e4\uc774 \ub178\uc870\uc758 \ud575\uc2ec \uc9c1\ucc45\uc5d0 \uc120\ucd9c\ub418\ub294 \uac83\uc744 \uae08\uc9c0\ud588\uae30 \ub54c\ubb38\uc774\uc5c8\ub2e4. \uadf8\ub7ec\ub098