30\ub294 \uc7ac\uc0b0 \ud5cc\ub0a9\uc5d0 \ubc18\ub300\ud558\uc600\uace0 \ubc15\uc815\ud76c\uc758 \ubbf8\uc6c0\uc744 \ubc1b\uac8c\ub418\uc5c8\ub2e4.\n1962\ub144 \ub2f9\uc2dc \uc911\uc559\uc815\ubcf4\ubd80 \ubd80\uc0b0\uc9c0\ubd80\uc7a5\uc774\uc5c8\ub358 \ubc15\uc6a9\uae30\uc528\ub294 &lt;\uc2dc\uc0ac\uc800\ub110&gt;\uacfc\uc758 \ud1b5\ud654\uc5d0\uc11c 1962\ub144 \uc911\uc559\uc815\ubcf4\ubd80 \ubd80\uc0b0\uc9c0\ubd80\uac00 5\u00b716\uc7a5\ud559\ud68c(\ud604 \uc815\uc218\uc7a5\ud559\ud68c)\uc758 \uc804\uc2e0\uc778 \ubd80\uc77c\uc7a5\ud559\ud68c\ub97c \uc124\ub9bd\ud55c \uace0 \uae40\uc9c0\ud0dc\uc528\uc5d0 \ub300\ud55c \uc218\uc0ac\uc5d0 \ucc29\uc218\ud588\ub358 \ubc30\uacbd\uc5d0\ub294 \ubc15\uc815\ud76c \uc804 \ub300\ud1b5\ub839\uc758 \uc9c1\uc811 \uc9c0\uc2dc\uac00 \uc788\uc5c8\ub2e4\uace0 \uc804\uaca9 \uc99d\uc5b8\ud588\ub2e4. \uc774\ub978\ubc14 \u2018\uae40\uc9c0\ud0dc \uc0ac\uac74\u2019 \uc218\uc0ac \ucc45\uc784\uc790\uc600\ub358 \ubc15\uc528\uc758 \uc721\uc131\uc774 \uc9c1\uc811 \uc5b8\ub860\uc5d0 \uacf5\uac1c\ub41c \uac83\uc740 \uc774\ubc88\uc774 \ucc98\uc74c\uc774\ub2e4. \ubc15\uc6a9\uae30\uc528\ub294 \u201d1962\ub144 \ucd08 \ubd80\uc0b0\uc5d0 \ub0b4\ub824\uc628 \ubc15\uc815\ud76c \uc804 \ub300\ud1b5\ub839\uacfc \ub2e8\ub458\uc774 \ub9cc\ub0ac\uc744 \ub54c \ubc15 \uc804\ub300\ud1b5\ub839\uc774 (5\u00b716\uc7a5\ud559\ud68c\uc758 \uc804\uc2e0\uc778 \ubd80\uc77c\uc7a5\ud559\ud68c\ub97c \uc124\ub9bd\ud55c) \uae40\uc9c0\ud0dc\uc528\ub97c \uc218\uc0ac\ud558\ub77c\uace0 \uc9c1\uc811 \uc9c0\uc2dc\ud588\ub2e4\u201d\ub77c\uace0 \ub9d0\ud588\ub2e4. \ubd80\uc815 \ucd95\uc7ac, \ud0c8\uc138\ubfd0 \uc544\ub2c8\ub77c \ud601\uba85 \uc0ac\uc5c5\uc5d0 \ube44\ud611\uc870\uc801\uc774\uc5c8\ub358 \uae40\uc528\uc758 \ud589\uc801 \ub4f1\uc744 \uc870\uc0ac\ud558\ub77c\uace0 \ud588\ub2e4\ub294 \uac83\uc774\ub2e4.\n1962\ub144 9\uc6d4 24\uc77c \ubc15\uc6a9\uae30 \ub300\ub839\uc740 \ud601\uba85\uc815\ubd80\uc5d0 \uc758\ud574 \uad6c\uc18d\ub418\uc5c8\ub2e4. \uc911\uc559\uc815\ubcf4\ubd80