\ub4e4\uc774 \ub9e4\ub2ec 5\uac1c\uc758 \ubb34\ub8cc \uae30\uc0ac\ub97c \ubcfc \uc218 \uc788\ub3c4\ub85d \ud588\uc73c\uba70, \uadf8 \uc774\ud6c4\uc5d0\ub294 \uae30\uc0ac\ub97c \ubcf4\ub824\uba74 \uad6c\ub3c5\uc774 \ud544\uc694\ud588\ub2e4.\n2009\ub144 11\uc6d4, 300\uac1c\uac00 \ub118\ub294 \ud0c0\uc774\ud2c0\uc744 \ubc1c\ud589\ud558\ub294 \uc601\uad6d\uc758 \uc9c0\uc5ed \ucd9c\ud310\uc0ac\ub294 Carrick Gazette\uc640 \uc704\ud2b8\ube44 \uac00\uc81c\ud2b8\ub97c \ud3ec\ud568\ud55c 6\uac1c \uc9c0\uc5ed \uc2e0\ubb38 \uc6f9\uc0ac\uc774\ud2b8\uc5d0 \ud398\uc774\uc6d4\uc744 \uc124\uce58\ud588\ub2e4. \uc774 \ubaa8\ub378\uc740 2010\ub144 3\uc6d4\uc5d0 \uc911\ub2e8\ub418\uc5c8\ub294\ub370, 4\uac1c\uc6d4 \ub3d9\uc548 \uc720\ub8cc \uad6c\ub3c5\uc790 \uc99d\uac00\ub294 \ud55c \uc790\ub9bf\uc218 \ud6c4\ubc18\uc5d0 \ubd88\uacfc\ud588\ub2e4\uace0 \ud55c\ub2e4.\n2014\ub144, 2015\ub144, \uadf8\ub9ac\uace0 2016\ub144 \ub300\ubd80\ubd84 \ub3d9\uc548 \uc624\uadf8\ub358 \uc2e0\ubb38\uc0ac\uc758 \uc77c\uac04 \uc2e0\ubb38\ub4e4\uc740 \ud398\uc774\uc6d4 \ub4a4\uc5d0 \uc788\uc5c8\ub2e4. \uc774 \uc2dc\uc2a4\ud15c\uc740 \ud2f0\uc800 \ud5e4\ub4dc\ub77c\uc778\uacfc \uae30\uc0ac\uc758 \uccab \ubb38\ub2e8\uc744 \ud45c\uc2dc\ud588\ub2e4. \uc720\ub8cc \uad6c\ub3c5\uc790\ub294 \uc2e0\ubb38\uc758 \uc804\uc790\ud310\ubfd0\ub9cc \uc544\ub2c8\ub77c \uc2a4\ub9c8\ud2b8\ud3f0 \ubc0f \ud0dc\ube14\ub9bf \uc571\uc744 \ud1b5\ud574 \ucd9c\ud310\ubb3c\uc5d0 \uc811\uadfc\ud560 \uc218 \uc788\uc5c8\ub2e4. \uc624\uadf8\ub358\uc758 \uc2e0\ubb38\ub4e4\uc740 2016\ub144 11\uc6d4\uc5d0 \ud398\uc774\uc6d4\uc744 \uc81c\uac70\ud558\uae30 \uc2dc\uc791\ud588\uc73c\uba70, \ub3d9\uc2dc\uc5d0 \uc7ac\uc124\uacc4\ub41c \ubaa8\ubc14\uc77c \ubc0f \ud0dc\ube14\ub9bf \uce5c\ud654\uc801\uc778 \uc6f9\uc0ac\uc774\ud2b8\ub97c \ucd9c\uc2dc\ud588\ub2e4.\n2022\ub144 4\uc6d4, \ubbf8\uad6d \uc8fc\uc694 \uacbd\uc81c \ub274\uc2a4 \uc6f9\uc0ac\uc774\ud2b8 \uc911 \ud558\ub098\uc778 \ucffc\uce20\uac00 2019\ub144\ubd80\ud130 \uc6b4\uc601\ud574\uc628 \