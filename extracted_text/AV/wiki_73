758 \uac74\uc124\uc774 \ub0b4\ubd80 \ubcbd\uc774 \uc644\uc131\ub418\uae30 \uc804\uc5d0 \uc911\ub2e8\ub418\uc5c8\uae30 \ub54c\ubb38\uc774\ub2e4.\n\ud604\uc7ac \uc0c1\ud0dc.\n\uc624\ub298\ub0a0 \ub0a8\uc544\uc788\ub294 \uacbd(\u8f15) \uad6c\uc870\ubb3c\uc740 \uac70\uc758 \ubaa8\ub450 \uc790\uc720\ub86d\uac8c \uc811\uadfc\ud560 \uc218 \uc788\ub2e4. \uc911(\u91cd) \uad6c\uc870\ubb3c \uc911 \uc77c\ubd80\ub3c4 \uc811\uadfc \uac00\ub2a5\ud558\uba70, \ub098\uba38\uc9c0\ub294 \uc560\ud638\uac00\uc5d0\uac8c \uc784\ub300\ub418\uac70\ub098 \ud310\ub9e4\ub41c \uacbd\uc6b0\ub3c4 \uc788\ub2e4. \uc77c\uc815 \uc218\ub294 \ubc15\ubb3c\uad00\uc73c\ub85c \uc804\ud658\ub418\uc5c8\uace0, \ub450 \uac1c\uc758 \ud3ec\ubcd1 \uc694\uc0c8, \uc989 \"\uc544\ub2f4\"\uacfc \"\uc2a4\ubab0\ucf54\ud504\"\ub294 \uad70\uc218\ud488 \uc800\uc7a5\uace0\uc774\uba70, \"\ud6c4\ub974\uce74\" \uc694\uc0c8\ub294 2008\ub144\uae4c\uc9c0 \uad70\uc218\ud488 \uc800\uc7a5\uace0\uc600\uc73c\ub098 \ud604\uc7ac\ub294 \ubc15\ubb3c\uad00\uc774\ub2e4. \"\ud558\ub2c8\uce58\uce74\" \ud3ec\ubcd1 \uc694\uc0c8\ub294 1979\ub144\ubd80\ud130 1993\ub144\uae4c\uc9c0 \ub0b4\ubb34\ubd80\ub97c \uc704\ud55c \ud604\ub300\uc801\uc778 \ub300\ud53c\uc18c\ub85c \uc7ac\uac74\ub418\uc5c8\uc73c\ub098 1995\ub144\uc5d0 \ubd88\ud544\uc694\ud558\ub2e4\uace0 \uc120\uc5b8\ub418\uc5c8\ub2e4. \uadf8\uacf3\uc5d0\ub294 \ubc15\ubb3c\uad00\uc774 \uc870\uc131\ub418\uc5c8\ub2e4.\n\ub9ce\uc740 \uac1c\ubc29\ud615 \ubc15\ubb3c\uad00\uc740 \uc624\uc2a4\ud2b8\ub77c\ubc14\uc640 \uc624\ud30c\ubc14 \uc0ac\uc774\uc5d0 \uc788\uc73c\uba70, \ud604\uc7ac \ud3f4\ub780\ub4dc \uad6d\uacbd\uacfc \uac00\uae4c\uc6b4 \ud06c\uc6cc\uce20\ucf54 \uc778\uadfc \ud06c\ub784\ub9ac\ud0a4 \ub9c8\uc744 \uadfc\ucc98\uc5d0 \ub354 \ub9ce\uc740 \ubc15\ubb3c\uad00\uc774 \uc788\ub2e4. \uc774 \uc9c0\uc5ed\uc740 \uc81c2\ucc28 \uc138\uacc4 \ub300\uc804 \uc774\uc804\uc5d0\ub294 \ub3c5\uc77c \uad6d\uacbd\uc774\uc5c8\ub2e4. 1938\ub144 9\uc6d4\uae4c\uc9c0 \uc644\uacf5\ub418\uc5c8\uac70\ub098 \uac74\uc