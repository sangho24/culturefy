4 \ud68c\uc0ac\ub294 1862\ub144 \uce74\uc2a4\ud30c\ub974 \ud0c0\ub108\uc5d0 \uc758\ud574 \uc2a4\uc704\uc2a4 \ub518\ud2f0\ucf58 \ub9c8\uc744\uc5d0 \ubc27\uc904 \uacf5\uc7a5\uc73c\ub85c \uc124\ub9bd\ub418\uc5c8\uc73c\uba70 1878\ub144 \uc778\uadfc \ub80c\uce20\ubd80\ub974\ud06c\ub85c \uc774\uc804\ud588\ub2e4. 1898\ub144 \uce74\uc2a4\ud30c\ub974\uc758 \uc544\ub4e4 \uc624\uc2a4\uce74\uac00 \ud68c\uc0ac\ub97c \ubb3c\ub824\ubc1b\uc558\uc73c\uba70, \ud68c\uc0ac\ub294 \ud2b9\ud788 \uc81c1\ucc28 \uc138\uacc4 \ub300\uc804 \ub3d9\uc548 \ud06c\uac8c \uc131\uc7a5\ud588\ub2e4. 1919\ub144\ubd80\ud130 \ud68c\uc0ac\ub294 Seilwarenfabrik AG Lenzburg\ub77c\ub294 \uc774\ub984\uc744 \uc0ac\uc6a9\ud588\ub2e4. \uc624\uc2a4\uce74 \ud0c0\ub108\ub294 1924\ub144 \ud68c\uc0ac\ub97c \ub5a0\ub0ac\ub2e4.\n1943\ub144\ubd80\ud130 Seilwarenfabrik AG Lenzburg\uc758 \uc81c\ud488\uc740 \ub9c8\ubb34\ud2b8(Mammut) \ube0c\ub79c\ub4dc\ub85c \ud310\ub9e4\ub418\uc5c8\uc73c\uba70, \ube0c\ub79c\ub4dc \ubcc0\uacbd \uc2dc \ub9e4\uba38\ub4dc (\ub3c5\uc77c\uc5b4: Mammut)\uac00 \ud68c\uc0ac \ub85c\uace0\ub85c \ub3c4\uc785\ub418\uc5c8\ub2e4. 1968\ub144 \uc2a4\uc704\uc2a4 \ud5e4\ubc8c\ub77c\uc778 \uadf8\ub8f9\uc774 \ud68c\uc0ac\ub97c \uc778\uc218\ud558\uc5ec \uc544\ub85c\ubc14 \ub80c\uce20\ubd80\ub974\ud06c AG\ub85c \uc774\ub984\uc744 \ubcc0\uacbd\ud588\ub2e4. \ud5e4\ubc8c\ub77c\uc778 \uadf8\ub8f9\uc740 1982\ub144 \uc544\ub85c\ubc14 \ub80c\uce20\ubd80\ub974\ud06c AG\ub97c \uc2a4\uc704\uc2a4 \ub300\uae30\uc5c5 \ucf58\uccb4\ud0c0\uc5d0 \ub9e4\uac01\ud588\uc73c\uba70, \ucf58\uccb4\ud0c0\ub294 1984\ub144 \uc774\ub97c \uc544\ub85c\ubc14 \ub9c8\ubb34\ud2b8 AG\ub85c \uc774\ub984\uc744 \ubcc0\uacbd\ud558\uace0 1992\ub144 \uc2a4\uc704\uc2a4 \uc81c\uc628\uc73c\ub85c \ubcf8\uc0ac\ub97c \uc774\uc804\ud588\uc73c\uba70, \uc774\ud6c4 \uc774 \ud68c\uc0ac\ub294 \uc774\uacf3\uc5d0 \uae30\ubc18\uc744 \ub450\uace0 \uc788\ub2e4.\n\ubc27\uc904 \uacf5\uc7a5\uc758 \uc6d0\ub798 \ud575\uc2ec \uc0ac\uc5c5\uc774 \uc704\uae30\uc5d0 \ucc98\ud558\u