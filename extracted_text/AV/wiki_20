\uad6c\uc5ed\uc744 \uc21c\ucc30\ud560 \uc218 \uc788\ub2e4. \uc5f4\ucc28 \uc11c\ud551\uc774 \uc815\uae30\uc801\uc73c\ub85c \ubc1c\uc0dd\ud558\ub294 \uad6d\uac00\uc5d0\uc11c\ub294 \uacbd\ucc30\uc774 \uc11c\ud37c\ub97c \uc801\ubc1c\ud558\uc5ec \uc81c\uac70\ud558\uace0 \uccb4\ud3ec\ud558\uae30 \uc704\ud574 \uc790\uc8fc \ub2e8\uc18d\uc744 \uc870\uc9c1\ud55c\ub2e4. \uc5f4\ucc28 \uc11c\ud37c\uc5d0 \ub300\ud55c \uac00\uc7a5 \ud754\ud55c \ucc98\ubc8c \ud615\ud0dc\ub294 \ubc8c\uae08\uc774\ub2e4. \uadf8\ub7ec\ub098 \ubbf8\uad6d\uc774\ub098 \uce90\ub098\ub2e4\uc640 \uac19\uc740 \uc77c\ubd80 \uad6d\uac00\uc5d0\uc11c\ub294 \uc5f4\ucc28 \uc11c\ud37c\uac00 \ubc8c\uae08\ud615\uacfc \uc9d5\uc5ed\ud615\uc744 \ubaa8\ub450 \ubc1b\uc744 \uc218 \uc788\ub2e4.\n\uc601\uad6d\uc5d0\uc11c\ub294 \uc5f4\ucc28 \uc11c\ud551\uc774 \ucca0\ub3c4 \uc870\ub840 10\ud638\uc5d0 \ub530\ub77c \uae08\uc9c0\ub418\uc5b4 \uc788\uc73c\uba70, \uc774\ub294 \uc5f4\ucc28\uc758 \uc678\ubd80 \ub610\ub294 \ub0b4\ubd80\uc5d0\uc11c \ud574\ub2f9 \uc0ac\ub78c\uc774 \uc0ac\uc6a9\ud558\ub3c4\ub85d \uc758\ub3c4\ub41c \uad6c\uc5ed \uc774\uc678\uc758 \uc7a5\uc18c\uc5d0\uc11c \uc5ec\ud589\ud558\ub294 \uac83\uc744 \uae08\uc9c0\ud55c\ub2e4.\n2010\ub144 \ub9c8\uc9c0\ub9c9 4\uac1c\uc6d4 \ub3d9\uc548 \uba5c\ubc84\ub978 \ucca0\ub3c4\uc5d0\uc11c \uc5f4\ucc28 \uc11c\ud551 \uad00\ub828 \uc704\ubc18\uc73c\ub85c \ucd5c\uc18c 87\uba85\uc774 \uccb4\ud3ec\ub418\uc5c8\ub2e4. \ub7ec\uc2dc\uc544\uc5d0\uc11c\ub294 2011\ub144 10\uac1c\uc6d4 \ub3d9\uc548 \ubaa8\uc2a4\ud06c\ubc14 \ucca0\ub3c4\uc5d0\uc11c 1000\uba85 \uc774\uc0c1\uc758 \uc5f4\ucc28 \uc11c\ud37c\uac00 \uccb4\ud3ec\ub418\uc5c8\ub2e4. \uc778\ub3c4\uc5d0\uc11c\ub294 2012\ub144 6\uc6d4 \uc911\uc559 \ucca0\ub3c4\uc5d0\uc11c \uc5f4\ucc28 \uc11c\ud551\uc73c\ub85c \ud558\ub8e8 \ub9cc\uc5d0 153\uba85\uc774 \uae30\uc18c\ub418\uc5c8\ub2e4.\n\uc5b5\uc81c\ucc45.\n\uc5f4\ucc28 \uc678\ubd80 \ud0d1\uc2b9 \uad00\ud589\uc744 \uc904\uc774\uae30 \uc704\ud574 \ucca0\ub3c4 \ud68c\uc0ac\ub4e4\uc740 \uc885\uc