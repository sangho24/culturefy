\n\ubb34\uacac\ubc95(\u6709\u898b\u6cd5,\n,\n) \uc989 \ubd04\uc774 \uc5c6\ub294 \ubc95\uc774\ub780 \uc720\uacac\ubc95\uc744 \uc81c\uc678\ud55c \ubaa8\ub4e0 \ubc95\uc744 \ub9d0\ud55c\ub2e4.\n\uc720\uacac(\u6709\u898b)\uc758 \uc131\uc9c8 \uc989 \uc720\uacac\uc131(\u6709\u898b\u6027)\uc744 \uac00\uacac\uc131(\u53ef\u898b\u6027)\uc774\ub77c\uace0\ub3c4 \ud55c\ub2e4. \ub9c8\ucc2c\uac00\uc9c0\ub85c, \uc720\uacac\uc744 \uac00\uacac(\u53ef\u898b)\uc774\ub77c\uace0\ub3c4 \ud55c\ub2e4. \ubb34\uacac(\u7121\u898b)\uc758 \uc131\uc9c8 \uc989 \ubb34\uacac\uc131(\u7121\u898b\u6027)\uc744 \ubd88\uac00\uacac\uc131(\u4e0d\u53ef\u898b\u6027)\uc774\ub77c\uace0\ub3c4 \ud55c\ub2e4. \ub9c8\ucc2c\uac00\uc9c0\ub85c, \ubb34\uacac\uc744 \ubd88\uac00\uacac(\u4e0d\u53ef\u898b)\uc774\ub77c\uace0\ub3c4 \ud55c\ub2e4.\n\uacbd\ub860\ubcc4 \uc124\uba85.\n\uc544\ube44\ub2ec\ub9c8\ud488\ub958\uc871\ub860.\n\ubd80\ud30c\ubd88\uad50\uc758 \uc124\uc77c\uccb4\uc720\ubd80\uc758 \ub17c\uc11c \u300a\uc544\ube44\ub2ec\ub9c8\ud488\ub958\uc871\ub860\u300b \uc81c6\uad8c\uc5d0 \ub530\ub974\uba74,\n \uc720\uacac\ubc95(\u6709\u898b\u6cd5)\uc740 \uc77c\uccb4\ubc95\uc744 \uc774\ub8e8\ub294 12\ucc98(\u5341\u4e8c\u8655) \uac00\uc6b4\ub370 1\ucc98\ub97c \ub9d0\ud55c\ub2e4. \uc989, \uc0c9\ucc98(\u8272\u8655)\ub97c \ub9d0\ud55c\ub2e4.\n \ubb34\uacac\ubc95(\u7121\u898b\u6cd5)\uc740 \uc77c\uccb4\ubc95\uc744 \uc774\ub8e8\ub294 12\ucc98(\u5341\u4e8c\u8655) \uac00\uc6b4\ub370 11\ucc98\ub97c \ub9d0\ud55c\ub2e4. \uc989, \uc0c9\ucc98(\u8272\u8655)\ub97c \uc81c\uc678\ud55c \ub098\uba38\uc9c0 \uc548\ucc98(\u773c\u8655)\uc774\ucc98(\u8033\u8655)\ube44\ucc98(\u9f3b\u8655)\uc124\ucc98(\u820c\u8655)\uc2e0\ucc98(\u8eab\u8655)\uc758\ucc98(\u610f\u8655)\uc131\ucc98(\u8072\u8655)\ud5a5\ucc98(\u9999\u8655)\ubbf8\ucc98(\u5473\u8655)\ucd09\ucc98(\u89f8\u8655)\ubc95\ucc98(\u6cd5\u8655)\uc758 11\ucc98\ub97c \ub9d0\ud55c\ub2e4.\n\uc544\ube44\ub2ec\ub9c8\uad6c\uc0ac\ub860.\n\ubd80\ud30c\ubd88\uad50\uc758 \uc124\uc77c\uccb4\uc720\ubd80\uc758 \ub17c\