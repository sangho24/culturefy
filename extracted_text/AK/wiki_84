740 \ud2b8\ub808\uc77c\ub7ec\ub97c \ucc3e\uc544 \uc5f4\uc1e0\ub97c \uc190\uc5d0 \ub123\uc73c\ub824\uace0 \ud558\uc9c0\ub9cc \ud2b8\ub808\uc77c\ub7ec \uc5b4\ub514\uc5d0\ub3c4 \uc5f4\uc1e0\ub294 \ubcf4\uc774\uc9c0 \uc54a\ub294\ub2e4. \uba3c\ub85c\ub85c\ubd80\ud130 \uc904\ub9ac\uc5e3\uacfc \ub808\ub108\ub4dc \ubc18\uc7a5\uc5d0 \ub300\ud55c \uc598\uae30\ub97c \uc804\ud574\ub4e4\uc740 \uc774 \ubaa8\ub4e0 \uc77c\uc758 \uc5f0\uad00\uc131\uc744 \uc9d0\uc791\ud558\uace0 \ud3ec\ud2c0\ub79c\ub4dc\ub85c \ub3cc\uc544\uc640 \uc560\ub4e4\ub9b0\uc758 \uc8fc\ubb38\uc744 \ud478\ub294 \ubc29\ubc95\uc744 \ucc3e\uc544\ub0b4\uace0\uc790 \ud55c\ub2e4.\n\uc2dc\ub0b4\uc758 \ud55c \uc740\ud589\uc5d0 \uac00\uba74\uc744 \uc4f4 \uac15\ub3c4\ub4e4\uc774 \ub4e4\uc774\ub2e5\uccd0 \ucd1d\uc744 \uc3d8\uba70 \ub3c8\uc744 \ud138\uc5b4 \uac04\ub2e4. \uadf8 \ud604\uc7a5\uc5d0 \uc788\uc5c8\ub358 \uba3c\ub85c\ub294 \uac15\ub3c4\ub4e4\uc774 \uac00\uba74\uc744 \uc4f4 \uac83\uc774 \uc544\ub2c8\ub77c \u2018\ubcf4\uac70\u2019\ub97c \ud55c \uc0c1\ud0dc\uc5d0\uc11c \uadf8\ub7f0 \ubc94\ud589\uc744 \uc800\uc9c8\ub800\ub2e4\ub294 \uac83\uc744 \uc54c\uace0 \ucda9\uaca9\uc5d0 \ube60\uc9c4\ub2e4. \ub9c8\uce68 \uc774 \uc0ac\uac74\uc744 \uc218\uc0ac\ud558\uac8c \ub41c \ub2c9\uacfc \ud589\ud06c\uc5d0\uac8c \uba3c\ub85c\ub294 \ubca0\uc13c\uc774 \ud3c9\ubc94\ud55c \uc885\uc871\ub4e4\ub85c\ubd80\ud130 \uac15\ud0c8\uc744 \ud558\uae30 \uc704\ud574 \uc77c\ubd80\ub7ec \ubcc0\uc2e0\ud55c \uc0c1\ud0dc\uc758 \ubaa8\uc2b5\uc744 \ubcf4\uc774\ub294 \uac83\uc740 \ubca0\uc13c \uc0ac\ud68c\uc758 \uac00\uc7a5 \uc911\ub300\ud55c \uaddc\uc728\uc744 \uc5b4\uae30\ub294 \uac83\uc774\ub77c\uace0 \uc124\uba85\ud574 \uc900\ub2e4. \ud55c\ud3b8 \ub85c\uc798\ub9ac\uc758 \uce58\ub8cc \ub355\ubd84\uc5d0 \ub808\ub108\ub4dc \ubc18\uc7a5\uacfc \uc904\ub9ac\uc5e3\uc740 \uc11c\ub85c\uc5d0 \ub300\ud55c \uac10\uc815\uc774 \uc870\uae08\uc529 \uc0ac\ub77c\uc9c0\uace0 \ub808\ub108\ub4dc \ubc18\uc7a5\uc740 \ub2c9\uc5d0\uac8c \uc560\ub4e4\ub9b0\uc774\ub098 \uc65