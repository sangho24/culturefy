\uc624 \ub4f1\ub3c4 \uc788\uc5c8\ub2e4.\n\ud65c\ub3d9.\n5.16 \uad70\uc0ac\ud601\uba85 \uac00\ub2f4.\n1961\ub144 5\uc6d4 16\uc77c, \uc81c1\uacf5\uc218\ud2b9\uc804\ub2e8\uc740 \ub2e8\uc7a5 \ubc15\uce58\uc625 \ub300\ub839\uc5d0 \uc758\ud574 2\uad70 \ubd80\uc0ac\ub839\uad00 \ubc15\uc815\ud76c \uc18c\uc7a5\uc774 \uc77c\uc73c\ud0a8 5\u00b716 \uad70\uc0ac\uc815\ubcc0\uc5d0 \uac00\ub2f4\ud558\uc600\ub2e4.\n1964\ub144 5\uc6d4 21\uc77c \uc0c8\ubcbd, \ucd5c\ubb38\uc601 \ub300\ub839\uc758 \uc8fc\ub3c4\uc5d0 \uc758\ud574 \ubd80\ub300\uc6d0\ub4e4\uc774 \ubc95\uc6d0\uc5d0 \ub09c\uc785\ud558\ub294 \uc18c\ub3d9\uc744 \uc77c\uc73c\ucf30\ub2e4. \ub2f9\uc2dc \ubc95\uc6d0 \ubc0f \ud310\uc0ac\uc758 \uc790\ud0dd\uc744 \ubd88\ubc95\uc801\uc73c\ub85c \uce68\uc785\ud55c \ubd80\ub300\uc6d0\ub4e4\uc740 \ud559\uc0dd \uc2dc\uc704\uc640 \uad00\ub828\ub41c \uc601\uc7a5 \ubc1c\ubd80\ub97c \uc694\uad6c\ud558\uc600\ub2e4.\n\ubca0\ud2b8\ub0a8 \uc804\uc7c1 \ucc38\uc804.\n1960\ub144\ub300\uc5d0\ub294 \uc218\ub3c4\uc0ac\ub2e8 \"\ub9f9\ud638\"\ubd80\ub300\uc640 \uc81c9\uc0ac\ub2e8 \"\ubc31\ub9c8\"\ubd80\ub300\uc5d0 \ubc30\uc18d\ub418\uc5b4 \uc0ac\ub2e8 \uacf5\uc218\ud2b9\uc804\ub300\ub85c\uc11c \ubca0\ud2b8\ub0a8 \uc804\uc7c1\uc5d0 \ucc38\uac00\ud558\uc600\ub2e4.\n1969\ub144 8\uc6d4 18\uc77c, \ub3d9\ud574\uc548\uacbd\ube44\uc0ac\ub839\ubd80\uc758 1, 2 \uc720\uaca9 \uc5ec\ub2e8\uacfc \ud568\uaed8 \uc0c8\ub85c \ucc3d\uc124\ub41c \ud2b9\uc218\uc804\uc0ac\ub839\ubd80\uc5d0 \uc608\uc18d\ub418\uc5c8\ub2e4.\n1972\ub144 9\uc6d4, \uc900\uc7a5\uc774 \ubd80\ub300\uc7a5\uc778 \uc5ec\ub2e8\uae09 \ubd80\ub300\ub85c \uc99d\ud3b8\ub418\uc5c8\uace0 \ubd80\ub300\uba85\ub3c4 \uc81c1\uacf5\uc218\ud2b9\uc804\uc5ec\ub2e8(1st Special Forces Brigade)\uc73c\ub85c \uac1c\uce6d\ub418\uc5c8\ub2e4.\n1973\ub144 4\uc6d4, \uc804\ub450\ud658 \uc900\uc7a5\uc740 \ud558\ub098\ud68c \uc0ac\uac74\uc73c\ub85c \uc778\ud574 \ub4e4\ud1b5\ub098 \ucc98\ubc8c\ud560 \uc608\uc815\uc774\uc600\uc73c\ub098, \ubc15\uc885\uaddc\uc758 \ub3c4\uc6c0\uc73c\ub8