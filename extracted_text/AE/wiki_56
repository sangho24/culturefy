\ub610\ub294 \ucf00\ub784\ub77c\"\ub85c \uc9c0\uce6d\ud588\ub2e4.\n\ucf00\ub784\ub77c \ub0a8\ubd80\uc758 \ucf5c\ub78c \ud56d\uad6c\uc5d0 \uae30\ubc18\uc744 \ub454 \ubca0\ub098\ub4dc(\ubca0\ub098\ub4dc \uccb4\ub77c \ub610\ub294 \ucfe8\ub77c\uc138\uce74\ub77c)\uc758 \ud1b5\uce58\uc790\ub4e4\uc740 \uc790\uc2e0\ub4e4\uc758 \uc870\uc0c1\uc774 \ud398\ub8e8\ub9d0\uc871\uc774\ub77c\uace0 \uc8fc\uc7a5\ud588\ub2e4. \uccb4\ub77c\ub098\ub4dc\ub294 \ucf54\uc9c0\ucf54\ub4dc \uc655\uad6d\uc5d0 \uc788\ub358 \uc61b \uc9c0\ubc29\uc758 \uc774\ub984\uc774\uae30\ub3c4 \ud588\ub294\ub370, \uc624\ub298\ub0a0\uc758 \ud2f0\ub8e8\ub791\uac00\ub514\uc640 \ub9d0\ub77c\ud478\ub78c\uad6c\uc758 \ud2f0\ub8e8\ub974 \ud0c8\ub8e8\ud06c\ub4e4 \ud3ec\ud568\ud558\uace0 \uc788\uc5c8\ub2e4. \ub098\uc911\uc5d0 \ub9d0\ub77c\ubc14\ub974\uac00 \uc601\uad6d\ub839 \uc778\ub3c4\uc758 \uc9c0\ubc30\ub97c \ubc1b\uac8c \ub418\uc5c8\uc744 \ub54c \uc774 \uc9c0\uc5ed\uc740 \ub9d0\ub77c\ubc14\ub974\uad6c\uc758 \ud0c8\ub8e8\ud06c\uac00 \ub418\uc5c8\ub2e4. \uccb4\ub77c\ub098\ub4dc \ud0c8\ub8e8\ud06c\uc758 \uc18c\uc7ac\uc9c0\ub294 \ud2f0\ub8e8\ub791\uac00\ub514 \ub9c8\uc744\uc774\uc5c8\ub2e4. \ud6d7\ub0a0 \uccb4\ub77c\ub098\ub4dc \ud0c8\ub8e8\ud06c\ub294 \uc5d0\ub77c\ub098\ub4dc \ud0c8\ub8e8\ud06c\uc640 \ud1b5\ud3d0\ud569\ub418\uc5c8\ub2e4.\n\uc5b4\uc6d0.\n\ub4dc\ub77c\ube44\ub2e4\uc5b4 \uacc4\ud1b5\uc758 \"\uccb4\ub77c\"\ub77c\ub294 \uce6d\ud638\ub294 \"\uccb4\ub77c\ub9cc\" \ubc0f \"\uccb4\ub784\ub77c\ub974/\uccb4\ub784\"\uacfc \uac19\uc740 \ubcc0\ud615\uacfc \"\ucf00\ub784\ub77c\ud478\ud2b8\ub77c\" \ub610\ub294 \"\ucf00\ub784\ub77c\"\uc640 \uac19\uc740 \ub2e4\uc591\ud55c \uc778\ub3c4\uc544\ub9ac\uc544\uc5b4\uad70\uc5d0 \ud574\ub2f9\ud558\ub294 \uc774\ub984\uacfc \ud568\uaed8 \ud1b5\uce58 \uc655\uc870\ub098 \uac00\ubb38, \ubbfc\uc871, \uadf8\ub9ac\uace0 \uadf8\ub4e4\uacfc \uad00\ub828\ub41c \uc9c0\ub9ac\uc801 \uc9c0\uc5ed\uc744 \uc9c0\uce6d\ud55c\ub2e4. \"\uccb4\ub77c\ub9cc\"\uc774\ub77c\ub294 \uce6d\ud638\ub294 \uc77c\ub