0\uc11c \uc120\ud615 \ub3c5\ub9bd\uc778 \ub2e8\uba74\uc744 \uac16\ub294 \uac83\uc5d0 \ub300\ud55c \ubc29\ud574 \uc870\uac74()\uc774\ub2e4. \uc989, formula_28\uc774\uc5b4\uc57c\uc9c0\ub9cc \uc774\ub9cc\ud07c\uc758 \uc120\ud615 \ub3c5\ub9bd \ubca1\ud130\uc7a5\ub4e4\uc774 \uc874\uc7ac\ud560 \uc218 \uc788\ub2e4. \ud2b9\ud788, formula_29\uc740 \ubaa8\ub4e0 \uacf3\uc5d0\uc11c 0\uc774 \uc544\ub2cc \ubca1\ud130\uc7a5\uc774 \uc874\uc7ac\ud560 \ubc29\ud574 \uc870\uac74\uc774\uba70, formula_30\uc740 \ubaa8\ub4e0 \uacf3\uc5d0\uc11c 0\uc774 \uc544\ub2cc (\ubcf5\uc18c\uc218) \ud544\ubc14\uc778\uc774 \uc874\uc7ac\ud560 \ubc29\ud574 \uc870\uac74\uc774\ub2e4.\n\ubcf5\uc18c\uc218 formula_22\ucc28\uc6d0 \ubcf5\uc18c\ub2e4\uc591\uccb4 formula_1\uc758 \ucd5c\uace0\ucc28 \ucc9c \ud2b9\uc131\ub958 formula_29\ub294 \uadf8 \uc624\uc77c\ub7ec \ud2b9\uc131\ub958 formula_34\uacfc \uac19\ub2e4.\n\uc608.\n\uc790\uba85\ud55c \ubcf5\uc18c\uc218 \ubca1\ud130 \ub2e4\ubc1c\uc758 \ucc9c \ud2b9\uc131\ub958\ub294 \ud56d\uc0c1 0\uc774\ub2e4. \ubca1\ud130 \ub2e4\ubc1c\uc758 \uc30d\ub300\ub2e4\ubc1c\uc758 \ucc9c \ud2b9\uc131\ub958\ub294 \uc6d0\ub798 \ubca1\ud130 \ub2e4\ubc1c\uc758 \ucc9c \ud2b9\uc131\ub958\uc758 \u22121\ubc30\uc774\ub2e4.\nformula_35\n\ub9ac\ub9cc \uace1\uba74.\n\ucf64\ud329\ud2b8 \ub9ac\ub9cc \uace1\uba74 formula_36 \uc704\uc758, \uc778\uc790 formula_37\uc5d0 \ub300\uc751\ud558\ub294 \uc120\ub2e4\ubc1c formula_38\uc758 \ucc9c \uc218\ub294 \uadf8 \ucc28\uc218 formula_39\uc640 \uac19\ub2e4.\nformula_40\n\ud2b9\ud788, \ub9ac\ub9cc \uace1\uba74\uc758 \uc811\ub2e4\ubc1c formula_41 \ubc0f \ud45c\uc900 \uc120\ub2e4\ubc1c(=\uacf5\ubcc0\uc811\ub2e4\ubc1c) formula_42\uc758 \ucc28\uc218\ub294 \uac01\uac01 formula_43 \ubc0f formula_44\uc774\ubbc0\ub85c, \uc774\ub4e4\uc758 \ucc9c \ud2b9\uc131\ub958\ub294 \ub2e4\uc74c\uacfc \uac19\ub2e4.\nformula_45\nformula_46\n\ub9cc\uc57d formula_47\uc77c \uacbd\uc6b0 (\uc6d0\ud658\uba74 = \ubcf5\uc18c\uc218 \ud0c0\uc6d0 \uace1\uba74), \uc811\ub2e4\ubc1c \ubc0f \ud4