\ub144\ub300 \uc18c\uac1c\ub418\uba74\uc11c, \uba87\ub144 \ud6c4 \ub9ce\uc740 \uc870\ud604\ubcd1 \ud658\uc790\ub4e4\uc774 \uc2dc\uc124\uc5d0\uc11c \ub098\uc624\uac8c \ub418\uc5c8\ub2e4. 1970\ub144\ub300 \uc5f0\uad6c\uc790\ub4e4\uc740 \uc815\ud615\uc815 \ud560\uc815\uc2e0\ubcd1\uc81c\uc81c\uac00 D2 \uc218\uc6a9\uc81c(D2 receptor)\uc758 \uae38\ud56d\uc81c(antagonist)\ub85c \uc791\uc6a9\ud55c\ub2e4\ub294 \uac83\uc744 \uc54c\uc558\ub2e4. \uc774\ub7ec\ud55c \uc778\uc2dd\uc740 \uc870\ud604\ubcd1\uc774 \ub1cc\uc758 \ub3c4\ud30c\ubbfc\uccb4\uacc4\uac00 \uacfc\uc789\ud65c\ub3d9\ud558\ub294 \uac83\uc73c\ub85c \uc720\ubc1c\ub41c\ub2e4\ub294 \uac00\uc815\uc758 '\uc870\ud604\ubcd1\uc758 \ub3c4\ud30c\ubbfc \uac00\uc124'\uc758 \uae30\ubc18\uc774 \ub41c\ub2e4. \ub3c4\ud30c\ubbfc \uac00\uc124\uc740 \uba54\uc2a4\uc554\ud398\ud0c0\ubbfc \ub4f1 \ub3c4\ud30c\ubbfc \uc0c1\uc2b9 \uac01\uc131\uc81c\uc5d0 \uc758\ud558\uc5ec \uc99d\uc0c1\uc774 \uac15\ud654\ub41c\ub2e4\ub294 \uad00\ucc30\uc744 \ud1b5\ud558\uc5ec \uc0c8\ub85c\uc6b4 \uc9c0\uc9c0\ub97c \uc5bb\uac8c \ub418\uc5c8\ub2e4. \uadf8\ub9ac\uace0 \uc774\ub7ec\ud55c \uc57d\ubb3c\uc744 \uacfc\ub2e4\ubcf5\uc6a9\ud55c \uacbd\uc6b0 \uac74\uac15\ud55c \uc0ac\ub78c\ub3c4 \uc815\uc2e0\ubcd1\uc744 \uc5bb\uc744 \uc218 \uc788\ub2e4\ub294 \uac83\uc5d0 \ub300\ud558\uc5ec\uc11c\ub3c4 \uc99d\uba85\ub418\uc5c8\ub2e4. 1980\ub144\ub300\uc5d0\ub294 \ubd80\uc791\uc6a9\uc774 \ub35c\ud55c \ube44\uc815\ud615\uc801 \ud56d\uc815\uc2e0\ubcd1\uc81c\uc81c\uac00 \uac1c\ubc1c\ub418\uc5c8\ub2e4. \uc774\ub7ec\ud55c \uc0c8\ub85c\uc6b4 \uc57d\ubb3c\ub4e4\uc740 \ub3c4\ud30c\ubbfc \uc218\uc6a9\uccb4\uc5d0 \uc9c1\uc811 \uc791\uc6a9\ud558\uc9c0 \uc54a\uc9c0\ub9cc, \uac04\uc811\uc801\uc73c\ub85c \ub3c4\ud30c\ubbfc \ud65c\ub3d9\uc758 \ubcc0\ud654\ub97c \uc57c\uae30\ud55c\ub2e4. \uc774\ub7ec\ud55c \uc57d\ubb3c\ub4e4\uc740 \ub2e4\ub978 \uc815\uc2e0\ubcd1 \uce58\ub8cc\uc5d0\ub3c4 \uc0ac\uc6a9\ub41c\ub2e4. \ud56d\uc815\uc2e0\ubcd1\uc81c\uc81c\ub294 \ub300\ubd80\ubd84 \ud65c\ub3d9\uc5d0 \uad11\ubc94\uc704\ud55c \uc5b5\uc81c \ud6a8\uacfc\ub97c \uac00\uc9c0\uace0 \uc788\uc73c\uba70 \ud2b9\ud788 \uac89\uc73c\ub85c \ub4dc\ub7ec\ub09c \uc815\uc2e0\ubcd1\uc758 \ub9dd\uc0c1\uc801\uc774\uace0 \ub3d9\uc694\uc801\uc778 \ud589\ub3d9 \ud2b9\uc131\uc744 \uc904\uc5ec\uc900\ub2e4.\n\uadf8\ub7ec\ub098 \uc774\ud6c4 \uad00\ucc30\ub4e4\uc744 \ud1b5\ud558\uc5ec \ucd5c\uc18c\ud55c \uac04\ub2e8\ud55c \uc6d0\ud615\uc5d0\uc11c \ub3c4\ud30c\ubbfc \uac00\uc124\uc774 \uc778\uae30\ub97c \uc783\uc5c8\ub2e4. \uc77c\ub840\ub85c \uc870\ud604\ubcd1 \ud658\uc790\ub294 \ub450\ub1cc \ub3c4\ud30c\ubbfc \ud65c\ub3d9 \uce21\uc815 \uc218\uce58\uac00 \uc99d\uac00\ud55c \uac83\uc744 \ubcf4\uc774\uc9c0 \uc54a\ub294\ub2e4. \uadf8\ub807\uae34 \ud574\ub3c4 \ub9ce\uc740 \uc815\uc2e0\uacfc\uc758\uc0ac\uc640 \uc2e0\uacbd\uacfc\ud559\uc790\ub4e4\uc740 \uc870\ud604\ubcd1\uc774 \ub3c4\ud30c\ubbfc\uccb4\uacc4 \uc7a5\uc560\uc640 \uad00\ub828\uc788\ub2e4\uace0 \uc0dd\uac01\ud55c\ub2e4. \uadf8\ub7ec\ub098 \uc2dc\uac04\uc774 \ud750\ub974\uba74\uc11c \ub3c4\ud30c\ubbfc \uac00\uc124\ub3c4 \ubc1c\uc804\ud558\uc600\uace0, \uac00\uc124\uc774 \uc0c1\uc815\ud558\ub294 \ub3c4\ud30c\ubbfc\uccb4\uacc4 \uc7a5\uc560\uac00 \uc810\ucc28 \ubbf8\ubb18\ud558\uace0 \ubcf5\uc7a1\ud574\uc9c0\uac8c \ub418\uc5c8\ub2e4.\n\uc815\uc2e0\uc57d\ud559\uc790 \uc2a4\ud2f0\ube10 \uc2a4\ud0c8(Stephen M. Stahl)\uc740 2018\ub144 \ub9ac\ubdf0\uc5d0\uc11c \uc870\ud604\ubcd1 \ub4f1 \ub9ce\uc740 \uc815\uc2e0\ubcd1 \uc0ac\ub840\uc5d0\uc11c \ub3c4\ud30c\ubbfc, \uc138\ub85c\ud1a0\ub2cc, \uae00\ub8e8\ud0c0\uba54\uc774\ud2b8\uac00 \uac01\uac01 \ud639\uc740 \uc11c\ub85c \ub2e4\uc591\ud558\uac8c \uc870\ud569\ub41c \uac83\uc744 \uae30\ubc18\uc73c\ub85c \uad6c\ucd95\ub418\ub294 \uc138 \uad00\ub828 \ub124\ud2b8\uc6cc\ud06c\uac00 \uc120\uc870\uccb4 \ub0b4 \ub3c4\ud30c\ubbfc D2 \uc218\uc6a9\uccb4\uc758 \uacfc\uc790\uadf9(overexcitation)\uc758 \uc6d0\uc778\uc774\ub77c\ub294 \uac83\uc744 \uc81c\uc2dc\ud558\uc600\ub2e4.\n\uc8fc\uc758\ub825\uacb0\ud54d \uacfc\uc789\ud589\ub3d9\uc7a5\uc560.\n\ub3c4\ud30c\ubbfc \uc2dc\uc2a4\ud15c\uc5d0 \ubb38\uc81c\uac00 \uc0dd\uae34 \uacbd\uc6b0 \uc8fc\uc758\ub825\uacb0\ud54d \uacfc\uc789\ud589\ub3d9\uc7a5\uc560(ADHD)\uac00 \ub098\ud0c0\ub098\uae30\ub3c4 \ud55c\ub2e4. ADHD \ud658\uc790\ub294 \uc778\uc9c0\uae30\ub2a5 \uc911 \uc9d1\uc911\ub825 \ubc0f \ud589\ub3d9 \uc5b5\uc81c, \uc791\uc5c5 \uae30\uc5b5 \ub4f1\uc774 \uc57d\ud654\ub418\ub294 \uc99d\uc0c1\uc744 \ubcf4\uc778\ub2e4. \ub3c4\ud30c\ubbfc\uc5d0 \ub300\ud55c \uc2e0\uacbd\uc804\ub2ec\ubb3c\uc9c8 \uc218\uc6a9\uccb4\uc640 \uc218\uc1a1 \ub2e8\ubc31\uc9c8\uacfc \uad00\ub828\ub41c \uc720\uc804\uc790\uac00 ADHD \ud658\uc790\uc5d0\uc11c \uc815\uc0c1\uc778\uad70\uacfc \ube44\uad50\ud588\uc744 \ub54c \ucc28\uc774\uac00 \ub098\ud0c0\ub09c\ub2e4\ub294 \uac83\uc774 \ubcf4\uace0\ub418\uc5c8\ub2e4. \ub3c4\ud30c\ubbfc\uacfc ADHD \uac04\uc758 \uac00\uc7a5 \uc911\uc694\ud55c \uad00\uacc4\ub294 ADHD \uce58\ub8cc\uc5d0 \uc0ac\uc6a9\ub418\ub294 \uc57d\ubb3c\uacfc \uad00\ub828\ub41c\ub2e4. ADHD\uc5d0 \ud6a8\uacfc\ub97c \ubcf4\uc774\ub294 \uce58\ub8cc\uc81c\ub294\uc0c1\ud488\uba85 \ub9ac\ud0c8\ub9b0(Ritalin)\uc774\ub098 \ucf58\uc138\ub974\ud0c0(Concerta) \ub4f1\uc758 \uba54\ud2f8\ud398\ub2c8\ub370\uc774\ud2b8(methylphenidate), \uc0c1\ud488\uba85 \uc5d0\ubca0\ucf00\uc624(Evekeo), \uc544\ub370\ub864(Adderall), \ub371\uc138\ub4dc\ub9b0(Dexedrine) \ub4f1\uc758 \uc554\ud398\ud0c0\ubbfc(amphetamine) \ub4f1\uc758 \uac01\uc131\uc81c, \uadf8\ub9ac\uace0 \ub1cc\uc758 \ub3c4\ud30c\ubbfc\uacfc \ub178\ub974\uc5d0\ud53c\ub124\ud504\ub9b0 \uc218\uce58\ub97c \ub192\uc5ec\uc8fc\ub294 \uc57d\ubb3c \ub4f1\uc774 \uc788\ub2e4. ADHD \uce58\ub8cc \uac01\uc131\uc81c\uc758 \uc784\uc0c1 \ud6a8\uacfc\ub294 \ub3c4\ud30c\ubbfc\uacfc \ub178\ub808\ud53c\ub124\ud504\ub9b0 \uc218\uc6a9\uccb4, \ud2b9\ud788 \uc804\uc804\ub450\uc5fd\ud53c\uc9c8 \ub0b4 \ub3c4\ud30c\ubbfc \uc218\uc6a9\uccb4 D1(dopamine receptor D1)\uacfc \uc54c\ud30c-2 \uc544\ub4dc\ub808\ub0a0\ub9b0 \uc218\uc6a9\uccb4(Alpha-2 adrenergic receptor, adrenoceptor \u03b12)\uc758 \uac04\uc811 \ud65c\uc131\ud654\ub97c \ud1b5\ud574 \ub9e4\uac1c\ub41c\ub2e4.\n\ud1b5\uc99d.\n\ub3c4\ud30c\ubbfc\uc740 \ucc99\uc218, \uc911\uc2ec\ud68c\uc0c9\uc9c8, \uc2dc\uc0c1, \uae30\uc800\ud575, \ub300\uc0c1\ud53c\uc9c8 \ub4f1\uc758 \uc911\ucd94\uc2e0\uacbd\uacc4\uc758 \ubcf5\ud569\uc801 \uc218\uc900\uc5d0\uc11c \ud1b5\uc99d \uacfc\uc815\uc5d0 \uc911\uc694\ud55c \uc5ed\ud560\uc744 \ud55c\ub2e4. \ub3c4\ud30c\ubbfc \uc218\uce58 \uc800\ud558\ub294 \ud30c\ud0a8\uc2a8\ubcd1\uc5d0\uc11c \uc790\uc8fc \ubc1c\uc0dd\ud558\ub294 \ud1b5\uc99d\uacfc \uad00\ub828 \uc788\ub2e4. \ub3c4\ud30c\ubbfc \uc2e0\uacbd\uc804\ub2ec\uc791\uc6a9(dopaminergic neurotransmission)\uc5d0\uc11c \ubc1c\uc0dd\ud558\ub294 \uae30\ud615\uc740 \ub610\ud55c \uad6c\uac15\uc791\uc5f4\uac10\uc99d\ud6c4\uad70, \uc12c\uc720\uadfc\uc721\ud1b5, \ud558\uc9c0\ubd88\uc548\uc99d\ud6c4\uad70 \ub4f1\uc758 \uc9c8\ud658\uc5d0\uc11c\ub3c4 \ubc1c\uc0dd\ud55c\ub2e4.\n\uad6c\uc5ed\uc9c8.\n\uad6c\uc5ed\uc9c8\uacfc \uad6c\ud1a0\ub294 \ud654\ud559\uc218\uc6a9\uccb4\uc790\uadf9\ub300(chemoreceptor trigger zone)\ub85c \uc54c\ub824\uc9c4 \ub1cc\uac04\uc758 \uc228\ub1cc\uc5d0 \uc788\ub294 \uad6c\ud1a0\uc911\ucd94(area postrema)\uc758 \ud65c\ub3d9\uc5d0 \ud06c\uac8c \uacb0\uc815\ub41c\ub2e4. \uc774 \uc601\uc5ed\uc740 D2\ud615 \ub3c4\ud30c\ubbfc \uc218\uc6a9\uccb4(type D2 dopamine receptor)\uac00 \ubd84\ud3ec\ub418\uc5b4 \uc788\ub2e4. \uadf8 \uacb0\uacfc, D2 \uc218\uc6a9\uccb4\ub97c \ud65c\uc131\ud654\ud558\ub294 \uc57d\ubb3c\uc740 \uad6c\uc5ed\uc9c8 \uc720\ubc1c \uac00\ub2a5\uc131\uc774 \ub192\ub2e4. \uc774 \uadf8\ub8f9\uc740 \ud30c\ud0a8\uc2a8\ubcd1\uc5d0 \ud22c\uc5ec\ub418\ub294 \uc57d\ubb3c\uc740 \ubb3c\ub860 \uc544\ud3ec\ubaa8\ub974\ud540(apomorphine)\uacfc \uac19\uc740 \ub3c4\ud30c\ubbfc \uc791\uc6a9\uc81c(dopamine agonist)\ub3c4 \ud3ec\ud568\ub41c\ub2e4. \uc77c\ubd80 \uacbd\uc6b0, \uba54\ud1a0\ud074\ub85c\ud504\ub77c\ubbf8\ub4dc \uac19\uc740 D2 \uc218\uc6a9\uccb4 \uae3