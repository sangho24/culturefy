uc758\uc815\uc744 \uc5f4\uc5c8\ub2e4. \ucd5c\ub300\uc758 \uc758\uc81c\ub294 \uc138\ub825\uc744 \ud68c\ubcf5\ud558\uc5ec \ud6c4\ucfe0\ud558\ub77c\uae4c\uc9c0 \uc9c4\ucd9c\ud55c \ud5e4\uc774\uc2dc\uc5d0 \uc5b4\ub5bb\uac8c \ub9de\uc11c\ub290\ub0d0 \ud558\ub294 \uac83\uc774\uc5c8\ub2e4. \uc624\uc774\ub178\ubbf8\uce74\ub3c4 \uc4f0\ub124\ubb34\ub124()\uc640 \ub3c4\ucfe0\ub2e4\uc774\uc9c0 \uc0ac\ub124\uc0ac\ub2e4(\u5fb3\u5927\u5bfa\u5b9f\u5b9a)\ub294 \uace0\uc2dc\ub77c\uce74\uc640\uc778\uc758 \ub73b\uc5d0 \ub530\ub77c \ucd94\ud1a0\ub97c \uc8fc\uc7a5\ud588\uace0(\u300a\uad50\ucfe0\uc694\u300b \uc815\uc6d4 22\uc77c\uc870) \ucd9c\uc11d\uc790 \ub300\ubd80\ubd84\uc774 \uc0bc\uc885\uc2e0\uae30\uc758 \uc548\uc804\uc744 \uc704\ud574\uc11c \uc0ac\uc790\ub97c \ud30c\uacac\ud558\uc790\ub294 \uc758\uacac\uc744 \ube44\uce5c \uac00\uc6b4\ub370 \uc778\uc758 \uadfc\uc2e0\uc774\uc5c8\ub358 \ud6c4\uc9c0\uc640\ub77c\ub178 \ub3c4\ubaa8\uce74\ud0c0(\u85e4\u539f\u671d\u65b9)\u00b7\ubbf8\ub098\uc138 \uc9c0\uce74\ub178\ubd80(\u6c34\u7121\u702c\u89aa\u4fe1)\u00b7\ub2e4\uc774\ub77c\ub178 \uc9c0\uce74\ubb34\ub124(\u5e73\u89aa\u5b97)\ub3c4 \u201c\uc624\ub85c\uc9c0 \uc815\ubc8c\ub9cc\uc774 \uc788\uc744 \ubfd0(\u504f\u306b\u5f81\u4f10\u305b\u3089\u308b\u3079\u3057)\u201d\uc774\ub77c\uace0 \uc8fc\uc7a5\ud55c\ub2e4. \uc774\ub294 \u201c\ubc95\ud669\uc758 \ubc14\ub7a8(\u6cd5\u7687\u306e\u5fa1\u7d20\u61d0)\u201d(\u300a\uad50\ucfe0\uc694\u300b \uc815\uc6d4 27\uc77c\uc870, 2\uc6d4 2\uc77c\uc870)\uc5d0 \uc758\ud55c \uac83\uc73c\ub85c 26\uc77c, \ub2e4\uc774\ub77c\ub178 \ubb34\ub124\ubaa8\ub9ac \ucd94\ud1a0\ub97c, 29\uc77c\uc5d0 \uc694\uc2dc\ub098\uce74 \uc794\ub2f9\uc744 \ucd94\ud1a0\ub97c \uba85\ud558\ub294 \uc120\uc9c0\uac00 \ub0b4\ub824\uc84c\ub2e4(\u300a\uad50\ucfe0\uc694\u300b 2\uc6d4 23\uc77c\uc870). \ud5e4\uc774\uc2dc\uac00 \uc815\uad8c\uc5d0 \ubcf5\uadc0\ud558\uba74 \ub2e4\uc2dc \uc778\uc138\uc774\uac00 \uc815\uc9c0\ub418\uace0 \uc720\ud3d0\ub2f9\ud560 \uac83\uc744 \uace0\uc2dc\ub77c\uce74\uc640\uc778\u